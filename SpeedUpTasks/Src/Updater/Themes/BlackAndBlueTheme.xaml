<ResourceDictionary 
                    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:dxwui="http://schemas.devexpress.com/winfx/2008/xaml/windowsui"                   
                    xmlns:dxlc="http://schemas.devexpress.com/winfx/2008/xaml/layoutcontrol"
                    xmlns:dxe="http://schemas.devexpress.com/winfx/2008/xaml/editors">



    <ImageSource x:Key="MinimizeImageSource" >/GeosWorkbenchInstaller;component/Assets/Images/Minimize_White_16x16.png</ImageSource>

    <Style TargetType="Label" x:Key="LabelForegroundstyle" BasedOn="{StaticResource BaseUniversalStyle}" >
        <Setter Property="Foreground" Value="#FFFFFFFF" />
    </Style>

    <SolidColorBrush x:Key="ForegroundColorLabel" Color="#FFFFFFFF"/>

    <Style TargetType="Border" x:Key="BorderBacgroungInner">
        <Setter Property="Background" Value="#FF2AB7FF"></Setter>
    </Style>

    <Style TargetType="Border" x:Key="BorderBacgroungMainWindow">
        <Setter Property="Background" Value="Black"></Setter>
    </Style>
    <Style TargetType="Border" x:Key="BorderBacgroungOuter">
        <Setter Property="Background" Value="#FF333333"></Setter>
    </Style>

    <Style TargetType="Image" x:Key="GeosImagePath">
        <Setter Property="Source" Value="/GeosWorkbenchInstaller;component/Assets/Images/GeosLogo.png"></Setter>
    </Style>

    <Style TargetType="dxe:ProgressBarEdit" x:Key="ProgressBarEditForeground" BasedOn="{StaticResource BaseUniversalStyle}" >
        <Setter Property="Foreground" Value="#FF2AB7FF"></Setter>
        <Setter Property="ShowBorder" Value="False"></Setter>
    </Style>

    <Style TargetType="Image" x:Key="EmdepGeosLogo">
        <Setter Property="Source" Value="/GeosWorkbenchInstaller;component/Assets/Images/EmdeplogoWhite.png" />
    </Style>

    <Style TargetType="Label" x:Key="LableForegroundColor" BasedOn="{StaticResource BaseUniversalStyle}" >
        <Setter Property="Foreground" Value="#FFFFFFFF" />
    </Style>

    <Style TargetType="TextBlock" x:Key="TextBlockForegroundColor" BasedOn="{StaticResource TextBlockStyle}" >
        <Setter Property="Foreground" Value="#FFFFFFFF" />
    </Style>

    <Style TargetType="Border" x:Key="SplasherBackgroundColor">
        <Setter Property="Background" Value="#FF333333" />
    </Style>
    <Style TargetType="{x:Type Button}" BasedOn="{StaticResource BaseUniversalStyle}" >
        <Setter Property="Control.Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type Button}">
                    <Border Name="Border" BorderBrush="{TemplateBinding BorderBrush}" BorderThickness="{TemplateBinding BorderThickness}" Background="{TemplateBinding Background}">
                        <ContentPresenter Content="{TemplateBinding Content}" ContentTemplate="{TemplateBinding ContentTemplate}" Margin="{TemplateBinding Padding}"  HorizontalAlignment="Center" VerticalAlignment="Center" />
                    </Border>
                    <ControlTemplate.Triggers>
                        <Trigger Property="Button.IsFocused" Value="True">
                            <Setter TargetName="Border" Property="BorderBrush" Value="#FF2AB7FF" />
                            <Setter TargetName="Border" Property="Background" Value="#FF333333" />
                        </Trigger>
                        <Trigger Property="Button.IsFocused" Value="False">
                            <Setter TargetName="Border" Property="Background" Value="#FF333333" />
                        </Trigger>
                        <Trigger Property="Button.IsEnabled" Value="false">
                            <Setter TargetName="Border" Property="Opacity" Value="0.3" />
                            <Setter TargetName="Border" Property="BorderBrush" Value="#FFB9B9B9" />
                        </Trigger>

                        <Trigger Property="Button.IsMouseOver" Value="True">
                            <Setter TargetName="Border" Property="Background" Value="#FF515151" />
                        </Trigger>
                        <Trigger Property="Button.IsMouseOver" Value="False">
                            <Setter TargetName="Border" Property="Background" Value="#FF333333" />
                        </Trigger>
                        <DataTrigger Binding="{Binding IsBusy}" Value="True">
                            <Setter Property="Cursor" Value="Wait" />
                        </DataTrigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <Style TargetType="{x:Type TextBlock}"  x:Key="VersionTitle" BasedOn="{StaticResource TextBlockStyle}" >
        <Style.Triggers>
            <DataTrigger Binding="{Binding VersionTitle}" Value="0">
                <Setter Property="Text" Value="{DynamicResource VersionTitleInstallWorkbench}" />
                <Setter Property="Foreground" Value="White" />
            </DataTrigger>
            <DataTrigger Binding="{Binding VersionTitle}" Value="1">
                <Setter Property="Text" Value="{DynamicResource VersionTitleUpdateWorkbench}" />
                <Setter Property="Foreground" Value="White" />
            </DataTrigger>
            <DataTrigger Binding="{Binding VersionTitle}" Value="2">
                <Setter Property="Text" Value="{DynamicResource VersionTitleAlreadyUpdatedWorkbench}"/>
                <Setter Property="Foreground" Value="White" />
            </DataTrigger>
        </Style.Triggers>
    </Style>


    <Style TargetType="{x:Type TextBlock}" x:Key="TextShowUpdater" BasedOn="{StaticResource TextBlockStyle}">
        <Style.Triggers>
            <DataTrigger Binding="{Binding VersionTitle}" Value="0">
                <Setter Property="Text" Value="{DynamicResource ShowUpdater}" />
                <Setter Property="Foreground" Value="White" />
            </DataTrigger>
            <DataTrigger Binding="{Binding VersionTitle}" Value="1">
                <Setter Property="Text" Value="{DynamicResource ShowUpdater}" />
                <Setter Property="Foreground" Value="White" />
            </DataTrigger>
            <DataTrigger Binding="{Binding VersionTitle}" Value="2">
                <Setter Property="Text" Value="{DynamicResource VersionTitleAlreadyUpdated}"/>
                <Setter Property="Foreground" Value="White" />
            </DataTrigger>
        </Style.Triggers>
    </Style>

    <Style TargetType="Label" x:Key="LableHeader" BasedOn="{StaticResource BaseUniversalStyle}" >
        <Setter Property="FontSize" Value="28"/>
        <Style.Triggers>
            <DataTrigger Binding="{Binding ReleaseNoteType}" Value="0">
                <Setter Property="Content" Value="{DynamicResource WhatisNew}" />
                <Setter Property="Foreground" Value="White" />
            </DataTrigger>

            <DataTrigger Binding="{Binding ReleaseNoteType}" Value="1">
                <Setter Property="Content" Value="{DynamicResource UserAccessPermission}" />
                <Setter Property="Foreground" Value="White" />
            </DataTrigger>

            <DataTrigger Binding="{Binding ReleaseNoteType}" Value="2">
                <Setter Property="Content" Value="{DynamicResource ReleaseNoteErrorMessage}" />
                <Setter Property="Foreground" Value="White" />
            </DataTrigger>
        </Style.Triggers>
    </Style>

    <Style TargetType="UserControl" BasedOn="{StaticResource BaseUniversalStyle}" >
        <Style.Triggers>
            <DataTrigger Binding="{Binding IsBusy}" Value="True">
                <Setter Property="Cursor" Value="Wait" />
            </DataTrigger>
        </Style.Triggers>
    </Style>
    <Style TargetType="UserControl" x:Key="UserControlUpdaterBackgroundStyle" BasedOn="{StaticResource BaseUniversalStyle}" >
        <Setter Property="Background" Value="#FF333333"/>
        <Style.Triggers>
            <DataTrigger Binding="{Binding IsBusy}" Value="True">
                <Setter Property="Cursor" Value="Wait" />
            </DataTrigger>
        </Style.Triggers>
    </Style>

    <Style TargetType="Image" x:Key="SettingImagePath">
        <Setter Property="Source" Value="/GeosWorkbenchInstaller;component/Assets/Images/settings_White_32x32.png"></Setter>
    </Style>
</ResourceDictionary>