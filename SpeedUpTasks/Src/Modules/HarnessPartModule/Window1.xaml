<Window xmlns:dxe="http://schemas.devexpress.com/winfx/2008/xaml/editors"
        x:Class="Emdep.Geos.Modules.HarnessPart.Window1"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
          xmlns:dxco="http://schemas.devexpress.com/winfx/2008/xaml/controls"
        xmlns:dxg="clr-namespace:DevExpress.Xpf.Grid;assembly=DevExpress.Xpf.Grid.v19.2"
      
        Title="Window1" Height="300" Width="300">
    <Grid>
        <dxg:GridControl Background="Bisque" x:Name="Gridsample">
            <dxg:GridControl.Columns>

                <dxg:GridColumn FieldName="Code" MinWidth="270">
                    <dxg:GridColumn.CellTemplate>
                        <DataTemplate>
                            <dxco:BarCodeControl Text="EMDEP">
                                <dxco:BarCodeControl.Symbology>
                                    <dxco:Code128Symbology />
                                </dxco:BarCodeControl.Symbology>
                            </dxco:BarCodeControl>
                        </DataTemplate>
                    </dxg:GridColumn.CellTemplate>
                </dxg:GridColumn>


            </dxg:GridControl.Columns>
            <dxg:GridControl.View>
                <dxg:TableView />
            </dxg:GridControl.View>
        </dxg:GridControl>

    </Grid>
</Window>
