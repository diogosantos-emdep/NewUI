    <UserControl.Resources>
        <Style x:Key="VerticalTile" TargetType="dxlc:Tile">

            <Setter Property="VerticalContentAlignment" Value="Center" />
            <Setter Property="HorizontalContentAlignment" Value="Center" />
            <Setter Property="Size" Value="Large" />
            <Setter Property="HeaderTemplate">
                <Setter.Value>
                    <DataTemplate />
                </Setter.Value>
            </Setter>
            <Setter Property="ContentTemplate">
                <Setter.Value>
                    <DataTemplate>
                        <StackPanel>
                            <Image Source="{Binding}" Stretch="Uniform" Height="100" />
                            <TextBlock Text="{Binding Path=Header, RelativeSource={RelativeSource AncestorType=dxlc:Tile}}" FontSize="14" Margin="0,5,0,0" HorizontalAlignment="Center" Foreground="Black" FontWeight="Bold" TextWrapping="Wrap"   FontFamily="calibri" />
                        </StackPanel>
                    </DataTemplate>
                </Setter.Value>
            </Setter>
            <Setter Property="Background">
                <Setter.Value>
                    <LinearGradientBrush EndPoint="0.504,1.5" StartPoint="0.504,0.03">

                        <!--<GradientStop Color="DarkGray" Offset="0.0"/> <GradientStop Color="#FFFFFFFF" Offset="0.8"/>-->
                        <!--<GradientStop Color="#FFFFFFFF" Offset="0.8"/>-->
                        <GradientStop Color="WhiteSmoke"   ></GradientStop>


                    </LinearGradientBrush>
                </Setter.Value>
            </Setter>
            <Setter Property="BorderBrush"  Value="Gray">

            </Setter>
            <Style.Triggers>
                <Trigger Property="IsMouseOver" Value="True">
                    <Setter Property="BorderBrush" Value="#2AB7FF" />
                    <Setter Property="BorderThickness" Value="3"></Setter>
                </Trigger>
                <Trigger Property="IsMouseOver" Value="False">
                    <Setter Property="BorderBrush" Value="Gray" />
                    <Setter Property="BorderThickness" Value="3"></Setter>
                </Trigger>
            </Style.Triggers>
        </Style>
