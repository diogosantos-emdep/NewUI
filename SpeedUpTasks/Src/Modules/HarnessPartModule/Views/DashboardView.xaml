                <Border  BorderBrush="WhiteSmoke" BorderThickness="0 0 0 1"  Grid.Column="1" >
                    <ContentPresenter x:Name="PART_Content" VerticalAlignment="Center" Grid.Column="1" HorizontalAlignment="Stretch" />
                </Border>

            </Grid>

        </ControlTemplate>
        <Style TargetType="dxwuii:NavigationHeaderControl">
