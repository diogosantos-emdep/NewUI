                            </dxc:SimpleDiagram2D>
                        </dxc:ChartControl.Diagram>
                        <dxc:ChartControl.Legend>
                            <dxc:Legend />
                        </dxc:ChartControl.Legend>
                        <dxc:ChartControl.Titles>
                            <dxc:Title Dock="Top" HorizontalAlignment="Center" Content="Nested Donut"/>
                        </dxc:ChartControl.Titles>
                    </dxc:ChartControl>
                </dxlc:LayoutGroup>
            </dxlc:LayoutGroup>

            <dxlc:LayoutGroup dx:ScrollViewerTouchBehavior.IsEnabled="True" >
                <dxlc:LayoutGroup Header="RATING EDITOR" x:Name="lgPartNumber7" View="GroupBox" Orientation="Vertical"  IsCollapsible="False"  Margin="0,0,0,0" VerticalAlignment="Stretch" Width="400">
                    <dxe:RatingEdit x:Name="editor" HorizontalAlignment="Center" VerticalAlignment="Center" Value="{Binding SelectedStarIndex,UpdateSourceTrigger=PropertyChanged,Mode=TwoWay}">
                        <dxe:RatingEdit.ItemStyle>
                            <Style TargetType="dxe:RatingItem">
                                <Setter Property="DataContext" Value="{Binding Path=(dxe:BaseEdit.OwnerEdit).DataContext}"/>
                                <Style.Triggers>
                                    <DataTrigger Binding="{Binding Background, Converter={UIConverter:IsNullColorConverter}}" Value="False">
                                        <Setter Property="Background" Value="{Binding Background, Converter={UIConverter:ColorToBrushConverter}}"/>
                                    </DataTrigger>
                                    <DataTrigger Binding="{Binding HoverBackground, Converter={UIConverter:IsNullColorConverter}}" Value="False">
                                        <Setter Property="HoverBackground" Value="{Binding HoverBackground, Converter={UIConverter:ColorToBrushConverter}}"/>
                                    </DataTrigger>
                                    <DataTrigger Binding="{Binding SelectedBackground,Mode=TwoWay, Converter={UIConverter:IsNullColorConverter}}" Value="False">
                                        <Setter Property="SelectedBackground" Value="{Binding SelectedBackground,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged, Converter={UIConverter:ColorToBrushConverter}}"/>
                                    </DataTrigger>
                                </Style.Triggers>
                            </Style>
                        </dxe:RatingEdit.ItemStyle>
                    </dxe:RatingEdit>

