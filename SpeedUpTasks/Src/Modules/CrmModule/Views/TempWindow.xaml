            <dxlc:LayoutGroup dx:ScrollViewerTouchBehavior.IsEnabled="True" >
                <dxlc:LayoutGroup Header="TARGET ACHIEVED"   x:Name="lgPartNumber4" View="GroupBox" Orientation="Vertical"  IsCollapsible="False"  Margin="0,0,0,0" VerticalAlignment="Stretch" Width="400" >
                    <dxc:ChartControl HorizontalAlignment="Stretch" Grid.RowSpan="3"  Name="chartControl4" VerticalAlignment="Stretch" >
                        <dxc:ChartControl.Legend>
                            <dxc:Legend />
                        </dxc:ChartControl.Legend>
                        <dxc:SimpleDiagram2D>
                            <dxc:PieSeries2D Template="{StaticResource template}">
                                <dxc:PieSeries2D.Label>
                                    <dxc:SeriesLabel dxc:PieSeries.LabelPosition="Inside" />
                                </dxc:PieSeries2D.Label>
                                <dxc:PieSeries2D.Model>
                                    <dxc:FlatPie2DModel />
                                </dxc:PieSeries2D.Model>
                                <dxc:SeriesPoint Argument="A" Value="14.2" Brush="WhiteSmoke" />
                                <dxc:SeriesPoint Argument="B" Value="85.8" Brush="#FF2AB7FF" />
                            </dxc:PieSeries2D>
                        </dxc:SimpleDiagram2D>
                    </dxc:ChartControl>
                </dxlc:LayoutGroup>
            </dxlc:LayoutGroup>

            <dxlc:LayoutGroup dx:ScrollViewerTouchBehavior.IsEnabled="True" >
                <dxlc:LayoutGroup Header="TARGET ACHIEVED"   x:Name="lgPartNumber5" View="GroupBox" Orientation="Vertical"  IsCollapsible="False"  Margin="0,0,0,0" VerticalAlignment="Stretch" Width="400" >
                    <dxc:ChartControl x:Name="chartControl5" >
                        <dxc:ChartControl.Legend >
                            <dxc:Legend VerticalPosition="Bottom" 
                                        HorizontalPosition="RightOutside" 
                                        Height="200" 
                                        Width="100"/>
                        </dxc:ChartControl.Legend>
                        <dxc:SimpleDiagram2D >
                            <dxc:PieSeries2D >
                                <dxc:PieSeries2D.LegendPointOptions>
                                    <!--<dxc:PointOptions Pattern=" {A}"/>-->
                                    <dxc:PointOptions>
                                        <dxc:PointOptions.ValueNumericOptions>
                                            <dxc:NumericOptions Format="Percent" Precision="0" />
                                        </dxc:PointOptions.ValueNumericOptions>
                                    </dxc:PointOptions>
                                </dxc:PieSeries2D.LegendPointOptions>

                                <dxc:PieSeries2D.Label>
                                    <dxc:SeriesLabel Visible="True"
                                                     dxc:PieSeries.LabelPosition="Inside"
                                                     RenderMode="Rectangle"
                                                     TextPattern=" {VP:P0}"/>
                                </dxc:PieSeries2D.Label>

                                <dxc:SeriesPoint Argument="Directors" Value="1"/>
                                <dxc:SeriesPoint Argument="GARP" Value="2" />
                                <dxc:SeriesPoint Argument="Growth" Value="3" />
                                <dxc:SeriesPoint Argument="Value And Growth" Value="4" />
                                <dxc:SeriesPoint Argument="Quant" Value="5" />
                                <dxc:SeriesPoint Argument="Retail" Value="6" />
                                <dxc:SeriesPoint Argument="Trading" Value="7" />
                            </dxc:PieSeries2D>
                        </dxc:SimpleDiagram2D>
                    </dxc:ChartControl>
                </dxlc:LayoutGroup>
            </dxlc:LayoutGroup>

            <dxlc:LayoutGroup dx:ScrollViewerTouchBehavior.IsEnabled="True" >
                <dxlc:LayoutGroup Header="TARGET ACHIEVED NESTED"   x:Name="lgPartNumber6" View="GroupBox" Orientation="Vertical"  IsCollapsible="False"  Margin="0,0,0,0" VerticalAlignment="Stretch" Width="400" >
                    <dxc:ChartControl Name="chartControl6" ToolTipEnabled="True" >
                        <dxc:ChartControl.Diagram>
                            <dxc:SimpleDiagram2D>
                                <dxc:SimpleDiagram2D.Series>
                                    <dxc:NestedDonutSeries2D  DisplayName="Products" InnerIndent="5" 
                                                  ToolTipPointPattern="{} {A}: {VP:##.##%}"
                                                  HoleRadiusPercent="0" LegendTextPattern="{}{A}">
                                        <dxc:NestedDonutSeries2D.Points>
                                            <dxc:SeriesPoint Argument="Fish" Value="1" />
                                            <dxc:SeriesPoint Argument="Meat" Value="0" />
                                            <!--<dxc:SeriesPoint Argument="Dairy Product" Value="30" />-->
                                        </dxc:NestedDonutSeries2D.Points>
                                        <dxc:PieSeries2D.Label>
                                            <dxc:SeriesLabel Visible="True"
                                                     dxc:PieSeries.LabelPosition="Inside"
                                                     RenderMode="Rectangle"
                                                     TextPattern=" {VP:P0}"/>
                                        </dxc:PieSeries2D.Label>
                                    </dxc:NestedDonutSeries2D>
                                    <dxc:NestedDonutSeries2D  DisplayName="Products" InnerIndent="5" 
                                                  ToolTipPointPattern="{} {A}: {VP:##.##%}"
                                                  HoleRadiusPercent="0" LegendTextPattern="{}{A}">
                                        <dxc:NestedDonutSeries2D.Points>
                                            <dxc:SeriesPoint Argument="Fish" Value="2" />
                                            <dxc:SeriesPoint Argument="Meat" Value="1" />
                                            <!--<dxc:SeriesPoint Argument="Dairy Product" Value="30" />-->
                                        </dxc:NestedDonutSeries2D.Points>
                                    </dxc:NestedDonutSeries2D>
                                    <dxc:NestedDonutSeries2D  DisplayName="Products" InnerIndent="5" 
                                                  ToolTipPointPattern="{} {A}: {VP:##.##%}"
                                                  HoleRadiusPercent="0" LegendTextPattern="{}{A}">
                                        <dxc:NestedDonutSeries2D.Points>
                                            <dxc:SeriesPoint Argument="Fish" Value="0" />
                                            <dxc:SeriesPoint Argument="Meat" Value="1" />
                                            <!--<dxc:SeriesPoint Argument="Dairy Product" Value="10" />-->
                                        </dxc:NestedDonutSeries2D.Points>
                                    </dxc:NestedDonutSeries2D>
                                    <dxc:NestedDonutSeries2D  DisplayName="Products" InnerIndent="5" 
                                                  ToolTipPointPattern="{} {A}: {VP:##.##%}"
                                                  HoleRadiusPercent="0" LegendTextPattern="{}{A}">
                                        <dxc:NestedDonutSeries2D.Points>
                                            <dxc:SeriesPoint Argument="Fish" Value="0"  />
                                            <dxc:SeriesPoint Argument="Meat" Value="1" />
