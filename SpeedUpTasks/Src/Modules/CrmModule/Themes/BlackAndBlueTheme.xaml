    <Style x:Key="hyperlinkGridStyle" TargetType="Hyperlink">
        <Setter Property="Foreground" Value="#FF2AB7FF"/>
        <Style.Triggers>
            <DataTrigger Binding="{Binding RowData.SelectionState}" Value="Focused">
                <Setter Property="Foreground" Value="White" />
            </DataTrigger>
            <Trigger Property="IsMouseOver" Value="true">
                <Setter Property="Foreground" Value="OrangeRed" />
            </Trigger>
        </Style.Triggers>
    </Style>

    <Style x:Key="HyperlinkTextBlock" TargetType="TextBlock">
        <Setter Property="TextBlock.Foreground" Value="Black" />
        <Style.Triggers>
            <MultiDataTrigger>
                <MultiDataTrigger.Conditions>
                    <Condition Binding="{Binding RowData.SelectionState}" Value="Focused"/>
                </MultiDataTrigger.Conditions>
                <Setter Property="TextBlock.Foreground" Value="Black"/>
            </MultiDataTrigger>
            <MultiDataTrigger>
                <MultiDataTrigger.Conditions>
                    <Condition Binding="{Binding RowData.SelectionState}" Value="None"/>
                </MultiDataTrigger.Conditions>
                <Setter Property="TextBlock.Foreground" Value="#FF2AB7FF"/> 
            </MultiDataTrigger>
            <Trigger Property="IsMouseOver" Value="true">
                <Setter Property="Foreground" Value="OrangeRed" />
            </Trigger>           
        </Style.Triggers>
    </Style>
    
    <Style x:Key="hyperlinkGridStyleCell" TargetType="Hyperlink">
        <Setter Property="Foreground" Value="#FF2AB7FF"/>
        <Style.Triggers>
            <DataTrigger Binding="{Binding RowData.SelectionState}" Value="Focused">
                <Setter Property="Foreground" Value="Black" />
            </DataTrigger>
            <DataTrigger Binding="{Binding RowData.SelectionState}" Value="Focused">
                <Setter Property="Foreground" Value="Black" />
            </DataTrigger>
            <Trigger Property="IsMouseOver" Value="true">
                <Setter Property="Foreground" Value="OrangeRed" />
            </Trigger>
            <Trigger Property="dxg:DataViewBase.IsFocusedCell" Value="True">
                <Setter Property="Background" Value="#FF2AB7FF" />
                <Setter Property="Foreground" Value="White" />
                <Setter Property="TextBlock.Foreground" Value="Black"/>
            </Trigger>
        </Style.Triggers>
    </Style>
  

