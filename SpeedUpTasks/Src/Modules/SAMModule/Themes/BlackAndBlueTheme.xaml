<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:dxwui="http://schemas.devexpress.com/winfx/2008/xaml/windowsui"
                    xmlns:dxlc="http://schemas.devexpress.com/winfx/2008/xaml/layoutcontrol"
                    xmlns:dxc="http://schemas.devexpress.com/winfx/2008/xaml/core"
                    xmlns:dxe="http://schemas.devexpress.com/winfx/2008/xaml/editors"
                    xmlns:dxg="http://schemas.devexpress.com/winfx/2008/xaml/grid"
                    xmlns:dxgt="http://schemas.devexpress.com/winfx/2008/xaml/grid/themekeys"
                    xmlns:dxmvvm="http://schemas.devexpress.com/winfx/2008/xaml/mvvm"
                    xmlns:system="clr-namespace:System;assembly=mscorlib"
                    xmlns:dxnav="http://schemas.devexpress.com/winfx/2008/xaml/navigation">

    <!--Work Log-->
    <SolidColorBrush x:Key="SAMColor" Color="#FF2AB7FF"></SolidColorBrush>
    <ImageSource x:Key="FemaleUserImage">/Emdep.Geos.Modules.SAM;component/Assets/Images/wFemaleUser.png</ImageSource>
    <ImageSource x:Key="MaleUserImage">/Emdep.Geos.Modules.SAM;component/Assets/Images/wMaleUser.png</ImageSource>
    <ImageSource x:Key="StartImage">/Emdep.Geos.Modules.SAM;component/Assets/Images/bStart.png</ImageSource>
    <ImageSource x:Key="StopImage">/Emdep.Geos.Modules.SAM;component/Assets/Images/bStop.png</ImageSource>
    
    <!--Pending Work Order--> 
    <Style TargetType="Image" x:Key="WorkLogScanButtonImageStyle">
        <Setter Property="Source" Value="/Emdep.Geos.Modules.SAM;component/Assets/Images/wScan.png" />
    </Style>
    <Style TargetType="Image" x:Key="SaveButtonImageStyle">
        <Setter Property="Source" Value="/Emdep.Geos.Modules.Crm;component/Assets/Images/wSave.png" />
    </Style>

    <!--Scan validation Order-->
    <Style TargetType="Image" x:Key="ScanvalidationButtonImageStyle">
        <Setter Property="Source" Value="/Emdep.Geos.Modules.SAM;component/Assets/Images/wScanvalidation.png" />
    </Style>

    <ImageSource x:Key="DownloadAttachmentButtonImage">/Emdep.Geos.Modules.SAM;component/Assets/Images/w_download.png</ImageSource>

    <!--<Style TargetType="Image" x:Key="DownloadAttachmentButtonImageStyle">
        <Setter Property="Source" Value="/Emdep.Geos.Modules.SAM;component/Assets/Images/w_download.png" />
    </Style>-->
    

    <!--End-->
</ResourceDictionary>