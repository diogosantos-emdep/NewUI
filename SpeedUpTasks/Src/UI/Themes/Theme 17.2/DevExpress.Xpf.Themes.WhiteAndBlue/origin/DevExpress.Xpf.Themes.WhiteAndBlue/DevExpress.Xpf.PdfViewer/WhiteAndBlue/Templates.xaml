<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:sys="clr-namespace:System;assembly=mscorlib" xmlns:dx="http://schemas.devexpress.com/winfx/2008/xaml/core" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:dxpdf="http://schemas.devexpress.com/winfx/2008/xaml/pdf" xmlns:themes="http://schemas.devexpress.com/winfx/2008/xaml/pdf/themekeys" xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" xmlns:dxb="http://schemas.devexpress.com/winfx/2008/xaml/bars" xmlns:dxcn="http://schemas.devexpress.com/winfx/2008/xaml/core/internal" xmlns:dxbt="http://schemas.devexpress.com/winfx/2008/xaml/bars/themekeys" xmlns:cs="colorscheme_ignorable_uri" mc:Ignorable="cs">
    <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary Source="/DevExpress.Xpf.Themes.WhiteAndBlue.v17.2;component/DevExpress.Xpf.PdfViewer/Generic/Templates.xaml" />
    </ResourceDictionary.MergedDictionaries>
    <Thickness x:Key="{themes:PdfViewerThemeKey ResourceKey=PdfPresenterControlMargin}">3</Thickness>
    <Thickness x:Key="{themes:PdfViewerThemeKey ResourceKey=PdfPresenterControlBorderThickness}">1</Thickness>
    <SolidColorBrush x:Key="{themes:PdfViewerThemeKey ResourceKey=PdfPresenterControlBorderBrush}" Color="#FFB9B9B9" cs:Name="SolidColorBrush_0002" />
    <Thickness x:Key="{themes:PdfViewerThemeKey ResourceKey=PdfOutlinesViewerControlToolBarBorderThickness}">0,0,0,0</Thickness>
    <SolidColorBrush x:Key="{themes:PdfViewerThemeKey ResourceKey=PdfOutlinesViewerControlToolBarBorderBrush}" Color="#FFB9B9B9" cs:Name="SolidColorBrush_0003" />
    <Thickness x:Key="{themes:PdfViewerThemeKey ResourceKey=PagePaddingThickness}">10</Thickness>
    <Color x:Key="{themes:PdfViewerThemeKey ResourceKey=HighlightSearchTextColor}" cs:Name="Color_0001">#596098C0</Color>
    <SolidColorBrush x:Key="{themes:PdfViewerThemeKey ResourceKey=HighlightSelectionColor}" Opacity="0.5" Color="#F78A09" />
    <Color x:Key="{themes:PdfViewerThemeKey ResourceKey=CaretColor}" cs:Name="Color_0007">#FF000000</Color>
    <Color x:Key="{themes:PdfViewerThemeKey ResourceKey=StartScreenCaptionForegroundColor}" cs:Name="Color_0002">#7F3C3C3C</Color>
    <Color x:Key="{themes:PdfViewerThemeKey ResourceKey=StartScreenDocumentLinkForegroundColor}" cs:Name="Color_0003">#FFF78A09</Color>
    <Color x:Key="{themes:PdfViewerThemeKey ResourceKey=StartScreenOpenDocumentForegroundColor}" cs:Name="Color_0004">#BF3C3C3C</Color>
    <Color x:Key="{themes:PdfViewerThemeKey ResourceKey=LoadingDocumentLinkForegroundColor}" cs:Name="Color_0005">#FFF78A09</Color>
    <sys:Double x:Key="{themes:PdfViewerThemeKey ResourceKey=PrintDialogRightColumnWidth}">120</sys:Double>
    <GridLength x:Key="{themes:PdfViewerThemeKey ResourceKey=PrintDialogRightColumnGridLength}">120</GridLength>
    <SolidColorBrush x:Key="{themes:PdfViewerThemeKey ResourceKey=PdfPresenterControlBackgroundBrush}" Color="#FFFFFFFF" cs:Name="SolidColorBrush_0001" />
    <ControlTemplate TargetType="{x:Type dxpdf:PdfPageControlItem}" x:Key="{themes:PdfViewerThemeKey ResourceKey=PdfPageControlItemTemplate}">
        <Grid>
            <Border x:Name="Border" BorderBrush="#FFC3C3C3" BorderThickness="4" Margin="-4" cs:Name="Border_0001" />
        </Grid>
        <ControlTemplate.Triggers>
            <Trigger Property="IsSelected" Value="true">
                <Setter TargetName="Border" Property="BorderBrush" Value="#FFF78A09" cs:Name="Setter_0001" />
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>
    <ControlTemplate TargetType="{x:Type dxpdf:PdfThumbnailPageControlItem}" x:Key="{themes:PdfViewerThemeKey ResourceKey=PdfThumbnailControlItemTemplate}">
        <Grid>
            <Grid>
                <Grid.RowDefinitions>
                    <RowDefinition Height="*" />
                    <RowDefinition Height="Auto" />
                </Grid.RowDefinitions>
                <Grid x:Name="Shadow" Grid.Row="0">
                    <Border BorderBrush="#FF000000" BorderThickness="5" Margin="-6" CornerRadius="5" Opacity="0.01" cs:Name="Border_0004" />
                    <Border BorderBrush="#FF000000" BorderThickness="4" Margin="-5" CornerRadius="4" Opacity="0.01" cs:Name="Border_0005" />
                    <Border BorderBrush="#FF000000" BorderThickness="3" Margin="-4" CornerRadius="3" Opacity="0.02" cs:Name="Border_0006" />
                    <Border BorderBrush="#FF000000" BorderThickness="2" Margin="-3" CornerRadius="2" Opacity="0.03" cs:Name="Border_0007" />
                    <Border BorderBrush="#FF000000" BorderThickness="1" Margin="-2" CornerRadius="1" Opacity="0.05" cs:Name="Border_0008" />
                    <Border BorderBrush="#FFFFFFFF" BorderThickness="1" Margin="-1" cs:Name="Border_0009" />
                </Grid>
                <Border Grid.Row="0" x:Name="Highlight" BorderBrush="#7FF78A09" BorderThickness="6" Margin="-7" Opacity="0" cs:Name="Border_0011" />
                <Border Grid.Row="0" x:Name="Selection" BorderBrush="#FFFFDC58" BorderThickness="2" Margin="-9" Opacity="0" cs:Name="Border_0010" />
                <TextBlock Grid.Row="1" Text="{Binding Path=PageNumber}" TextAlignment="Center" Margin="0,12,0,10" />
            </Grid>
        </Grid>
        <ControlTemplate.Triggers>
            <Trigger Property="IsSelected" Value="true">
                <Setter TargetName="Selection" Property="Opacity" Value="1" />
            </Trigger>
            <Trigger Property="IsHighlighted" Value="true">
                <Setter TargetName="Highlight" Property="Opacity" Value="1" />
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>
    <DataTemplate x:Key="{themes:PdfViewerThemeKey ResourceKey=SelectionCanvasContentTemplate}">
        <Border VerticalAlignment="Stretch" HorizontalAlignment="Stretch" Background="#45007FFF" BorderBrush="#FF007FFF" BorderThickness="1" cs:Name="Border_0003" />
    </DataTemplate>
</ResourceDictionary>