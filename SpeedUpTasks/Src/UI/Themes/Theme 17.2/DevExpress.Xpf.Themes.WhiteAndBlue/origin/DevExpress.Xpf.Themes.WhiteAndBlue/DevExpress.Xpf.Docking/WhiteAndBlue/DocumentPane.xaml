        <ve:DockingSplitLayoutPanel Margin="1" Content1Location="{Binding CaptionLocation, Converter={StaticResource groupCaptionLocationToDockConverter}}" DataContext="{Binding LayoutItem}" SnapsToDevicePixels="True" ve:DockPane.HitTestType="Bounds">
