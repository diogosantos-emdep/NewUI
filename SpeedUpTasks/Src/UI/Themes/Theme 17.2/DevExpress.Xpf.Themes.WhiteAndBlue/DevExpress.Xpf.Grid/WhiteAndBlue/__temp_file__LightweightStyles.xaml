<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:dx="http://schemas.devexpress.com/winfx/2008/xaml/core" xmlns:dxe="http://schemas.devexpress.com/winfx/2008/xaml/editors" xmlns:dxcn="http://schemas.devexpress.com/winfx/2008/xaml/core/internal" xmlns:dxtEx="clr-namespace:DevExpress.Xpf.Utils.Themes;assembly=DevExpress.Xpf.Core.v17.2" xmlns:dxg="http://schemas.devexpress.com/winfx/2008/xaml/grid" xmlns:dxgt="http://schemas.devexpress.com/winfx/2008/xaml/grid/themekeys" xmlns:dxdst="http://schemas.devexpress.com/winfx/2008/xaml/core/themekeys" xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" xmlns:cs="colorscheme_ignorable_uri" mc:Ignorable="cs" xmlns:dxcore_unique_xmlns="http://schemas.devexpress.com/winfx/2008/xaml/core">
    <ResourceDictionary.MergedDictionaries>
        <dxdst:ResourceDictionaryEx Source="/DevExpress.Xpf.Themes.WhiteAndBlue.v17.2;component/DevExpress.Xpf.Grid/WhiteAndBlue/Constants.xaml" />
    </ResourceDictionary.MergedDictionaries>
    <dxcn:BrushSet x:Key="{dxgt:GridRowThemeKey ResourceKey=CellForegroundBrushes}">
        <dxcn:BrushSet.Elements>
            <dxcn:BrushInfo x:Key="Focused" Brush="{StaticResource {dxgt:GridRowThemeKey ResourceKey=CellTextFocusedBrush}}" />
            <dxcn:BrushInfo x:Key="Selected" Brush="{StaticResource {dxgt:GridRowThemeKey ResourceKey=TextRowSelectedBrush}}" />
            <dxcn:BrushInfo x:Key="FocusedAndSelected" Brush="{StaticResource {dxgt:GridRowThemeKey ResourceKey=TextFocusedBrush}}" />
        </dxcn:BrushSet.Elements>
    </dxcn:BrushSet>
    <dxcn:BrushSet x:Key="{dxgt:GridRowThemeKey ResourceKey=CellBackgroundBrushes}">
        <dxcn:BrushSet.Elements>
            <dxcn:BrushInfo x:Key="CellMerge" Brush="{StaticResource {dxgt:GridRowThemeKey ResourceKey=CellMergeNoneBrush}}" />
            <dxcn:BrushInfo x:Key="Focused" Brush="{StaticResource {dxgt:GridRowThemeKey ResourceKey=CellBorderFocusedBrush}}" />
            <dxcn:BrushInfo x:Key="Selected" Brush="{StaticResource {dxgt:GridRowThemeKey ResourceKey=BorderSelectedBrush}}" />
            <dxcn:BrushInfo x:Key="FocusedAndSelected" Brush="{StaticResource {dxgt:GridRowThemeKey ResourceKey=BorderFocusedBrush}}" />
            <dxcn:BrushInfo x:Key="BorderBrush" Brush="{StaticResource {dxgt:GridRowThemeKey ResourceKey=GridDataRowDelimiterBrush}}" />
            <dxcn:BrushInfo x:Key="BorderBrushFocusedRow" Brush="{StaticResource {dxgt:GridRowThemeKey ResourceKey=CellBorderFocusedBorderBrush}}" />
        </dxcn:BrushSet.Elements>
    </dxcn:BrushSet>
    <dxcn:BrushSet x:Key="{dxgt:GroupRowThemeKey ResourceKey=GroupRowColumnSummaryBrushes}">
        <dxcn:BrushSet.Elements>
            <dxcn:BrushInfo x:Key="Normal" Brush="{StaticResource {dxgt:GridRowThemeKey ResourceKey=GridDataRowDelimiterBrush}}" />
            <dxcn:BrushInfo x:Key="Focused" Brush="{StaticResource {dxgt:GroupRowThemeKey ResourceKey=GroupColumnSummaryFocusedBorderBrush}}" />
        </dxcn:BrushSet.Elements>
    </dxcn:BrushSet>
    <ControlTemplate x:Key="{dxgt:RowIndicatorThemeKey ResourceKey=RowIndicatorTemplate}" TargetType="{x:Type dxg:RowIndicator}">
        <Grid dxcore_unique_xmlns:BlendHelper2.ThemeInfo="DevExpress.Xpf.Grid\WhiteAndBlue\LightweightStyles.xaml;29;10">
            <Border x:Name="PART_ContentBorder" Background="{StaticResource RowIndicatorBackground}" BorderBrush="{StaticResource RowIndicatorBorderBrush}" BorderThickness="0,0,1,1" dxcore_unique_xmlns:BlendHelper2.ThemeInfo="DevExpress.Xpf.Grid\WhiteAndBlue\LightweightStyles.xaml;30;14" />
            <Border x:Name="BottomBorder" BorderBrush="{StaticResource {dxgt:GridRowThemeKey ResourceKey=RowBreakBrush}}" BorderThickness="{StaticResource {dxgt:GridRowThemeKey ResourceKey=RowBreakThickness}}" Visibility="Collapsed" dxcore_unique_xmlns:BlendHelper2.ThemeInfo="DevExpress.Xpf.Grid\WhiteAndBlue\LightweightStyles.xaml;31;14" />
        </Grid>
        <ControlTemplate.Triggers>
            <Trigger Property="ShowRowBreak" Value="True">
                <Setter Property="Visibility" TargetName="BottomBorder" Value="Visible" />
            </Trigger>
            <Trigger Property="IsLastFixedRow" Value="True">
                <Setter Property="BorderThickness" TargetName="BottomBorder" Value="0,1,0,1" />
                <Setter Property="Visibility" TargetName="BottomBorder" Value="Visible" />
                <Setter Property="VerticalAlignment" TargetName="BottomBorder" Value="Bottom" />
            </Trigger>
            <Trigger Property="FixedRowPosition" Value="Bottom">
                <Setter Property="BorderThickness" Value="0,1,1,0" TargetName="PART_ContentBorder" />
                <Setter Property="VerticalAlignment" TargetName="BottomBorder" Value="Top" />
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>
    <ControlTemplate x:Key="{dxgt:RowIndicatorThemeKey ResourceKey=GroupRowIndicatorTemplate}" TargetType="{x:Type dxg:RowIndicator}">
        <Border x:Name="PART_ContentBorder" Background="{StaticResource RowIndicatorBackground}" BorderBrush="{StaticResource RowIndicatorBorderBrush}" BorderThickness="0,0,1,1" dxcore_unique_xmlns:BlendHelper2.ThemeInfo="DevExpress.Xpf.Grid\WhiteAndBlue\LightweightStyles.xaml;49;10" />
    </ControlTemplate>
    <ControlTemplate x:Key="{dxgt:GridRowThemeKey ResourceKey=FixedLineSeparatorTemplate}" TargetType="dxg:RowFixedLineSeparatorControl">
        <Border x:Name="border" BorderBrush="{DynamicResource {dxgt:GridRowThemeKey ResourceKey=GridDataRowDelimiterBrush}}" BorderThickness="1,0,1,0" dxcore_unique_xmlns:BlendHelper2.ThemeInfo="DevExpress.Xpf.Grid\WhiteAndBlue\LightweightStyles.xaml;52;10" />
        <!--<ControlTemplate.Triggers>
            <Trigger Property="ShowVerticalLines" Value="false">
                <Setter Property="Background" Value="#00FFFFFF" TargetName="border"/>
            </Trigger>
        </ControlTemplate.Triggers>-->
    </ControlTemplate>
    <ControlTemplate x:Key="{dxgt:GroupRowThemeKey ResourceKey=GroupRowFixedLineSeparatorTemplate}" TargetType="dxg:GroupRowFixedLineSeparatorControl">
        <Border x:Name="Border" BorderBrush="#FFB9B9B9" BorderThickness="1,0,1,0" cs:Name="Border_0001" Margin="{DynamicResource {dxgt:GroupRowThemeKey ResourceKey=GroupRowBottomLineThickness}}" dxcore_unique_xmlns:BlendHelper2.ThemeInfo="DevExpress.Xpf.Grid\WhiteAndBlue\LightweightStyles.xaml;60;10" />
    </ControlTemplate>
    <ControlTemplate x:Key="{dxgt:GridRowThemeKey ResourceKey=RowTemplate}" TargetType="dxg:RowControl">
        <Grid dxcore_unique_xmlns:BlendHelper2.ThemeInfo="DevExpress.Xpf.Grid\WhiteAndBlue\LightweightStyles.xaml;63;10">
            <Border x:Name="Background" Background="{TemplateBinding Background}" dxcore_unique_xmlns:BlendHelper2.ThemeInfo="DevExpress.Xpf.Grid\WhiteAndBlue\LightweightStyles.xaml;64;14" />
            <Border x:Name="BottomLine" BorderThickness="0,0,0,1" BorderBrush="{TemplateBinding BorderBrush}" VerticalAlignment="Bottom" dxcore_unique_xmlns:BlendHelper2.ThemeInfo="DevExpress.Xpf.Grid\WhiteAndBlue\LightweightStyles.xaml;65;14" />
            <Grid x:Name="PART_LayoutPanel" dxcore_unique_xmlns:BlendHelper2.ThemeInfo="DevExpress.Xpf.Grid\WhiteAndBlue\LightweightStyles.xaml;66;14" />
        </Grid>
        <ControlTemplate.Triggers>
            <Trigger Property="ShowHorizontalLine" Value="False">
                <Setter Property="Visibility" Value="Collapsed" TargetName="BottomLine" />
            </Trigger>
            <Trigger Property="ShowBottomLine" Value="True">
                <Setter Property="Visibility" Value="Visible" TargetName="BottomLine" />
            </Trigger>
            <Trigger Property="FadeSelection" Value="True">
                <Setter Property="Background" Value="{StaticResource UnfocusedRowBrush}" />
            </Trigger>
            <Trigger Property="ShowRowBreak" Value="True">
                <Setter Property="BorderThickness" Value="{StaticResource {dxgt:GridRowThemeKey ResourceKey=RowBreakThickness}}" TargetName="BottomLine" />
                <Setter Property="BorderBrush" Value="{StaticResource {dxgt:GridRowThemeKey ResourceKey=RowBreakBrush}}" TargetName="BottomLine" />
            </Trigger>
            <Trigger Property="FixedRowPosition" Value="Bottom">
                <Setter Property="VerticalAlignment" Value="Top" TargetName="BottomLine" />
            </Trigger>
            <Trigger Property="IsLastFixedRow" Value="True">
                <Setter Property="BorderThickness" Value="0,1,0,1" TargetName="BottomLine" />
                <Setter Property="Background" Value="{DynamicResource {dxgt:GridRowThemeKey ResourceKey=RowSeparatorBrush}}" TargetName="BottomLine" />
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>
    <ControlTemplate x:Key="{dxgt:GridRowThemeKey ResourceKey=RowDetailsTemplate}" TargetType="dxg:RowDetailsControl">
        <Grid dxcore_unique_xmlns:BlendHelper2.ThemeInfo="DevExpress.Xpf.Grid\WhiteAndBlue\LightweightStyles.xaml;92;10">
            <Grid.RowDefinitions>
                <RowDefinition Height="Auto" />
                <RowDefinition Height="*" />
            </Grid.RowDefinitions>
            <Border x:Name="Border" BorderBrush="{TemplateBinding BorderBrush}" BorderThickness="0,1,0,0" Background="{TemplateBinding Background}" dxcore_unique_xmlns:BlendHelper2.ThemeInfo="DevExpress.Xpf.Grid\WhiteAndBlue\LightweightStyles.xaml;97;14" />
            <dxcore_unique_xmlns:DataContentPresenter x:Name="Content" Grid.Row="1" dxcore_unique_xmlns:BlendHelper2.ThemeInfo="DevExpress.Xpf.Grid\WhiteAndBlue\LightweightStyles.xaml;98;14" />
        </Grid>
        <ControlTemplate.Triggers>
            <DataTrigger Binding="{Binding Path=View.ShowHorizontalLines}" Value="False">
                <Setter TargetName="Border" Property="BorderThickness" Value="0" />
            </DataTrigger>
            <DataTrigger Binding="{Binding Path=ActualHeight, ElementName=Content}" Value="0">
                <Setter TargetName="Border" Property="BorderThickness" Value="0" />
            </DataTrigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>
    <ControlTemplate x:Key="{dxgt:GroupRowThemeKey ResourceKey=GroupRowLightweightTemplate}" TargetType="dxg:GroupRowControl">
        <Grid x:Name="PART_RootPanel" Background="{DynamicResource {dxgt:GroupRowThemeKey ResourceKey=GroupRowBackground}}" MinHeight="{DynamicResource {dxgt:GroupRowThemeKey ResourceKey=GroupRowMinHeight}}" dxcore_unique_xmlns:BlendHelper2.ThemeInfo="DevExpress.Xpf.Grid\WhiteAndBlue\LightweightStyles.xaml;110;10">
            <Border x:Name="Background" Background="{TemplateBinding Background}" dxcore_unique_xmlns:BlendHelper2.ThemeInfo="DevExpress.Xpf.Grid\WhiteAndBlue\LightweightStyles.xaml;111;14" />
            <Border x:Name="BottomLine" BorderThickness="{DynamicResource {dxgt:GroupRowThemeKey ResourceKey=GroupRowBottomLineThickness}}" BorderBrush="{TemplateBinding BorderBrush}" VerticalAlignment="Bottom" dxcore_unique_xmlns:BlendHelper2.ThemeInfo="DevExpress.Xpf.Grid\WhiteAndBlue\LightweightStyles.xaml;112;14" />
            <dxg:GroupRowControlPanel x:Name="PART_LayoutPanel" />
        </Grid>
        <ControlTemplate.Triggers>
            <Trigger Property="FadeSelection" Value="True">
                <Setter Property="Background" Value="{StaticResource UnfocusedRowBrush}" TargetName="Background" />
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>
    <ControlTemplate x:Key="{dxgt:CardViewThemeKey ResourceKey=GroupRowLightweightTemplate}" TargetType="dxg:GroupCardRowControl">
        <Grid x:Name="PART_RootPanel" Background="{DynamicResource {dxgt:GroupRowThemeKey ResourceKey=GroupRowBackground}}" MinHeight="{DynamicResource {dxgt:GroupRowThemeKey ResourceKey=GroupRowMinHeight}}" dxcore_unique_xmlns:BlendHelper2.ThemeInfo="DevExpress.Xpf.Grid\WhiteAndBlue\LightweightStyles.xaml;122;10">
            <Grid.RowDefinitions>
                <RowDefinition Height="Auto" />
                <RowDefinition />
            </Grid.RowDefinitions>
            <Border x:Name="BorderTop" Grid.Row="0" BorderBrush="{TemplateBinding BorderBrush}" DockPanel.Dock="Top" BorderThickness="0,1,0,0" dxcore_unique_xmlns:BlendHelper2.ThemeInfo="DevExpress.Xpf.Grid\WhiteAndBlue\LightweightStyles.xaml;127;14" />
            <Border x:Name="Background" Grid.Row="1" Background="{TemplateBinding Background}" dxcore_unique_xmlns:BlendHelper2.ThemeInfo="DevExpress.Xpf.Grid\WhiteAndBlue\LightweightStyles.xaml;128;14" />
            <Border x:Name="BottomLine" Grid.RowSpan="2" BorderThickness="{DynamicResource {dxgt:GroupRowThemeKey ResourceKey=GroupRowBottomLineThickness}}" BorderBrush="{TemplateBinding BorderBrush}" VerticalAlignment="Bottom" dxcore_unique_xmlns:BlendHelper2.ThemeInfo="DevExpress.Xpf.Grid\WhiteAndBlue\LightweightStyles.xaml;129;14" />
            <dxg:GroupRowControlPanel Grid.Row="1" x:Name="PART_LayoutPanel" />
        </Grid>
        <ControlTemplate.Triggers>
            <Trigger Property="FadeSelection" Value="True">
                <Setter Property="Background" Value="{StaticResource UnfocusedRowBrush}" TargetName="Background" />
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>
    <ControlTemplate x:Key="{dxgt:TreeListViewThemeKey ResourceKey=RowMarginBorderTemplate}">
        <Border x:Name="background" dxcore_unique_xmlns:BlendHelper2.ThemeInfo="DevExpress.Xpf.Grid\WhiteAndBlue\LightweightStyles.xaml;139;10" />
        <ControlTemplate.Triggers>
            <DataTrigger Binding="{Binding IsAlternateRow, RelativeSource={RelativeSource AncestorType=dxg:RowMarginControlBase}}" Value="True">
                <Setter Property="Background" Value="{Binding Path=View.ActualAlternateRowBackground}" TargetName="background" />
            </DataTrigger>
            <DataTrigger Binding="{Binding Path=IndentSelectionState}" Value="Focused">
                <Setter Property="Background" Value="{DynamicResource {dxgt:GridRowThemeKey ResourceKey=CellBorderFocusedBrush}}" TargetName="background" />
            </DataTrigger>
            <DataTrigger Binding="{Binding Path=IndentSelectionState}" Value="Selected">
                <Setter Property="Background" Value="{DynamicResource {dxgt:GridRowThemeKey ResourceKey=BorderSelectedBrush}}" TargetName="background" />
            </DataTrigger>
            <DataTrigger Binding="{Binding Path=IndentSelectionState}" Value="FocusedAndSelected">
                <Setter Property="Background" Value="{DynamicResource {dxgt:GridRowThemeKey ResourceKey=BorderFocusedBrush}}" TargetName="background" />
            </DataTrigger>
            <DataTrigger Binding="{Binding FadeSelection, RelativeSource={RelativeSource AncestorType=dxg:RowMarginControlBase}}" Value="True">
                <Setter Property="Background" Value="{StaticResource UnfocusedRowBrush}" TargetName="background" />
            </DataTrigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>
</ResourceDictionary>