    <Thickness x:Key="{dxgt:ExtendedColumnChooserThemeKey ResourceKey=SearchPanelBorderThickness}">0,0,0,0</Thickness>
    <SolidColorBrush x:Key="{dxgt:ExtendedColumnChooserThemeKey ResourceKey=SearchPanelBorderBrush}" Color="Transparent" />
    <ControlTemplate x:Key="{dxgt:ExtendedColumnChooserThemeKey ResourceKey=OuterBorderElementTemplate}" TargetType="{x:Type dxg:OuterBorderElement}">
