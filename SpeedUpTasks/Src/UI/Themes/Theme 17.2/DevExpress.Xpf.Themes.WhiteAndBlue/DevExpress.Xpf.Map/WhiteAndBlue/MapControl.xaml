<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:dxt="http://schemas.devexpress.com/winfx/2008/xaml/map/themekeys" xmlns:System="clr-namespace:System;assembly=mscorlib" xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" xmlns:cs="colorscheme_ignorable_uri" mc:Ignorable="cs">
    <!-- Setup -->
    <CornerRadius x:Key="{dxt:MapThemeKey ResourceKey=OverlayRadius}">3</CornerRadius>
    <System:Double x:Key="{dxt:MapThemeKey ResourceKey=HighlightShapeStrokeThickness}">1.0</System:Double>
    <System:Double x:Key="{dxt:MapThemeKey ResourceKey=SelectedShapeStrokeThickness}">2.0</System:Double>
    <!-- Brushes -->
    <SolidColorBrush x:Key="{dxt:MapBrushesThemeKey ResourceKey=MapBackground}" Color="#FFFFFFFF" />
    <!-- Shape -->
    <SolidColorBrush x:Key="{dxt:MapBrushesThemeKey ResourceKey=ShapeFill}" Color="#FFE0E0E0" />
    <SolidColorBrush x:Key="{dxt:MapBrushesThemeKey ResourceKey=ShapeStroke}" Color="#FFFFFFFF" />
    <SolidColorBrush x:Key="{dxt:MapBrushesThemeKey ResourceKey=HighlightShapeStroke}" Color="#FF083493" />
    <SolidColorBrush x:Key="{dxt:MapBrushesThemeKey ResourceKey=SelectedShapeStroke}" Color="#FF083493" />
    <!-- Overlay -->
    <SolidColorBrush x:Key="{dxt:MapBrushesThemeKey ResourceKey=OverlayBackground}" Color="#FFEFEFEF" Opacity="0.84" />
    <SolidColorBrush x:Key="{dxt:MapBrushesThemeKey ResourceKey=OverlayElementForeground}" Color="#FF373A3D" />
    <SolidColorBrush x:Key="{dxt:MapBrushesThemeKey ResourceKey=OverlayElementAccentColor}" Color="#FF083493" />
    <Color x:Key="{dxt:MapBrushesThemeKey ResourceKey=OverlayElementHotColor}">#FF083493</Color>
    <Color x:Key="{dxt:MapBrushesThemeKey ResourceKey=OverlayElementPressedColor}">#FF083493</Color>
    <!-- ToolTip -->
    <SolidColorBrush x:Key="{dxt:MapBrushesThemeKey ResourceKey=ToolTipBackground}" Color="#FF083493" />
    <SolidColorBrush x:Key="{dxt:MapBrushesThemeKey ResourceKey=ToolTipForeground}" Color="#FFFFFFFF" />
    <!-- SearchPanel -->
    <SolidColorBrush x:Key="{dxt:MapBrushesThemeKey ResourceKey=SearchPanelBackground}" Color="White" />
    <SolidColorBrush x:Key="{dxt:MapBrushesThemeKey ResourceKey=SearchPanelForeground}" Color="#FF353535" />
    <SolidColorBrush x:Key="{dxt:MapBrushesThemeKey ResourceKey=SearchPanelNullTextForeground}" Color="#FF373A3D" Opacity="0.62" />
    <SolidColorBrush x:Key="{dxt:MapBrushesThemeKey ResourceKey=SearchPanelClearButtonFill}" Color="#FF083493" />
    <SolidColorBrush x:Key="{dxt:MapBrushesThemeKey ResourceKey=SearchPanelLoadingBrush}" Color="#FF083493" />

    <!--Pushpin-->
    <!-- [apawar][20171124] change color  -->
    <SolidColorBrush x:Key="{dxt:MapBrushesThemeKey ResourceKey=PushpinBackground}" Color="#FF8AFBFF" /> <!-- previous color = #FF999999  -->
    <SolidColorBrush x:Key="{dxt:MapBrushesThemeKey ResourceKey=PushpinForeground}" Color="#FF8AFBFF" /> <!-- previous color = #FF373A3D  -->
    <SolidColorBrush x:Key="{dxt:MapBrushesThemeKey ResourceKey=PushpinSelectionBrush}" Color="#FFFF0000" /> <!-- previous color = #FF083493  -->
    <SolidColorBrush x:Key="{dxt:MapBrushesThemeKey ResourceKey=PushpinSelectionForeground}" Color="#FFFF0000" /> <!-- previous color = #FFFFFFFF  -->
</ResourceDictionary>