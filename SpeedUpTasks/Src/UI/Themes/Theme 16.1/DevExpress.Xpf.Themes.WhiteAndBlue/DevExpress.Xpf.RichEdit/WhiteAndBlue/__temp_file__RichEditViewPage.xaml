    <ControlTemplate x:Key="{dxret:RichEditControlThemeKey ResourceKey=PrintLayoutViewPageBorderControlTemplate}" TargetType="{x:Type dxrec:RichEditViewPageContentControl}">
        <Grid dxcore_unique_xmlns:BlendHelper2.ThemeInfo="DevExpress.Xpf.RichEdit\WhiteAndBlue\RichEditViewPage.xaml;7;10">
            <Border Background="{TemplateBinding Background}" BorderBrush="#FFB9B9B9" BorderThickness="8" Margin="-8" cs:Name="Border_0001" dxcore_unique_xmlns:BlendHelper2.ThemeInfo="DevExpress.Xpf.RichEdit\WhiteAndBlue\RichEditViewPage.xaml;8;14">
                <Grid dxcore_unique_xmlns:BlendHelper2.ThemeInfo="DevExpress.Xpf.RichEdit\WhiteAndBlue\RichEditViewPage.xaml;9;18">
