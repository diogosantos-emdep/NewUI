                    <Border HorizontalAlignment="Right" Background="#FFFFFFFF" CornerRadius="0, 0, 0, 0" BorderThickness="1,0,0,0" BorderBrush="#FFFFFFFF" Visibility="{TemplateBinding CommentsVisibility}" cs:Name="Border_0005" dxcore_unique_xmlns:BlendHelper2.ThemeInfo="DevExpress.Xpf.RichEdit\WhiteAndBlue\RichEditViewPage.xaml;11;22">
