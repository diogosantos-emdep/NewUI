    <Style x:Key="{dxrt:DXTreeViewThemeKey ResourceKey=BackgroundStyle}" TargetType="{x:Type ContentControl}">
        <Setter Property="Template" Value="{DynamicResource {dxrt:DXTreeViewThemeKey ResourceKey=BackgroundTemplate}}" />
    </Style>
</ResourceDictionary>