<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:dxwui="http://schemas.devexpress.com/winfx/2008/xaml/windowsui"
                    xmlns:dxlc="http://schemas.devexpress.com/winfx/2008/xaml/layoutcontrol"
                    xmlns:dxc="http://schemas.devexpress.com/winfx/2008/xaml/core"
                    xmlns:dxwuin="http://schemas.devexpress.com/winfx/2008/xaml/windowsui/navigation"
                    xmlns:dxe="http://schemas.devexpress.com/winfx/2008/xaml/editors"
                    xmlns:dxet="http://schemas.devexpress.com/winfx/2008/xaml/editors/themekeys"
                    xmlns:dx="http://schemas.devexpress.com/winfx/2008/xaml/core"
                    xmlns:dxg="http://schemas.devexpress.com/winfx/2008/xaml/grid"
                    xmlns:dxt="http://schemas.devexpress.com/winfx/2008/xaml/core/themekeys"
                    xmlns:local="clr-namespace:Workbench.ViewModels"
                    xmlns:GeosApplication="clr-namespace:Emdep.Geos.UI.Common;assembly=Emdep.Geos.UI.Common">

    <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary Source="pack://application:,,,/Emdep.Geos.Modules.EPC;component/Themes/BlackAndBlueTheme.xaml" />
        <ResourceDictionary Source="pack://application:,,,/Emdep.Geos.Modules.Crm;component/Themes/BlackAndBlueTheme.xaml" />
        <ResourceDictionary Source="pack://application:,,,/Emdep.Geos.Modules.Warehouse;component/Themes/BlackAndBlueTheme.xaml" />
        <ResourceDictionary Source="pack://application:,,,/Emdep.Geos.Modules.Hrm;component/Themes/BlackAndBlueTheme.xaml" />
        <ResourceDictionary Source="pack://application:,,,/Emdep.Geos.Modules.PCM;component/Themes/BlackAndBlueTheme.xaml" />
        <ResourceDictionary Source="pack://application:,,,/Emdep.Geos.Modules.SAM;component/Themes/BlackAndBlueTheme.xaml" />
        <ResourceDictionary Source="pack://application:,,,/Emdep.Geos.Modules.SRM;component/Themes/BlackAndBlueTheme.xaml" />
        <ResourceDictionary Source="pack://application:,,,/Emdep.Geos.Modules.PLM;component/Themes/BlackAndBlueTheme.xaml" />
    </ResourceDictionary.MergedDictionaries>

    <ImageSource x:Key="ShutDownImage">/GeosWorkbench;component/Assets/Images/shutdown.png</ImageSource>
    <ImageSource x:Key="ShutDownImageMouseOver">/GeosWorkbench;component/Assets/Images/shutdownBlue.png</ImageSource>

    <Style TargetType="Image" x:Key="ImageStyle">
        <Setter Property="Source" Value="/GeosWorkbench;component/Assets/Images/Desktop.png" />
    </Style>

    <Style TargetType="Image" x:Key="GeosLogoImageStyle">
        <Setter Property="Source" Value="/GeosWorkbench;component/Assets/Images/Geos_logo_whiteSplash.png" />
    </Style>

    <Style TargetType="Image" x:Key="SplashScreenWorkbenchStyle">
        <Setter Property="Source" Value="/GeosWorkbench;component/Assets/Images/Geos_logo_whiteSplash.png" />
    </Style>

    <Style TargetType="TextBlock" x:Key="LoginForegroundColor" BasedOn="{StaticResource TextBlockStyle}">
        <Setter Property="Foreground" Value="White" />
        <Style.Triggers>
            <Trigger Property="IsMouseOver" Value="True">
                <Setter Property="Cursor" Value="Hand" />
            </Trigger>
            <Trigger Property="IsMouseOver" Value="False">
                <Setter Property="Cursor" Value="Arrow" />
            </Trigger>
        </Style.Triggers>
    </Style>

    <!--<Style TargetType="Image" x:Key="GeosLogoImageStyle">
        <Setter Property="Source" Value="/GeosWorkbench;component/Assets/Images/LOGO_GEOS_WHITE.png" />
    </Style>-->

    <Style TargetType="Grid" x:Key="GridLogoImageStyle">
        <Setter Property="Background">
            <Setter.Value>
                <ImageBrush ImageSource="/GeosWorkbench;component/Assets/Images/World.png" Stretch="Uniform" />
            </Setter.Value>
        </Setter>
    </Style>

    <Style TargetType="Image" x:Key="EmdepImageStyle">
        <Setter Property="Source" Value="/GeosWorkbench;component/Assets/Images/Emdep_logo.png" />
    </Style>

    <Style TargetType="Image" x:Key="EmdepLoginImageStyle">
        <Setter Property="Source" Value="/GeosWorkbench;component/Assets/Images/Emdep_logo.png" />
    </Style>

    <Style TargetType="dxwui:NavigationFrame" x:Key="NavigationFrameImageStyle">
        <Setter Property="dxwui:NavigationFrame.Background">
            <Setter.Value>
                <ImageBrush ImageSource="/GeosWorkbench;component/Assets/Images/BackgroundGeos.png"  Stretch="None" AlignmentX="Right" AlignmentY="Bottom"   />
            </Setter.Value>
        </Setter>
    </Style>

    <Style x:Key="UserControlBackgroundStyle" BasedOn="{StaticResource BaseUniversalStyle}"  TargetType="UserControl" >
        <Setter Property="Background" Value="#FF333333"/>
        <Style.Triggers>
            <DataTrigger Binding="{Binding IsBusy}" Value="True">
                <Setter Property="Cursor" Value="Wait" />
            </DataTrigger>
        </Style.Triggers>
    </Style>

    <Style TargetType="Ellipse" x:Key="EllipseUserLoginImageStyle">
        <Setter Property="Fill">
            <Setter.Value>
                <ImageBrush ImageSource="/GeosWorkbench;component/Assets/Images/LoginUser.png" Stretch="Fill" />
            </Setter.Value>
        </Setter>
        <Setter Property="Stroke" Value="White"></Setter>
    </Style>

    <Style TargetType="TextBlock" BasedOn="{StaticResource TextBlockStyle}" x:Key="TextblockForgroundStyle">
        <Setter Property="Foreground" Value="WhiteSmoke" />
    </Style>

    <Style TargetType="Ellipse" x:Key="EllipseImpersonateUserStyle">
        <Setter Property="Fill">
            <Setter.Value>
                <ImageBrush ImageSource="pack://application:,,,/GeosWorkbench;component/Assets/Images/Impersonate.png" Stretch="Fill" />
            </Setter.Value>
        </Setter>
        <Setter Property="Stroke" Value="White"/>
    </Style>

    <Style TargetType="Border" x:Key="BorderBrushStyle">
        <Setter Property="BorderBrush" Value="WhiteSmoke" />
    </Style>

    <Style TargetType="Label" x:Key="LabelForegroundStyle" BasedOn="{StaticResource BaseUniversalStyle}">
        <Setter Property="Foreground" Value="#FF2AB7FF" />
        <Style.Triggers>
            <Trigger Property="IsMouseOver" Value="True">
                <Setter Property="Cursor" Value="Hand" />
            </Trigger>
            <Trigger Property="IsMouseOver" Value="False">
                <Setter Property="Cursor" Value="Arrow" />
            </Trigger>
        </Style.Triggers>
    </Style>

    <Style TargetType="dxc:DXWindow" x:Key="DesktopSetting" BasedOn="{StaticResource BaseUniversalStyle}">
        <Setter Property="Background" Value="#1B1B1B" />
        <Setter Property="WindowStyle" Value="SingleBorderWindow"/>
        <Style.Triggers>
            <DataTrigger Binding="{Binding SelectedViewIndex}" Value="0">
                <Setter Property="WindowStyle" Value="SingleBorderWindow" />
                <Setter Property="WindowState" Value="Maximized"></Setter>
            </DataTrigger>
            <DataTrigger Binding="{Binding SelectedViewIndex}" Value="1">
                <Setter Property="WindowStyle" Value="None" />
                <Setter Property="WindowState" Value="Maximized"></Setter>
            </DataTrigger>
            <DataTrigger Binding="{Binding SelectedViewIndex}" Value="2">
                <Setter Property="WindowStyle" Value="SingleBorderWindow" />
                <Setter Property="WindowState" Value="Maximized"></Setter>
            </DataTrigger>
            <DataTrigger Binding="{Binding SelectedViewIndex}" Value="3">
                <Setter Property="WindowStyle" Value="SingleBorderWindow" />
                <Setter Property="WindowState" Value="Maximized"></Setter>
            </DataTrigger>
            <DataTrigger Binding="{Binding SelectedViewIndex}" Value="4">
                <Setter Property="WindowStyle" Value="SingleBorderWindow" />
                <Setter Property="WindowState" Value="Maximized"></Setter>
            </DataTrigger>
        </Style.Triggers>
        <Style.Resources>
            <DataTemplate  x:Key="{dxt:FloatingContainerThemeKey ThemeName=BlackAndBlue, ResourceKey=FloatingContainerCaptionTemplate}"  >
                <Grid Height="Auto">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="Auto" />
                        <ColumnDefinition />
                    </Grid.ColumnDefinitions>
                    <Image x:Name="PART_Icon" Margin="6,3,0,3" Stretch="None" 
                       Source="{Binding Path=(dx:FloatingContainer.FloatingContainer).Icon, RelativeSource={RelativeSource AncestorType=ContentPresenter}}" />
                    <dx:FloatingContainerHeaderPanel Grid.Column="1" >
                        <StackPanel Orientation="Horizontal" Grid.Column="1">
                            <Image  x:Name="PART_Image" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="4,0,0,0" 
                                    Source="/GeosWorkbench;component/Assets/Images/GreenIndicator.png">
                            </Image>
                            <TextBlock x:Name="PART_Text" Style="{DynamicResource TextBlockForegroundColor}"  HorizontalAlignment="Left" VerticalAlignment="Center" Margin="0" IsHitTestVisible="False" />
                        </StackPanel>
                    </dx:FloatingContainerHeaderPanel>
                </Grid>
                <DataTemplate.Triggers>
                    <Trigger Property="Source" SourceName="PART_Icon" Value="{x:Null}">
                        <Setter TargetName="PART_Icon" Property="Visibility" Value="Collapsed" />
                    </Trigger>
                    <DataTrigger Binding="{Binding DataContext.IsServiceActive, RelativeSource={RelativeSource AncestorType=Window}, Mode=OneWay, UpdateSourceTrigger=PropertyChanged}" Value="True">
                        <Setter TargetName="PART_Image" Property="Source" Value="/GeosWorkbench;component/Assets/Images/GreenIndicator.png"/>
                        <Setter Property="ToolTip" Value="{DynamicResource Serviceproviderstart}"></Setter>
                    </DataTrigger>
                    <DataTrigger Binding="{Binding DataContext.IsServiceActive, RelativeSource={RelativeSource AncestorType=Window}, Mode=OneWay, UpdateSourceTrigger=PropertyChanged}" Value="False">
                        <Setter TargetName="PART_Image" Property="Source" Value="/GeosWorkbench;component/Assets/Images/RedIndicator.png"/>
                        <Setter Property="ToolTip" Value="{DynamicResource Serviceproviderstop}"></Setter>
                    </DataTrigger>
                    <DataTrigger Binding="{Binding DataContext.ServiceIndicatorVisibility, RelativeSource={RelativeSource AncestorType=Window}, Mode=OneWay, UpdateSourceTrigger=PropertyChanged}" Value="Visible">
                        <Setter TargetName="PART_Image" Property="Visibility" Value="Visible"/>
                    </DataTrigger>
                    <DataTrigger Binding="{Binding DataContext.ServiceIndicatorVisibility, RelativeSource={RelativeSource AncestorType=Window}, Mode=OneWay, UpdateSourceTrigger=PropertyChanged}" Value="Hidden">
                        <Setter TargetName="PART_Image" Property="Visibility" Value="Collapsed"/>
                    </DataTrigger>
                </DataTemplate.Triggers>
            </DataTemplate>
        </Style.Resources>
    </Style>

    <Style TargetType="Border" x:Key="BorderBorderBrushColor">
        <Setter Property="BorderBrush" Value="WhiteSmoke" />
    </Style>

    <Style TargetType="Label" x:Key="LabelBackgroundImageBrush" BasedOn="{StaticResource BaseUniversalStyle}">
        <Setter Property="Label.Background">
            <Setter.Value>
                <ImageBrush ImageSource="/GeosWorkbench;component/Assets/Images/BarcodeScan.png"/>
            </Setter.Value>
        </Setter>
    </Style>

    <Style TargetType="TextBlock" x:Key="TextBlockForeground" BasedOn="{StaticResource TextBlockStyle}">
        <Setter Property="Foreground" Value="White">
        </Setter>
    </Style>

    <Style TargetType="Button" x:Key="ButtonForeground" BasedOn="{StaticResource BaseUniversalStyle}"  >
        <Setter Property="Foreground" Value="#2AB7FF" />
        <Style.Triggers>
            <Trigger Property="IsMouseOver" Value="true">
                <Setter Property="Foreground" Value="OrangeRed" />
            </Trigger>
        </Style.Triggers>
    </Style>

    <Style TargetType="Image" x:Key="ImageCursor">
        <Setter Property="Cursor" Value="Arrow" ></Setter>
        <Style.Triggers>
            <Trigger Property="IsMouseOver" Value="true">
                <Setter Property="Cursor" Value="Hand" />
            </Trigger>
            <DataTrigger Binding="{Binding IsBusy}" Value="true">
                <Setter Property="Cursor" Value="Wait" />
            </DataTrigger>
        </Style.Triggers>
    </Style>

    <Style TargetType="Border" x:Key="BorderBackground">
        <Setter Property="Background" Value="White" />
        <Setter Property="BorderThickness" Value="2" />
        <Setter Property="BorderBrush" Value="Transparent"></Setter>
        <Style.Triggers>
            <Trigger Property="IsMouseOver" Value="True">
                <Setter Property="BorderThickness" Value="2" />
                <Setter Property="BorderBrush" Value="#2AB7FF"></Setter>
            </Trigger>
        </Style.Triggers>
    </Style>

    <Style TargetType="{x:Type Ellipse}" x:Key="ElipseAppbar"  >
        <Setter Property="Stroke" Value="White" />
        <Style.Triggers>
            <Trigger Property="IsMouseOver" Value="True">
                <Setter Property="Stroke" Value="#2AB7FF">
                </Setter>
            </Trigger>
        </Style.Triggers>
    </Style>

    <Style TargetType="{x:Type Ellipse}" x:Key="ElipseEditProfile"  >
        <Setter Property="Stroke" Value="#2AB7FF"/>
    </Style>

    <Style TargetType="{x:Type Button}">
        <Setter Property="Control.Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type Button}">
                    <Border Name="Border" BorderBrush="{TemplateBinding BorderBrush}" BorderThickness="{TemplateBinding BorderThickness}" Background="{TemplateBinding Background}">
                        <ContentPresenter Content="{TemplateBinding Content}" ContentTemplate="{TemplateBinding ContentTemplate}" Margin="{TemplateBinding Padding}"  HorizontalAlignment="Center" VerticalAlignment="Center" />
                    </Border>
                    <ControlTemplate.Triggers>
                        <Trigger Property="Button.IsFocused" Value="True">
                            <Setter TargetName="Border" Property="BorderBrush" Value="#FF2AB7FF" />
                            <Setter TargetName="Border" Property="Background" Value="#FF333333" />
                        </Trigger>
                        <Trigger Property="Button.IsFocused" Value="False">
                            <Setter TargetName="Border" Property="Background" Value="#FF333333" />
                        </Trigger>
                        <Trigger Property="Button.IsEnabled" Value="false">
                            <Setter TargetName="Border" Property="Opacity" Value="0.3" />
                            <Setter TargetName="Border" Property="BorderBrush" Value="#FFB9B9B9" />
                        </Trigger>

                        <Trigger Property="Button.IsMouseOver" Value="True">
                            <Setter TargetName="Border" Property="Background" Value="#FF515151" />
                        </Trigger>
                        <Trigger Property="Button.IsMouseOver" Value="False">
                            <Setter TargetName="Border" Property="Background" Value="#FF333333" />
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <Style TargetType="Path" x:Key="PleaseWaitingPath">
        <Setter Property="Fill" Value="#FF2AB7FF" />
    </Style>

    <Style TargetType="Image" x:Key="ImageClockStyle">
        <Setter Property="Source" Value="/GeosWorkbench;component/Assets/Images/Clock.png" />
    </Style>

    <Style TargetType="dxwui:WinUIDialogWindow" x:Key="WinUIDialogWindowworkbenchBackgroundStyle" BasedOn="{StaticResource BaseUniversalStyle}" >
        <Setter Property="Background" Value="#FF333333"/>
        <Style.Triggers>
            <DataTrigger Binding="{Binding IsBusy}" Value="True">
                <Setter Property="Cursor" Value="Wait" />
            </DataTrigger>
        </Style.Triggers>
    </Style>

    <Style x:Key="WinUIDialogWindowTitleStyle" TargetType="ContentControl" BasedOn="{StaticResource WinUIDialogTitleStyle}">
        <Setter Property="Foreground" Value="#E0E0E0" />
    </Style>

    <Style x:Key="WinUIDialogTitleEditProfileStyle" TargetType="ContentControl" BasedOn="{StaticResource WinUIDialogWindowTitleStyle}">
        <Setter Property="Margin" Value="120,0,0,0" />
    </Style>

    <Style x:Key="WinUIDialogWindowCenterTitleStyle" TargetType="ContentControl" BasedOn="{StaticResource WinUIDialogWindowTitleStyle}">
        <Setter Property="HorizontalAlignment" Value="Center" />
    </Style>

    <Style x:Key="WinUIDialogWindowWorkOrderTitleStyle" TargetType="ContentControl" BasedOn="{StaticResource WinUIDialogWindowTitleStyle}">
        <Setter Property="Margin" Value="150,0,0,0" />
    </Style>

    <Style TargetType="dxwui:WinUIDialogWindow" x:Key="WinUIDialogWindowStyle" BasedOn="{StaticResource BaseUniversalStyle}">
        <Style.Triggers>
            <DataTrigger Binding="{Binding IsBusy}" Value="True">
                <Setter Property="Cursor" Value="Wait" />
            </DataTrigger>
        </Style.Triggers>
    </Style>

    <Style TargetType="dxlc:LayoutItem" x:Key="LayoutItemForgroundStyle">
        <Setter Property="Foreground" Value="WhiteSmoke" />
    </Style>

    <Style TargetType="Button" x:Key="ButtonStyle" BasedOn="{StaticResource BaseUniversalStyle}">
        <Setter Property="Control.Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type Button}">
                    <Border Name="Border" BorderBrush="{TemplateBinding BorderBrush}" BorderThickness="{TemplateBinding BorderThickness}" Background="{TemplateBinding Background}">
                        <ContentPresenter Content="{TemplateBinding Content}" ContentTemplate="{TemplateBinding ContentTemplate}" Margin="{TemplateBinding Padding}"  HorizontalAlignment="Center" VerticalAlignment="Center" />
                    </Border>
                    <ControlTemplate.Triggers>
                        <Trigger Property="Button.IsFocused" Value="True">
                            <Setter TargetName="Border" Property="BorderBrush" Value="#FF2AB7FF" />
                            <Setter TargetName="Border" Property="Background" Value="#FF333333" />
                        </Trigger>
                        <Trigger Property="Button.IsFocused" Value="False">
                            <Setter TargetName="Border" Property="Background" Value="#FF333333" />
                        </Trigger>
                        <Trigger Property="Button.IsEnabled" Value="false">
                            <Setter TargetName="Border" Property="Opacity" Value="0.3" />
                            <Setter TargetName="Border" Property="BorderBrush" Value="#FFB9B9B9" />
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
        <Setter Property="Cursor" Value="Arrow"/>
        <Style.Triggers>
            <DataTrigger Binding="{Binding IsBusy}" Value="True">
                <Setter Property="Cursor" Value="Wait" />
            </DataTrigger>
        </Style.Triggers>
    </Style>

    <Style TargetType="Border" x:Key="BorderBackgroundInner">
        <Setter Property="Background" Value="#FF2AB7FF"></Setter>
    </Style>

    <Style TargetType="Border" x:Key="BorderBackgroundMainWindow">
        <Setter Property="Background" Value="Black"></Setter>
    </Style>

    <Style TargetType="Border" x:Key="BorderBackgroundOuter">
        <Setter Property="Background" Value="#FF333333"></Setter>
    </Style>

    <Style TargetType="Image" x:Key="GeosImagePath">
        <Setter Property="Source" Value="/GeosWorkbench;component/Assets/Images/Geos_logo_white.png"></Setter>
    </Style>

    <Style TargetType="dxe:ProgressBarEdit" x:Key="ProgressBarEditForeground">
        <Setter Property="Foreground" Value="#FF2AB7FF"/>
        <Setter Property="ShowBorder" Value="False"/>
    </Style>

    <Style TargetType="Image" x:Key="EmdepGeosLogo">
        <Setter Property="Source" Value="/GeosWorkbench;component/Assets/Images/Emdep_logo.png" />
    </Style>

    <Style TargetType="Label" x:Key="LableForegroundColor" BasedOn="{StaticResource BaseUniversalStyle}">
        <Setter Property="Foreground" Value="#FFFFFFFF" />
    </Style>

    <Style TargetType="TextBlock" x:Key="TextBlockForegroundColor" BasedOn="{StaticResource TextBlockStyle}">
        <Setter Property="Foreground" Value="#FFFFFFFF" />
    </Style>

    <Style TargetType="Border" x:Key="SplasherBackgroundColor">
        <Setter Property="Background" Value="#FF333333" />
    </Style>

    <!--This style and DataTemplate for show indicator of serviece is On or Off-->
    <Style x:Key="ServiceIconStyle" TargetType="dxc:DXWindow" BasedOn="{StaticResource BaseUniversalStyle}" >
        <Style.Resources>
            <DataTemplate  x:Key="{dxt:FloatingContainerThemeKey ThemeName=BlackAndBlue, ResourceKey=FloatingContainerCaptionTemplate}">
                <Grid Height="Auto">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="Auto" />
                        <ColumnDefinition />
                    </Grid.ColumnDefinitions>
                    <Image x:Name="PART_Icon" Margin="6,3,0,3" Stretch="None" 
                       Source="{Binding Path=(dx:FloatingContainer.FloatingContainer).Icon, RelativeSource={RelativeSource AncestorType=ContentPresenter}}" />
                    <dx:FloatingContainerHeaderPanel Grid.Column="1" >
                        <StackPanel Orientation="Horizontal" Grid.Column="1">
                            <Image  x:Name="PART_Image" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="4,0,0,0" 
                                    Source="/GeosWorkbench;component/Assets/Images/GreenIndicator.png">
                            </Image>
                            <TextBlock x:Name="PART_Text" Style="{DynamicResource TextBlockForegroundColor}"  HorizontalAlignment="Left" VerticalAlignment="Center" Margin="0" IsHitTestVisible="False" />
                        </StackPanel>
                    </dx:FloatingContainerHeaderPanel>
                </Grid>
                <DataTemplate.Triggers>
                    <Trigger Property="Source" SourceName="PART_Icon" Value="{x:Null}">
                        <Setter TargetName="PART_Icon" Property="Visibility" Value="Collapsed" />
                    </Trigger>
                    <DataTrigger Binding="{Binding DataContext.IsServiceActive, RelativeSource={RelativeSource AncestorType=Window}, Mode=OneWay, UpdateSourceTrigger=PropertyChanged}" Value="True">
                        <Setter TargetName="PART_Image" Property="Source" Value="/GeosWorkbench;component/Assets/Images/GreenIndicator.png"/>
                        <Setter Property="ToolTip" Value="{DynamicResource Serviceproviderstart}"></Setter>
                    </DataTrigger>
                    <DataTrigger Binding="{Binding DataContext.IsServiceActive, RelativeSource={RelativeSource AncestorType=Window}, Mode=OneWay, UpdateSourceTrigger=PropertyChanged}" Value="False">
                        <Setter TargetName="PART_Image" Property="Source" Value="/GeosWorkbench;component/Assets/Images/RedIndicator.png"/>
                        <Setter Property="ToolTip" Value="{DynamicResource Serviceproviderstop}"></Setter>
                    </DataTrigger>
                    <DataTrigger Binding="{Binding DataContext.ServiceIndicatorVisibility, RelativeSource={RelativeSource AncestorType=Window}, Mode=OneWay, UpdateSourceTrigger=PropertyChanged}" Value="Visible">
                        <Setter TargetName="PART_Image" Property="Visibility" Value="Visible"/>
                    </DataTrigger>
                    <DataTrigger Binding="{Binding DataContext.ServiceIndicatorVisibility, RelativeSource={RelativeSource AncestorType=Window}, Mode=OneWay, UpdateSourceTrigger=PropertyChanged}" Value="Hidden">
                        <Setter TargetName="PART_Image" Property="Visibility" Value="Collapsed"/>
                    </DataTrigger>
                </DataTemplate.Triggers>
            </DataTemplate>
        </Style.Resources>
    </Style>

    <Style TargetType="Image" x:Key="IconImageDxWindows">
        <Setter Property="Source" Value="/GeosWorkbench;component/Assets/Images/GreenIndicator.png"/>
        <Style.Triggers>
            <DataTrigger Binding="{Binding IsServiceActive }" Value="True">
                <Setter Property="Source" Value="/GeosWorkbench;component/Assets/Images/GreenIndicator.png"/>
                <Setter Property="ToolTip" Value="Service is Connected"></Setter>
            </DataTrigger>
            <DataTrigger Binding="{Binding IsServiceActive,BindsDirectlyToSource=True}" Value="False">
                <Setter Property="Source" Value="/GeosWorkbench;component/Assets/Images/RedIndicator.png"/>
                <Setter Property="ToolTip" Value="Service is not Connected"></Setter>
            </DataTrigger>
        </Style.Triggers>
    </Style>
    <!--End style and DataTemplate-->

    <Style TargetType="Image" x:Key="ExcelExportButtonImageStyle">
        <Setter Property="Source" Value="/GeosWorkbench;component/Assets/Images/wXlsx.png" />
    </Style>

    <Style TargetType="{x:Type Image}" x:Key="TileBarButtonImageStyle">
        <Style.Triggers>
            <DataTrigger Binding="{Binding Source={x:Static GeosApplication:GeosApplication.Instance}, Path=TileBarVisibility}" Value="Visible">
                <Setter Property="Source" Value="/GeosWorkbench;component/Assets/Images/ArrowUpWhite.png"/>
            </DataTrigger>
            <DataTrigger Binding="{Binding Source={x:Static GeosApplication:GeosApplication.Instance}, Path=TileBarVisibility}" Value="Collapsed">
                <Setter Property="Source" Value="/GeosWorkbench;component/Assets/Images/ArrowDownWhite.png"/>
            </DataTrigger>
        </Style.Triggers>
    </Style>

    <Style TargetType="Hyperlink">
        <Setter Property="Foreground" Value="#2AB7FF"/>
    </Style>

    <ImageSource x:Key="OfficeLogin">/GeosWorkbench;component/Assets/Images/OfficeLogin.png</ImageSource>
    <ImageSource x:Key="ProductionLogin">/GeosWorkbench;component/Assets/Images/ProductionLogin.png</ImageSource>

    <Style TargetType="ToggleButton" x:Key="ToggleButtonStyle" BasedOn="{StaticResource BaseUniversalStyle}">
        <Setter Property="Control.Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type ToggleButton}">
                    <Border Name="Border" BorderBrush="{TemplateBinding BorderBrush}" BorderThickness="{TemplateBinding BorderThickness}" Background="{TemplateBinding Background}">
                        <ContentPresenter Content="{TemplateBinding Content}" ContentTemplate="{TemplateBinding ContentTemplate}" Margin="{TemplateBinding Padding}"  HorizontalAlignment="Center" VerticalAlignment="Center" />
                    </Border>
                    <ControlTemplate.Triggers>
                        <Trigger Property="ToggleButton.IsChecked" Value="True">
                            <Setter TargetName="Border" Property="BorderBrush" Value="#FF2AB7FF" />
                            <Setter TargetName="Border" Property="Background" Value="#FF333333" />
                        </Trigger>
                        <Trigger Property="ToggleButton.IsChecked" Value="false">
                            <Setter TargetName="Border" Property="Background" Value="#FF333333" />
                        </Trigger>
                        <Trigger Property="ToggleButton.IsEnabled" Value="false">
                            <Setter TargetName="Border" Property="Opacity" Value="0.3" />
                            <Setter TargetName="Border" Property="BorderBrush" Value="#FFB9B9B9" />
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
        <Setter Property="Cursor" Value="Arrow"/>
        <Style.Triggers>
            <DataTrigger Binding="{Binding IsBusy}" Value="True">
                <Setter Property="Cursor" Value="Wait" />
            </DataTrigger>
        </Style.Triggers>
    </Style>

    <Style x:Key="{dxet:HyperLinkEditThemeKey ResourceKey=TextBlockInplaceStyle, IsThemeIndependent=True}"
           TargetType="{x:Type TextBlock}"
           BasedOn="{StaticResource {dxet:HyperLinkEditThemeKey ResourceKey=TextBlockInplaceStyle}}">
        <Setter Property="TextDecorations" Value="Underline"/>
        <Style.Triggers>
            <MultiDataTrigger>
                <MultiDataTrigger.Conditions>
                    <Condition Binding="{Binding RowData.SelectionState}" Value="Focused"/>
                </MultiDataTrigger.Conditions>
                <Setter Property="TextBlock.Foreground" Value="Black"/>
            </MultiDataTrigger>
            <MultiDataTrigger>
                <MultiDataTrigger.Conditions>
                    <Condition Binding="{Binding RowData.SelectionState}" Value="None"/>
                </MultiDataTrigger.Conditions>
                <Setter Property="TextBlock.Foreground" Value="#FF2AB7FF"/>
            </MultiDataTrigger>
            <Trigger Property="IsMouseOver" Value="True">
                <Setter Property="Foreground" Value="OrangeRed"/>
            </Trigger>
        </Style.Triggers>
    </Style>

    <Style x:Key="CustomTitleStyle" TargetType="TextBlock" BasedOn="{StaticResource TextBlockStyle}">
        <Setter Property="Foreground" Value="#E0E0E0" />
        <Setter Property="FontSize" Value="28" />
        <Setter Property="FontWeight" Value="Bold" />
    </Style>
</ResourceDictionary>