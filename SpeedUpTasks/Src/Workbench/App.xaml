        <!--Show Week in Day View-->

        <Style x:Key="{dxscht:DayViewThemeKey ResourceKey=DateHeaderStyle, IsThemeIndependent=True}" TargetType="{x:Type dxschd:VisualDateHeader}">
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="{x:Type dxschd:VisualDateHeader}">
                        <Grid Background="Transparent" SnapsToDevicePixels="True" HorizontalAlignment="Stretch" VerticalAlignment="Stretch"
                              dxsch:SchedulerControl.HitTestType="DayHeader" IsHitTestVisible="True">
                            <dxschd:HeaderControl Orientation="Horizontal" SnapsToDevicePixels="True" HorizontalAlignment="Stretch" 
                                                  VerticalAlignment="Stretch" Content="{TemplateBinding Content}" 
                                                  ContentTemplate="{TemplateBinding ContentTemplate}" 
                                                  DisableResourceColor="{TemplateBinding DisableResourceColor}"
                                                  IsAlternate="{Binding IsAlternate}" ResourceColor="{Binding Brushes.ResourceHeader}" 
                                                  dxschd:SchedulerItemsControl.ElementPosition="{Binding (dxschd:SchedulerItemsControl.ElementPosition), RelativeSource={RelativeSource TemplatedParent}}" />
                        </Grid>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
            <Setter Property="ContentTemplate">
                <Setter.Value>
                    <DataTemplate>
                        <StackPanel>
                            <TextBlock Text="{Binding IntervalStart, Converter={UIConverter:DisplayWeekInDayMonthSchedulerConverter}, StringFormat=CW\{0:D2\}}" HorizontalAlignment="Center"
                                       Foreground="{dxcn:ThemeResource {dxscht:SchedulerViewThemeKey ResourceKey=HeaderForegroundColor}}" />
                            <dxschd:DayHeaderControl Date="{Binding IntervalStart}" DateFormats="{Binding DateFormats}"
                                                     x:Name="DayViewDayHeaderControl" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" SnapsToDevicePixels="True">
                                <TextBlock SnapsToDevicePixels="True" TextAlignment="Center" 
                                                    Foreground="{dxcn:ThemeResource {dxscht:SchedulerViewThemeKey ResourceKey=HeaderForegroundColor}}" />
                            </dxschd:DayHeaderControl>
                        </StackPanel>
                    </DataTemplate>
                </Setter.Value>
            </Setter>
        </Style>
        <!--End Show Week in Day View-->

        <!-- Show Week in Week View-->
        <dxschd:HorizontalPositionToMarginConverter x:Key="horizontalPositionToMarginConverter" />
        <!--WeekViewVerticalWeekTemplate-->
        <DataTemplate x:Key="{dxscht:WeekViewThemeKey ResourceKey=WeekViewVerticalWeekTemplate, IsThemeIndependent=True}">
            <dx:XPFContentControl Content="{Binding CellContainer}" 
                                  dxschd:SchedulerItemsControl.ElementPosition="{Binding Path=(dxschd:SchedulerItemsControl.ElementPosition), 
                                  RelativeSource={RelativeSource TemplatedParent}}">
                <ContentControl.Template>
                    <ControlTemplate TargetType="{x:Type ContentControl}">
                        <ContentPresenter Content="{TemplateBinding Content}"
                                          ContentTemplate="{TemplateBinding ContentTemplate}"
                                          dxschd:SchedulerItemsControl.ElementPosition="{Binding Path=(dxschd:SchedulerItemsControl.ElementPosition), 
                                          RelativeSource={RelativeSource TemplatedParent}}" />
                    </ControlTemplate>
                </ContentControl.Template>
                <ContentControl.ContentTemplate>
                    <DataTemplate>
                        <Grid Grid.Row="1">
                            <Grid.Resources>
                                <dxschd:AppointmentStatusDisplayTypeToVisibileConverter x:Key="StatusDisplayTypeToVisibileConverter" />
                            </Grid.Resources>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="Auto" />
                                <RowDefinition />
                            </Grid.RowDefinitions>
