                                            </dxschd:NavigationButtonPairControl.ItemsPanel>
                                        </dxschd:NavigationButtonPairControl>
                                        <dxschd:SharedSizePanel IsBaseSizePanel="True" Grid.Column="1" Orientation="Horizontal" 
                                                                SharedSizeGroup="MonthView_VerticalScrollBarGroup">
                                            <ScrollBar Grid.Column="1" dxschd:SchedulerScrollBarManager.BindToSchedulerControl="True" Margin="{dxci:ThemeResource {dxscht:MonthViewThemeKey ResourceKey=WeekScrollBarMargin}}" SnapsToDevicePixels="True"
                                                                x:Name="PART_ScrollBar" Orientation="Vertical" />
                                        </dxschd:SharedSizePanel>
                                        <dxschd:ResourceNavigatorControl Margin="{dxci:ThemeResource {dxscht:MonthViewThemeKey ResourceKey=ResourceNavigatorMargin}}"
                                                                         Grid.Row="1" x:Name="Navigator"
                                                                         Visibility="{Binding Control.ResourceNavigator.Visibility, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource ResourceNavigatorVisibilityToConverter}}" SchedulerControl="{TemplateBinding Control}" Style="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=Control.ResourceNavigatorHorizontalStyle}" 
                                                                         dxschd:HitTestManager.IsHitTestContainer="True"
                                                                         IsHitTestVisible="True" dxsch:SchedulerControl.HitTestType="None" />
                                        <dx:CornerBox x:Name="Corner" Grid.Row="1" Grid.Column="1" UseLayoutRounding="True" SnapsToDevicePixels="True" />
                                    </Grid>
                                </dx:XPFContentControl>

                                <dx:DXContentControl Grid.Row="1">
                                    <Grid SnapsToDevicePixels="True">
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="*" />
                                            <RowDefinition Height="Auto" />
                                        </Grid.RowDefinitions>
                                        <dxschd:SchedulerItemsControl SnapsToDevicePixels="True" Width="30" 
                                                                    DataContext="{TemplateBinding ResourceContainers}"
                                                                    HorizontalContentAlignment="Left" 
                                                                    VerticalContentAlignment="Stretch" 
                                                                    dxschd:SchedulerItemsControl.ElementPosition="{Binding Path=(dxschd:SchedulerItemsControl.ElementPosition),
                                                                    RelativeSource={RelativeSource Self}}" 
                                                                    ItemsSource="{Binding Path=[0].SimpleIntervals}">   
                                            <dxschd:SchedulerItemsControl.ItemTemplate>
                                                <DataTemplate>
                                                    <Grid>
                                                        <dxschd:MultiColorBorder DefaultBorderThickness="1" 
                                                                         Tag="{dxci:ThemeResource  {dxscht:SchedulerControlThemeKey ResourceKey=ResourceColorSchemaTemplate}}"
                                                                         OuterBorderBrush="{Binding RelativeSource={RelativeSource Self}, Path=Tag[0].CellLightBorder, Converter={UIConverter:ColorToBrush}}"
                                                                         Background="{Binding RelativeSource={RelativeSource Self}, Path=Tag[0].Cell, Converter={UIConverter:ColorToBrush}}"
                                                                         ElementPosition="{Binding Path=(dxschd:SchedulerItemsControl.ElementPosition), 
                                                    RelativeSource={RelativeSource Self}}" 
                                                                         />
                                                        <TextBlock Text="{Binding Path=IntervalStart, Converter={UIConverter:DisplayWeekInDayMonthSchedulerConverter}, StringFormat='{}CW{0:D2}'}" VerticalAlignment="Center" HorizontalAlignment="Center">
                                                            <TextBlock.LayoutTransform>
                                                                <RotateTransform Angle="-90" CenterX="0.5" CenterY="0.5" />
                                                            </TextBlock.LayoutTransform>
                                                        </TextBlock>
                                                    </Grid>
                                                </DataTemplate>
                                            </dxschd:SchedulerItemsControl.ItemTemplate>
                                            <dxschd:SchedulerItemsControl.ItemsPanel>
                                                <ItemsPanelTemplate>
                                                    <dxschd:PixelSnappedUniformGrid Orientation="Vertical"  
                                                                dxschd:SchedulerItemsControl.ElementPosition="{Binding Path=(dxschd:SchedulerItemsControl.ElementPosition), 
                                                                ElementName=weeks}" />
                                                </ItemsPanelTemplate>
                                            </dxschd:SchedulerItemsControl.ItemsPanel>
                                        </dxschd:SchedulerItemsControl>
                                        <Border Grid.Row="1"
                                                Margin="{dxci:ThemeResource {dxscht:MonthViewThemeKey ResourceKey=ResourceNavigatorMargin}}" 
                                                Height="{Binding ElementName=Navigator, Path=ActualHeight}" />
                                    </Grid>
                                </dx:DXContentControl>

                            </Grid>
                        </dxschd:SharedGroupSizeContainer>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style>


