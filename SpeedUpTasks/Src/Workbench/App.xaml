                                    <ContentControl>
                                        <UIHelper:FlyoutHelperScrollBehavior />
                                    </ContentControl>
                                </DataTemplate>
                            </Setter.Value>
                        </Setter>
                    </Style>
                </dxnav:TileBar.Resources>
            </dxnav:TileBar>
        </DataTemplate>

