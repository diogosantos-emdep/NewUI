                                                                SharedSizeGroup="MonthView_VerticalScrollBarGroup">
                                            <Grid SnapsToDevicePixels="True">
                                                <Grid.RowDefinitions>
                                                    <RowDefinition Height="Auto">
                                                    </RowDefinition>
                                                    <RowDefinition Height="*">
                                                    </RowDefinition>
                                                </Grid.RowDefinitions>
                                                <dxschd:SharedSizePanel IsBaseSizePanel="False" Orientation="Vertical" SharedSizeGroup="MonthResourceHeader" dxschd:SchedulerItemsControl.ElementPosition="{dxci:ThemeResource {dxscht:MonthViewThemeKey ResourceKey=TopRightHeaderCornerGroupByResourceElementPosition}}">
                                                    <dxschd:HeaderControl Orientation="Horizontal" SnapsToDevicePixels="True" ResourceColor="{Binding ResourceContainers.Last.ResourceHeader.Brushes.ResourceHeader, RelativeSource={RelativeSource TemplatedParent}}" Visibility="{dxci:ThemeResource {dxscht:SchedulerViewThemeKey ResourceKey=HeaderCornerVisibility}}" />
                                                </dxschd:SharedSizePanel>
                                                <dxschd:SharedSizePanel Grid.Row="1" IsBaseSizePanel="False" Orientation="Vertical" SharedSizeGroup="MonthDayHeader" dxschd:SchedulerItemsControl.ElementPosition="{dxci:ThemeResource {dxscht:MonthViewThemeKey ResourceKey=MiddleRightHeaderCornerGroupByResourceElementPosition}}">
                                                    <dxschd:HeaderControl Orientation="Horizontal" SnapsToDevicePixels="True" ResourceColor="{Binding ResourceContainers.Last.ResourceHeader.Brushes.ResourceHeader, RelativeSource={RelativeSource TemplatedParent}}" Visibility="{dxci:ThemeResource {dxscht:SchedulerViewThemeKey ResourceKey=HeaderCornerVisibility}}" />
                                                </dxschd:SharedSizePanel>
                                            </Grid>
                                        </dxschd:SharedSizePanel>
                                    </Grid>
                                </dx:XPFContentControl>
                                <dx:XPFContentControl Grid.Column="1" Grid.Row="1" Style="{dxci:ThemeResource {dxscht:MonthViewThemeKey ResourceKey=MonthViewContainerStyle}}">
                                    <Grid SnapsToDevicePixels="True">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="*" />
                                            <ColumnDefinition Width="Auto">
                                            </ColumnDefinition>
                                        </Grid.ColumnDefinitions>
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="*" />
                                            <RowDefinition Height="Auto" />
