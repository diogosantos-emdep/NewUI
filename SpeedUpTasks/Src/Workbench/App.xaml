                                                    <dxschd:PixelSnappedUniformGrid Orientation="Horizontal" dxschd:SchedulerItemsControl.ElementPosition="Start,End,Start,End,None,None" />
                                                </ItemsPanelTemplate>
                                            </ItemsControl.ItemsPanel>
                                        </ItemsControl>
                                        <dxschd:NavigationButtonPairControl Style="{dxci:ThemeResource {dxscht:MonthViewThemeKey ResourceKey=NavigationButtonPairGroupByResourceStyle}}" HorizontalContentAlignment="Stretch" VerticalContentAlignment="Stretch" ShowBorder="{Binding Control.ShowBorder, RelativeSource={RelativeSource TemplatedParent}}" 
                                                                            ItemsSource="{TemplateBinding ResourceContainers}" 
                                                                            ItemTemplate="{dxci:ThemeResource {dxscht:SchedulerViewThemeKey ResourceKey=NavigationButtonsPair}}">
                                            <dxschd:NavigationButtonPairControl.ItemsPanel>
                                                <ItemsPanelTemplate>
                                                    <dxschd:PixelSnappedUniformGrid Orientation="Horizontal" dxschd:SchedulerItemsControl.ElementPosition="{dxci:ThemeResource {dxscht:WeekViewThemeKey ResourceKey=GroupByResourceElementPosition}}" ApplyInnerContentPadding="True" />
