                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="Auto" />
                            <ColumnDefinition Width="*" />
                        </Grid.ColumnDefinitions>
                        <Path x:Name="path" Stretch="Fill" Fill="#FF666666" Data="F1 M 6,2L 7.46699,3.47572L 2.88984,8.05286L 7.41782,12.5808L 6,14L -1.32341e-007,8L 6,2 Z " Width="8" Height="12" Margin="8,3,8,3" HorizontalAlignment="Center" VerticalAlignment="Center" Grid.Column="0" cs:Name="Path_0001" dxcore_unique_xmlns:BlendHelper2.ThemeInfo="DevExpress.Xpf.Core\DevExpress.Xpf.Core\Bars\Themes\WhiteAndBlue\PopupControlContainer.xaml;30;26" />
                        <TextBlock x:Name="backText" MaxWidth="60" Foreground="#FF666666" Text="{TemplateBinding Content}" TextTrimming="CharacterEllipsis" VerticalAlignment="Center" HorizontalAlignment="Center" Grid.Column="1" cs:Name="TextBlock_0003" dxcore_unique_xmlns:BlendHelper2.ThemeInfo="DevExpress.Xpf.Core\DevExpress.Xpf.Core\Bars\Themes\WhiteAndBlue\PopupControlContainer.xaml;31;26" />
