                                                        <dxb:BarButtonItem dxdiag:DiagramDesignerControl.BarItemName="{x:Static dxdiag:DefaultBarItemNames.SendToBack}" Style="{DynamicResource {dxdiagt:DiagramDesignerControlThemeKeys ResourceKey=SendToBackButtonStyle}}" />
                                                    </dxb:PopupMenu>
                                                </dxb:BarSplitButtonItem.PopupControl>
                                            </dxb:BarSplitButtonItem>
                                        </dxr:RibbonPageGroup>
                                    </dxr:RibbonPage>
                                    <dxr:RibbonPage Caption="{dxdiag:DiagramControlLocalizedString RibbonPage_Design}" dxdiag:DiagramDesignerControl.BarItemName="{x:Static dxdiag:DefaultBarItemNames.DesignRibbonPage}">
                                        <dxr:RibbonPageGroup Caption="{dxdiag:DiagramControlLocalizedString RibbonPageGroup_PageSetup}" dxdiag:DiagramDesignerControl.BarItemName="{x:Static dxdiag:DefaultBarItemNames.PageSetupGroup}">
                                            <dxb:BarSubItem Style="{DynamicResource {dxdiagt:DiagramDesignerControlThemeKeys ResourceKey=PaperOrientationBarItemStyle}}" dxdiag:DiagramDesignerControl.BarItemName="{x:Static dxdiag:DefaultBarItemNames.PageOrientation}">
                                                <dxb:BarItemSelector dxdiag:DiagramDesignerControl.BarItemName="{x:Static dxdiag:DefaultBarItemNames.PageOrientationSelector}" ItemLinksSource="{Binding PageOrientation, RelativeSource={RelativeSource TemplatedParent}}" SelectedItem="{Binding RootToolsModel[PageSize], RelativeSource={RelativeSource TemplatedParent}, Converter={dxdiag:PaperOrientationConverter}, Mode=TwoWay}">
                                                    <dxb:BarItemSelector.ItemTemplate>
                                                        <DataTemplate>
                                                            <ContentControl>
                                                                <dxb:BarCheckItem Style="{DynamicResource {dxdiagt:DiagramDesignerControlThemeKeys ResourceKey=PaperOrientationButtonsStyle}}" />
                                                            </ContentControl>
                                                        </DataTemplate>
                                                    </dxb:BarItemSelector.ItemTemplate>
                                                </dxb:BarItemSelector>
                                            </dxb:BarSubItem>
                                            <dxb:BarSubItem dxdiag:DiagramDesignerControl.BarItemName="{x:Static dxdiag:DefaultBarItemNames.PageSize}" Style="{DynamicResource {dxdiagt:DiagramDesignerControlThemeKeys ResourceKey=PageSizeBarItemStyle}}" Content="{dxdiag:DiagramControlLocalizedString Ribbon_PageSize}">
                                                <dxb:BarItemSelector dxdiag:DiagramDesignerControl.BarItemName="{x:Static dxdiag:DefaultBarItemNames.PageSizeSelector}" ItemLinksSource="{x:Static dxdiagcore:PaperSizeCalculator.SelectedPaperSizeTable}" AllowEmptySelection="True" SelectedItem="{Binding RootToolsModel[PageSize], RelativeSource={RelativeSource TemplatedParent}, Converter={dxdiag:SelectedPaperSizeConverter}, Mode=TwoWay}">
                                                    <dxb:BarItemSelector.ItemTemplate>
                                                        <DataTemplate>
                                                            <ContentControl>
                                                                <dxdiag:BarMouseCheckOnlyItem Style="{DynamicResource {dxdiagt:DiagramDesignerControlThemeKeys ResourceKey=PageSizeButtonsStyle}}" />
                                                            </ContentControl>
                                                        </DataTemplate>
                                                    </dxb:BarItemSelector.ItemTemplate>
                                                </dxb:BarItemSelector>
                                                <dxb:BarItemSeparator dxdiag:DiagramDesignerControl.BarItemName="{x:Static dxdiag:DefaultBarItemNames.PageSizeSeparator}" />
                                                <dxb:BarButtonItem dxdiag:DiagramDesignerControl.BarItemName="{x:Static dxdiag:DefaultBarItemNames.MorePageSize}" Style="{DynamicResource {dxdiagt:DiagramDesignerControlThemeKeys ResourceKey=SetPageSizeButtonStyle}}" />
                                            </dxb:BarSubItem>
                                            <dxb:BarSubItem dxdiag:DiagramDesignerControl.BarItemName="{x:Static dxdiag:DefaultBarItemNames.AutoSize}" Style="{DynamicResource {dxdiagt:DiagramDesignerControlThemeKeys ResourceKey=CanvasAutoSizeBarItemStyle}}">
                                                <dxb:BarItemSelector dxdiag:DiagramDesignerControl.BarItemName="{x:Static dxdiag:DefaultBarItemNames.AutoSizeSelector}" ItemLinksSource="{x:Static dxdiag:CanvasSizeModeInfo.Modes}" SelectedItem="{Binding RootToolsModel[CanvasSizeMode], RelativeSource={RelativeSource TemplatedParent}, Mode=TwoWay, Converter={dxdiag:CanvasSizeModeToCanvasSizeModeInfoConverter}}" AllowEmptySelection="False">
                                                    <dxb:BarItemSelector.ItemTemplate>
                                                        <DataTemplate>
                                                            <ContentControl>
                                                                <dxb:BarCheckItem Style="{DynamicResource {dxdiagt:DiagramDesignerControlThemeKeys ResourceKey=CanvasAutoSizeButtonStyle}}" />
                                                            </ContentControl>
                                                        </DataTemplate>
                                                    </dxb:BarItemSelector.ItemTemplate>
                                                </dxb:BarItemSelector>
                                            </dxb:BarSubItem>
                                        </dxr:RibbonPageGroup>
                                        <dxr:RibbonPageGroup Caption="{dxdiag:DiagramControlLocalizedString RibbonPageGroup_Themes}" dxdiag:DiagramDesignerControl.BarItemName="{x:Static dxdiag:DefaultBarItemNames.ThemesGroup}">
                                            <dxr:RibbonGalleryBarItem Content="{dxdiag:DiagramControlLocalizedString RibbonPageGroup_Themes}" dxdiag:DiagramDesignerControl.BarItemName="{x:Static dxdiag:DefaultBarItemNames.Themes}">
                                                <dxr:RibbonGalleryBarItem.Resources>
                                                    <Style TargetType="{x:Type dxb:Gallery}" x:Key="themeGalleryStyle">
                                                        <Setter Property="ItemGlyphBorderTemplate">
                                                            <Setter.Value>
                                                                <ControlTemplate>
                                                                    <dxdiag:ThemePresenter DataContext="{Binding Path=Item.Caption, RelativeSource={RelativeSource Mode=FindAncestor, AncestorType={x:Type dxb:GalleryItemControl}}}" DiagramTheme="{Binding}" ToolTip="{Binding}" />
                                                                </ControlTemplate>
                                                            </Setter.Value>
                                                        </Setter>
                                                        <Setter Property="ItemGlyphVerticalAlignment" Value="Center" />
                                                        <Setter Property="ItemGlyphHorizontalAlignment" Value="Center" />
                                                        <Setter Property="ItemAutoHeight" Value="True" />
                                                        <Setter Property="AllowToolTips" Value="False" />
                                                        <Setter Property="IsItemDescriptionVisible" Value="False" />
                                                        <Setter Property="IsItemCaptionVisible" Value="False" />
                                                        <Setter Property="IsGroupCaptionVisible" Value="False" />
                                                        <Setter Property="AllowFilter" Value="False" />
                                                        <Setter Property="MinColCount" Value="8" />
                                                        <Setter Property="ColCount" Value="8" />
                                                        <Setter Property="AutoHideGallery" Value="False" />
                                                        <Setter Property="ItemCheckMode" Value="Single" />
                                                        <Setter Property="FirstCheckedItem" Value="{Binding Path=RootToolsModel[Theme], RelativeSource={RelativeSource FindAncestor, AncestorType={x:Type dxdiag:DiagramDesignerControl}}, Mode=TwoWay}" />
                                                    </Style>
                                                </dxr:RibbonGalleryBarItem.Resources>
                                                <dxr:RibbonGalleryBarItem.Gallery>
                                                    <dxb:Gallery Style="{StaticResource themeGalleryStyle}">
                                                        <dxb:GalleryItemGroup ItemsSource="{x:Static dxdiagcore:ThemeRegistrator.Themes}" ItemTemplate="{StaticResource galleryControlItemTemplate}" />
                                                    </dxb:Gallery>
                                                </dxr:RibbonGalleryBarItem.Gallery>
                                                <dxr:RibbonGalleryBarItem.DropDownGallery>
                                                    <dxb:Gallery Style="{StaticResource themeGalleryStyle}">
                                                        <dxb:GalleryItemGroup ItemsSource="{x:Static dxdiagcore:ThemeRegistrator.Themes}" ItemTemplate="{StaticResource galleryControlItemTemplate}" />
                                                    </dxb:Gallery>
                                                </dxr:RibbonGalleryBarItem.DropDownGallery>
                                            </dxr:RibbonGalleryBarItem>
                                        </dxr:RibbonPageGroup>
                                        <dxr:RibbonPageGroup Caption="{dxdiag:DiagramControlLocalizedString RibbonPageGroup_Options}" dxdiag:DiagramDesignerControl.BarItemName="{x:Static dxdiag:DefaultBarItemNames.OptionsGroup}">
                                            <dxb:BarCheckItem dxdiag:DiagramDesignerControl.BarItemName="{x:Static dxdiag:DefaultBarItemNames.SnapToGrid}" Style="{DynamicResource {dxdiagt:DiagramDesignerControlThemeKeys ResourceKey=SnapToGridButtonStyle}}" />
                                            <dxb:BarCheckItem dxdiag:DiagramDesignerControl.BarItemName="{x:Static dxdiag:DefaultBarItemNames.SnapToItems}" Style="{DynamicResource {dxdiagt:DiagramDesignerControlThemeKeys ResourceKey=SnapToItemsButtonStyle}}" />
                                            <dxb:BarEditItem dxdiag:DiagramDesignerControl.BarItemName="{x:Static dxdiag:DefaultBarItemNames.SnapDistance}" Style="{DynamicResource {dxdiagt:DiagramDesignerControlThemeKeys ResourceKey=SnapDistanceEditorStyle}}">
                                                <dxb:BarEditItem.EditSettings>
                                                    <dxe:SpinEditSettings MinValue="2" MaxValue="20" />
                                                </dxb:BarEditItem.EditSettings>
                                            </dxb:BarEditItem>
                                        </dxr:RibbonPageGroup>
                                        <dxr:RibbonPageGroup Caption="{dxdiag:DiagramControlLocalizedString RibbonPageGroup_TreeLayout}" dxdiag:DiagramDesignerControl.BarItemName="{x:Static dxdiag:DefaultBarItemNames.LayoutGroup}">
                                            <dxb:BarSubItem Style="{DynamicResource {dxdiagt:DiagramDesignerControlThemeKeys ResourceKey=TreeLayoutDirectionButtonStyle}}" dxdiag:DiagramDesignerControl.BarItemName="{x:Static dxdiag:DefaultBarItemNames.ReLayout}">
                                                <dxb:BarItemMenuHeader dxdiag:DiagramDesignerControl.BarItemName="{x:Static dxdiag:DefaultBarItemNames.ReLayoutSelector}" Style="{DynamicResource {dxdiagt:DiagramDesignerControlThemeKeys ResourceKey=TreeLayoutMenuHeaderStyle}}">
                                                    <dxb:BarButtonItem dxdiag:DiagramDesignerControl.BarItemName="{x:Static dxdiag:DefaultBarItemNames.ReLayoutTopToBottom}" Style="{DynamicResource {dxdiagt:DiagramDesignerControlThemeKeys ResourceKey=TreeLayoutTopToBottomButtonStyle}}" />
                                                    <dxb:BarButtonItem dxdiag:DiagramDesignerControl.BarItemName="{x:Static dxdiag:DefaultBarItemNames.ReLayoutBottomToTop}" Style="{DynamicResource {dxdiagt:DiagramDesignerControlThemeKeys ResourceKey=TreeLayoutBottomToTopButtonStyle}}" />
                                                    <dxb:BarButtonItem dxdiag:DiagramDesignerControl.BarItemName="{x:Static dxdiag:DefaultBarItemNames.ReLayoutLeftToRight}" Style="{DynamicResource {dxdiagt:DiagramDesignerControlThemeKeys ResourceKey=TreeLayoutLeftToRightButtonStyle}}" />
                                                    <dxb:BarButtonItem dxdiag:DiagramDesignerControl.BarItemName="{x:Static dxdiag:DefaultBarItemNames.ReLayoutRightToLeft}" Style="{DynamicResource {dxdiagt:DiagramDesignerControlThemeKeys ResourceKey=TreeLayoutRightToLeftButtonStyle}}" />
                                                </dxb:BarItemMenuHeader>
                                            </dxb:BarSubItem>
                                        </dxr:RibbonPageGroup>
                                    </dxr:RibbonPage>
                                    <dxr:RibbonPage Caption="{dxdiag:DiagramControlLocalizedString RibbonPage_View}" dxdiag:DiagramDesignerControl.BarItemName="{x:Static dxdiag:DefaultBarItemNames.ViewRibbonPage}">
                                        <dxr:RibbonPageGroup Caption="{dxdiag:DiagramControlLocalizedString RibbonPageGroup_Show}" dxdiag:DiagramDesignerControl.BarItemName="{x:Static dxdiag:DefaultBarItemNames.ShowGroup}">
                                            <dxb:BarEditItem dxdiag:DiagramDesignerControl.BarItemName="{x:Static dxdiag:DefaultBarItemNames.ShowRuler}" Style="{DynamicResource {dxdiagt:DiagramDesignerControlThemeKeys ResourceKey=ShowRulerItemStyle}}" />
                                            <dxb:BarEditItem dxdiag:DiagramDesignerControl.BarItemName="{x:Static dxdiag:DefaultBarItemNames.ShowGrid}" Style="{DynamicResource {dxdiagt:DiagramDesignerControlThemeKeys ResourceKey=ShowGridItemStyle}}" />
                                        </dxr:RibbonPageGroup>
                                        <dxr:RibbonPageGroup Caption="{dxdiag:DiagramControlLocalizedString RibbonPageGroup_Zoom}" dxdiag:DiagramDesignerControl.BarItemName="{x:Static dxdiag:DefaultBarItemNames.ZoomGroup}">
                                            <dxb:BarButtonItem dxdiag:DiagramDesignerControl.BarItemName="{x:Static dxdiag:DefaultBarItemNames.FitToWindow}" Style="{DynamicResource {dxdiagt:DiagramDesignerControlThemeKeys ResourceKey=FitToPage}}" />
                                            <dxb:BarButtonItem dxdiag:DiagramDesignerControl.BarItemName="{x:Static dxdiag:DefaultBarItemNames.FitToWidth}" Style="{DynamicResource {dxdiagt:DiagramDesignerControlThemeKeys ResourceKey=FitToWidth}}" />
                                        </dxr:RibbonPageGroup>
                                    </dxr:RibbonPage>
                                </dxr:RibbonDefaultPageCategory>
                            </dxr:RibbonControl>
                            <dxr:RibbonStatusBarControl DockPanel.Dock="Bottom" dxdiag:DiagramDesignerControl.BarItemName="{x:Static dxdiag:DefaultBarItemNames.StatusBar}">
                                <dxr:RibbonStatusBarControl.RightItems>
                                    <dxb:BarEditItem Content="{TemplateBinding ZoomFactor, Converter={dxmvvm:FormatStringConverter FormatString=P0}}" dxdiag:DiagramDesignerControl.BarItemName="{x:Static dxdiag:DefaultBarItemNames.StatusBarZoomEditor}" EditValue="{Binding ZoomFactor, RelativeSource={RelativeSource TemplatedParent}, Converter={dxdiag:NonlinearZoomConverter}, Mode=TwoWay}" EditWidth="100">
                                        <dxb:BarEditItem.EditSettings>
                                            <dxe:TrackBarEditSettings Minimum="0" Maximum="1" SmallStep="0.01" LargeStep="0.1" TickFrequency="0.5" />
                                        </dxb:BarEditItem.EditSettings>
                                    </dxb:BarEditItem>
                                </dxr:RibbonStatusBarControl.RightItems>
                                <dxr:RibbonStatusBarControl.LeftItems>
                                    <dxb:BarStaticItem RibbonStyle="SmallWithText" dxdiag:DiagramDesignerControl.BarItemName="{x:Static dxdiag:DefaultBarItemNames.StatusBarShapeInfo}">
                                        <dxb:BarStaticItem.Content>
                                            <MultiBinding Converter="{dxdiag:ItemPropertiesToStringConverter}">
                                                <Binding Path="SelectionToolsModel[Size]" RelativeSource="{RelativeSource TemplatedParent}" />
                                                <Binding Path="SelectionToolsModel[Angle]" RelativeSource="{RelativeSource TemplatedParent}" />
                                            </MultiBinding>
                                        </dxb:BarStaticItem.Content>
                                    </dxb:BarStaticItem>
                                </dxr:RibbonStatusBarControl.LeftItems>
                            </dxr:RibbonStatusBarControl>
                            <dxdo:DockLayoutManager DockPanel.Dock="Left" x:Name="PART_DockLayoutManager">
                                <dxdo:LayoutGroup Orientation="Horizontal">
                                    <dxdo:LayoutPanel x:Name="PART_ToolboxPane" ItemWidth="250" MaxWidth="500" Style="{StaticResource DiagramLayoutPanelStyle}" AllowDock="True" AllowFloat="False" AllowContextMenu="False" AllowDockToDocumentGroup="False">
                                        <dxdo:LayoutPanel.CaptionTemplate>
                                            <DataTemplate>
                                                <TextBlock Text="{dxdiag:DiagramControlLocalizedString Panel_Shapes_Name}" Visibility="{Binding ElementName=CollapsedBehavior, Path=IsCompact, Converter={dxmvvm:BooleanToVisibilityConverter Inverse=True}}" />
                                            </DataTemplate>
                                        </dxdo:LayoutPanel.CaptionTemplate>
                                        <dxdo:LayoutPanel.ControlBoxContent>
                                            <ToggleButton Style="{StaticResource HideButtonStyle}" x:Name="CollapseButton" IsChecked="{Binding ElementName=CollapsedBehavior, Path=ButtonChecked, Mode=TwoWay}" />
                                        </dxdo:LayoutPanel.ControlBoxContent>
                                        <dxmvvm:Interaction.Behaviors>
                                            <dxdiag:PanelCollapsedBehavior x:Name="CollapsedBehavior" LayoutError="{Binding ElementName=Toolbox, Path=LayoutError}" ToolboxViewMode="{Binding ToolboxViewMode, RelativeSource={RelativeSource TemplatedParent}, Mode=TwoWay}" />
                                        </dxmvvm:Interaction.Behaviors>
                                        <dxdiag:DiagramToolboxControl x:Name="Toolbox" DiagramControl="{Binding RelativeSource={RelativeSource TemplatedParent}}" SelectedStencils="{Binding SelectedStencils, RelativeSource={RelativeSource TemplatedParent}}" IsCompact="{Binding ElementName=CollapsedBehavior, Path=IsCompact}" CompactWidth="{Binding ElementName=CollapsedBehavior, Path=InitialCompactWidth, Mode=OneWay}" />
                                    </dxdo:LayoutPanel>
                                    <dxdo:DocumentPanel>
                                        <ScrollViewer Background="{DynamicResource {dxdiagt:DiagramControlThemeKeys ResourceKey=DiagramControlBackground}}" dx:ScrollBarExtensions.ScrollBarMode="TouchOverlap" CanContentScroll="True" HorizontalScrollBarVisibility="Auto" VerticalScrollBarVisibility="Auto" Name="ScrollViewer">
                                            <dxdiag:LayersHost x:Name="LayersHost" Focusable="True" FocusVisualStyle="{x:Null}" />
                                        </ScrollViewer>
                                    </dxdo:DocumentPanel>
                                    <dxdo:LayoutPanel x:Name="PART_PropertiesPane" AllowClose="True" Style="{StaticResource DiagramLayoutPanelStyle}" AllowHide="True" MinWidth="200" MaxWidth="500" ItemWidth="300" Caption="{dxdiag:DiagramControlLocalizedString Panel_Properties_Name}">
                                        <dxmvvm:Interaction.Behaviors>
                                            <dxdiag:PropertiesPanelVisibilityBehavior PropertiesPanelVisibility="{Binding PropertiesPanelVisibility, RelativeSource={RelativeSource TemplatedParent}, Mode=TwoWay}" />
                                        </dxmvvm:Interaction.Behaviors>
                                        <dxpg:PropertyGridControl ScrollViewer.VerticalScrollBarVisibility="Auto" ShowCategories="False" SelectedObject="{Binding SelectionModel, RelativeSource={RelativeSource TemplatedParent}}">
                                            <dxpg:PropertyGridControl.Resources>
                                                <DataTemplate x:Key="propertyGridComboboxEdit">
                                                    <dxe:ComboBoxEdit x:Name="PART_Editor" ItemsSource="{Binding StandardValues}" />
                                                </DataTemplate>
                                            </dxpg:PropertyGridControl.Resources>
                                        </dxpg:PropertyGridControl>
                                    </dxdo:LayoutPanel>
                                </dxdo:LayoutGroup>
                            </dxdo:DockLayoutManager>
                        </DockPanel>
                    </dxb:BarManager>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
</ResourceDictionary>