                                            <dxb:BarSplitButtonItem dxdiag:DiagramDesignerControl.BarItemName="{x:Static dxdiag:DefaultBarItemNames.SendToBackContainer}" Style="{DynamicResource {dxdiagt:DiagramDesignerControlThemeKeys ResourceKey=SendToBackSplitButtonStyle}}">
                                                <dxb:BarSplitButtonItem.PopupControl>
