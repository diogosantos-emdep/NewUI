<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:dxdiag="http://schemas.devexpress.com/winfx/2008/xaml/diagram" xmlns:dxdiagt="http://schemas.devexpress.com/winfx/2008/xaml/diagram/themekeys" xmlns:dxdiagn="http://schemas.devexpress.com/winfx/2008/xaml/diagram/internal" xmlns:dxmvvm="http://schemas.devexpress.com/winfx/2008/xaml/mvvm" xmlns:dxpg="http://schemas.devexpress.com/winfx/2008/xaml/propertygrid" xmlns:dxb="http://schemas.devexpress.com/winfx/2008/xaml/bars" xmlns:dxe="http://schemas.devexpress.com/winfx/2008/xaml/editors" xmlns:dx="http://schemas.devexpress.com/winfx/2008/xaml/core" xmlns:dxr="http://schemas.devexpress.com/winfx/2008/xaml/ribbon" xmlns:dxdo="http://schemas.devexpress.com/winfx/2008/xaml/docking" xmlns:dxdockt="http://schemas.devexpress.com/winfx/2008/xaml/docking/themekeys" xmlns:dxdiagcore="clr-namespace:DevExpress.Diagram.Core;assembly=DevExpress.Diagram.v17.1.Core" xmlns:sys="clr-namespace:System;assembly=mscorlib" xmlns:dxdst="http://schemas.devexpress.com/winfx/2008/xaml/core/themekeys">

    <x:Array x:Key="DiagramFontSizes" Type="sys:Double">
        <sys:Double>6</sys:Double>
        <sys:Double>8</sys:Double>
        <sys:Double>9</sys:Double>
        <sys:Double>10</sys:Double>
        <sys:Double>11</sys:Double>
        <sys:Double>12</sys:Double>
        <sys:Double>14</sys:Double>
        <sys:Double>16</sys:Double>
        <sys:Double>18</sys:Double>
        <sys:Double>24</sys:Double>
        <sys:Double>30</sys:Double>
        <sys:Double>36</sys:Double>
        <sys:Double>48</sys:Double>
        <sys:Double>60</sys:Double>
    </x:Array>

    <dxmvvm:ObjectToObjectConverter x:Key="HideButtonTransformConverter" DefaultTarget="1">
        <dxmvvm:MapItem Source="True" Target="-1" />
    </dxmvvm:ObjectToObjectConverter>

    <Style x:Key="HideButtonStyle" TargetType="{x:Type ToggleButton}">
        <Setter Property="Background" Value="Transparent" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type ToggleButton}">
                    <Grid Margin="0,0,0,0" Width="23" Height="23" Background="#00000000">
                        <Path Fill="{TemplateBinding Foreground}" Stretch="Fill" x:Name="glyph" Width="9" Height="10" RenderTransformOrigin="0.5,0.5" Data="F1 M 1395,798L 1400,793L 1400,795L 1397,798L 1400,801L 1400,803L 1395,798 Z M 1399,798L 1404,793L 1404,795L 1401,798L 1404,801L 1404,803L 1399,798 Z " HorizontalAlignment="Center" VerticalAlignment="Center">
                            <Path.RenderTransform>
                                <ScaleTransform ScaleX="{Binding IsChecked, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource HideButtonTransformConverter}}" />
                            </Path.RenderTransform>
                        </Path>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
        <Setter Property="Foreground" Value="{DynamicResource {dxdockt:DockPaneElementsThemeKey ResourceKey=CaptionNormalForeground}}" />
        <Style.Triggers>
            <DataTrigger Binding="{Binding IsActive, ElementName=PART_ToolboxPane}" Value="True">
                <Setter Property="Foreground" Value="{DynamicResource {dxdockt:DockPaneElementsThemeKey ResourceKey=CaptionActiveForeground}}" />
            </DataTrigger>
        </Style.Triggers>
    </Style>
    <Style x:Key="{dxdst:DefaultStyleThemeKey FullName=DevExpress.Xpf.Diagram.DiagramControl}" TargetType="{x:Type dxdiag:DiagramControl}">
        <Setter Property="Background" Value="{DynamicResource {dxdiagt:DiagramControlThemeKeys ResourceKey=DiagramControlBackground}}" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type dxdiag:DiagramControl}">
                    <ScrollViewer Background="{TemplateBinding Background}" CanContentScroll="True" HorizontalScrollBarVisibility="Auto" VerticalScrollBarVisibility="Auto" Name="ScrollViewer">
                        <dxdiag:LayersHost x:Name="LayersHost" Focusable="True" FocusVisualStyle="{x:Null}" Background="{TemplateBinding Background}" />
                    </ScrollViewer>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <Style x:Key="{dxdst:DefaultStyleThemeKey FullName=DevExpress.Xpf.Diagram.ThemePresenter}" TargetType="{x:Type dxdiag:ThemePresenter}">
        <Setter Property="Text" Value="Aa" />
        <Setter Property="Width" Value="62" />
        <Setter Property="Height" Value="48" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type dxdiag:ThemePresenter}">
                    <Canvas Height="{TemplateBinding Height}" Width="{TemplateBinding Width}">
                        <Rectangle x:Name="PART_RectangleVariant1" Fill="LightGray" Stroke="Black" Canvas.Left="5" Canvas.Top="4" Height="34" Width="34" />
                        <Rectangle x:Name="PART_RectangleVariant4" Fill="LightGray" Stroke="Black" Canvas.Left="41" Canvas.Top="18" Height="16" Width="16" />
                        <Rectangle x:Name="PART_RectangleVariant2" Fill="LightGray" Stroke="Black" Canvas.Left="36" Canvas.Top="13" Height="16" Width="16" />
                        <Rectangle x:Name="PART_RectangleVariant3" Fill="LightGray" Stroke="Black" Canvas.Left="31" Canvas.Top="8" Height="16" Width="16" />
                        <Polyline x:Name="PART_Line1Variant1" Stroke="Black" Points="22,38 22,43 49,43 49,33" />
                        <Line x:Name="PART_Line2Variant1" Stroke="Black" X1="0" Y1="0" X2="2" Y2="4" Canvas.Left="49" Canvas.Top="34" />
                        <Line x:Name="PART_Line3Variant1" Stroke="Black" X1="0" Y1="0" X2="-2" Y2="4" Canvas.Left="49" Canvas.Top="34" />
                        <TextBlock x:Name="PART_TextVariant1" Text="Aa" Canvas.Left="8" Canvas.Top="18" FontSize="12" />
                    </Canvas>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <DataTemplate x:Key="galleryControlItemTemplate">
        <ContentControl>
            <dxb:GalleryItem Caption="{Binding}">
                <dxb:GalleryItem.Glyph>
                    <DrawingImage />
                </dxb:GalleryItem.Glyph>
            </dxb:GalleryItem>
        </ContentControl>
    </DataTemplate>

    <Style x:Key="DiagramLayoutPanelStyle" TargetType="{x:Type dxdo:LayoutPanel}">
        <Setter Property="AllowMaximize" Value="False" />
        <Setter Property="AllowMinimize" Value="False" />
        <Setter Property="AllowClose" Value="False" />
        <Setter Property="AllowHide" Value="False" />
        <Setter Property="AllowDock" Value="True" />
        <Setter Property="AllowFloat" Value="False" />
        <Setter Property="AllowContextMenu" Value="False" />
        <Setter Property="AllowDockToDocumentGroup" Value="False" />
    </Style>

    <Style x:Key="{dxdst:DefaultStyleThemeKey FullName=DevExpress.Xpf.Diagram.DiagramDesignerControl}" TargetType="{x:Type dxdiag:DiagramDesignerControl}">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type dxdiag:DiagramDesignerControl}">
                    <dxb:BarManager>
                        <dxb:BarManager.Items>
                            <dxb:BarButtonItem dxdiag:DiagramDesignerControl.BarItemName="{x:Static dxdiag:DefaultBarItemNames.Save}" Style="{DynamicResource {dxdiagt:DiagramDesignerControlThemeKeys ResourceKey=SaveFileButtonStyle}}" />
                            <dxb:BarButtonItem dxdiag:DiagramDesignerControl.BarItemName="{x:Static dxdiag:DefaultBarItemNames.Undo}" Style="{DynamicResource {dxdiagt:DiagramDesignerControlThemeKeys ResourceKey=UndoButtonStyle}}" />
                            <dxb:BarButtonItem dxdiag:DiagramDesignerControl.BarItemName="{x:Static dxdiag:DefaultBarItemNames.Redo}" Style="{DynamicResource {dxdiagt:DiagramDesignerControlThemeKeys ResourceKey=RedoButtonStyle}}" />
                        </dxb:BarManager.Items>
                        <DockPanel>
                            <dxr:RibbonControl RibbonStyle="Office2010" DockPanel.Dock="Top" dxdiag:DiagramDesignerControl.BarItemName="{x:Static dxdiag:DefaultBarItemNames.Ribbon}">
                                <dxr:RibbonControl.ToolbarItemLinks>
                                    <dxb:BarButtonItemLink BarItemName="{x:Static dxdiag:DefaultBarItemNames.Save}" />
                                    <dxb:BarButtonItemLink BarItemName="{x:Static dxdiag:DefaultBarItemNames.Undo}" />
                                    <dxb:BarButtonItemLink BarItemName="{x:Static dxdiag:DefaultBarItemNames.Redo}" />
                                </dxr:RibbonControl.ToolbarItemLinks>
                                <dxr:RibbonControl.ApplicationMenu>
                                    <dxr:ApplicationMenu>
                                        <dxb:BarButtonItem dxdiag:DiagramDesignerControl.BarItemName="{x:Static dxdiag:DefaultBarItemNames.NewFile}" Style="{DynamicResource {dxdiagt:DiagramDesignerControlThemeKeys ResourceKey=NewFileButtonStyle}}" />
                                        <dxb:BarButtonItem dxdiag:DiagramDesignerControl.BarItemName="{x:Static dxdiag:DefaultBarItemNames.OpenFile}" Style="{DynamicResource {dxdiagt:DiagramDesignerControlThemeKeys ResourceKey=OpenFileButtonStyle}}" />
                                        <dxb:BarButtonItem dxdiag:DiagramDesignerControl.BarItemName="{x:Static dxdiag:DefaultBarItemNames.SaveFile}" Style="{DynamicResource {dxdiagt:DiagramDesignerControlThemeKeys ResourceKey=SaveFileButtonStyle}}" />
                                        <dxb:BarButtonItem dxdiag:DiagramDesignerControl.BarItemName="{x:Static dxdiag:DefaultBarItemNames.SaveAsFile}" Style="{DynamicResource {dxdiagt:DiagramDesignerControlThemeKeys ResourceKey=SaveFileAsButtonStyle}}" />
                                    </dxr:ApplicationMenu>
                                </dxr:RibbonControl.ApplicationMenu>
                                <dxr:RibbonDefaultPageCategory dxdiag:DiagramDesignerControl.BarItemName="{x:Static dxdiag:DefaultBarItemNames.DefaultPageCategory}">
                                    <dxr:RibbonPage Caption="{dxdiag:DiagramControlLocalizedString RibbonPage_Home}" dxdiag:DiagramDesignerControl.BarItemName="{x:Static dxdiag:DefaultBarItemNames.HomeRibbonPage}">
                                        <dxr:RibbonPageGroup Caption="{dxdiag:DiagramControlLocalizedString RibbonPageGroup_Clipboard}" dxdiag:DiagramDesignerControl.BarItemName="{x:Static dxdiag:DefaultBarItemNames.ClipboardRibbonGroup}">
                                            <dxb:BarButtonItem dxdiag:DiagramDesignerControl.BarItemName="{x:Static dxdiag:DefaultBarItemNames.Paste}" Style="{DynamicResource {dxdiagt:DiagramDesignerControlThemeKeys ResourceKey=PasteButtonStyle}}" />
                                            <dxb:BarButtonItem dxdiag:DiagramDesignerControl.BarItemName="{x:Static dxdiag:DefaultBarItemNames.Cut}" Style="{DynamicResource {dxdiagt:DiagramDesignerControlThemeKeys ResourceKey=CutButtonStyle}}" />
                                            <dxb:BarButtonItem dxdiag:DiagramDesignerControl.BarItemName="{x:Static dxdiag:DefaultBarItemNames.Copy}" Style="{DynamicResource {dxdiagt:DiagramDesignerControlThemeKeys ResourceKey=CopyButtonStyle}}" />
                                        </dxr:RibbonPageGroup>
                                        <dxr:RibbonPageGroup Caption="{dxdiag:DiagramControlLocalizedString RibbonPageGroup_Font}" dxdiag:DiagramDesignerControl.BarItemName="{x:Static dxdiag:DefaultBarItemNames.FontRibbonGroup}">
                                            <dxr:BarButtonGroup dxdiag:DiagramDesignerControl.BarItemName="{x:Static dxdiag:DefaultBarItemNames.FontSizeAndFamilyButtonGroup}">
                                                <dxb:BarEditItem dxdiag:DiagramDesignerControl.BarItemName="{x:Static dxdiag:DefaultBarItemNames.FontFamily}" Style="{DynamicResource {dxdiagt:DiagramDesignerControlThemeKeys ResourceKey=FontFamilySelectorStyle}}" EditValue="{Binding SelectionToolsModel[FontFamily], RelativeSource={RelativeSource TemplatedParent}}">
                                                    <dxb:BarEditItem.EditSettings>
                                                        <dxe:FontEditSettings />
                                                    </dxb:BarEditItem.EditSettings>
                                                </dxb:BarEditItem>
                                                <dxb:BarEditItem dxdiag:DiagramDesignerControl.BarItemName="{x:Static dxdiag:DefaultBarItemNames.FontSize}" Style="{DynamicResource {dxdiagt:DiagramDesignerControlThemeKeys ResourceKey=FontSizeSelectorStyle}}" EditValue="{Binding SelectionToolsModel[FontSize], RelativeSource={RelativeSource TemplatedParent}}">
                                                    <dxb:BarEditItem.EditSettings>
                                                        <dxe:ComboBoxEditSettings ItemsSource="{StaticResource DiagramFontSizes}" MaskType="Numeric" />
                                                    </dxb:BarEditItem.EditSettings>
                                                </dxb:BarEditItem>
                                                <dxb:BarButtonItem dxdiag:DiagramDesignerControl.BarItemName="{x:Static dxdiag:DefaultBarItemNames.FontSizeIncrease}" Style="{DynamicResource {dxdiagt:DiagramDesignerControlThemeKeys ResourceKey=FontSizeIncreaseButtonStyle}}" />
                                                <dxb:BarButtonItem dxdiag:DiagramDesignerControl.BarItemName="{x:Static dxdiag:DefaultBarItemNames.FontSizeDecrease}" Style="{DynamicResource {dxdiagt:DiagramDesignerControlThemeKeys ResourceKey=FontSizeDecreaseButtonStyle}}" />
                                            </dxr:BarButtonGroup>
                                            <dxr:BarButtonGroup dxdiag:DiagramDesignerControl.BarItemName="{x:Static dxdiag:DefaultBarItemNames.FontTypeAndColorButtonGroup}">
                                                <dxb:BarCheckItem dxdiag:DiagramDesignerControl.BarItemName="{x:Static dxdiag:DefaultBarItemNames.FontBold}" Style="{DynamicResource {dxdiagt:DiagramDesignerControlThemeKeys ResourceKey=FontBoldButtonStyle}}" />
                                                <dxb:BarCheckItem dxdiag:DiagramDesignerControl.BarItemName="{x:Static dxdiag:DefaultBarItemNames.FontItalic}" Style="{DynamicResource {dxdiagt:DiagramDesignerControlThemeKeys ResourceKey=FontItalicButtonStyle}}" />
                                                <dxb:BarCheckItem dxdiag:DiagramDesignerControl.BarItemName="{x:Static dxdiag:DefaultBarItemNames.FontUnderline}" Style="{DynamicResource {dxdiagt:DiagramDesignerControlThemeKeys ResourceKey=FontUnderlineButtonStyle}}" />
                                                <dxb:BarCheckItem dxdiag:DiagramDesignerControl.BarItemName="{x:Static dxdiag:DefaultBarItemNames.FontStrikethrough}" Style="{DynamicResource {dxdiagt:DiagramDesignerControlThemeKeys ResourceKey=FontStrikethroughButtonStyle}}" />
                                                <dxb:BarItemSeparator dxdiag:DiagramDesignerControl.BarItemName="{x:Static dxdiag:DefaultBarItemNames.FontRibbonGroupSeparator}" />
                                                <dxb:BarSplitButtonItem dxdiag:DiagramDesignerControl.BarItemName="{x:Static dxdiag:DefaultBarItemNames.ForegroundColor}" Style="{DynamicResource {dxdiagt:DiagramDesignerControlThemeKeys ResourceKey=ShapeForegroundColorSplitButtonStyle}}">
                                                    <dxmvvm:Interaction.Behaviors>
                                                        <dxdiagn:BarColorEditBehavior EditablePropertyName="{x:Static dxdiagcore:DiagramItemController.ForegroundPropertyName}" Diagram="{Binding RelativeSource={RelativeSource TemplatedParent}}" ColorEditUnit="{Binding ElementName=foregroundColorEditBehavior, Path=EditorColor}" />
                                                    </dxmvvm:Interaction.Behaviors>
                                                    <dxb:BarSplitButtonItem.PopupControl>
                                                        <dxb:PopupControlContainer>
                                                            <dxdiagn:ExtendedColorEdit AllowNullInput="True" ShowDefaultColorButton="False" ShowBorder="False" Palettes="{Binding Path=Theme, RelativeSource={RelativeSource TemplatedParent}, Converter={dxdiag:DiagramThemeToColorEditPaletteConverter}}">
                                                                <dxmvvm:Interaction.Behaviors>
                                                                    <dxdiagn:DiagramColorEditBehavior x:Name="foregroundColorEditBehavior" EditablePropertyName="{x:Static dxdiagcore:DiagramItemController.ForegroundPropertyName}" Diagram="{Binding RelativeSource={RelativeSource TemplatedParent}}" DiagramColor="{Binding Path=SelectionToolsModel[ForegroundColor], RelativeSource={RelativeSource TemplatedParent}}" />
                                                                </dxmvvm:Interaction.Behaviors>
                                                            </dxdiagn:ExtendedColorEdit>
                                                        </dxb:PopupControlContainer>
                                                    </dxb:BarSplitButtonItem.PopupControl>
                                                </dxb:BarSplitButtonItem>
                                            </dxr:BarButtonGroup>
                                        </dxr:RibbonPageGroup>
                                        <dxr:RibbonPageGroup Caption="{dxdiag:DiagramControlLocalizedString Tools_Text}" dxdiag:DiagramDesignerControl.BarItemName="{x:Static dxdiag:DefaultBarItemNames.TextToolsGroup}">
                                            <dxr:BarButtonGroup dxdiag:DiagramDesignerControl.BarItemName="{x:Static dxdiag:DefaultBarItemNames.VerticalTextAlignmentButtonGroup}">
                                                <dxb:BarItemSelector dxdiag:DiagramDesignerControl.BarItemName="{x:Static dxdiag:DefaultBarItemNames.VerticalTextAlignmentSelector}" ItemLinksSource="{x:Static dxdiag:DiagramDesignerControl.TextVerticalAlignment}" SelectedItem="{Binding SelectionToolsModel[VerticalContentAlignment], RelativeSource={RelativeSource TemplatedParent}}">
                                                    <dxb:BarItemSelector.ItemTemplate>
                                                        <DataTemplate>
                                                            <ContentControl>
                                                                <dxb:BarCheckItem Style="{DynamicResource {dxdiagt:DiagramDesignerControlThemeKeys ResourceKey=TextVerticalAlignmentButtonsStyle}}" />
                                                            </ContentControl>
                                                        </DataTemplate>
                                                    </dxb:BarItemSelector.ItemTemplate>
                                                </dxb:BarItemSelector>
                                            </dxr:BarButtonGroup>
                                            <dxr:BarButtonGroup dxdiag:DiagramDesignerControl.BarItemName="{x:Static dxdiag:DefaultBarItemNames.HorizontalTextAlignmentButtonGroup}">
                                                <dxb:BarItemSelector dxdiag:DiagramDesignerControl.BarItemName="{x:Static dxdiag:DefaultBarItemNames.HorizontalTextAlignmentSelector}" ItemLinksSource="{x:Static dxdiag:DiagramDesignerControl.TextHorizontalAlignment}" SelectedItem="{Binding SelectionToolsModel[TextAlignment], RelativeSource={RelativeSource TemplatedParent}}">
                                                    <dxb:BarItemSelector.ItemTemplate>
                                                        <DataTemplate>
                                                            <ContentControl>
                                                                <dxb:BarCheckItem Style="{DynamicResource {dxdiagt:DiagramDesignerControlThemeKeys ResourceKey=TextHorizontalAlignmentButtonsStyle}}" />
                                                            </ContentControl>
                                                        </DataTemplate>
                                                    </dxb:BarItemSelector.ItemTemplate>
                                                </dxb:BarItemSelector>
                                            </dxr:BarButtonGroup>
                                        </dxr:RibbonPageGroup>
                                        <dxr:RibbonPageGroup Caption="{dxdiag:DiagramControlLocalizedString RibbonPageGroup_Tools}" dxdiag:DiagramDesignerControl.BarItemName="{x:Static dxdiag:DefaultBarItemNames.ToolsGroup}">
                                            <dxdiag:BarMouseCheckOnlyItem dxdiag:DiagramDesignerControl.BarItemName="{x:Static dxdiag:DefaultBarItemNames.PointerTool}" Style="{DynamicResource {dxdiagt:DiagramDesignerControlThemeKeys ResourceKey=PointerToolStyle}}" />
                                            <dxdiag:BarMouseCheckOnlyItem dxdiag:DiagramDesignerControl.BarItemName="{x:Static dxdiag:DefaultBarItemNames.ConnectorTool}" Style="{DynamicResource {dxdiagt:DiagramDesignerControlThemeKeys ResourceKey=ConnectorToolStyle}}" />
                                            <dxdiag:BarMouseCheckOnlySplitItem dxdiag:DiagramDesignerControl.BarItemName="{x:Static dxdiag:DefaultBarItemNames.ToolsContainer}">
                                                <dxmvvm:Interaction.Behaviors>
                                                    <dxdiag:BarToolsSelectorBehavior SelectedTool="{Binding ActiveTool, RelativeSource={RelativeSource TemplatedParent}}" />
                                                </dxmvvm:Interaction.Behaviors>
                                                <dxdiag:BarMouseCheckOnlySplitItem.PopupControl>
                                                    <dxb:PopupMenu>
                                                        <dxb:PopupMenu.Items>
                                                            <dxdiag:BarMouseCheckOnlyItem dxdiag:DiagramDesignerControl.BarItemName="{x:Static dxdiag:DefaultBarItemNames.RectangleTool}" Style="{DynamicResource {dxdiagt:DiagramDesignerControlThemeKeys ResourceKey=RectangleToolStyle}}" />
                                                            <dxdiag:BarMouseCheckOnlyItem dxdiag:DiagramDesignerControl.BarItemName="{x:Static dxdiag:DefaultBarItemNames.EllipseTool}" Style="{DynamicResource {dxdiagt:DiagramDesignerControlThemeKeys ResourceKey=EllipseToolStyle}}" />
                                                            <dxdiag:BarMouseCheckOnlyItem dxdiag:DiagramDesignerControl.BarItemName="{x:Static dxdiag:DefaultBarItemNames.RightTriangleTool}" Style="{DynamicResource {dxdiagt:DiagramDesignerControlThemeKeys ResourceKey=RightTriangleToolStyle}}" />
                                                            <dxdiag:BarMouseCheckOnlyItem dxdiag:DiagramDesignerControl.BarItemName="{x:Static dxdiag:DefaultBarItemNames.HexagonTool}" Style="{DynamicResource {dxdiagt:DiagramDesignerControlThemeKeys ResourceKey=HexagonToolStyle}}" />
                                                        </dxb:PopupMenu.Items>
                                                    </dxb:PopupMenu>
                                                </dxdiag:BarMouseCheckOnlySplitItem.PopupControl>
                                            </dxdiag:BarMouseCheckOnlySplitItem>
                                        </dxr:RibbonPageGroup>
                                        <dxr:RibbonPageGroup Caption="{dxdiag:DiagramControlLocalizedString RibbonPageGroup_ShapeStyles}" dxdiag:DiagramDesignerControl.BarItemName="{x:Static dxdiag:DefaultBarItemNames.ShapeStylesGroup}">
                                            <dxr:RibbonGalleryBarItem Content="{dxdiag:DiagramControlLocalizedString RibbonPageGroup_ShapeStyles}" dxdiag:DiagramDesignerControl.BarItemName="{x:Static dxdiag:DefaultBarItemNames.ShapeStyles}" IsEnabled="{Binding SelectionModel, RelativeSource={RelativeSource TemplatedParent}, Converter={dxmvvm:ObjectToBooleanConverter}}">
                                                <dxr:RibbonGalleryBarItem.Resources>
                                                    <Style TargetType="{x:Type dxb:Gallery}" x:Key="commonGalleryStyle">
                                                        <Setter Property="ItemGlyphBorderTemplate">
                                                            <Setter.Value>
                                                                <ControlTemplate>
                                                                    <!--TODO: remove Tag-->
                                                                    <dxdiag:ShapeStylePresenter Theme="{Binding Path=Gallery.Tag.Theme, RelativeSource={RelativeSource FindAncestor, AncestorType={x:Type dxb:GalleryControl}}}" StyleId="{Binding Caption}" ToolTip="{Binding Caption.Name}" />
                                                                </ControlTemplate>
                                                            </Setter.Value>
                                                        </Setter>
                                                        <Setter Property="ItemGlyphMargin" Value="6" />
                                                        <Setter Property="ItemGlyphVerticalAlignment" Value="Center" />
                                                        <Setter Property="ItemGlyphHorizontalAlignment" Value="Center" />
                                                        <Setter Property="ItemAutoHeight" Value="True" />
                                                        <Setter Property="Tag" Value="{Binding RelativeSource={RelativeSource TemplatedParent}}" />
                                                        <Setter Property="ItemCaptionHorizontalAlignment" Value="Center" />
                                                        <Setter Property="AllowToolTips" Value="False" />
                                                        <Setter Property="IsItemDescriptionVisible" Value="False" />
                                                        <Setter Property="IsItemCaptionVisible" Value="False" />
                                                        <Setter Property="AllowFilter" Value="False" />
                                                        <Setter Property="MinColCount" Value="7" />
                                                        <Setter Property="ColCount" Value="7" />
                                                        <Setter Property="AutoHideGallery" Value="False" />
                                                        <Setter Property="ItemCheckMode" Value="Single" />
                                                        <Setter Property="FirstCheckedItem" Value="{Binding SelectionToolsModel[StyleInfo], RelativeSource={RelativeSource TemplatedParent}, Mode=TwoWay, Converter={dxdiag:StyleInfoToStyleIdConverter}}" />
                                                    </Style>
                                                    <CollectionViewSource x:Key="StylesCollection" Source="{Binding LastShapeStyles, RelativeSource={RelativeSource TemplatedParent}}">
                                                        <CollectionViewSource.GroupDescriptions>
                                                            <PropertyGroupDescription PropertyName="Id" Converter="{dxdiag:StyleIdToGroupNameConverter}" />
                                                        </CollectionViewSource.GroupDescriptions>
                                                    </CollectionViewSource>
                                                </dxr:RibbonGalleryBarItem.Resources>
                                                <dxr:RibbonGalleryBarItem.Gallery>
                                                    <dxb:Gallery Style="{StaticResource commonGalleryStyle}">
                                                        <dxb:GalleryItemGroup ItemsSource="{Binding LastShapeStyles, RelativeSource={RelativeSource TemplatedParent}}" ItemTemplate="{StaticResource galleryControlItemTemplate}" />
                                                    </dxb:Gallery>
                                                </dxr:RibbonGalleryBarItem.Gallery>
                                                <dxr:RibbonGalleryBarItem.DropDownGallery>
                                                    <dxb:Gallery Style="{StaticResource commonGalleryStyle}" GroupsSource="{Binding Source={StaticResource StylesCollection}, Path=Groups}">
                                                        <dxb:Gallery.GroupTemplate>
                                                            <DataTemplate>
                                                                <ContentControl>
                                                                    <dxb:GalleryItemGroup ItemsSource="{Binding Items}" Caption="{Binding Name}" ItemTemplate="{StaticResource galleryControlItemTemplate}" />
                                                                </ContentControl>
                                                            </DataTemplate>
                                                        </dxb:Gallery.GroupTemplate>
                                                    </dxb:Gallery>
                                                </dxr:RibbonGalleryBarItem.DropDownGallery>
                                            </dxr:RibbonGalleryBarItem>
                                            <dxb:BarSplitButtonItem dxdiag:DiagramDesignerControl.BarItemName="{x:Static dxdiag:DefaultBarItemNames.BackgroundColor}" Style="{DynamicResource {dxdiagt:DiagramDesignerControlThemeKeys ResourceKey=ShapeBackgroundColorSplitButtonStyle}}">
                                                <dxmvvm:Interaction.Behaviors>
                                                    <dxdiagn:BarColorEditBehavior EditablePropertyName="{x:Static dxdiagcore:DiagramItemController.BackgroundPropertyName}" Diagram="{Binding RelativeSource={RelativeSource TemplatedParent}}" ColorEditUnit="{Binding ElementName=backgroundColorEditBehavior, Path=EditorColor}" />
                                                </dxmvvm:Interaction.Behaviors>
                                                <dxb:BarSplitButtonItem.PopupControl>
                                                    <dxb:PopupControlContainer>
                                                        <dxdiagn:ExtendedColorEdit AllowNullInput="True" ShowDefaultColorButton="False" ShowBorder="False" Palettes="{Binding Path=Theme, RelativeSource={RelativeSource TemplatedParent}, Converter={dxdiag:DiagramThemeToColorEditPaletteConverter}}">
                                                            <dxmvvm:Interaction.Behaviors>
                                                                <dxdiagn:DiagramColorEditBehavior x:Name="backgroundColorEditBehavior" EditablePropertyName="{x:Static dxdiagcore:DiagramItemController.BackgroundPropertyName}" Diagram="{Binding RelativeSource={RelativeSource TemplatedParent}}" DiagramColor="{Binding Path=SelectionToolsModel[BackgroundColor], RelativeSource={RelativeSource TemplatedParent}}" />
                                                            </dxmvvm:Interaction.Behaviors>
                                                        </dxdiagn:ExtendedColorEdit>
                                                    </dxb:PopupControlContainer>
                                                </dxb:BarSplitButtonItem.PopupControl>
                                            </dxb:BarSplitButtonItem>
                                            <dxb:BarSplitButtonItem dxdiag:DiagramDesignerControl.BarItemName="{x:Static dxdiag:DefaultBarItemNames.StrokeColor}" Style="{DynamicResource {dxdiagt:DiagramDesignerControlThemeKeys ResourceKey=ShapeStrokeColorSplitButtonStyle}}">
                                                <dxmvvm:Interaction.Behaviors>
                                                    <dxdiagn:BarColorEditBehavior EditablePropertyName="{x:Static dxdiagcore:DiagramItemController.StrokePropertyName}" Diagram="{Binding RelativeSource={RelativeSource TemplatedParent}}" ColorEditUnit="{Binding ElementName=strokeColorEditBehavior, Path=EditorColor}" />
                                                </dxmvvm:Interaction.Behaviors>
                                                <dxb:BarSplitButtonItem.PopupControl>
                                                    <dxb:PopupControlContainer>
                                                        <dxdiagn:ExtendedColorEdit x:Name="strokeColorEdit" AllowNullInput="True" ShowDefaultColorButton="False" ShowBorder="False" Palettes="{Binding Path=Theme, RelativeSource={RelativeSource TemplatedParent}, Converter={dxdiag:DiagramThemeToColorEditPaletteConverter}}">
                                                            <dxmvvm:Interaction.Behaviors>
                                                                <dxdiagn:DiagramColorEditBehavior x:Name="strokeColorEditBehavior" EditablePropertyName="{x:Static dxdiagcore:DiagramItemController.StrokePropertyName}" Diagram="{Binding RelativeSource={RelativeSource TemplatedParent}}" DiagramColor="{Binding Path=SelectionToolsModel[StrokeColor], RelativeSource={RelativeSource TemplatedParent}}" />
                                                            </dxmvvm:Interaction.Behaviors>
                                                        </dxdiagn:ExtendedColorEdit>
                                                    </dxb:PopupControlContainer>
                                                </dxb:BarSplitButtonItem.PopupControl>
                                            </dxb:BarSplitButtonItem>
                                        </dxr:RibbonPageGroup>
                                        <dxr:RibbonPageGroup Caption="{dxdiag:DiagramControlLocalizedString RibbonPageGroup_Arrange}" dxdiag:DiagramDesignerControl.BarItemName="{x:Static dxdiag:DefaultBarItemNames.ArrangeGroup}">
                                            <dxb:BarSplitButtonItem dxdiag:DiagramDesignerControl.BarItemName="{x:Static dxdiag:DefaultBarItemNames.BringToFrontContainer}" Style="{DynamicResource {dxdiagt:DiagramDesignerControlThemeKeys ResourceKey=BringToFrontSplitButtonStyle}}">
                                                <dxb:BarSplitButtonItem.PopupControl>
                                                    <dxb:PopupMenu>
                                                        <dxb:BarButtonItem dxdiag:DiagramDesignerControl.BarItemName="{x:Static dxdiag:DefaultBarItemNames.BringForward}" Style="{DynamicResource {dxdiagt:DiagramDesignerControlThemeKeys ResourceKey=BringForwardButtonStyle}}" />
                                                        <dxb:BarButtonItem dxdiag:DiagramDesignerControl.BarItemName="{x:Static dxdiag:DefaultBarItemNames.BringToFront}" Style="{DynamicResource {dxdiagt:DiagramDesignerControlThemeKeys ResourceKey=BringToFrontButtonStyle}}" />
                                                    </dxb:PopupMenu>
