<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:dxt="http://schemas.devexpress.com/winfx/2008/xaml/map/themekeys" xmlns:System="clr-namespace:System;assembly=mscorlib" xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" xmlns:cs="colorscheme_ignorable_uri" mc:Ignorable="cs" xmlns:dxcore_unique_xmlns="http://schemas.devexpress.com/winfx/2008/xaml/core">
    <!-- Setup -->
    <CornerRadius x:Key="{dxt:MapThemeKey ResourceKey=OverlayRadius}">3</CornerRadius>
    <System:Double x:Key="{dxt:MapThemeKey ResourceKey=HighlightShapeStrokeThickness}">1.0</System:Double>
    <System:Double x:Key="{dxt:MapThemeKey ResourceKey=SelectedShapeStrokeThickness}">2.0</System:Double>
    <!-- Brushes -->
    <SolidColorBrush x:Key="{dxt:MapBrushesThemeKey ResourceKey=MapBackground}" Color="#FFFFFFFF" />
    <!-- Shape -->
    <SolidColorBrush x:Key="{dxt:MapBrushesThemeKey ResourceKey=ShapeFill}" Color="#FF666666" />
    <SolidColorBrush x:Key="{dxt:MapBrushesThemeKey ResourceKey=ShapeStroke}" Color="#FF3C3C3C" />
    <SolidColorBrush x:Key="{dxt:MapBrushesThemeKey ResourceKey=HighlightShapeStroke}" Color="#FF2AB7FF" />
    <SolidColorBrush x:Key="{dxt:MapBrushesThemeKey ResourceKey=SelectedShapeStroke}" Color="#FF2AB7FF" />
    <!-- Overlay -->
    <SolidColorBrush x:Key="{dxt:MapBrushesThemeKey ResourceKey=OverlayBackground}" Color="#FF333333" Opacity="0.84" />
    <SolidColorBrush x:Key="{dxt:MapBrushesThemeKey ResourceKey=OverlayElementForeground}" Color="#FFF0F0F0" />
    <SolidColorBrush x:Key="{dxt:MapBrushesThemeKey ResourceKey=OverlayElementAccentColor}" Color="#FF2AB7FF" />
    <Color x:Key="{dxt:MapBrushesThemeKey ResourceKey=OverlayElementHotColor}">#FF2AB7FF</Color>
    <Color x:Key="{dxt:MapBrushesThemeKey ResourceKey=OverlayElementPressedColor}">#FF2AB7FF</Color>
    <!-- ToolTip -->
    <SolidColorBrush x:Key="{dxt:MapBrushesThemeKey ResourceKey=ToolTipBackground}" Color="#FF333333" />
    <SolidColorBrush x:Key="{dxt:MapBrushesThemeKey ResourceKey=ToolTipForeground}" Color="#FFF0F0F0" />
    <!-- SearchPanel -->
    <SolidColorBrush x:Key="{dxt:MapBrushesThemeKey ResourceKey=SearchPanelBackground}" Color="#FF333333" />
    <SolidColorBrush x:Key="{dxt:MapBrushesThemeKey ResourceKey=SearchPanelForeground}" Color="#FFF0F0F0" />
    <SolidColorBrush x:Key="{dxt:MapBrushesThemeKey ResourceKey=SearchPanelNullTextForeground}" Color="#FFF0F0F0" Opacity="0.62" />
    <SolidColorBrush x:Key="{dxt:MapBrushesThemeKey ResourceKey=SearchPanelClearButtonFill}" Color="#FF2AB7FF" />
    <SolidColorBrush x:Key="{dxt:MapBrushesThemeKey ResourceKey=SearchPanelLoadingBrush}" Color="#FF2AB7FF" />

    <!--Pushpin-->
    <!-- [apawar][20171124] change color  -->
    <SolidColorBrush x:Key="{dxt:MapBrushesThemeKey ResourceKey=PushpinBackground}" Color="#FF8AFBFF" /> <!-- previous color = #FF999999  -->
    <SolidColorBrush x:Key="{dxt:MapBrushesThemeKey ResourceKey=PushpinForeground}" Color="#FF8AFBFF" /> <!-- previous color = #FF373A3D  -->
    <SolidColorBrush x:Key="{dxt:MapBrushesThemeKey ResourceKey=PushpinSelectionBrush}" Color="#FFFF0000" /> <!-- previous color = #FF083493  -->
    <SolidColorBrush x:Key="{dxt:MapBrushesThemeKey ResourceKey=PushpinSelectionForeground}" Color="#FFFF0000" /> <!-- previous color = #FFFFFFFF  -->
</ResourceDictionary>