                                                <RowDefinition Height="Auto" />
                                                <RowDefinition Height="*" />
                                            </Grid.RowDefinitions>
                                            <dxschd:SharedSizePanel
                                                IsBaseSizePanel="True"
                                                Orientation="Vertical"
                                                SharedSizeGroup="MonthResourceHeader">
                                                <dxschd:SchedulerItemsControl
                                                    HorizontalContentAlignment="Stretch"
                                                    VerticalContentAlignment="Stretch"
                                                    ItemTemplate="{dxci:ThemeResource {dxscht:MonthViewThemeKey ResourceKey=GroupByResourceHeaderTemplate}}"
                                                    ItemsSource="{TemplateBinding ResourceContainers}"
                                                    SnapsToDevicePixels="True">
                                                    <dxschd:SchedulerItemsControl.ItemsPanel>
                                                        <ItemsPanelTemplate>
                                                            <dxschd:PixelSnappedUniformGrid
                                                                dxschd:SchedulerItemsControl.ElementPosition="{dxci:ThemeResource {dxscht:MonthViewThemeKey ResourceKey=ResourceHeadersGroupByResourceElementPosition}}"
                                                                Orientation="Horizontal"
                                                                SnapsToDevicePixels="True" />
                                                        </ItemsPanelTemplate>
                                                    </dxschd:SchedulerItemsControl.ItemsPanel>
                                                </dxschd:SchedulerItemsControl>
                                            </dxschd:SharedSizePanel>
                                            <dxschd:SharedSizePanel
                                                Grid.Row="1"
                                                IsBaseSizePanel="True"
                                                Orientation="Vertical"
                                                SharedSizeGroup="MonthDayHeader">
                                                <dxschd:SchedulerItemsControl
                                                    HorizontalContentAlignment="Stretch"
                                                    VerticalContentAlignment="Stretch"
                                                    ItemTemplate="{dxci:ThemeResource {dxscht:MonthViewThemeKey ResourceKey=GroupByResourceDayOfWeekHeadersTemplate}}"
                                                    ItemsSource="{TemplateBinding ResourceContainers}"
                                                    SnapsToDevicePixels="True">
                                                    <dxschd:SchedulerItemsControl.ItemsPanel>
                                                        <ItemsPanelTemplate>
                                                            <dxschd:PixelSnappedUniformGrid
                                                                dxschd:SchedulerItemsControl.ElementPosition="{dxci:ThemeResource {dxscht:MonthViewThemeKey ResourceKey=DayOfWeekHeadersGroupByResourceElementPosition}}"
                                                                Orientation="Horizontal"
                                                                SnapsToDevicePixels="True" />
                                                        </ItemsPanelTemplate>
                                                    </dxschd:SchedulerItemsControl.ItemsPanel>
                                                </dxschd:SchedulerItemsControl>
                                            </dxschd:SharedSizePanel>
                                        </Grid>
                                        <dxschd:SharedSizePanel
                                            Grid.Column="1"
                                            IsBaseSizePanel="False"
                                            Orientation="Horizontal"
                                            SharedSizeGroup="MonthView_VerticalScrollBarGroup">
                                            <Grid SnapsToDevicePixels="True">
                                                <Grid.RowDefinitions>
                                                    <RowDefinition Height="Auto" />
                                                    <RowDefinition Height="*" />
                                                </Grid.RowDefinitions>
                                                <dxschd:SharedSizePanel
                                                    dxschd:SchedulerItemsControl.ElementPosition="{dxci:ThemeResource {dxscht:MonthViewThemeKey ResourceKey=TopRightHeaderCornerGroupByResourceElementPosition}}"
                                                    IsBaseSizePanel="False"
                                                    Orientation="Vertical"
                                                    SharedSizeGroup="MonthResourceHeader">
                                                    <dxschd:HeaderControl
                                                        Orientation="Horizontal"
                                                        ResourceColor="{Binding ResourceContainers.Last.ResourceHeader.Brushes.ResourceHeader, RelativeSource={RelativeSource TemplatedParent}}"
                                                        SnapsToDevicePixels="True"
                                                        Visibility="{dxci:ThemeResource {dxscht:SchedulerViewThemeKey ResourceKey=HeaderCornerVisibility}}" />
                                                </dxschd:SharedSizePanel>
                                                <dxschd:SharedSizePanel
                                                    Grid.Row="1"
                                                    dxschd:SchedulerItemsControl.ElementPosition="{dxci:ThemeResource {dxscht:MonthViewThemeKey ResourceKey=MiddleRightHeaderCornerGroupByResourceElementPosition}}"
                                                    IsBaseSizePanel="False"
                                                    Orientation="Vertical"
                                                    SharedSizeGroup="MonthDayHeader">
                                                    <dxschd:HeaderControl
                                                        Orientation="Horizontal"
                                                        ResourceColor="{Binding ResourceContainers.Last.ResourceHeader.Brushes.ResourceHeader, RelativeSource={RelativeSource TemplatedParent}}"
                                                        SnapsToDevicePixels="True"
                                                        Visibility="{dxci:ThemeResource {dxscht:SchedulerViewThemeKey ResourceKey=HeaderCornerVisibility}}" />
                                                </dxschd:SharedSizePanel>
                                            </Grid>
                                        </dxschd:SharedSizePanel>
                                    </Grid>
                                </dx:XPFContentControl>
                                <dx:XPFContentControl
                                    Grid.Row="1"
                                    Grid.Column="1"
                                    Style="{dxci:ThemeResource {dxscht:MonthViewThemeKey ResourceKey=MonthViewContainerStyle}}">
                                    <Grid SnapsToDevicePixels="True">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="*" />
                                            <ColumnDefinition Width="Auto" />
                                        </Grid.ColumnDefinitions>
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="*" />
                                            <RowDefinition Height="Auto" />
                                        </Grid.RowDefinitions>
                                        <ItemsControl
                                            x:Name="ResourcesItemsControl"
                                            HorizontalContentAlignment="Stretch"
                                            VerticalContentAlignment="Stretch"
                                            dxsch:TouchSurfaceSynchronizer.Surface="{Binding RelativeSource={RelativeSource AncestorType=dxsch:SchedulerControl}}"
                                            ClipToBounds="True"
                                            ItemTemplate="{dxci:ThemeResource {dxscht:MonthViewThemeKey ResourceKey=GroupByResourceMonthHorizontalTemplate}}"
                                            ItemsSource="{TemplateBinding ResourceContainers}"
                                            SnapsToDevicePixels="True">
                                            <ItemsControl.ItemsPanel>
                                                <ItemsPanelTemplate>
                                                    <dxschd:PixelSnappedUniformGrid dxschd:SchedulerItemsControl.ElementPosition="Start,End,Start,End,None,None" Orientation="Horizontal" />
                                                </ItemsPanelTemplate>
                                            </ItemsControl.ItemsPanel>
                                        </ItemsControl>
                                        <dxschd:NavigationButtonPairControl
                                            HorizontalContentAlignment="Stretch"
                                            VerticalContentAlignment="Stretch"
                                            ItemTemplate="{dxci:ThemeResource {dxscht:SchedulerViewThemeKey ResourceKey=NavigationButtonsPair}}"
                                            ItemsSource="{TemplateBinding ResourceContainers}"
                                            ShowBorder="{Binding Control.ShowBorder, RelativeSource={RelativeSource TemplatedParent}}"
                                            Style="{dxci:ThemeResource {dxscht:MonthViewThemeKey ResourceKey=NavigationButtonPairGroupByResourceStyle}}">
                                            <dxschd:NavigationButtonPairControl.ItemsPanel>
                                                <ItemsPanelTemplate>
                                                    <dxschd:PixelSnappedUniformGrid
                                                        dxschd:SchedulerItemsControl.ElementPosition="{dxci:ThemeResource {dxscht:WeekViewThemeKey ResourceKey=GroupByResourceElementPosition}}"
                                                        ApplyInnerContentPadding="True"
                                                        Orientation="Horizontal" />
                                                </ItemsPanelTemplate>
                                            </dxschd:NavigationButtonPairControl.ItemsPanel>
                                        </dxschd:NavigationButtonPairControl>
                                        <dxschd:SharedSizePanel
                                            Grid.Column="1"
                                            IsBaseSizePanel="True"
                                            Orientation="Horizontal"
                                            SharedSizeGroup="MonthView_VerticalScrollBarGroup">
                                            <ScrollBar
                                                x:Name="PART_ScrollBar"
                                                Grid.Column="1"
                                                Margin="{dxci:ThemeResource {dxscht:MonthViewThemeKey ResourceKey=WeekScrollBarMargin}}"
                                                dxschd:SchedulerScrollBarManager.BindToSchedulerControl="True"
                                                Orientation="Vertical"
                                                SnapsToDevicePixels="True" />
                                        </dxschd:SharedSizePanel>
                                        <dxschd:ResourceNavigatorControl
                                            x:Name="Navigator"
                                            Grid.Row="1"
                                            Margin="{dxci:ThemeResource {dxscht:MonthViewThemeKey ResourceKey=ResourceNavigatorMargin}}"
                                            dxsch:SchedulerControl.HitTestType="None"
                                            dxschd:HitTestManager.IsHitTestContainer="True"
                                            IsHitTestVisible="True"
                                            SchedulerControl="{TemplateBinding Control}"
                                            Style="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=Control.ResourceNavigatorHorizontalStyle}"
                                            Visibility="{Binding Control.ResourceNavigator.Visibility, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource ResourceNavigatorVisibilityToConverter}}" />
                                        <dx:CornerBox
                                            x:Name="Corner"
                                            Grid.Row="1"
                                            Grid.Column="1"
                                            SnapsToDevicePixels="True"
                                            UseLayoutRounding="True" />
                                    </Grid>
                                </dx:XPFContentControl>

                                <dx:DXContentControl Grid.Row="1">
                                    <Grid SnapsToDevicePixels="True">
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="*" />
                                            <RowDefinition Height="Auto" />
                                        </Grid.RowDefinitions>
                                        <dxschd:SchedulerItemsControl
                                            Width="30"
                                            HorizontalContentAlignment="Left"
                                            VerticalContentAlignment="Stretch"
                                            dxschd:SchedulerItemsControl.ElementPosition="{Binding Path=(dxschd:SchedulerItemsControl.ElementPosition), RelativeSource={RelativeSource Self}}"
                                            DataContext="{TemplateBinding ResourceContainers}"
                                            ItemsSource="{Binding Path=[0].SimpleIntervals}"
                                            SnapsToDevicePixels="True">
                                            <dxschd:SchedulerItemsControl.ItemTemplate>
                                                <DataTemplate>
                                                    <Grid>
                                                        <dxschd:MultiColorBorder
                                                            Background="{Binding RelativeSource={RelativeSource Self}, Path=Tag[0].Cell, Converter={UIConverter:ColorToBrush}}"
                                                            DefaultBorderThickness="1"
                                                            ElementPosition="{Binding Path=(dxschd:SchedulerItemsControl.ElementPosition), RelativeSource={RelativeSource Self}}"
                                                            OuterBorderBrush="{Binding RelativeSource={RelativeSource Self}, Path=Tag[0].CellLightBorder, Converter={UIConverter:ColorToBrush}}"
                                                            Tag="{dxci:ThemeResource {dxscht:SchedulerControlThemeKey ResourceKey=ResourceColorSchemaTemplate}}" />
                                                        <TextBlock
                                                            HorizontalAlignment="Center"
                                                            VerticalAlignment="Center"
                                                            Text="{Binding Path=IntervalStart, Converter={UIConverter:DisplayWeekInDayMonthSchedulerConverter}, StringFormat='{}CW{0:D2}'}">
                                                            <TextBlock.LayoutTransform>
                                                                <RotateTransform Angle="-90" CenterX="0.5" CenterY="0.5" />
                                                            </TextBlock.LayoutTransform>
                                                        </TextBlock>
                                                    </Grid>
                                                </DataTemplate>
                                            </dxschd:SchedulerItemsControl.ItemTemplate>
                                            <dxschd:SchedulerItemsControl.ItemsPanel>
                                                <ItemsPanelTemplate>
                                                    <dxschd:PixelSnappedUniformGrid dxschd:SchedulerItemsControl.ElementPosition="{Binding Path=(dxschd:SchedulerItemsControl.ElementPosition), ElementName=weeks}" Orientation="Vertical" />
                                                </ItemsPanelTemplate>
                                            </dxschd:SchedulerItemsControl.ItemsPanel>
                                        </dxschd:SchedulerItemsControl>
                                        <Border
                                            Grid.Row="1"
                                            Height="{Binding ElementName=Navigator, Path=ActualHeight}"
                                            Margin="{dxci:ThemeResource {dxscht:MonthViewThemeKey ResourceKey=ResourceNavigatorMargin}}" />
                                    </Grid>
                                </dx:DXContentControl>

                            </Grid>
                        </dxschd:SharedGroupSizeContainer>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style>


