        <!--  Show Week in Day View  -->

        <Style x:Key="{dxscht:DayViewThemeKey ResourceKey=DateHeaderStyle, IsThemeIndependent=True}" TargetType="{x:Type dxschd:VisualDateHeader}">
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="{x:Type dxschd:VisualDateHeader}">
                        <Grid
                            HorizontalAlignment="Stretch"
                            VerticalAlignment="Stretch"
                            dxsch:SchedulerControl.HitTestType="DayHeader"
                            Background="Transparent"
                            IsHitTestVisible="True"
                            SnapsToDevicePixels="True">
                            <dxschd:HeaderControl
                                HorizontalAlignment="Stretch"
                                VerticalAlignment="Stretch"
                                dxschd:SchedulerItemsControl.ElementPosition="{Binding (dxschd:SchedulerItemsControl.ElementPosition), RelativeSource={RelativeSource TemplatedParent}}"
                                Content="{TemplateBinding Content}"
                                ContentTemplate="{TemplateBinding ContentTemplate}"
                                DisableResourceColor="{TemplateBinding DisableResourceColor}"
                                IsAlternate="{Binding IsAlternate}"
                                Orientation="Horizontal"
                                ResourceColor="{Binding Brushes.ResourceHeader}"
                                SnapsToDevicePixels="True" />
                        </Grid>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
            <Setter Property="ContentTemplate">
                <Setter.Value>
                    <DataTemplate>
                        <StackPanel>
                            <TextBlock
                                HorizontalAlignment="Center"
                                Foreground="{dxcn:ThemeResource {dxscht:SchedulerViewThemeKey ResourceKey=HeaderForegroundColor}}"
                                Text="{Binding IntervalStart, Converter={UIConverter:DisplayWeekInDayMonthSchedulerConverter}, StringFormat=CW\{0:D2\}}" />
                            <dxschd:DayHeaderControl
                                x:Name="DayViewDayHeaderControl"
                                HorizontalAlignment="Stretch"
                                VerticalAlignment="Stretch"
                                Date="{Binding IntervalStart}"
                                DateFormats="{Binding DateFormats}"
                                SnapsToDevicePixels="True">
                                <TextBlock
                                    Foreground="{dxcn:ThemeResource {dxscht:SchedulerViewThemeKey ResourceKey=HeaderForegroundColor}}"
                                    SnapsToDevicePixels="True"
                                    TextAlignment="Center" />
                            </dxschd:DayHeaderControl>
                        </StackPanel>
                    </DataTemplate>
                </Setter.Value>
            </Setter>
        </Style>
        <!--  End Show Week in Day View  -->

        <!--  Show Week in Week View  -->
        <dxschd:HorizontalPositionToMarginConverter x:Key="horizontalPositionToMarginConverter" />
        <!--  WeekViewVerticalWeekTemplate  -->
        <DataTemplate x:Key="{dxscht:WeekViewThemeKey ResourceKey=WeekViewVerticalWeekTemplate, IsThemeIndependent=True}">
            <dx:XPFContentControl dxschd:SchedulerItemsControl.ElementPosition="{Binding Path=(dxschd:SchedulerItemsControl.ElementPosition), RelativeSource={RelativeSource TemplatedParent}}" Content="{Binding CellContainer}">
                <ContentControl.Template>
                    <ControlTemplate TargetType="{x:Type ContentControl}">
                        <ContentPresenter
                            dxschd:SchedulerItemsControl.ElementPosition="{Binding Path=(dxschd:SchedulerItemsControl.ElementPosition), RelativeSource={RelativeSource TemplatedParent}}"
                            Content="{TemplateBinding Content}"
                            ContentTemplate="{TemplateBinding ContentTemplate}" />
                    </ControlTemplate>
                </ContentControl.Template>
                <ContentControl.ContentTemplate>
                    <DataTemplate>
                        <Grid Grid.Row="1">
                            <Grid.Resources>
                                <dxschd:AppointmentStatusDisplayTypeToVisibileConverter x:Key="StatusDisplayTypeToVisibileConverter" />
                            </Grid.Resources>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="Auto" />
                                <RowDefinition />
                            </Grid.RowDefinitions>
                            <TextBlock HorizontalAlignment="Center" Text="{Binding Path=Cells.First.IntervalStart, Converter={UIConverter:DisplayWeekInDayMonthSchedulerConverter}, StringFormat='{}CW{0:D2}'}" />
                            <dxschd:SchedulerHorizontalTimeCellsControl
                                x:Name="HorizontalWeek"
                                Grid.Row="1"
                                HorizontalContentAlignment="Stretch"
                                VerticalContentAlignment="Stretch"
                                dxschd:SchedulerItemsControl.ElementPosition="{Binding Path=(dxschd:SchedulerItemsControl.ElementPosition), RelativeSource={RelativeSource TemplatedParent}}"
                                ItemContainerStyle="{Binding View.VerticalWeekCellStyle}"
                                ItemsSource="{Binding Cells}">
                                <dxschd:SchedulerHorizontalTimeCellsControl.ItemsPanel>
                                    <ItemsPanelTemplate>
                                        <dxschd:TwoColumnVerticalWeekPanel
                                            Margin="0,0,0,0"
                                            dxschd:SchedulerItemsControl.ElementPosition="{Binding Path=(dxschd:SchedulerItemsControl.ElementPosition), ElementName=HorizontalWeek}"
                                            CompressedIndex="{Binding CompressedIndex}"
                                            IsCompressed="True" />
                                    </ItemsPanelTemplate>
                                </dxschd:SchedulerHorizontalTimeCellsControl.ItemsPanel>
                            </dxschd:SchedulerHorizontalTimeCellsControl>
                            <dxschd:SelectionPresenter
                                Grid.Row="1"
                                CellContainer="{Binding}"
                                Resource="{Binding Resource}"
                                SchedulerTimeCellControl="{Binding ElementName=HorizontalWeek}"
                                SelectedCells="{Binding SelectedCells}"
                                SelectionTemplate="{Binding View.SelectionTemplate}" />
                            <dxschd:DynamicHorizontalAppointmentsPanel
                                x:Name="AppointmentsPanel"
                                Grid.Row="1"
                                dxschd:SchedulerItemsControl.ElementPosition="{Binding Path=(dxschd:SchedulerItemsControl.ElementPosition), ElementName=HorizontalWeek}"
                                AppointmentControls="{Binding AppointmentControlCollection}"
                                AutoHeight="{Binding View.AppointmentDisplayOptions.AppointmentAutoHeight}"
                                CellContainer="{Binding}"
                                CellPadding="1,0,0,0"
                                DefaultAppointmentHeight="{dxcn:ThemeResource {dxscht:SchedulerControlThemeKey ResourceKey=DefaultAppointmentHeight}}"
                                IsStatusVisible="{Binding View.AppointmentDisplayOptions.StatusDisplayType, Converter={StaticResource StatusDisplayTypeToVisibileConverter}}"
                                SchedulerTimeCellControl="{Binding ElementName=HorizontalWeek}">
                                <dxschd:DynamicHorizontalAppointmentsPanel.Margin>
                                    <Binding
                                        Converter="{StaticResource horizontalPositionToMarginConverter}"
                                        ElementName="HorizontalWeek"
                                        Path="(dxschd:SchedulerItemsControl.ElementPosition)">
                                        <Binding.ConverterParameter>
                                            <Thickness>1,0,0,0</Thickness>
                                        </Binding.ConverterParameter>
                                    </Binding>
                                </dxschd:DynamicHorizontalAppointmentsPanel.Margin>
                            </dxschd:DynamicHorizontalAppointmentsPanel>
                            <dxschd:DynamicHorizontalAppointmentsPanel
                                Grid.Row="1"
                                dxschd:SchedulerItemsControl.ElementPosition="{Binding Path=(dxschd:SchedulerItemsControl.ElementPosition), ElementName=HorizontalWeek}"
                                AppointmentControls="{Binding DraggedAppointmentControlCollection}"
                                AppointmentStyle="{Binding View.DragDropHoverTimeCellsStyle}"
                                CellContainer="{Binding}"
                                DefaultAppointmentHeight="{dxcn:ThemeResource {dxscht:SchedulerControlThemeKey ResourceKey=DefaultAppointmentHeight}}"
                                IsDraggedMode="True"
                                SchedulerTimeCellControl="{Binding ElementName=HorizontalWeek}">
                                <dxschd:DynamicHorizontalAppointmentsPanel.Margin>
                                    <Binding
                                        Converter="{StaticResource horizontalPositionToMarginConverter}"
                                        ElementName="HorizontalWeek"
                                        Path="(dxschd:SchedulerItemsControl.ElementPosition)">
                                        <Binding.ConverterParameter>
                                            <Thickness>1,0,0,0</Thickness>
                                        </Binding.ConverterParameter>
                                    </Binding>
                                </dxschd:DynamicHorizontalAppointmentsPanel.Margin>
                            </dxschd:DynamicHorizontalAppointmentsPanel>

                            <dxschd:MoreButtonItemsControl
                                Grid.Row="1"
                                HorizontalContentAlignment="Stretch"
                                VerticalContentAlignment="Stretch"
                                ItemTemplate="{dxcn:ThemeResource {dxscht:SchedulerViewThemeKey ResourceKey=MoreButtonCellTemplate}}"
                                ItemsSource="{Binding ElementName=AppointmentsPanel, Path=MoreButtonInfos}"
                                View="{Binding Path=View}">
                                <dxschd:MoreButtonItemsControl.ItemsPanel>
                                    <ItemsPanelTemplate>
                                        <dxschd:HorizontalWeekMoreButtonPanel2 />
                                    </ItemsPanelTemplate>
                                </dxschd:MoreButtonItemsControl.ItemsPanel>
                            </dxschd:MoreButtonItemsControl>
                        </Grid>

                    </DataTemplate>
                </ContentControl.ContentTemplate>
            </dx:XPFContentControl>
        </DataTemplate>
        <!--  End Show Week in Week View  -->

        <!--  Show week in month view  -->
        <Style x:Key="{dxscht:MonthViewThemeKey ResourceKey=MonthViewGroupByResource, IsThemeIndependent=True}" TargetType="{x:Type dxschd:VisualMonthViewGroupByResource}">
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="{x:Type dxschd:VisualMonthViewGroupByResource}">
                        <dxschd:SharedGroupSizeContainer>
                            <Grid>
                                <Grid.Resources>
                                    <dxschd:ResourceNavigatorVisibilityToConverter x:Key="ResourceNavigatorVisibilityToConverter" />
                                </Grid.Resources>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition />
                                </Grid.ColumnDefinitions>
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="Auto" />
                                    <RowDefinition Height="*" />
                                </Grid.RowDefinitions>
                                <dx:XPFContentControl
                                    Grid.Column="1"
                                    VerticalAlignment="Top"
                                    Style="{dxci:ThemeResource {dxscht:SchedulerViewThemeKey ResourceKey=HorizontalHeadersContainerStyle}}">
                                    <Grid SnapsToDevicePixels="True">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="*" />
                                            <ColumnDefinition Width="Auto" />
                                        </Grid.ColumnDefinitions>
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="Auto" />
                                        </Grid.RowDefinitions>
                                        <Grid SnapsToDevicePixels="True">
