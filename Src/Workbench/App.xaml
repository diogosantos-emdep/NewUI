    <Application.Resources>

        <!--  Custom Template for Show notification  -->
        <dxmvvm:NotificationService x:Key="NotificationService">
            <dxmvvm:NotificationService.CustomNotificationTemplate>
                <DataTemplate>
                    <dx:BackgroundPanel>
                        <Border BorderThickness="1" Style="{DynamicResource BorderBrushStyle}">
                            <!--<StackPanel Orientation="Vertical" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" >-->
                            <Grid>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="80" />
                                    <ColumnDefinition Width="40*" />
                                    <ColumnDefinition Width="40" />
                                </Grid.ColumnDefinitions>
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="30" />
                                    <RowDefinition Height="30" />
                                    <RowDefinition Height="30" />
                                    <RowDefinition Height="10" />
                                </Grid.RowDefinitions>

                                <!--<Image Grid.Column="0" Grid.Row="0" Grid.RowSpan="3" HorizontalAlignment="Center" VerticalAlignment="Stretch" Source="{Binding FromUserImage}" />-->

                                <Ellipse
                                    Grid.Row="0"
                                    Grid.RowSpan="3"
                                    Grid.Column="0"
                                    Width="70"
                                    Height="70"
                                    HorizontalAlignment="Center"
                                    VerticalAlignment="Stretch"
                                    StrokeThickness="1"
                                    Style="{DynamicResource ElipseEditProfile}">
                                    <Ellipse.Fill>
                                        <ImageBrush ImageSource="{Binding FromUserImage, UpdateSourceTrigger=PropertyChanged}" Stretch="UniformToFill" />
                                    </Ellipse.Fill>
                                </Ellipse>
                                <TextBlock
                                    Grid.Row="0"
                                    Grid.Column="1"
                                    Margin="15,0,0,0"
                                    HorizontalAlignment="Left"
                                    VerticalAlignment="Bottom"
                                    FontWeight="Bold"
                                    Style="{DynamicResource TextBlock16Style}"
                                    Text="{Binding Title}" />
                                <TextBlock
                                    Grid.Row="1"
                                    Grid.RowSpan="2"
                                    Grid.Column="1"
                                    Margin="15,0,0,5"
                                    HorizontalAlignment="Left"
                                    VerticalAlignment="Bottom"
                                    Style="{DynamicResource TextBlock14Style}"
                                    Text="{Binding Message}"
                                    TextWrapping="WrapWithOverflow" />
                                <Image
                                    Grid.Row="2"
                                    Grid.Column="2"
                                    Width="25"
                                    Height="25"
                                    HorizontalAlignment="Left"
                                    VerticalAlignment="Top"
                                    Source="/GeosWorkbench;component/Assets/Images/geosicon.png" />
                            </Grid>
                            <!--</StackPanel>-->
                        </Border>
                    </dx:BackgroundPanel>
                </DataTemplate>
            </dxmvvm:NotificationService.CustomNotificationTemplate>
        </dxmvvm:NotificationService>
        <!--  Custom Template for Show notification  -->

        <!--  Style For Font Family setting for all user control  -->
        <Style x:Key="TextBlockStyle" TargetType="{x:Type TextBlock}">
            <Setter Property="FontFamily" Value="{Binding Source={x:Static GeosApplication:GeosApplication.Instance}, Path=FontFamilyAsPerTheme, UpdateSourceTrigger=PropertyChanged}" />
            <!--<Setter Property="FontFamily" Value="{Binding Path= FontFamilyAsPerTheme,Source={x:Static Application.Current},UpdateSourceTrigger=PropertyChanged}"/>-->
            <!--<Setter Property="FontFamily" Value="Tahoma"/>-->
            <!--<Setter Property="FontFamily" Value="Times New Roman"/>-->
            <!--<Setter Property="FontFamily" Value="Comic Sans MS"/>-->
            <!--<Setter Property="FontFamily" Value="Calibri"/>-->
            <!--<Setter Property="FontFamily" Value="Segoe UI"/>-->
            <Setter Property="FontSize" Value="14" />
        </Style>

        <Style x:Key="BaseUniversalStyle" TargetType="{x:Type Control}">
            <Setter Property="FontFamily" Value="{Binding Source={x:Static GeosApplication:GeosApplication.Instance}, Path=FontFamilyAsPerTheme, UpdateSourceTrigger=PropertyChanged}" />
            <!--<Setter Property="FontFamily" Value="{Binding Source={x:Static FontProvider:FontProvider.Instance}, Path=FontFamilyAsPerTheme, UpdateSourceTrigger=PropertyChanged}"/>-->
            <!--<Setter Property="FontFamily" Value="{Binding Path= FontFamilyAsPerTheme,Source={x:Static Application.Current},UpdateSourceTrigger=PropertyChanged}"/>-->
            <!--<Setter Property="FontFamily" Value="Tahoma"/>-->
            <!--<Setter Property="FontFamily" Value="Times New Roman"/>-->
            <!--<Setter Property="FontFamily" Value="Comic Sans MS"/>-->
            <!--<Setter Property="FontFamily" Value="Calibri"/>-->
            <!--<Setter Property="FontFamily" Value="Segoe UI"/>-->
            <Setter Property="FontSize" Value="14" />
            <Setter Property="Cursor" Value="Arrow" />
            <Style.Triggers>
                <DataTrigger Binding="{Binding IsBusy}" Value="True">
                    <Setter Property="Cursor" Value="Wait" />
                </DataTrigger>
            </Style.Triggers>
        </Style>

        <UIHelper:FlyoutContentTemplateSelector x:Key="FlyoutContentTemplateSelector" />

        <Style x:Key="TileBarItemStyleBase" TargetType="{x:Type dxnav:TileBarItem}">
            <Setter Property="FontFamily" Value="{Binding Source={x:Static GeosApplication:GeosApplication.Instance}, Path=FontFamilyAsPerTheme, UpdateSourceTrigger=PropertyChanged}" />
            <!--<Setter Property="FontFamily" Value="{Binding Source={x:Static FontProvider:FontProvider.Instance}, Path=FontFamilyAsPerTheme, UpdateSourceTrigger=PropertyChanged}"/>-->
            <Setter Property="Content" Value="{Binding Caption}" />
            <Setter Property="TileGlyph" Value="{Binding GlyphUri, Converter={UIConverter:StringToImageConverter}}" />
            <Setter Property="Command" Value="{Binding NavigateCommand}" />
            <Setter Property="FontWeight" Value="Bold" />
            <Setter Property="Height" Value="{Binding Height}" />
            <Setter Property="dxmvvm:Interaction.BehaviorsTemplate">
                <Setter.Value>
                    <DataTemplate>
                        <ContentControl>
                            <UIHelper:FlyoutHelperBehavior />
                        </ContentControl>
                    </DataTemplate>
                </Setter.Value>
            </Setter>
        </Style>

        <Style
            x:Key="TileBarItemStyleExtended"
            BasedOn="{StaticResource TileBarItemStyleBase}"
            TargetType="{x:Type dxnav:TileBarItem}">
            <Setter Property="FontFamily" Value="{Binding Source={x:Static GeosApplication:GeosApplication.Instance}, Path=FontFamilyAsPerTheme, UpdateSourceTrigger=PropertyChanged}" />
            <Setter Property="Background" Value="{Binding BackColor}" />
            <Style.Triggers>
                <DataTrigger Binding="{Binding IsHasChildren}" Value="true">
                    <Setter Property="Height" Value="{Binding Height}" />
                    <Setter Property="FlyoutContent" Value="{Binding Children}" />
                    <Setter Property="FlyoutContentTemplateSelector" Value="{StaticResource FlyoutContentTemplateSelector}" />
                </DataTrigger>
            </Style.Triggers>
        </Style>

        <Style TargetType="dxe:FlyoutControl">
            <Setter Property="StaysOpen" Value="True" />
        </Style>

        <DataTemplate x:Key="CollectionTemplate">
            <dxnav:TileBar
                HorizontalContentAlignment="Center"
                ItemContainerStyle="{StaticResource TileBarItemStyleExtended}"
                ItemsSource="{Binding}">
                <dxnav:TileBar.Resources>
                    <Style TargetType="dxcop:ScrollableControlButton">
                        <Setter Property="dxmvvm:Interaction.BehaviorsTemplate">
                            <Setter.Value>
                                <DataTemplate>
                                    <ContentControl>
                                        <UIHelper:FlyoutHelperScrollBehavior />
                                    </ContentControl>
                                </DataTemplate>
                            </Setter.Value>
                        </Setter>
                    </Style>
                </dxnav:TileBar.Resources>
            </dxnav:TileBar>
        </DataTemplate>

