<dxc:DXWindow xmlns:Views="clr-namespace:Workbench.Views" 
                xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
                xmlns:d="http://schemas.microsoft.com/expression/blend/2008"   
                xmlns:dx="http://schemas.devexpress.com/winfx/2008/xaml/core"
                xmlns:dxwui="http://schemas.devexpress.com/winfx/2008/xaml/windowsui"
                xmlns:lc="http://schemas.devexpress.com/winfx/2008/xaml/layoutcontrol"
                xmlns:dxe="http://schemas.devexpress.com/winfx/2008/xaml/editors" 
                xmlns:dxm="http://schemas.devexpress.com/winfx/2008/xaml/map"
                xmlns:dxc="http://schemas.devexpress.com/winfx/2008/xaml/core"
                xmlns:dxb="http://schemas.devexpress.com/winfx/2008/xaml/bars"
                xmlns:dxmvvm="http://schemas.devexpress.com/winfx/2008/xaml/mvvm"
                xmlns:Converts="clr-namespace:Emdep.Geos.UI.Converters;assembly=Emdep.Geos.UI.Converters"
                xmlns:dxgt="http://schemas.devexpress.com/winfx/2008/xaml/grid/themekeys"
                xmlns:dxwuit="http://schemas.devexpress.com/winfx/2008/xaml/windowsui/themekeys"
                xmlns:dxwuii="http://schemas.devexpress.com/winfx/2008/xaml/windowsui/internal"
                xmlns:dxi="clr-namespace:DevExpress.Mvvm.UI.Interactivity;assembly=DevExpress.Xpf.Core.v19.2"
                xmlns:system="clr-namespace:System;assembly=mscorlib" 
                xmlns:dxg="http://schemas.devexpress.com/winfx/2008/xaml/grid"
                xmlns:dxwuin="http://schemas.devexpress.com/winfx/2008/xaml/windowsui/navigation"
                xmlns:dxt="http://schemas.devexpress.com/winfx/2008/xaml/core/themekeys"
                xmlns:local="clr-namespace:Workbench.ViewModels" 
                x:Class="Workbench.Views.WorkbenchWindow"
                xmlns:GeosApplication="clr-namespace:Emdep.Geos.UI.Common;assembly=Emdep.Geos.UI.Common" d:DataContext="{d:DesignInstance Type=local:WorkbenchWindowViewModel}"
                mc:Ignorable="d" d:DesignHeight="800" d:DesignWidth="900" 
                Style="{DynamicResource ServiceIconStyle}"
                Title="{Binding Installedversion}"
                WindowStartupLocation="CenterScreen" ResizeMode="CanResize" WindowState="Maximized"
                ShowIcon="False" Unloaded="DXWindow_Unloaded"
                PreviewMouseRightButtonDown="DXWindow_PreviewMouseRightButtonDown">

    <dxmvvm:Interaction.Triggers>
        <dxmvvm:EventToCommand  EventName="Closing" PassEventArgsToCommand="True" Command="{Binding WorkbenchWindowCloseCommand,Mode=TwoWay}"></dxmvvm:EventToCommand>
        
        <!--<dxmvvm:EventToCommand EventName="Unloaded" PassEventArgsToCommand="True" Command="{Binding PageUnLoadedCommand, Mode=TwoWay}" />-->
    </dxmvvm:Interaction.Triggers> 

    <dxc:DXWindow.Resources>
        <DataTemplate x:Key="pushpin">
            <StackPanel ToolTipService.ToolTip="{Binding MapItem.Information}" Margin="-30 -60">
                <Image Source="/GeosWorkbench;component/Assets/Images/Icon.ico" />
                <TextBlock Margin="5"
                           Foreground="Blue"
                           Text="{Binding MapItem.Text}" Style="{DynamicResource TextBlockStyle}" />
            </StackPanel>
        </DataTemplate>
        <!--[GEOS2-9057][rdixit][08.10.2025]-->
        <SolidColorBrush x:Key="contentBrush1" Color="White" />
        <Converts:RangeToBooleanConverter x:Key="rangeToBooleanConverter" />
        <Color x:Key="contentColor">#E9FFFFFF</Color>
        <Color x:Key="contentColorHover">#E9FFFFFF</Color>
        <Color x:Key="contentColorPressed">#FF0072C6</Color>
        <Color x:Key="contentColorChecked">#FF0072C6</Color>
        <Color x:Key="contentColorHoverChecked">#FF0072C6</Color>
        <Color x:Key="contentColorPressedChecked">#E9FFFFFF</Color>
        <SolidColorBrush x:Key="contentBrush" Color="{StaticResource contentColor}" />
        <SolidColorBrush x:Key="borderBrush" Color="#E9FFFFFF" />
        <SolidColorBrush x:Key="backBrush" Color="#00FFFFFF" />
        <SolidColorBrush x:Key="contentBrushHover" Color="{StaticResource contentColorHover}" />
        <SolidColorBrush x:Key="borderBrushHover" Color="#E9FFFFFF" />
        <SolidColorBrush x:Key="backBrushHover" Color="#3FFFFFFF" />
        <SolidColorBrush x:Key="contentBrushPressed" Color="{StaticResource contentColorPressed}" />
        <SolidColorBrush x:Key="borderBrushPressed" Color="#E9FFFFFF" />
        <SolidColorBrush x:Key="backBrushPressed" Color="#E9FFFFFF" />
        <SolidColorBrush x:Key="contentBrushChecked" Color="{StaticResource contentColorChecked}" />
        <SolidColorBrush x:Key="borderBrushChecked" Color="#E9FFFFFF" />
        <SolidColorBrush x:Key="backBrushChecked" Color="#E9FFFFFF" />
        <SolidColorBrush x:Key="contentBrushHoverChecked" Color="{StaticResource contentColorHoverChecked}" />
        <SolidColorBrush x:Key="borderBrushHoverChecked" Color="#E9FFFFFF" />
        <SolidColorBrush x:Key="backBrushHoverChecked" Color="#E9FFFFFF" />
        <SolidColorBrush x:Key="contentBrushPressedChecked" Color="{StaticResource contentColorPressedChecked}" />
        <SolidColorBrush x:Key="borderBrushPressedChecked" Color="#E9FFFFFF" />
        <SolidColorBrush x:Key="backBrushPressedChecked" Color="#00FFFFFF" />
    </dxc:DXWindow.Resources>

    <Grid >
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto" />
            <RowDefinition Height="100*" />
        </Grid.RowDefinitions>
        <DockPanel LastChildFill="True"  >
            <StackPanel Grid.Row="1" Width="Auto" DockPanel.Dock="Top"  >
                <!--[rohit.rathod][GEOS2-9057][25.09.2025]-->

                <!--Big view--> <!--[GEOS2-9057][rdixit][08.10.2025]-->
                <dxwui:AppBar Grid.Row="1" ItemSpacing="-25" HideMode="AlwaysVisible" Alignment="Top" MinHeight="60" AllowDrop="False" 
                               IsOpen="true" Height="100" IsExitButtonEnabled="False" Grid.ColumnSpan="5" 
                               HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Padding="0" Margin="0,0,0,10"
                               Visibility="{Binding IsMaxVisible,UpdateSourceTrigger=PropertyChanged, Mode=TwoWay}">

                    <StackPanel Grid.Row="0" Orientation="Vertical" VerticalAlignment="Stretch" Margin="0,20,0,0">
                        <Image x:Name="GeosLogo" Stretch="Uniform"  
                               RenderOptions.BitmapScalingMode="HighQuality"                          
                               Style="{DynamicResource GeosImagePath}"
                               HorizontalAlignment="Left" VerticalAlignment="Stretch"  
                               Height="57"  Margin="50,-5,0,0" />
                        <TextBlock Text="GLOBAL EMDEP OPERATIONAL SYSTEM" Foreground="White" VerticalAlignment="Center" 
                                   FontSize="10" FontWeight="Regular" Padding="3" Margin="50,-1,0,8"/>
                    </StackPanel>
                    <!-- **this button for temporary validate the user profile image.-->
                    <!--<Button Grid.Column="1" HorizontalAlignment="Left" Content="Validate Picture" Command="{Binding ValidateProfilePicture}" Visibility="Collapsed"></Button>-->
                    <StackPanel Grid.Column="1" VerticalAlignment="Center" HorizontalAlignment="Right" Width="1000" Margin="0,0,30,40">
                        <TextBlock Text="{Binding LoginUserFullName}" Style="{DynamicResource TextBlockForegroundUpper}" FontSize="20.5" Width="1000" />
                    </StackPanel>

                    <dxwui:AppBarButton Label="{DynamicResource WorkbenchWindowProfile}" 
                                        HorizontalAlignment="Right"
                                        Glyph="{Binding UserProfileImage,UpdateSourceTrigger=PropertyChanged}" 
                                        GlyphHeight="30"  
                                        Margin="0,17,0,0"
                                        GlyphStretch="UniformToFill" Style="{DynamicResource AppBarButtonStyle}" >
                        <dxwui:AppBarButton.Resources>
                            <ControlTemplate TargetType="dxwui:AppBarButton" 
                                x:Key="{dxwuit:AppBarElementsThemeKey ResourceKey=AppBarButtonTemplate, IsThemeIndependent=True}">
                                <Grid x:Name="RootGrid" Background="Transparent">
                                    <dxi:Interaction.Behaviors>
                                        <dxwuii:AnimatedPanelBehavior x:Name="animatedPanelBehavior" Width="100" />
                                    </dxi:Interaction.Behaviors>
                                    <StackPanel x:Name="panel" Margin="0,0,20,-8" VerticalAlignment="Top">
                                        <Border Height="{TemplateBinding EllipseDiameter}" Width="{TemplateBinding EllipseDiameter}" Padding="1">
                                            <Grid x:Name="PART_ContentGrid">
                                                <Ellipse StrokeThickness="1.5" Style="{DynamicResource ElipseAppbar}">
                                                    <Ellipse.Fill>
                                                        <!--<ImageBrush  Stretch="UniformToFill" />-->
                                                        <ImageBrush ImageSource="{Binding ImpersonateUserProfileImage,UpdateSourceTrigger=PropertyChanged}" Stretch="UniformToFill" />
                                                    </Ellipse.Fill>
                                                </Ellipse>

                                            </Grid>
                                        </Border>
                                        <dxwuii:ShapeContentPresenter x:Name="PART_Label"
                                              Margin="0,5,0,0" 
                                              HorizontalAlignment="Stretch"   
                                              Content="{DynamicResource WorkbenchWindowProfile}"
                                              ContentTemplate="{TemplateBinding LabelTemplate}" 
                                              ContentTemplateSelector="{TemplateBinding LabelTemplateSelector}"
                                              FontFamily="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=FontFamily}" 
                                              FontSize="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=FontSize}" 
                                              Foreground="{StaticResource contentBrush1}">
                                        </dxwuii:ShapeContentPresenter>
                                    </StackPanel>
                                </Grid>
                            </ControlTemplate>
                        </dxwui:AppBarButton.Resources>

                        <dxwui:AppBarButton.Flyout>
                            <dxwui:MenuFlyout ShowIndicator="True">
                                <dxwui:MenuFlyoutItem>
                                    <StackPanel Orientation="Horizontal">
                                        <Grid Width="350" Height="160" VerticalAlignment="Top" HorizontalAlignment="Left" Margin="0,0,20,0">
                                            <Grid.RowDefinitions>
                                                <RowDefinition Height="100" />
                                                <RowDefinition Height="50" />
                                            </Grid.RowDefinitions>
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="140" />
                                                <ColumnDefinition Width="84*" />
                                            </Grid.ColumnDefinitions>
                                            <Ellipse Grid.Column="0" Grid.Row="0" Style="{DynamicResource ElipseEditProfile}" StrokeThickness="1" Canvas.Top="2" ToolTip="Edit profile" Canvas.Left="4" Height="75" Width="95">
                                                <Ellipse.Fill>
                                                    <ImageBrush ImageSource="{Binding EditUserProfileImage ,UpdateSourceTrigger=PropertyChanged}" />
                                                </Ellipse.Fill>
                                            </Ellipse>
                                            <Label Height="30" Content="{Binding LoginUserFullName}" HorizontalAlignment="Left" VerticalAlignment="Top" Grid.Row="0" Grid.Column="1" Margin="0,-1,0,0" Style="{DynamicResource Label20Style}" />
                                            <Label Height="30" Content="{Binding LoginUserEmail}" HorizontalAlignment="Left" VerticalAlignment="Top" Grid.Row="0" Grid.Column="1" Margin="0,21,0,0" Style="{DynamicResource LabelStyle}" />
                                            <Button Height="40" Width="108" Grid.Row="0" Grid.Column="1" Margin="5,0,97,9"
                                HorizontalAlignment="Center" VerticalAlignment="Bottom"  
                                Command="{Binding WorkbenchWindowSettingsButtonCommand}" 
                                Style="{DynamicResource ButtonStyle}">
                                                <StackPanel Orientation="Horizontal">
                                                    <Image Source="{dxc:DXImage Image=Properties_32x32.png}" Height="24" Width="24" Stretch="UniformToFill" />
                                                    <TextBlock VerticalAlignment="Center" HorizontalAlignment="Right" Width="60" Text="{DynamicResource EditProfileToolTipSetting}" Style="{DynamicResource TextBlockStyle}"></TextBlock>
                                                </StackPanel>
                                            </Button>
                                            <Border BorderThickness="0,1,0,0" Grid.Column="0" Grid.Row="1" Grid.ColumnSpan="2" HorizontalAlignment="Left" Width="370" Margin="0,0,-20,0" Style="{DynamicResource BorderBrushStyle}" />
                                            <Button Content="{DynamicResource EditProfileWindowViewProfile}" 
                                Height="30" Width="110" VerticalAlignment="Bottom"  
                                HorizontalAlignment="Left" Grid.Row="1" Grid.Column="0" Margin="7,0,0,0" 
                                Command="{Binding WorkbenchWindowEditProfileButtonCommand}" 
                                Style="{DynamicResource ButtonStyle}" />
                                            <Button Content="{DynamicResource EditProfileToolTipSignOut}" 
                                Height="30" Width="110" HorizontalAlignment="Right" VerticalAlignment="Bottom"  
                                Grid.Row="1" Grid.Column="1" Margin="0,0,6,0" 
                                Command="{Binding WorkbenchWindowSignOutButtonCommand}" 
                                Style="{DynamicResource ButtonStyle}" />
                                        </Grid>
                                    </StackPanel>
                                </dxwui:MenuFlyoutItem>
                            </dxwui:MenuFlyout>
                        </dxwui:AppBarButton.Flyout>
                    </dxwui:AppBarButton>

                    <dxwui:AppBarButton Label="{Binding CompanyAlias,UpdateSourceTrigger=PropertyChanged,Mode=TwoWay}" 
                                        HorizontalAlignment="Right"
                                        IsCompact="True"
                                        Command="{Binding EmdepSitesWindowCommand, Mode=TwoWay}" 
                                        Margin="0,17,0,0"
                                         GlyphHeight="38"
                                        Padding="0,0,30,0"
                                        IsEllipseEnabled="False"
                                        AllowGlyphTheming="False"
                                        Style="{DynamicResource AppBarButtonLocationStyle}">
                    </dxwui:AppBarButton>

                    <!--[GEOS2-9057][rdixit][08.10.2025]-->
                    <dxwui:AppBarButton Label="{DynamicResource WorkbenchWindowNotifications}"                         
                                        HorizontalAlignment="Right" IsCompact="False" Margin="10,17,0,0" GlyphHeight="33"                        
                                        Padding="0,0,30,0" IsEllipseEnabled="False" AllowGlyphTheming="False"                       
                                        Style="{DynamicResource AppBarButtonNotificationsStyle}">

                        <dxmvvm:Interaction.Triggers>
                            <dxmvvm:EventToCommand EventName="Click" PassEventArgsToCommand="True" Command="{Binding UpdateUserNotificationCommand,Mode=TwoWay}"></dxmvvm:EventToCommand>
                        </dxmvvm:Interaction.Triggers>

                        <dxwui:AppBarButton.Resources>
                            
                            <ControlTemplate TargetType="dxwui:AppBarButton" x:Key="{dxwuit:AppBarElementsThemeKey ResourceKey=AppBarButtonTemplate, IsThemeIndependent=True}">
                                <Grid x:Name="RootGrid" Background="Transparent">

                                    <dxi:Interaction.Behaviors>
                                        <dxwuii:AnimatedPanelBehavior x:Name="animatedPanelBehavior" Width="100" />
                                    </dxi:Interaction.Behaviors>


                                    <StackPanel x:Name="panel" Margin="-10,0,30,-8" VerticalAlignment="Top">
                                        <Border x:Name="border" Height="{TemplateBinding EllipseDiameter}" Width="{TemplateBinding EllipseDiameter}" Padding="1">
                                            <Grid x:Name="PART_ContentGrid">
                                                <Ellipse x:Name="back" StrokeThickness="0" />

                                            </Grid>
                                        </Border>

                                        <dxwuii:ImagePresenter x:Name="glyph" Margin="0,-40,0,0" Height="40" Width="40" AllowAccent="{TemplateBinding AllowGlyphTheming}"                                      
                           AccentColor="{Binding Path=(dxb:ImageColorizer.Color), RelativeSource={RelativeSource TemplatedParent}}"                                      
                           Source="{TemplateBinding Glyph}" Stretch="{TemplateBinding GlyphStretch}" />


                                        <dxwuii:ShapeContentPresenter x:Name="content" Margin="0,5,0,0" HorizontalAlignment="Stretch"
          Content="{DynamicResource WorkbenchWindowNotifications}"
          ContentTemplate="{TemplateBinding LabelTemplate}"
          ContentTemplateSelector="{TemplateBinding LabelTemplateSelector}"
          FontFamily="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=FontFamily}"
          FontSize="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=FontSize}"
          Foreground="{StaticResource contentBrush1}" />
                                    </StackPanel>
                                    
                                    <Grid Margin="0,2,15,60" Height="20" Width="20" Visibility="{Binding NotificationVisibility,UpdateSourceTrigger=PropertyChanged,Mode=TwoWay}">
                                        <Ellipse Height="13" Fill="Red" Margin="-13,0,2,-5" Width="13" Stroke="White" StrokeThickness="1"/>
                                         <TextBlock Foreground="White" Margin="0,-15,0,0" HorizontalAlignment="Center" VerticalAlignment="Center"                                       
                                                    FontWeight="Bold" FontSize="11" TextAlignment="Center" TextWrapping="Wrap">
        
                                        </TextBlock>
                                    </Grid>
                                    
                                    

                                    <!-- 👇 Move VisualStateManager to the end -->
                                    <VisualStateManager.VisualStateGroups>
                                        <VisualStateGroup x:Name="CommonStates">
                                            <VisualState x:Name="Normal" />
                                            <VisualState x:Name="MouseOver">
                                                <Storyboard>
                                                    <ObjectAnimationUsingKeyFrames Storyboard.TargetName="border" Storyboard.TargetProperty="BorderBrush">
                                                        <DiscreteObjectKeyFrame KeyTime="0" Value="{StaticResource borderBrushHover}" />
                                                    </ObjectAnimationUsingKeyFrames>
                                                    <ObjectAnimationUsingKeyFrames Storyboard.TargetName="back" Storyboard.TargetProperty="Fill">
                                                        <DiscreteObjectKeyFrame KeyTime="0" Value="{StaticResource backBrushHover}" />
                                                    </ObjectAnimationUsingKeyFrames>
                                                    <ObjectAnimationUsingKeyFrames Storyboard.TargetName="content" Storyboard.TargetProperty="Foreground">
                                                        <DiscreteObjectKeyFrame KeyTime="0" Value="{StaticResource contentBrushHover}" />
                                                    </ObjectAnimationUsingKeyFrames>
                                                    <ObjectAnimationUsingKeyFrames Storyboard.TargetName="content" Storyboard.TargetProperty="AccentColor">
                                                        <DiscreteObjectKeyFrame KeyTime="0" Value="{StaticResource contentColorHover}" />
                                                    </ObjectAnimationUsingKeyFrames>
                                                    <ObjectAnimationUsingKeyFrames Storyboard.TargetName="glyph" Storyboard.TargetProperty="AccentColor">
                                                        <DiscreteObjectKeyFrame KeyTime="0" Value="{StaticResource contentColorHover}" />
                                                    </ObjectAnimationUsingKeyFrames>
                                                </Storyboard>
                                            </VisualState>
                                        </VisualStateGroup>
                                    </VisualStateManager.VisualStateGroups>
                                </Grid>
                            </ControlTemplate>
                        </dxwui:AppBarButton.Resources>

                        <dxwui:AppBarButton.Flyout>

                            <dxwui:MenuFlyout ShowIndicator="True">
                                <dxwui:MenuFlyoutItem StaysOpenOnClick="True">
                                    <dxmvvm:Interaction.Triggers>
                                        <dxmvvm:EventToCommand EventName="Click" PassEventArgsToCommand="True" Command="{Binding UpdateUserNotificationCommand,Mode=TwoWay}"></dxmvvm:EventToCommand>
                                    </dxmvvm:Interaction.Triggers>
                                    <StackPanel Orientation="Vertical">
                                        <TextBlock Text="{Binding NotificationInString, UpdateSourceTrigger=PropertyChanged,Mode=TwoWay}"></TextBlock>
                                        <dxg:GridControl Height="270" HorizontalAlignment="Stretch" Width="430" DataSource="{Binding Source={x:Static GeosApplication:GeosApplication.Instance}, Path=NotificationsListCommon,UpdateSourceTrigger=PropertyChanged,Mode=TwoWay}" VerticalAlignment="Bottom" CurrentItem="{Binding SelectedNotification,Mode=TwoWay}">
                                            <dxg:GridControl.Columns>
                                                <dxg:GridColumn FieldName="FromUserImage" Width="75">
                                                    <dxg:GridColumn.CellTemplate>
                                                        <DataTemplate>
                                                            <Image Name="imgInfo" Source="{Binding Value}" Height="30" Width="30" Margin="0,5,0,0">
                                                                <Image.ToolTip>
                                                                    <TextBlock Text="{Binding Data.FromUserName.FullNames}" />
                                                                </Image.ToolTip>
                                                            </Image>
                                                        </DataTemplate>
                                                    </dxg:GridColumn.CellTemplate>
                                                </dxg:GridColumn>
                                                <dxg:GridColumn Width="150">
                                                    <dxg:GridColumn.CellTemplate>
                                                        <DataTemplate>
                                                            <StackPanel Orientation="Vertical">
                                                                <TextBlock Text="{Binding RowData.Row.Title}" FontWeight="Bold" Style="{DynamicResource TextBlock10Style}" />
                                                                <TextBlock Text="{Binding RowData.Row.Message}" TextWrapping="WrapWithOverflow" Style="{DynamicResource TextBlock10Style}" />
                                                            </StackPanel>
                                                        </DataTemplate>
                                                    </dxg:GridColumn.CellTemplate>
                                                </dxg:GridColumn>
                                                <dxg:GridColumn FieldName="TimeInElapsed" Width="90">
                                                    <dxg:GridColumn.CellTemplate>
                                                        <DataTemplate>
                                                            <StackPanel Orientation="Horizontal">
                                                                <Image Style="{DynamicResource ImageClockStyle}" HorizontalAlignment="Left" Height="10" Width="10" VerticalAlignment="Top" Stretch="UniformToFill" Margin="0,2,0,0"></Image>
                                                                <TextBlock Text="{Binding Value}" HorizontalAlignment="Right" Margin="2,0,0,0" Style="{DynamicResource TextBlock10Style}">
                                                                </TextBlock>
                                                            </StackPanel>
                                                        </DataTemplate>
                                                    </dxg:GridColumn.CellTemplate>
                                                    <dxg:GridColumn.EditSettings>
                                                        <dxe:TextEditSettings VerticalContentAlignment="Top" />
                                                    </dxg:GridColumn.EditSettings>
                                                </dxg:GridColumn>
                                                <dxg:GridColumn Width="30">
                                                    <dxg:GridColumn.CellTemplate>
                                                        <DataTemplate>
                                                            <Border>
                                                                <ContentControl>
                                                                    <dxmvvm:Interaction.Behaviors>
                                                                        <dxmvvm:EventToCommand EventName="MouseDown"
                                                               PassEventArgsToCommand="True" 
                                                               Command="{Binding DataContext.SingleNotificationDeleteButtonCommand, RelativeSource={RelativeSource AncestorType=Window}}" />
                                                                    </dxmvvm:Interaction.Behaviors>
                                                                    <Image Source="/GeosWorkbench;component/Assets/Images/Delete.png" Width="20" Height="16" HorizontalAlignment="Center" Style="{DynamicResource ImageCursor}" />
                                                                </ContentControl>
                                                            </Border>
                                                        </DataTemplate>
                                                    </dxg:GridColumn.CellTemplate>

                                                </dxg:GridColumn>
                                            </dxg:GridControl.Columns>
                                            <dxg:GridControl.View>
                                                <dxg:TableView Name="tableView1"
                                   AutoWidth="True" 
                                   ShowAutoFilterRow="False"
                                   ShowGroupPanel="False"
                                   NavigationStyle="Row" 
                                   RowMinHeight="5"
                                   ShowHorizontalLines="False"
                                   BestFitMode="AllRows" 
                                   AllowSorting="True" 
                                   AllowScrollHeaders="False" 
                                   AllowGrouping="False"
                                   AllowEditing="False"
                                   AllowColumnMoving="False" 
                                   AutoMoveRowFocus="False"
                                   ShowIndicator="False"
                                   ShowColumnHeaders="False"
                                   AllowScrollAnimation="True"
                                   FocusedRow="{x:Null}"
                                   ShowVerticalLines="False" 
                                   AllowBestFit="True" AllowResizing="False"
                                   AllowConditionalFormattingMenu="True"
                                   UseLightweightTemplates="None">
                                                </dxg:TableView>
                                            </dxg:GridControl.View>
                                        </dxg:GridControl>
                                    </StackPanel>
                                </dxwui:MenuFlyoutItem>

                                <dxwui:MenuFlyoutItem>
                                    <StackPanel HorizontalAlignment="Center" Margin="0" Orientation="Horizontal">
                                        <Button HorizontalAlignment="Left"
                            Margin="5,0,5,0" Height="30" Width="100"
                            Command="{Binding WorkbenchWindowViewAllCommand}"
                            Content="{DynamicResource NotificationViewAll}"
                            IsEnabled="{Binding IsNotificationViewMoreButtonEnable}" Style="{DynamicResource ButtonStyle}">
                                        </Button>
                                        <Button HorizontalAlignment="Right"
                            Command="{Binding WorkbenchWindowClearAllCommand}"
                            Margin="5,0,5,0" Height="30" Width="80"
                            Content="{DynamicResource NotificationClearAll}" IsEnabled="{Binding IsNotificationClearAllButtonEnable}" Style="{DynamicResource ButtonStyle}">

                                        </Button>

                                        <DockPanel Margin="5,0,5,0" VerticalAlignment="Bottom">
                                            <Label Content="{DynamicResource TotalNotification}" Style="{DynamicResource LabelStyle}"></Label>
                                            <Label Content="{Binding DispayNotification, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" Style="{DynamicResource LabelStyle}"></Label>
                                        </DockPanel>
                                    </StackPanel>
                                </dxwui:MenuFlyoutItem>

                            </dxwui:MenuFlyout>

                        </dxwui:AppBarButton.Flyout>
                    </dxwui:AppBarButton>


                    <dxwui:AppBarButton Label="{DynamicResource WorkbenchWindowSupport}" HorizontalAlignment="Right"
                                        IsCompact="False" 
                                        Command="{Binding SupportWindowCommand}"
                                        HorizontalContentAlignment="Stretch"
                                         GlyphHeight="38"
                                        Padding="0,0,30,0"
                                        IsEllipseEnabled="False"
                                       Margin="0,17,0,0"
                                        Style="{DynamicResource AppBarButtonSupportStyle}"
                                        GlyphStretch="Uniform">
                    </dxwui:AppBarButton>

                    <dxwui:AppBarButton Label="{DynamicResource FeedbackHeading}" HorizontalAlignment="Right"
                                        Foreground="White" Command="{Binding FeedbackWindowOpenCommand}"
                                        IsCompact="True"
                                        Style="{ DynamicResource AppBarButtonFeedbackStyle}"
                                        GlyphHeight="38" 
                                        Padding="0,0,30,0"   
                                        Margin="0,17,0,0"
                                        GlyphStretch="Uniform" IsEllipseEnabled="False"
                                        AllowGlyphTheming="False"
                                        >
                    </dxwui:AppBarButton>

                    <dxwui:AppBarButton Label="{DynamicResource AboutHeading}"
                                        HorizontalAlignment="Right"
                                        IsCompact="True"
                                        Command="{Binding AboutWindowOpenCommand}"
                                        Style="{ DynamicResource AppBarButtonAboutStyle}"
                                       
                                        GlyphHeight="38"
                                        Padding="0,0,30,0"
                                       Margin="0,17,0,0"
                                        GlyphStretch="Uniform" IsEllipseEnabled="False"
                                        AllowGlyphTheming="False">
                    </dxwui:AppBarButton>

                    <dxmvvm:Interaction.Triggers>
                        <dxmvvm:EventToCommand EventName="MouseLeftButtonDown" PassEventArgsToCommand="True" Command="{Binding AppBarButtonClickCommand,Mode=TwoWay}"></dxmvvm:EventToCommand>
                        <!--<dxmvvm:EventToCommand EventName="Unloaded" PassEventArgsToCommand="True"    PreviewMouseRightButtonDown="AppBar_PreviewMouseRightButtonDown" Command="{Binding PageUnLoadedCommand, Mode=TwoWay}" />-->
                    </dxmvvm:Interaction.Triggers>
                </dxwui:AppBar>


                <!--Small view-->
                <dxwui:AppBar Grid.Row="1" ItemSpacing="-40" HideMode="AlwaysVisible" Alignment="Top" MinHeight="26.5"                   
                              IsOpen="False" Height="46" Width="Auto" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Padding="0,0,0,10"                   
                              IsExitButtonEnabled="False"  Visibility="{Binding IsMinVisible,UpdateSourceTrigger=PropertyChanged, Mode=TwoWay}">

                    <StackPanel Orientation="Horizontal" VerticalAlignment="Center" Margin="55,0,0,0">

                        <!-- Logo -->
                        <Image Style="{DynamicResource GeosSmallImagePath}" x:Name="GeosLogo1" Stretch="Uniform" HorizontalAlignment="Left" RenderOptions.BitmapScalingMode="HighQuality" VerticalAlignment="Center" Height="30" Margin="-30,-11,0,0"/>

                        <!-- Title -->
                        <TextBlock Text="GLOBAL EMDEP OPERATIONAL SYSTEM" Foreground="White" VerticalAlignment="Center" FontSize="20" FontWeight="Regular" Margin="10,-2,0,8"/>
                    </StackPanel>
                    <!-- **this button for temporary validate the user profile image.-->
                    <StackPanel Grid.Column="1" VerticalAlignment="Center" HorizontalAlignment="Right" Width="200" Margin="0,-10,30,10">
                        <TextBlock Text="{Binding LoginUserFullName}" Style="{DynamicResource TextBlockForegroundUpper}" FontSize="19"  />
                    </StackPanel>

                    <dxwui:AppBarButton Label="{DynamicResource WorkbenchWindowProfile}" 
                                        HorizontalAlignment="Right"
                                        Glyph="{Binding UserProfileImage,UpdateSourceTrigger=PropertyChanged}"   
                                        GlyphHeight="30"  
                                        GlyphStretch="UniformToFill" Style="{DynamicResource AppBarButtonStyle}" >
                        <dxwui:AppBarButton.Resources>
                            <ControlTemplate TargetType="dxwui:AppBarButton" 
                                 x:Key="{dxwuit:AppBarElementsThemeKey ResourceKey=AppBarButtonTemplate, IsThemeIndependent=True}">
                                <Grid x:Name="RootGrid" Background="Transparent">
                                    <dxi:Interaction.Behaviors>
                                        <dxwuii:AnimatedPanelBehavior x:Name="animatedPanelBehavior" Width="100" />
                                    </dxi:Interaction.Behaviors>
                                    <StackPanel x:Name="panel" Margin="0,0,29,0"  VerticalAlignment="Center">
                                        <Border Height="{TemplateBinding EllipseDiameter}" Width="{TemplateBinding EllipseDiameter}" Padding="1">
                                            <Grid x:Name="PART_ContentGrid">
                                                <Ellipse StrokeThickness="1" Height="30" Width="30"   Style="{DynamicResource ElipseAppbar}">
                                                    <Ellipse.Fill>
                                                        <!--<ImageBrush  Stretch="UniformToFill" />-->
                                                        <ImageBrush ImageSource="{Binding ImpersonateUserProfileImage,UpdateSourceTrigger=PropertyChanged}" Stretch="UniformToFill" />
                                                    </Ellipse.Fill>
                                                </Ellipse>
                                            </Grid>
                                        </Border>
                                    </StackPanel>
                                </Grid>
                            </ControlTemplate>
                        </dxwui:AppBarButton.Resources>

                        <dxwui:AppBarButton.Flyout>
                            <dxwui:MenuFlyout ShowIndicator="True">
                                <dxwui:MenuFlyoutItem>
                                    <StackPanel Orientation="Horizontal">
                                        <Grid Width="350" Height="160" VerticalAlignment="Top" HorizontalAlignment="Left" Margin="0,0,10,0">
                                            <Grid.RowDefinitions>
                                                <RowDefinition Height="100" />
                                                <RowDefinition Height="50" />
                                            </Grid.RowDefinitions>
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="140" />
                                                <ColumnDefinition Width="84*" />
                                            </Grid.ColumnDefinitions>
                                            <Ellipse Grid.Column="0" Grid.Row="0" Style="{DynamicResource ElipseEditProfile}" StrokeThickness="1" Canvas.Top="2" ToolTip="Edit profile" Canvas.Left="4" Height="75" Width="75">
                                                <Ellipse.Fill>
                                                    <ImageBrush ImageSource="{Binding EditUserProfileImage ,UpdateSourceTrigger=PropertyChanged}" />
                                                </Ellipse.Fill>
                                            </Ellipse>
                                            <Label Height="30" Content="{Binding LoginUserFullName}" HorizontalAlignment="Left" VerticalAlignment="Top" Grid.Row="0" Grid.Column="1" Margin="0,-1,0,0" Style="{DynamicResource Label20Style}" />
                                            <Label Height="30" Content="{Binding LoginUserEmail}" HorizontalAlignment="Left" VerticalAlignment="Top" Grid.Row="0" Grid.Column="1" Margin="0,21,0,0" Style="{DynamicResource LabelStyle}" />
                                            <Button Height="40" Width="108" Grid.Row="0" Grid.Column="1" Margin="5,0,97,9"
                                                    HorizontalAlignment="Center" VerticalAlignment="Bottom"  
                                                    Command="{Binding WorkbenchWindowSettingsButtonCommand}" 
                                                    Style="{DynamicResource ButtonStyle}">
                                                <StackPanel Orientation="Horizontal">
                                                    <Image Source="{dxc:DXImage Image=Properties_32x32.png}" Height="24" Width="24" Stretch="UniformToFill" />
                                                    <TextBlock VerticalAlignment="Center" HorizontalAlignment="Right" Width="60" Text="{DynamicResource EditProfileToolTipSetting}" Style="{DynamicResource TextBlockStyle}"></TextBlock>
                                                </StackPanel>
                                            </Button>
                                            <Border BorderThickness="0,1,0,0" Grid.Column="0" Grid.Row="1" Grid.ColumnSpan="2" HorizontalAlignment="Left" Width="370" Margin="0,0,-20,0" Style="{DynamicResource BorderBrushStyle}" />
                                            <Button Content="{DynamicResource EditProfileWindowViewProfile}" 
                                                    Height="30" Width="110" VerticalAlignment="Bottom"  
                                                    HorizontalAlignment="Left" Grid.Row="1" Grid.Column="0" Margin="7,0,0,0" 
                                                    Command="{Binding WorkbenchWindowEditProfileButtonCommand}" 
                                                    Style="{DynamicResource ButtonStyle}" />    
                                            <Button Content="{DynamicResource EditProfileToolTipSignOut}" 
                                                    Height="30" Width="110" HorizontalAlignment="Right" VerticalAlignment="Bottom"  
                                                    Grid.Row="1" Grid.Column="1" Margin="0,0,6,0" 
                                                    Command="{Binding WorkbenchWindowSignOutButtonCommand}" 
                                                    Style="{DynamicResource ButtonStyle}" />
                                        </Grid>
                                    </StackPanel>
                                </dxwui:MenuFlyoutItem>
                            </dxwui:MenuFlyout>
                        </dxwui:AppBarButton.Flyout>
                    </dxwui:AppBarButton>

                    <!--[GEOS2-9057][rdixit][08.10.2025]-->
                    <dxwui:AppBarButton Command="{Binding EmdepSitesWindowCommand, Mode=TwoWay}"             
                                        HorizontalAlignment="Right"            
                                        IsCompact="True"                       
                                        GlyphHeight="30"            
                                        VerticalAlignment="Center"            
                                        Padding="0,2,30,0"           
                                        IsEllipseEnabled="False"            
                                        AllowGlyphTheming="False"           
                                        Style="{DynamicResource AppBarButtonLocationStyle}">
                    </dxwui:AppBarButton>

                    <dxwui:AppBarButton Label="{DynamicResource WorkbenchWindowNotifications}"             
                     HorizontalAlignment="Right" IsCompact="False" Margin="0,-47,13,0" GlyphHeight="26.5" VerticalAlignment="Center"             
                     Padding="0,0,0,0" IsEllipseEnabled="False" AllowGlyphTheming="False"           
                     Style="{DynamicResource AppBarButtonNotificationsStyle}">

                        <dxmvvm:Interaction.Triggers>
                            <dxmvvm:EventToCommand EventName="Click" PassEventArgsToCommand="True" Command="{Binding UpdateUserNotificationCommand,Mode=TwoWay}"></dxmvvm:EventToCommand>
                        </dxmvvm:Interaction.Triggers>

                        <dxwui:AppBarButton.Resources>
                            <ControlTemplate TargetType="dxwui:AppBarButton" 
              x:Key="{dxwuit:AppBarElementsThemeKey ResourceKey=AppBarButtonTemplate, IsThemeIndependent=True}">
                                <Grid x:Name="RootGrid" Background="Transparent" >
                                    <dxi:Interaction.Behaviors>
                                        <dxwuii:AnimatedPanelBehavior x:Name="animatedPanelBehavior" Width="100"  />
                                    </dxi:Interaction.Behaviors>
                                    <StackPanel x:Name="panel" VerticalAlignment="Center">
                                        <Border x:Name="border" Height="40" Width="40" Margin="-12,0,0,0">
                                            <Grid x:Name="PART_ContentGrid">
                                                <Ellipse x:Name="back" StrokeThickness="0"/>
                                            </Grid>
                                        </Border>

                                        <!-- Image -->
                                        <dxwuii:ImagePresenter x:Name="glyph" 
        Margin="-12,-40,0,0"
        Height="30" Width="30"
        AllowAccent="{TemplateBinding AllowGlyphTheming}"
        AccentColor="{Binding Path=(dxb:ImageColorizer.Color), RelativeSource={RelativeSource TemplatedParent}}"
        Source="{TemplateBinding Glyph}" Stretch="{TemplateBinding GlyphStretch}" />

                                        <!-- Red Dot after image -->
                                        <Grid Height="15" Width="15" Visibility="{Binding NotificationVisibility,UpdateSourceTrigger=PropertyChanged,Mode=TwoWay}"
                          Panel.ZIndex="9999" Margin="-5,-50,0,0">
                                            <Ellipse Height="10" Width="10"
                                  Fill="Red"
                                  Stroke="White"
                                  StrokeThickness="1" />
                                        </Grid>
                                    </StackPanel>
                                    <VisualStateManager.VisualStateGroups>
                                        <VisualStateGroup x:Name="CommonStates">
                                            <VisualState x:Name="Normal" />
                                            <VisualState x:Name="MouseOver">
                                                <Storyboard>
                                                    <ObjectAnimationUsingKeyFrames Storyboard.TargetName="border" Storyboard.TargetProperty="BorderBrush">
                                                        <DiscreteObjectKeyFrame KeyTime="0" Value="{StaticResource borderBrushHover}" />
                                                    </ObjectAnimationUsingKeyFrames>
                                                    <ObjectAnimationUsingKeyFrames Storyboard.TargetName="back" Storyboard.TargetProperty="Fill">
                                                        <DiscreteObjectKeyFrame KeyTime="0" Value="{StaticResource backBrushHover}" />
                                                    </ObjectAnimationUsingKeyFrames>
                                                    <ObjectAnimationUsingKeyFrames Storyboard.TargetName="glyph" Storyboard.TargetProperty="AccentColor">
                                                        <DiscreteObjectKeyFrame KeyTime="0" Value="{StaticResource contentColorHover}" />
                                                    </ObjectAnimationUsingKeyFrames>
                                                </Storyboard>
                                            </VisualState>
                                        </VisualStateGroup>
                                    </VisualStateManager.VisualStateGroups>
                                </Grid>
                            </ControlTemplate>
                        </dxwui:AppBarButton.Resources>

                        <dxwui:AppBarButton.Flyout>

                            <dxwui:MenuFlyout ShowIndicator="True">

                                <dxwui:MenuFlyoutItem StaysOpenOnClick="True">
                                    <dxmvvm:Interaction.Triggers>
                                        <dxmvvm:EventToCommand EventName="Click" PassEventArgsToCommand="True" Command="{Binding UpdateUserNotificationCommand,Mode=TwoWay}"></dxmvvm:EventToCommand>
                                    </dxmvvm:Interaction.Triggers>
                                    <StackPanel Orientation="Vertical">
                                        <TextBlock Text="{Binding NotificationInString, UpdateSourceTrigger=PropertyChanged,Mode=TwoWay}"></TextBlock>
                                        <dxg:GridControl Height="270" HorizontalAlignment="Stretch" Width="430" DataSource="{Binding Source={x:Static GeosApplication:GeosApplication.Instance}, Path=NotificationsListCommon,UpdateSourceTrigger=PropertyChanged,Mode=TwoWay}" VerticalAlignment="Bottom" CurrentItem="{Binding SelectedNotification,Mode=TwoWay}">
                                            <dxg:GridControl.Columns>
                                                <dxg:GridColumn FieldName="FromUserImage" Width="75">
                                                    <dxg:GridColumn.CellTemplate>
                                                        <DataTemplate>
                                                            <Image Name="imgInfo" Source="{Binding Value}" Height="30" Width="30" Margin="0,5,0,0">
                                                                <Image.ToolTip>
                                                                    <TextBlock Text="{Binding Data.FromUserName.FullNames}" />
                                                                </Image.ToolTip>
                                                            </Image>
                                                        </DataTemplate>
                                                    </dxg:GridColumn.CellTemplate>
                                                </dxg:GridColumn>
                                                <dxg:GridColumn Width="150">
                                                    <dxg:GridColumn.CellTemplate>
                                                        <DataTemplate>
                                                            <StackPanel Orientation="Vertical">
                                                                <TextBlock Text="{Binding RowData.Row.Title}" FontWeight="Bold" Style="{DynamicResource TextBlock10Style}" />
                                                                <TextBlock Text="{Binding RowData.Row.Message}" TextWrapping="WrapWithOverflow" Style="{DynamicResource TextBlock10Style}" />
                                                            </StackPanel>
                                                        </DataTemplate>
                                                    </dxg:GridColumn.CellTemplate>
                                                </dxg:GridColumn>
                                                <dxg:GridColumn FieldName="TimeInElapsed" Width="90">
                                                    <dxg:GridColumn.CellTemplate>
                                                        <DataTemplate>
                                                            <StackPanel Orientation="Horizontal">
                                                                <Image Style="{DynamicResource ImageClockStyle}" HorizontalAlignment="Left" Height="10" Width="10" VerticalAlignment="Top" Stretch="UniformToFill" Margin="0,2,0,0"></Image>
                                                                <TextBlock Text="{Binding Value}" HorizontalAlignment="Right" Margin="2,0,0,0" Style="{DynamicResource TextBlock10Style}">
                                                                </TextBlock>
                                                            </StackPanel>
                                                        </DataTemplate>
                                                    </dxg:GridColumn.CellTemplate>
                                                    <dxg:GridColumn.EditSettings>
                                                        <dxe:TextEditSettings VerticalContentAlignment="Top" />
                                                    </dxg:GridColumn.EditSettings>
                                                </dxg:GridColumn>
                                                <dxg:GridColumn Width="30">
                                                    <dxg:GridColumn.CellTemplate>
                                                        <DataTemplate>
                                                            <Border>
                                                                <ContentControl>
                                                                    <dxmvvm:Interaction.Behaviors>
                                                                        <dxmvvm:EventToCommand EventName="MouseDown" PassEventArgsToCommand="True"                                    
                                                                            Command="{Binding DataContext.SingleNotificationDeleteButtonCommand, RelativeSource={RelativeSource AncestorType=Window}}" />
                                                                    </dxmvvm:Interaction.Behaviors>
                                                                    <Image Source="/GeosWorkbench;component/Assets/Images/Delete.png" Width="20" Height="16" HorizontalAlignment="Center" Style="{DynamicResource ImageCursor}" />
                                                                </ContentControl>
                                                            </Border>
                                                        </DataTemplate>
                                                    </dxg:GridColumn.CellTemplate>
                                                </dxg:GridColumn>
                                            </dxg:GridControl.Columns>
                                            <dxg:GridControl.View>
                                                <dxg:TableView Name="tableView2"
                                AutoWidth="True" 
                                ShowAutoFilterRow="False"
                                ShowGroupPanel="False"
                                NavigationStyle="Row" 
                                RowMinHeight="5"
                                ShowHorizontalLines="False"
                                BestFitMode="AllRows" 
                                AllowSorting="True" 
                                AllowScrollHeaders="False" 
                                AllowGrouping="False"
                                AllowEditing="False"
                                AllowColumnMoving="False" 
                                AutoMoveRowFocus="False"
                                ShowIndicator="False"
                                ShowColumnHeaders="False"
                                AllowScrollAnimation="True"
                                FocusedRow="{x:Null}"
                                ShowVerticalLines="False" 
                                AllowBestFit="True" AllowResizing="False"
                                AllowConditionalFormattingMenu="True"
                                UseLightweightTemplates="None">
                                                </dxg:TableView>
                                            </dxg:GridControl.View>
                                        </dxg:GridControl>
                                    </StackPanel>
                                </dxwui:MenuFlyoutItem>

                                <dxwui:MenuFlyoutItem>
                                    <StackPanel HorizontalAlignment="Center" Margin="0" Orientation="Horizontal">
                                        <Button HorizontalAlignment="Left"
                             Margin="5,0,5,0" Height="30" Width="100"
                             Command="{Binding WorkbenchWindowViewAllCommand}"
                             Content="{DynamicResource NotificationViewAll}"
                             IsEnabled="{Binding IsNotificationViewMoreButtonEnable}" Style="{DynamicResource ButtonStyle}">
                                        </Button>
                                        <Button HorizontalAlignment="Right"
                             Command="{Binding WorkbenchWindowClearAllCommand}"
                             Margin="5,0,5,0" Height="30" Width="80"
                             Content="{DynamicResource NotificationClearAll}" IsEnabled="{Binding IsNotificationClearAllButtonEnable}" Style="{DynamicResource ButtonStyle}">
                                        </Button>

                                        <DockPanel Margin="5,0,5,0" VerticalAlignment="Bottom">
                                            <Label Content="{DynamicResource TotalNotification}" Style="{DynamicResource LabelStyle}"></Label>
                                            <Label Content="{Binding DispayNotification, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" Style="{DynamicResource LabelStyle}"></Label>
                                        </DockPanel>
                                    </StackPanel>
                                </dxwui:MenuFlyoutItem>

                            </dxwui:MenuFlyout>

                        </dxwui:AppBarButton.Flyout>
                    </dxwui:AppBarButton>

                    <dxwui:AppBarButton Command="{Binding SupportWindowCommand}"  
                                        HorizontalAlignment="Right"
                                        IsCompact="False" 
                                        VerticalAlignment="Center"             
                                        GlyphHeight="30"
                                        Padding="0,2,30,0"
                                        IsEllipseEnabled="False"           
                                        Style="{DynamicResource AppBarButtonSupportStyle}"
                                        GlyphStretch="Uniform">
                    </dxwui:AppBarButton>


                    <dxwui:AppBarButton  Command="{Binding FeedbackWindowOpenCommand}"                                        
                                         HorizontalAlignment="Right" Foreground="White"
                                         IsCompact="True"
                                         Style="{ DynamicResource AppBarButtonFeedbackStyle}"
                                         GlyphHeight="30" 
                                         Padding="0,2,30,0"  
                                         Margin="0,0,0,0"
                                         VerticalAlignment="Center" 
                                         GlyphStretch="Uniform" IsEllipseEnabled="False"
                                         AllowGlyphTheming="False">
                    </dxwui:AppBarButton>

                    <dxwui:AppBarButton Command="{Binding AboutWindowOpenCommand}"
                                        HorizontalAlignment="Right"
                                        IsCompact="True"
                                        Style="{ DynamicResource AppBarButtonAboutStyle}"
                                        GlyphHeight="30"
                                        Padding="0,2,30,0"
                                        Margin="0,0,25,0"
                                        GlyphStretch="Uniform" 
                                        IsEllipseEnabled="False"
                                        AllowGlyphTheming="False">
                    </dxwui:AppBarButton>

                    <dxmvvm:Interaction.Triggers>
                        <dxmvvm:EventToCommand EventName="MouseLeftButtonDown"  PassEventArgsToCommand="True" Command="{Binding AppBarButtonClickCommand,Mode=TwoWay}"></dxmvvm:EventToCommand>
                       </dxmvvm:Interaction.Triggers>
                </dxwui:AppBar>

            </StackPanel>
        </DockPanel>

        <dxwui:NavigationFrame Style="{DynamicResource NavigationFrameImageStyle}"
                                AnimationType="SlideHorizontal" VerticalAlignment="Stretch" NavigationCacheMode="Required"
                                HorizontalAlignment="Stretch" Height="Auto" Grid.Row="1" Padding="10" BackNavigationMode="PreviousScreen">
            <dxmvvm:Interaction.Triggers>
                <dxmvvm:EventToCommand EventName="Loaded" Command="{Binding OnViewLoadedCommand}" />
            </dxmvvm:Interaction.Triggers>
            <dxmvvm:Interaction.Behaviors>
                <dxwuin:FrameNavigationService />
            </dxmvvm:Interaction.Behaviors>
        </dxwui:NavigationFrame>
    </Grid>
</dxc:DXWindow>