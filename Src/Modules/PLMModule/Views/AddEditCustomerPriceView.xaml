                                                    <dxmvvm:Interaction.Behaviors>
                                                        <dxmvvm:EventToCommand 	
                                                        EventName="Checked" Command="{Binding DataContext.ChangeCheckAllLanguagesCommand,RelativeSource={RelativeSource AncestorType=dxlc:GroupBox}}"/>
                                                        <dxmvvm:EventToCommand 	
                                                        EventName="Unchecked" Command="{Binding DataContext.ChangeCheckAllLanguagesCommand,RelativeSource={RelativeSource AncestorType=dxlc:GroupBox}}"/>
