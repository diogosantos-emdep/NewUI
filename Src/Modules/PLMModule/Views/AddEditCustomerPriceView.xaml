                                                        <Binding Path="DataContext.ExpiryDate" RelativeSource="{RelativeSource AncestorType=dxlc:GroupBox}" UpdateSourceTrigger="PropertyChanged" Mode="TwoWay"
                                                        ValidatesOnDataErrors="True">
                                                        </Binding>
