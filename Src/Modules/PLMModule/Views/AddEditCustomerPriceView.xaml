                                                <!--<TextBlock Margin="5,0,0,0" Text="{DynamicResource CopyNameAndDescription}" 
                                                                   Style="{StaticResource TextBlock14Style}" 
                                                                   VerticalAlignment="Center" />-->
                                            </StackPanel>


