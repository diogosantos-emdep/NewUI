                    <DataTemplate>
                        <DockPanel Margin="-5,0,0,0">
                            <TextBlock Margin="5,0,0,0"
                                       VerticalAlignment="Center"
                                       FontWeight="Bold"
                                       Text="{DynamicResource AttachmentWithColon}"/>
