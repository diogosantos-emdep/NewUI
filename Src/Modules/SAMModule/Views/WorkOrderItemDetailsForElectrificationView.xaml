                                                    <dxg:GridColumn.CellTemplate>
                                                        <DataTemplate>
                                                            <Button Name="deleteRowItem" ToolTip="{DynamicResource Delete}" 
                                                                  Command="{Binding DataContext.DeleteItemCommand , Mode=TwoWay,UpdateSourceTrigger=PropertyChanged,RelativeSource={RelativeSource AncestorType=dxwui:WinUIDialogWindow}}"
                                                                  CommandParameter="{Binding RowData.Row}">
