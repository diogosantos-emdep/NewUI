<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:dxwui="http://schemas.devexpress.com/winfx/2008/xaml/windowsui"
                    xmlns:dxlc="http://schemas.devexpress.com/winfx/2008/xaml/layoutcontrol"
                    xmlns:dxc="http://schemas.devexpress.com/winfx/2008/xaml/core"
                    xmlns:dxe="http://schemas.devexpress.com/winfx/2008/xaml/editors"
                    xmlns:dxg="http://schemas.devexpress.com/winfx/2008/xaml/grid"
                    xmlns:dxgt="http://schemas.devexpress.com/winfx/2008/xaml/grid/themekeys"
                    xmlns:dxmvvm="http://schemas.devexpress.com/winfx/2008/xaml/mvvm"
                    xmlns:system="clr-namespace:System;assembly=mscorlib"
                    xmlns:dxnav="http://schemas.devexpress.com/winfx/2008/xaml/navigation">

    <!--Work Log-->
    <SolidColorBrush x:Key="SAMColor" Color="#FF2AB7FF"></SolidColorBrush>
    <ImageSource x:Key="FemaleUserImage">/Emdep.Geos.Modules.SAM;component/Assets/Images/wFemaleUser.png</ImageSource>
    <ImageSource x:Key="MaleUserImage">/Emdep.Geos.Modules.SAM;component/Assets/Images/wMaleUser.png</ImageSource>
    <ImageSource x:Key="StartImage">/Emdep.Geos.Modules.SAM;component/Assets/Images/bStart.png</ImageSource>
    <ImageSource x:Key="StopImage">/Emdep.Geos.Modules.SAM;component/Assets/Images/bStop.png</ImageSource>
    
    <!--Pending Work Order--> 
    <Style TargetType="Image" x:Key="WorkLogScanButtonImageStyle">
        <Setter Property="Source" Value="/Emdep.Geos.Modules.SAM;component/Assets/Images/wScan.png" />
    </Style>
    <Style TargetType="Image" x:Key="SaveButtonImageStyle">
        <Setter Property="Source" Value="/Emdep.Geos.Modules.Crm;component/Assets/Images/wSave.png" />
    </Style>

    <!--Scan validation Order-->
    <Style TargetType="Image" x:Key="ScanvalidationButtonImageStyle">
        <Setter Property="Source" Value="/Emdep.Geos.Modules.SAM;component/Assets/Images/wScanvalidation.png" />
    </Style>
    <Style TargetType="Image" x:Key="QualityCertificationButtonImageStyle">
        <Setter Property="Source" Value="/Emdep.Geos.Modules.SAM;component/Assets/Images/wQualityCertification.png" />
    </Style>
    <ImageSource x:Key="DownloadAttachmentButtonImage">/Emdep.Geos.Modules.SAM;component/Assets/Images/w_download.png</ImageSource>

    <!--<Style TargetType="Image" x:Key="DownloadAttachmentButtonImageStyle">
        <Setter Property="Source" Value="/Emdep.Geos.Modules.SAM;component/Assets/Images/w_download.png" />
    </Style>-->
    <Style x:Key="SAMCalendarImageGlyphStyle" TargetType="dxwui:AppBarButton">
        <Setter Property="Glyph" Value="/Emdep.Geos.Modules.SAM;component/Assets/Images/wCalendar16x16.png" />
        <!--<Setter Property="AllowGlyphTheming" Value="True" />-->
    </Style>

    <!--End-->
    <Style x:Key="ToggleSwitchlForegroundStyle" TargetType="dxe:ToggleSwitch">
        <Setter Property="Foreground" Value="#E0E0E0" />
        <Setter Property="ToggleSwitchWidth" Value="{Binding RelativeSource={RelativeSource Self}, Path=Parent.ActualWidth}" />
        <Setter Property="ToggleSwitchHeight" Value="{Binding RelativeSource={RelativeSource Self}, Path=Parent.ActualHeight}" />
        <Setter Property="ContentPlacement" Value="Inside" />
        <Setter Property="VerticalContentAlignment" Value="Center" />
        <Setter Property="CheckedStateContent" Value="ON" />
        <Setter Property="UncheckedStateContent" Value="OFF" />
        <Setter Property="FontSize" Value="24"></Setter>
        <Setter Property="FontWeight" Value="Bold"></Setter>
    </Style>
    <!--[pramod.misal][GEOS2-5472][04-07-2024]-->
    <ImageSource x:Key="SAMpdfIconImageStyle">/Emdep.Geos.Modules.SAM;component/Assets/Images/pdf-icon.png</ImageSource>
    <!--[GEOS2-5473][pramod.misal][08.07.2024]-->
    <ImageSource x:Key="AddButtonImage">/Emdep.Geos.Modules.SAM;component/Assets/Images/wAdd.png</ImageSource>

    <!--[GEOS2-5474][pramod.misal][10.07.2024]-->
    <Style TargetType="Image" x:Key="DeleteStyle">
        <Setter Property="Source" Value="/Emdep.Geos.Modules.SAM;component/Assets/Images/wDelete.png" />
    </Style>

    <!--[pramod.misal][GEOS2-5888][20-08-2024]-->
    <ImageSource x:Key="SAMClockIconImageStyle">/Emdep.Geos.Modules.SAM;component/Assets/Images/RClock.png</ImageSource>
    <ImageSource x:Key="SAMHeaderClockIconImageStyle">/Emdep.Geos.Modules.SAM;component/Assets/Images/WClock.png</ImageSource>

    <!--<Style TargetType="Image" x:Key="SAMClockIconImageStyle">
        <Setter Property="Source" Value="/Emdep.Geos.Modules.SAM;component/Assets/Images/Clock.png" />
    </Style>-->
</ResourceDictionary>