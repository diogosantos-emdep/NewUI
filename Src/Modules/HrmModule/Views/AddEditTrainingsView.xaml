<dxwui:WinUIDialogWindow x:Class="Emdep.Geos.Modules.Hrm.Views.AddEditTrainingsView"
                        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                         xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                         xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
                         xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
                         xmlns:dx="http://schemas.devexpress.com/winfx/2008/xaml/core"
                         xmlns:local="clr-namespace:Emdep.Geos.Modules.Hrm.Views"
                         xmlns:dxwui="http://schemas.devexpress.com/winfx/2008/xaml/windowsui" 
                         xmlns:dxe="http://schemas.devexpress.com/winfx/2008/xaml/editors" 
                         xmlns:dxg="http://schemas.devexpress.com/winfx/2008/xaml/grid"
                         xmlns:dxmvvm="http://schemas.devexpress.com/winfx/2008/xaml/mvvm"
                         xmlns:Interactive="clr-namespace:System.Windows.Interactivity;assembly=System.Windows.Interactivity"
                         xmlns:Converter="clr-namespace:Emdep.Geos.UI.Converters;assembly=Emdep.Geos.UI.Converters"
                         xmlns:Validation ="clr-namespace:Emdep.Geos.UI.Validations;assembly=Emdep.Geos.UI.Validations"
                         xmlns:dxlc="http://schemas.devexpress.com/winfx/2008/xaml/layoutcontrol"
                         xmlns:dxdo="http://schemas.devexpress.com/winfx/2008/xaml/docking"
                         xmlns:UIHelper="clr-namespace:Emdep.Geos.UI.Helper;assembly=Emdep.Geos.UI.Helper"
                         xmlns:dxre="http://schemas.devexpress.com/winfx/2008/xaml/richedit"
                         xmlns:sysglb="clr-namespace:System.Globalization;assembly=mscorlib"
                         xmlns:sys="clr-namespace:System;assembly=mscorlib"
                         mc:Ignorable="d" 
                         d:DesignHeight="300" d:DesignWidth="300"
                         Style="{DynamicResource WinUIDialogWindowStyle}"
                         Template="{StaticResource Maximized_DXDialogWindow}">

    <dxwui:WinUIDialogWindow.Resources>
        <dxmvvm:BooleanToVisibilityConverter x:Key="BooleanToVisibilityConverter"/>
        <Converter:GroupboxBackroundColorConvertor x:Key="GroupboxBackroundColorConvertor" />
        <Converter:ForegroundColorConverter x:Key="ForegroundColorConverter" />
        <sys:String x:Key="ShortDateFormat">dd/MM/yyyy</sys:String>
        <Style x:Key="myGroupBoxStyle" TargetType="{x:Type dxlc:GroupBox}">
            <Setter Property="MaximizeElementVisibility" Value="Visible" />
            <Setter Property="Width" Value="280" />
            <Setter Property="Height" Value="145" />
            <Style.Triggers>
                <DataTrigger Binding="{Binding MaximizedElementPosition}" Value="Bottom">
                    <Setter Property="Width" Value="280" />
                    <Setter Property="Height" Value="27" />
                </DataTrigger>
                <DataTrigger Binding="{Binding MaximizedElementPosition}" Value="Top">
                    <Setter Property="Width" Value="280" />
                    <Setter Property="Height" Value="27" />
                </DataTrigger>
            </Style.Triggers>

        </Style>
        
        <Style x:Key="myGroupBoxStyle1" TargetType="{x:Type dxlc:GroupBox}">
            <Setter Property="MaximizeElementVisibility" Value="Visible" />
            <Setter Property="Padding" Value="0" />
            <Setter Property="Width" Value="280" />
            <Setter Property="Height" Value="27" />
        </Style>


    </dxwui:WinUIDialogWindow.Resources>

    <dxwui:WinUIDialogWindow.InputBindings>
        <KeyBinding Key="Escape"  Command="{Binding EscapeButtonCommand}"/>
    </dxwui:WinUIDialogWindow.InputBindings>
    <dxwui:WinUIDialogWindow.TitleStyle>
        <Style TargetType="ContentControl">
            <Setter Property="ContentTemplate">
                <Setter.Value>
                    <DataTemplate>
                        <Grid HorizontalAlignment="Stretch" VerticalAlignment="Stretch" ShowGridLines="False">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="300"/>
                                <ColumnDefinition Width="*"/>
                            </Grid.ColumnDefinitions>

                            <TextBlock Margin="10 0 0 0" Text="{Binding DataContext.WindowHeader,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged,RelativeSource={RelativeSource AncestorType=dxwui:WinUIDialogWindow}}"    Style="{DynamicResource CustomTitleStyle}" FontWeight="Bold" />

                            <Button Content=" " Height="40" Width="40" Grid.Column="1" Command="{Binding  DataContext.GenearateCertificateCommand, RelativeSource={RelativeSource AncestorType=dxwui:WinUIDialogWindow}}" 
                                    HorizontalAlignment="Right" Margin="0 0 20 0" CommandParameter="{Binding RelativeSource={RelativeSource AncestorType=Grid}}">
                                <Button.ToolTip>
                                    <TextBlock Text="Generate Certificate"/>
                                </Button.ToolTip>
                                <Button.ContentTemplate>
                                    <DataTemplate>
                                        <Image Style="{ DynamicResource ERFButtonImageStyle}" Height="24" Width="24" HorizontalAlignment="Right" Margin="-10" />
                                    </DataTemplate>
                                </Button.ContentTemplate>
                                <!--//Shubham[skadam] GEOS2-4911 Download of the Training Report 14 02 2024-->
                                <Button.Style>
                                    <Style TargetType="Button">
                                        <Style.Triggers>
                                            <DataTrigger Binding="{Binding DataContext.IsCertificateGenerationEnabled, RelativeSource={RelativeSource AncestorType=dxwui:WinUIDialogWindow}}" Value="False">
                                                <Setter Property="IsEnabled" Value="False"/>
                                            </DataTrigger>
                                            <DataTrigger Binding="{Binding DataContext.IsCertificateGenerationEnabled, RelativeSource={RelativeSource AncestorType=dxwui:WinUIDialogWindow}}" Value="True">
                                                <Setter Property="IsEnabled" Value="True"/>
                                            </DataTrigger>
                                            <!-- Add more DataTriggers as needed -->
                                        </Style.Triggers>
                                    </Style>
                                </Button.Style>
                            </Button>

                          
                        </Grid>
                    </DataTemplate>
                </Setter.Value>
            </Setter>
        </Style>
    </dxwui:WinUIDialogWindow.TitleStyle>
    <Grid Height="{Binding DialogHeight}" Width="{Binding DialogWidth}">

        <Grid.RowDefinitions>
            <RowDefinition Height="*"/>
            <RowDefinition Height="5"/>
            <RowDefinition Height="45"/>
        </Grid.RowDefinitions>

        <!--<Grid.RowDefinitions>
            <RowDefinition Height="30"/>
            <RowDefinition Height="5"/>
            <RowDefinition Height="30"/>
            <RowDefinition Height="5"/>
            <RowDefinition Height="60"/>
            <RowDefinition Height="5"/>
            <RowDefinition Height="30"/>
            <RowDefinition Height="5"/>
            <RowDefinition Height="30"/>
            <RowDefinition Height="5"/>
            <RowDefinition Height="45"/>
        </Grid.RowDefinitions>-->

        <!--<Grid.ColumnDefinitions>
           
            <ColumnDefinition Width="155"/>
            <ColumnDefinition/>
            <ColumnDefinition Width="350"/>
        </Grid.ColumnDefinitions>-->



        
            <dxlc:FlowLayoutControl x:Name="flowLayoutControl"
                                            Grid.Row="0"
                                            Orientation="Horizontal"
                                            ItemSpace="5" 
                                            LayerSpace="5" 
                                            Background="Transparent" 
                                            AllowMaximizedElementMoving="False" 
                                            BreakFlowToFit="False"
                                            MaximizedElement="{Binding DataContext.SelectedBoxIndex}"
                                            Padding="5"
                                            MaximizedElementPosition="Right"
                                            >

                <dxmvvm:Interaction.Behaviors>
                    <UIHelper:FlowLayoutControlMinimiseMaximiseStretch/>
                </dxmvvm:Interaction.Behaviors>

                <!--INFORMATION-->
                <dxlc:GroupBox x:Name="Info" MaximizeElementVisibility="Visible" Style="{StaticResource myGroupBoxStyle}">
                    <dxlc:GroupBox.HeaderTemplate>
                        <DataTemplate>
                            <DockPanel Margin="-5,0,0,0">
                                <TextBlock VerticalAlignment="Center"
                                                   FontWeight="Bold"
                                                   Text="{DynamicResource INFORMATION}" />

                            <dxe:TextEdit Text="{DynamicResource INFORMATION}" FontWeight="Bold" HorizontalAlignment="Right" VerticalAlignment="Center"  ShowBorder="False" IsReadOnly="True">
                                    <Binding Path="DataContext.InformationError" RelativeSource="{RelativeSource AncestorType=dxlc:GroupBox}" UpdateSourceTrigger="PropertyChanged" Mode="TwoWay" ValidatesOnDataErrors="True">
                                        <Binding.ValidationRules>
                                            <Validation:AddEditTrainingValidation FieldName="InformationError" ValidationStep="UpdatedValue" ValidatesOnTargetUpdated="True" />
                                        </Binding.ValidationRules>
                                    </Binding>
                                </dxe:TextEdit>
                            </DockPanel>
                        </DataTemplate>
                    </dxlc:GroupBox.HeaderTemplate>

                    <dxlc:GroupBox.ContentTemplate>
                        <DataTemplate>
                            <Grid>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="*" />
                                </Grid.ColumnDefinitions>
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="*" />
                                    <RowDefinition Height="*" />
                                    <RowDefinition Height="*" />
                                    <RowDefinition Height="*" />
                                </Grid.RowDefinitions>
                                
                                <TextBlock Grid.Row="0"
                                       Grid.Column="0"
                                       Margin="2" VerticalAlignment="Stretch" HorizontalAlignment="Left"
                                       Text="{DynamicResource LanguageName}"
                                       FontWeight="Bold"
                                       Padding="2" />
                                <TextBlock Margin="2" Padding="2" Text="{Binding DataContext.Name, RelativeSource={RelativeSource AncestorType={x:Type dxlc:GroupBox}}}" VerticalAlignment="Stretch" HorizontalAlignment="Left" 
                                           Grid.Row="0" Grid.Column="1" TextWrapping="WrapWithOverflow" TextTrimming="CharacterEllipsis"
                                           ToolTip="{Binding DataContext.Name, RelativeSource={RelativeSource AncestorType={x:Type dxlc:GroupBox}}}"/>
                                
                                
                                <TextBlock Grid.Row="1"
                                       Grid.Column="0"
                                       Margin="2" VerticalAlignment="Stretch" HorizontalAlignment="Left"
                                       Text="{DynamicResource TrainingExpectedDateWithColon}"
                                       FontWeight="Bold" 
                                       Padding="2" TextWrapping="WrapWithOverflow" TextTrimming="CharacterEllipsis"/>
                                <!--<TextBlock Margin="2" Padding="2" Text="{Binding DataContext.ExpectedDate,StringFormat=d, RelativeSource={RelativeSource AncestorType={x:Type dxlc:GroupBox}}}" VerticalAlignment="Stretch" HorizontalAlignment="Left"   Grid.Row="1" Grid.Column="1" TextWrapping="WrapWithOverflow" TextTrimming="CharacterEllipsis"
                                                   ToolTip="{Binding DataContext.ExpectedDate,StringFormat=d, RelativeSource={RelativeSource AncestorType={x:Type dxlc:GroupBox}}}"/>-->
                                <dxe:TextEdit Grid.Row="1" Grid.Column="1" Margin="2" Padding="2" 
                                              Text="{Binding DataContext.ExpectedDate, RelativeSource={RelativeSource AncestorType={x:Type dxlc:GroupBox}}}"
                                              Style="{DynamicResource TextEditStyle}"
                                              Mask="{StaticResource ShortDateFormat}" MaskType="DateTime" MaskUseAsDisplayFormat="True" IsPrintingMode="True" />
                                <TextBlock Grid.Row="2"
                                       Grid.Column="0"
                                       Margin="2" VerticalAlignment="Stretch" HorizontalAlignment="Left"
                                       Text="{DynamicResource TrainingFinalizationDateWithColon}"
                                       FontWeight="Bold"
                                       Padding="2" />
                                <dxe:TextEdit Grid.Row="2" Grid.Column="1" Margin="2" Padding="2" 
                                              Text="{Binding DataContext.FinalizationDate, RelativeSource={RelativeSource AncestorType={x:Type dxlc:GroupBox}}}"
                                              Style="{DynamicResource TextEditStyle}"
                                              Mask="{StaticResource ShortDateFormat}" MaskType="DateTime" MaskUseAsDisplayFormat="True" IsPrintingMode="True" />
                                <!--<TextBlock Margin="2" Padding="2" Text="{Binding DataContext.FinalizationDate,StringFormat=d,RelativeSource={RelativeSource AncestorType={x:Type dxlc:GroupBox}}}" VerticalAlignment="Stretch" HorizontalAlignment="Left" 
                                           Grid.Row="2" Grid.Column="1" TextWrapping="WrapWithOverflow" TextTrimming="CharacterEllipsis"/>-->

                                <TextBlock Grid.Row="3"
                                       Grid.Column="0"
                                       Margin="2" VerticalAlignment="Stretch" HorizontalAlignment="Left"
                                       Text="{DynamicResource TrainingDurationWithColon}"
                                       FontWeight="Bold"
                                       Padding="2" />
                                <TextBlock Margin="2" Padding="2" Text="{Binding DataContext.Duration, RelativeSource={RelativeSource AncestorType={x:Type dxlc:GroupBox}}}" VerticalAlignment="Stretch" HorizontalAlignment="Left" 
                                           Grid.Row="3" Grid.Column="1" TextWrapping="WrapWithOverflow" TextTrimming="CharacterEllipsis"/>
                            </Grid>
                        </DataTemplate>
                    </dxlc:GroupBox.ContentTemplate>

                    <dxlc:GroupBox.MaximizedContentTemplate>
                        <DataTemplate>
                            <ScrollViewer ScrollViewer.VerticalScrollBarVisibility="Auto">
                                <Grid  MaxHeight="1500" MaxWidth="2000" ShowGridLines="False" VerticalAlignment="Stretch" HorizontalAlignment="Stretch">
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="150"/>
                                        <RowDefinition Height="5"/>
                                        <RowDefinition Height="*"/>
                                    </Grid.RowDefinitions>

                                    <!--Information-->
                                    <Grid  ShowGridLines="False" Grid.Row="0">

                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="30" />
                                            <RowDefinition Height="5" />
                                            <RowDefinition Height="30" />
                                            <RowDefinition Height="5" />
                                            <RowDefinition Height="30" />
                                            <RowDefinition Height="5" />
                                            <RowDefinition Height="30" />
                                            <RowDefinition Height="5" />
                                        </Grid.RowDefinitions>

                                        <Grid.ColumnDefinitions>
                                            <!--Code-->
                                            <ColumnDefinition Width="Auto"/>
                                            <ColumnDefinition Width="5"/>
                                            <ColumnDefinition Width="75"/>
                                            <ColumnDefinition Width="5"/>
                                            <!--Name-->
                                            <ColumnDefinition Width="Auto"/>
                                            <ColumnDefinition Width="5"/>
                                            <ColumnDefinition Width="15*"/>
                                            <ColumnDefinition Width="5"/>
                                            <!--Expected Date-->
                                            <ColumnDefinition Width="Auto"/>
                                            <ColumnDefinition Width="5"/>
                                            <ColumnDefinition Width="15*"/>
                                            <ColumnDefinition Width="5"/>
                                            <!--Finalization Date-->
                                            <ColumnDefinition Width="Auto"/>
                                            <ColumnDefinition Width="5"/>
                                            <ColumnDefinition Width="15*"/>
                                            <ColumnDefinition Width="5"/>
                                            <!--Duration-->
                                            <ColumnDefinition Width="Auto"/>
                                            <ColumnDefinition Width="5"/>
                                            <ColumnDefinition Width="15*"/>
                                            <ColumnDefinition Width="5"/>
                                            <!--Status--><!--
                                            <ColumnDefinition Width="Auto"/>
                                            <ColumnDefinition Width="5"/>
                                            <ColumnDefinition Width="15*"/>
                                            <ColumnDefinition Width="5"/>-->

                                        </Grid.ColumnDefinitions>

                                        <!--<Code>-->
                                        <TextBlock Grid.Row="0" Grid.Column="0" 
                                                           VerticalAlignment="Center"
                                                           Text="{DynamicResource GridColCode}" 
                                                           Style="{StaticResource TextBlock14Style}" />
                                        <dxe:TextEdit Grid.Row="0" Grid.Column="2"
                                                              HorizontalAlignment="Stretch"
                                                              EditValue="{Binding DataContext.SelectedProfessionalTrainingCode, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged,RelativeSource={RelativeSource AncestorType=dxlc:GroupBox}}"  
                                                              ShowNullText="True"
                                                              ShowNullTextForEmptyValue="True"
                                                              IsReadOnly="True"
                                                              IsEnabled="False">
                                        </dxe:TextEdit>


                                        <!--Name-->
                                        <TextBlock Grid.Row="0" Grid.Column="4"  Text="{DynamicResource Name}" Style="{StaticResource TextBlock14Style}" VerticalAlignment="Center" />
                                        <dxe:TextEdit Grid.Row="0" Grid.Column="6" TabIndex="0" 
                                                    Text="{Binding DataContext.Name,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged,RelativeSource={RelativeSource AncestorType=dxlc:GroupBox}}" 
                                                                  VerticalContentAlignment="Top" NullText="Name" HorizontalAlignment="Stretch"
                                                       ToolTip="{Binding DataContext.Name, RelativeSource={RelativeSource AncestorType={x:Type dxlc:GroupBox}}}">
                                            
                                            <dxe:TextEdit.EditValue>
                                                <Binding Path="DataContext.Name" RelativeSource="{RelativeSource AncestorType=dxwui:WinUIDialogWindow}" UpdateSourceTrigger="PropertyChanged" Mode="TwoWay" 
                                                                     ValidatesOnDataErrors="True">
                                                    <Binding.ValidationRules>
                                                        <Validation:AddEditTrainingValidation FieldName="Name" ValidationStep="UpdatedValue" ValidatesOnTargetUpdated="True"/>
                                                    </Binding.ValidationRules>
                                                </Binding>
                                            </dxe:TextEdit.EditValue>
                                            <dxmvvm:Interaction.Behaviors>
                                                <dxmvvm:EventToCommand 
                                                                EventName="EditValueChanged" Command="{Binding DataContext.ChangeNameCommand, Mode=TwoWay,UpdateSourceTrigger=PropertyChanged,RelativeSource={RelativeSource AncestorType={x:Type dxlc:GroupBox}}}"/>
                                            </dxmvvm:Interaction.Behaviors>
                                        </dxe:TextEdit>


                                        <!--<Expected Date>-->
                                        <TextBlock Grid.Row="0" Grid.Column="8" Text="{DynamicResource TrainingExpectedDate}"  HorizontalAlignment="Left" VerticalAlignment="Center" Style="{StaticResource TextBlock14Style}"/>
                                        <dxe:DateEdit Grid.Row="0" Grid.Column="10" HorizontalAlignment="Stretch" TabIndex="2">
                                            <dxe:DateEdit.EditValue>
                                                <Binding Path="DataContext.ExpectedDate" RelativeSource="{RelativeSource AncestorType=dxlc:GroupBox}" UpdateSourceTrigger="PropertyChanged" Mode="TwoWay">
                                                </Binding>
                                            </dxe:DateEdit.EditValue>
                                        </dxe:DateEdit>

                                        <!--Finalization Date-->
                                        <TextBlock Grid.Row="0" Grid.Column="12" Text="{DynamicResource TrainingFinalizationDate}" VerticalAlignment="Center"/>
                                        <dxe:DateEdit Grid.Row="0" Grid.Column="14" HorizontalAlignment="Stretch" IsReadOnly="False">
                                            <dxe:DateEdit.EditValue>
                                                <Binding Path="DataContext.FinalizationDate" RelativeSource="{RelativeSource AncestorType=dxlc:GroupBox}" UpdateSourceTrigger="PropertyChanged" Mode="TwoWay">
                                                </Binding>
                                            </dxe:DateEdit.EditValue>
                                        </dxe:DateEdit>

                                        <!--Duration-->
                                        <TextBlock Grid.Row="0" Grid.Column="16" Style="{StaticResource TextBlock14Style}" HorizontalAlignment="Left" Text="{DynamicResource TrainingDuration}" VerticalAlignment="Center" />
                                        <dxe:TextEdit Grid.Column="18" IsReadOnly="True"
                                                                    Name="CbeSaleCurrency" 
                                                                    HorizontalAlignment="Stretch"
                                                                    VerticalAlignment="Stretch"
                                                      Text="{Binding DataContext.Duration,UpdateSourceTrigger=PropertyChanged,Mode=TwoWay,RelativeSource={RelativeSource AncestorType=dxwui:WinUIDialogWindow}}">
                                        </dxe:TextEdit>

                                        <!--Status-->
                                        <TextBlock Grid.Row="2" Grid.Column="16" Text="{DynamicResource TrainingStatus}" Style="{StaticResource TextBlock14Style}" VerticalAlignment="Center"/>
                                        <dxe:ComboBoxEdit Grid.Row="2" Grid.Column="18" 
                                                      HorizontalAlignment="Stretch" 
                                                      ItemsSource="{Binding DataContext.StatusList,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged,RelativeSource={RelativeSource AncestorType=dxlc:GroupBox}}"
                                                      DisplayMember="Value" ValueMember="IdLookupValue"
                                                      AutoComplete="True" ImmediatePopup="True" IncrementalFiltering="True"
                                                      SelectedItem="{Binding DataContext.SelectedStatus, Mode=TwoWay,UpdateSourceTrigger=PropertyChanged,RelativeSource={RelativeSource AncestorType=dxlc:GroupBox}}">
                                        </dxe:ComboBoxEdit>

                                        <!--Description-->
                                        <TextBlock Grid.Row="2" Grid.Column="0" Text="{DynamicResource Description}" Style="{StaticResource TextBlock14Style}" VerticalAlignment="Center" HorizontalAlignment="Left" />
                                        <dxe:TextEdit Grid.Row="2" Grid.Column="2" Grid.RowSpan="3" TabIndex="1" 
                                                                  Grid.ColumnSpan="5" Text="{Binding DataContext.Description,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged,RelativeSource={RelativeSource AncestorType=dxlc:GroupBox}}" 
                                                               VerticalContentAlignment="Top" TextWrapping="Wrap"  HorizontalAlignment="Stretch"
                                                  ScrollViewer.VerticalScrollBarVisibility="Auto" ScrollViewer.HorizontalScrollBarVisibility="Disabled" 
                                                                  AcceptsReturn="True" NullText="Description"
                                                       ToolTip="{Binding DataContext.Description, RelativeSource={RelativeSource AncestorType={x:Type dxlc:GroupBox}}}">
                                        
                                            <dxmvvm:Interaction.Behaviors>
                                                <dxmvvm:EventToCommand 
                                                            EventName="EditValueChanged" Command="{Binding DataContext.ChangeDescriptionCommand,RelativeSource={RelativeSource AncestorType=dxlc:GroupBox}}"/>
                                            </dxmvvm:Interaction.Behaviors>
                                        </dxe:TextEdit>

                                        <!--Type-->
                                        <TextBlock Grid.Row="4" Grid.Column="8"  Text="{DynamicResource TrainingType}" Style="{StaticResource TextBlock14Style}" VerticalAlignment="Center" />
                                        <dxe:ComboBoxEdit Grid.Row="4" Grid.Column="10" TabIndex="3"
                                                      HorizontalAlignment="Stretch" 
                                                      ItemsSource="{Binding DataContext.TypeList,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged,RelativeSource={RelativeSource AncestorType=dxlc:GroupBox}}"
                                                      DisplayMember="Value" ValueMember="IdLookupValue"
                                                      AutoComplete="True" ImmediatePopup="True" IncrementalFiltering="True"
                                                      SelectedItem="{Binding DataContext.SelectedType, Mode=TwoWay,UpdateSourceTrigger=PropertyChanged,RelativeSource={RelativeSource AncestorType=dxlc:GroupBox}}">
                                        </dxe:ComboBoxEdit>

                                        <!--Acceptance-->
                                        <TextBlock Grid.Row="2" Grid.Column="8" Style="{StaticResource     TextBlock14Style}" HorizontalAlignment="Left" Text="{DynamicResource TrainingAcceptance}" VerticalAlignment="Center" />
                                        <dxe:ComboBoxEdit  Grid.Column="10" Grid.Row="2" 
                                                                    Name="CbeTrainingAcceptance" 
                                                                    Style="{DynamicResource ComboBoxEditStyle}" 
                                                                    HorizontalAlignment="Stretch"
                                                                    VerticalAlignment="Stretch"
                                                                    IsTextEditable="False"
                                                                    DisplayMember="Value" ValueMember="IdLookupValue"
                                                                    ItemsSource="{Binding DataContext.AcceptanceList,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged,RelativeSource={RelativeSource AncestorType=dxlc:GroupBox}}"
                                                                   >
                                            <dxe:ComboBoxEdit.SelectedItem>
                                                <Binding Path="DataContext.SelectedAcceptance"  Mode="TwoWay" UpdateSourceTrigger="PropertyChanged" RelativeSource="{RelativeSource AncestorType=dxlc:GroupBox}">
                                                </Binding>
                                            </dxe:ComboBoxEdit.SelectedItem>

                                        </dxe:ComboBoxEdit>

                                        <!--Acceptance Value-->
                                        <TextBlock Grid.Row="2" Grid.Column="12"  Text="{DynamicResource TrainingAcceptanceValue}" VerticalAlignment="Center" HorizontalAlignment="Right" />
                                        <dxe:SpinEdit Grid.Row="2" Grid.Column="14" TabIndex="5"
                                                       HorizontalAlignment="Stretch" IsReadOnly="False"
                                                       MinValue="0.01" MaxValue="10.00"
                                                      AllowRoundOutOfRangeValue="True"
                                           
                                                      Value="{Binding DataContext.AcceptanceValue, UpdateSourceTrigger=PropertyChanged,Mode=TwoWay,RelativeSource={RelativeSource AncestorType=dxwui:WinUIDialogWindow}}"
                                                      IsEnabled="{Binding DataContext.IsAcceptanceValueEnable , Mode=TwoWay,UpdateSourceTrigger=PropertyChanged,RelativeSource={RelativeSource AncestorType=dxlc:GroupBox} }"
                                                                >
                                        </dxe:SpinEdit>
                                        
                                        <!--Responsible-->
                                        <TextBlock Grid.Row="4" Grid.Column="12" Text="{DynamicResource TrainingResponsible}" 
                                                               HorizontalAlignment="Left" VerticalAlignment="Center" 
                                                               Style="{StaticResource TextBlock14Style}" Margin="0,0,0,5"/>
                                        <dxe:ComboBoxEdit Grid.Row="4" Grid.Column="14" Grid.ColumnSpan="5"
                                                      HorizontalAlignment="Stretch" 
                                                      ApplyItemTemplateToSelectedItem="True"
                                                      IsSynchronizedWithCurrentItem="True"
                                                      AllowUpdateTwoWayBoundPropertiesOnSynchronization="True"
                                                      ValueMember="IdEmployee"  DisplayMember="FullName"  
                                                      ItemsSource="{Binding DataContext.ResponsibleList,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged,RelativeSource={RelativeSource AncestorType=dxlc:GroupBox}}"
                                                      AutoComplete="True" ImmediatePopup="True" IncrementalFiltering="True"
                                                     >
                                            <dxe:ComboBoxEdit.SelectedItem>
                                                <Binding Path="DataContext.SelectedResponsible"  Mode="TwoWay" UpdateSourceTrigger="PropertyChanged" RelativeSource="{RelativeSource AncestorType=dxlc:GroupBox}" ValidatesOnDataErrors="True">
                                                    <Binding.ValidationRules>
                                                        <Validation:AddEditTrainingValidation FieldName="SelectedResponsible" ValidationStep="UpdatedValue" />
                                                    </Binding.ValidationRules>
                                                </Binding>
                                            </dxe:ComboBoxEdit.SelectedItem>

                                            <dxe:ComboBoxEdit.ItemContainerStyle>
                                                <Style TargetType="dxe:ComboBoxEditItem">
                                                    <Setter Property="IsEnabled" Value="{Binding IsNoLongerResponsible}" />
                                                </Style>
                                            </dxe:ComboBoxEdit.ItemContainerStyle>
                                        </dxe:ComboBoxEdit>


                                        <!--Trainer-->
                                        <TextBlock Grid.Row="6" Grid.Column="0" Text="{DynamicResource TrainingTrainer}" VerticalAlignment="Center"/>
                                        <dxe:ComboBoxEdit Grid.Row="6" Grid.Column="2" Grid.ColumnSpan="5" TabIndex="6"
                                                      HorizontalAlignment="Stretch" 
                                                      ApplyItemTemplateToSelectedItem="True"
                                                      IsSynchronizedWithCurrentItem="True"
                                                     AllowUpdateTwoWayBoundPropertiesOnSynchronization="True"
                                                      ItemsSource="{Binding DataContext.TrainerList,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged,RelativeSource={RelativeSource AncestorType=dxlc:GroupBox}}"
                                                       ValueMember="IdEmployee"  DisplayMember="FullName"  
                                                       AutoComplete="True" ImmediatePopup="True" IncrementalFiltering="True"
                                                      >

                                            <dxe:ComboBoxEdit.SelectedItem>
                                                <Binding Path="DataContext.SelectedTrainer"  Mode="TwoWay" UpdateSourceTrigger="PropertyChanged" RelativeSource="{RelativeSource AncestorType=dxlc:GroupBox}">
                                                                                                    </Binding>
                                            </dxe:ComboBoxEdit.SelectedItem>
                                            <dxe:ComboBoxEdit.ItemContainerStyle>
                                                
                                                <Style TargetType="dxe:ComboBoxEditItem">
                                                    <Setter Property="IsEnabled" Value="{Binding IsnolongerTrainer }" />
                                                </Style>
                                                
                                            </dxe:ComboBoxEdit.ItemContainerStyle>
                                        </dxe:ComboBoxEdit>

                                        <!--External Trainer-->
                                        <TextBlock Grid.Row="6" Grid.Column="8" Text="{DynamicResource TrainingExternalTrainer}" 
                                                               HorizontalAlignment="Left" VerticalAlignment="Center" 
                                                               Style="{StaticResource TextBlock14Style}" Margin="0,0,0,5"/>
                                        <dxe:TextEdit Grid.Row="6" Grid.Column="10"  Grid.ColumnSpan="5" 
                                                                  TabIndex="7" HorizontalAlignment="Stretch"
                                                                  Text="{Binding DataContext.ExternalTrainer,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged,RelativeSource={RelativeSource AncestorType=dxlc:GroupBox}}" VerticalContentAlignment="Top" TextWrapping="Wrap" 
                                                  ScrollViewer.VerticalScrollBarVisibility="Auto" ScrollViewer.HorizontalScrollBarVisibility="Disabled" AcceptsReturn="True"
                                                      IsEnabled="{Binding DataContext.IsEntityExtTraineeEnable , Mode=TwoWay,UpdateSourceTrigger=PropertyChanged,RelativeSource={RelativeSource AncestorType=dxlc:GroupBox} }"
                                                                 >
                                            <dxe:TextEdit.EditValue>
                                                <Binding Path="DataContext.ExternalTrainer" RelativeSource="{RelativeSource AncestorType=dxwui:WinUIDialogWindow}" UpdateSourceTrigger="PropertyChanged" Mode="TwoWay" 
                                                                     ValidatesOnDataErrors="True">
                                                    <Binding.ValidationRules>
                                                        <Validation:AddEditTrainingValidation FieldName="ExternalTrainer" ValidationStep="UpdatedValue" ValidatesOnTargetUpdated="True"/>
                                                    </Binding.ValidationRules>
                                                </Binding>
                                            </dxe:TextEdit.EditValue>
                                        </dxe:TextEdit>


                                        <!--<Entity>-->
                                        <TextBlock Grid.Row="6" Grid.Column="16" Text="{DynamicResource TrainingEntity}" VerticalAlignment="Center"/>
                                        <dxe:TextEdit TabIndex="8" Grid.Row="6" Grid.Column="18" 
