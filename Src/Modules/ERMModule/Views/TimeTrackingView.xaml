                                >

                    <dxg:GridColumn.CellTemplate>
                        <DataTemplate >
                            <!--<dxe:TextEdit Name="PART_Editor" Mask="hh:mm:ss" MaskType="TimeSpan" MaskUseAsDisplayFormat="True" MaskShowPlaceHolders="False" ValidateOnTextInput="True" HorizontalAlignment="Right" ShowBorder="False"
                                  >
                            </dxe:TextEdit>-->
                            <!--[GEOS2-5519][Rupali Sarode][12-04-2024]-->
                            <TextBlock HorizontalAlignment="Right">
                                <Hyperlink x:Name="Hyperlink_1"  Command="{Binding DataContext.VerifyTheSamplePopupWindowCommand, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged, RelativeSource={RelativeSource AncestorType=UserControl}}"
                                       CommandParameter="{Binding ElementName=TimeTrackingTableView}" >
                                    <dxe:TextEdit Name="PART_Editor" Mask="dd.hh:mm:ss" 
                                                 MaskType="TimeSpan" MaskUseAsDisplayFormat="True" 
                                                 MaskShowPlaceHolders="False" 
                                                 ValidateOnTextInput="True" 
                                                 HorizontalAlignment="Right" 
                                                 ShowBorder="False" 
                                                  ToolTip="Click to see the expected time details"
                              IsPrintingMode="True" 
                              Text="{Binding RowData.Row.ColumnFieldName}" />
                                </Hyperlink>
                            </TextBlock>
                            <DataTemplate.Triggers>
                                <Trigger Property="dxg:DataViewBase.IsFocusedCell" Value="True">
                                    <Setter TargetName="Hyperlink_1" Property="Foreground" Value="{DynamicResource ERMColorMain}" />
                                </Trigger>
                                <Trigger Property="IsMouseOver" Value="True">
