                                        <Label Content="{DynamicResource AddEditSODExpiryDateWithColon}" Grid.Row="2" Grid.Column="0" Margin="2" Padding="2"  FontWeight="Bold"/>
                                        <TextBlock Grid.Row="2" Grid.Column="1" Margin="2" Padding="2"  Text="{Binding DataContext.ExpiryDate, RelativeSource={RelativeSource AncestorType=dxlc:GroupBox},StringFormat=d,ConverterCulture={x:Static sysglb:CultureInfo.CurrentCulture}}"/>

                                    </Grid>
                                </DataTemplate>
                            </dxlc:GroupBox.ContentTemplate>

