        <UIHelper:PlantOperationPlanningColumnTemplateSelector x:Key="newColumnTemplateSelector"
                                                                    DefaultTemplate="{StaticResource DefaultColunmTemplate}"
                                                                    CalenderWeek="{StaticResource CalenderWeekTemplate}"
                                                                    Employee="{StaticResource EmployeeTemplate}"
                                                                    HRExpected="{StaticResource HRExpectedTemplate}"
                                                                    HRPlan="{StaticResource HRPlanTemplate}"
                                                                    JobDescription="{StaticResource JobDescriptionTemplate}"
                                                                    Real="{StaticResource RealTemplate}"
                                                                    TimeType="{StaticResource TimeTypeTemplate}"
                                                                    HiddenTemplate="{StaticResource HiddenColumnTemplate}"
                                                               Plant="{StaticResource PlantTemplate}"
                                                                   />
        <!--JobDescription="{StaticResource JobDescriptionTemplate}"-->
        <DataTemplate x:Key="BandTemplate">
            <ContentControl>
                <dxg:GridControlBand UIHelper:NameBinder.Name="{Binding BandName,Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"  Header="{Binding BandHeader,Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" 
