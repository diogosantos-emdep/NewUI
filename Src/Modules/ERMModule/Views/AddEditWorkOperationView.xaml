<dxwui:WinUIDialogWindow x:Class="Emdep.Geos.Modules.ERM.Views.AddEditWorkOperationView"
                         xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                         xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                         xmlns:sysglb="clr-namespace:System.Globalization;assembly=mscorlib"
                         xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
                         xmlns:dx="http://schemas.devexpress.com/winfx/2008/xaml/core"
                         xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                         xmlns:local="clr-namespace:Emdep.Geos.Modules.ERM.Views"
                         mc:Ignorable="d"
                         xmlns:dxwui="http://schemas.devexpress.com/winfx/2008/xaml/windowsui"
                         xmlns:dxg="http://schemas.devexpress.com/winfx/2008/xaml/grid"
                         xmlns:Validation ="clr-namespace:Emdep.Geos.UI.Validations;assembly=Emdep.Geos.UI.Validations"
                         xmlns:dxmvvm="http://schemas.devexpress.com/winfx/2008/xaml/mvvm"
                         xmlns:dxe="http://schemas.devexpress.com/winfx/2008/xaml/editors" 
                          xmlns:dxeh="http://schemas.devexpress.com/winfx/2008/xaml/editors/internal"
                         xmlns:Converters="clr-namespace:Emdep.Geos.UI.Converters;assembly=Emdep.Geos.UI.Converters"
                          xmlns:ve="http://schemas.devexpress.com/winfx/2008/xaml/docking/visualelements"
                          xmlns:UIHelper="clr-namespace:Emdep.Geos.UI.Helper;assembly=Emdep.Geos.UI.Helper"
                          xmlns:dxlc="http://schemas.devexpress.com/winfx/2008/xaml/layoutcontrol"
                         xmlns:ViewModel="clr-namespace:Emdep.Geos.Modules.ERM.ViewModels"
                         xmlns:dxet="http://schemas.devexpress.com/winfx/2008/xaml/editors/themekeys"
                         xmlns:GeosApplication="clr-namespace:Emdep.Geos.UI.Common;assembly=Emdep.Geos.UI.Common"
                         
                         Height="300" Width="300"
                         Style="{DynamicResource WinUIDialogWindowStyle}"
                         Title="{Binding WindowHeader}"
                         TitleStyle="{DynamicResource WinUIDialogWindowTitleStyle}"
                          Template="{StaticResource Maximized_DXDialogWindow}"
                         >

    <dxwui:WinUIDialogWindow.Resources>


        <Style x:Key="img" TargetType="Image">
            <Setter Property="Width" Value="24" />
            <Setter Property="Height" Value="24" />
            <Setter Property="Margin" Value="15,0,0,0"/>
            <Setter Property="HorizontalAlignment" Value="Left"/>

        </Style>

        <Style x:Key="myGroupBoxStyle" TargetType="{x:Type dxlc:GroupBox}">
            <Setter Property="MaximizeElementVisibility" Value="Visible" />
            <Setter Property="Width" Value="280" />
            <Setter Property="Height" Value="145" />
            <Style.Triggers>
                <DataTrigger Binding="{Binding MaximizedElementPosition}" Value="Bottom">
                    <Setter Property="Width" Value="280" />
                    <Setter Property="Height" Value="27" />
                </DataTrigger>
                <DataTrigger Binding="{Binding MaximizedElementPosition}" Value="Top">
                    <Setter Property="Width" Value="280" />
                    <Setter Property="Height" Value="27" />
                </DataTrigger>
            </Style.Triggers>

        </Style>
        <Style TargetType="{x:Type ve:AutoHideTray}">
            <Setter Property="ItemsPanel">
                <Setter.Value>
                    <ItemsPanelTemplate>
                        <ve:psvStackPanel HorizontalAlignment="Right" />
                    </ItemsPanelTemplate>
                </Setter.Value>
            </Setter>
        </Style>
    </dxwui:WinUIDialogWindow.Resources>

    <dxwui:WinUIDialogWindow.InputBindings>
        <KeyBinding Key="Escape" Command="{Binding CancelButtonCommand}"/>
    </dxwui:WinUIDialogWindow.InputBindings>
    <!--Start GEOS2-3880 - change for AddLog-->
    <!--<ScrollViewer>-->
    <ScrollViewer  VerticalScrollBarVisibility="Auto">
        <Grid ShowGridLines="False" Height="{Binding WindowHeight,UpdateSourceTrigger=PropertyChanged,Mode=TwoWay}">
            <!--Height="{Binding WindowHeight,UpdateSourceTrigger=PropertyChanged,Mode=TwoWay}"-->
            <Grid.RowDefinitions>
                <!--<RowDefinition Height="0"></RowDefinition> Gulab lakade-->
                <RowDefinition Height="*" />
                <RowDefinition Height="*" />
                <!--<RowDefinition Height="60" />-->
            </Grid.RowDefinitions>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="300" />
                <ColumnDefinition Width="{Binding ColumnMainWidth,Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" />

            </Grid.ColumnDefinitions>
            <dxlc:FlowLayoutControl ItemSpace="12" 
                                LayerSpace="12" 
                                Background="Transparent" 
                                Orientation="Vertical" 
                                Padding="0,0,0,0" 
                                StretchContent="True"
                                AllowMaximizedElementMoving="False" 
                                Grid.Row="0" 
                                Grid.Column="0" 
                                Grid.ColumnSpan="2"
                                BreakFlowToFit="True" 
                                 MaximizedElementPosition="{Binding MaximizedElementPosition}">
                <dxmvvm:Interaction.Behaviors>
                    <UIHelper:FlowLayoutControlForMinimiseMaximise />
                </dxmvvm:Interaction.Behaviors>
                <!--Information-->
                <dxlc:GroupBox x:Name="Information"  MaximizeElementVisibility="Visible" Style="{StaticResource myGroupBoxStyle}">
                    <dxlc:GroupBox.HeaderTemplate>
                        <DataTemplate>
                            <DockPanel Margin="-5,0,0,0">
                                <TextBlock Margin="0,0,0,0"
                                    VerticalAlignment="Center"
                                    FontWeight="Bold"
                                    Text="{DynamicResource EditActivityInformation}" />

                                <dxe:TextEdit Text="{DynamicResource EditActivityInformation}" FontWeight="Bold" HorizontalAlignment="Right" VerticalAlignment="Center" ShowBorder="False" IsReadOnly="True">
                                    <Binding Path="DataContext.EditActivityError" RelativeSource="{RelativeSource FindAncestor, AncestorType=dxlc:GroupBox}" UpdateSourceTrigger="PropertyChanged" Mode="TwoWay" ValidatesOnDataErrors="True">
                                        <Binding.ValidationRules>
                                            <Validation:ActivityValidation FieldName="EditActivityError" ValidationStep="UpdatedValue" />
                                        </Binding.ValidationRules>
                                    </Binding>
                                </dxe:TextEdit>
                            </DockPanel>
                        </DataTemplate>
                    </dxlc:GroupBox.HeaderTemplate>
                    <dxlc:GroupBox.ContentTemplate>
                        <DataTemplate>
                            <Grid >
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="Auto" />
                                    <RowDefinition Height="Auto" />
                                    <RowDefinition Height="Auto" />
                                    <RowDefinition Height="Auto" />
                                    <RowDefinition Height="0" />
                                    <RowDefinition Height="Auto" />
                                </Grid.RowDefinitions>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="18" />
                                    <ColumnDefinition Width="*" />
                                </Grid.ColumnDefinitions>

                                <!--WorkStage-->
                                <TextBlock Grid.Row="0" Grid.Column="0" FontWeight="Bold" Text="{DynamicResource CurrentGridColWorkStage}" Style="{StaticResource TextBlock14Style}" VerticalAlignment="Center" HorizontalAlignment="Left"  />

                                <TextBlock Grid.Row="0" Grid.Column="1"  Grid.ColumnSpan="2"  
                                           Text="{Binding DataContext.Tempworkstagename, RelativeSource={RelativeSource AncestorType=dxlc:GroupBox}}" 
                                           ToolTip="{Binding DataContext.Tempworkstagename, RelativeSource={RelativeSource AncestorType=dxlc:GroupBox}}"/>

                                <!--Name-->
                                <TextBlock Grid.Row="1" Grid.Column="0" FontWeight="Bold" Text="{DynamicResource CurrentGridColName}" Style="{StaticResource TextBlock14Style}" VerticalAlignment="Center"  HorizontalAlignment="Left" />
                                <TextBlock Grid.Row="1" Grid.Column="1"  Grid.ColumnSpan="2" 
                                           Text="{Binding DataContext.Name, RelativeSource={RelativeSource AncestorType=dxlc:GroupBox}}" 
                                           ToolTip="{Binding DataContext.Name, RelativeSource={RelativeSource AncestorType=dxlc:GroupBox}}"/>

                                <!--Type -->
                                <TextBlock Grid.Row="2" Grid.Column="0" FontWeight="Bold" Text="{DynamicResource CurrentType}" Style="{StaticResource TextBlock14Style}" VerticalAlignment="Top"  HorizontalAlignment="Left" />

                                <TextBlock Grid.Row="2" Grid.Column="1"  Grid.ColumnSpan="2" 
                                           Text="{Binding DataContext.SelectedType.Value, RelativeSource={RelativeSource AncestorType=dxlc:GroupBox}}" 
                                           ToolTip="{Binding DataContext.SelectedType.Value, RelativeSource={RelativeSource AncestorType=dxlc:GroupBox}}"/>


                                <!--Normal Time-->

                                <TextBlock  Grid.Row="3" Grid.Column="0" FontWeight="Bold" Text="{DynamicResource CurrentGridColNormalTime}" Style="{StaticResource TextBlock14Style}" VerticalAlignment="Top"  Height="19"   HorizontalAlignment="Left" />
                                <TextBlock Grid.Row="3" Grid.Column="1" Grid.ColumnSpan="2" 
                                           Text="{Binding DataContext.NormalTime, RelativeSource={RelativeSource AncestorType=dxlc:GroupBox}}" 
                                           ToolTip="{Binding DataContext.NormalTime, RelativeSource={RelativeSource AncestorType=dxlc:GroupBox}}"/>

                            </Grid>
                        </DataTemplate>
                    </dxlc:GroupBox.ContentTemplate>
                    <dxlc:GroupBox.MaximizedContentTemplate>
                        <DataTemplate>
                            <!--End GEOS2-3880 - change for AddLog Gulab lakade-->
                            <ScrollViewer HorizontalScrollBarVisibility="Auto" VerticalScrollBarVisibility="Auto">
                                <Grid Width="700" Height="700" >
                                    <!--Width="700" Height="700"-->
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="30"/>
                                        <RowDefinition Height="5"/>
                                        <RowDefinition Height="30"/>
                                        <RowDefinition Height="5"/>
                                        <RowDefinition Height="30"/>
                                        <RowDefinition Height="5"/>
                                        <RowDefinition Height="60"/>
                                        <RowDefinition Height="5"/>
                                        <RowDefinition Height="30"/>
                                        <RowDefinition Height="5"/>
                                        <RowDefinition Height="30"/>
                                        <RowDefinition Height="5"/>
                                        <RowDefinition Height="30"/>
                                        <RowDefinition Height="5"/>
                                        <RowDefinition Height="30"/>
                                        <RowDefinition Height="5"/>
                                        <RowDefinition Height="30"/>
                                        <RowDefinition Height="5"/>
                                        <!--<RowDefinition Height="0"/>-->
                                        <RowDefinition Height="30"/>
                                        <RowDefinition Height="5"/>
                                        <RowDefinition Height="30"/>
                                        <RowDefinition Height="5"/>
                                        <RowDefinition Height="30"/>
                                        <RowDefinition Height="5"/>
                                        <RowDefinition Height="30"/>
                                        <RowDefinition Height="5"/>
                                        <RowDefinition Height="60"/>
                                        <RowDefinition Height="5"/>
                                        <!--[GEOS2-3933][Rupali Sarode]-->
                                        <RowDefinition Height="60"/>
                                        <RowDefinition Height="5"/>
                                        <RowDefinition Height="60"/>
                                        <RowDefinition Height="5"/>
                                        <RowDefinition Height="45"/>
                                        <RowDefinition Height="5"/>

                                    </Grid.RowDefinitions>

                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="70"/>
                                        <ColumnDefinition Width="180"/>
                                        <ColumnDefinition Width="5"/>
                                        <ColumnDefinition Width="270"/>
                                        <ColumnDefinition Width="5"/>
                                        <ColumnDefinition Width="45"/>
                                    </Grid.ColumnDefinitions>

                                    <!--WorkStage-->
                                    <TextBlock Grid.Row="0" Grid.Column="1" Text="{DynamicResource GridColWorkStage}" Style="{StaticResource TextBlock14Style}" VerticalAlignment="Center" HorizontalAlignment="Left"  />
                                    <Grid Grid.Column="3"  Grid.Row="0">
                                        <dxe:ComboBoxEdit Grid.Column="0" Width="270"      
                       Style="{DynamicResource ComboBoxEditStyle}" 
                    HorizontalAlignment="Right"
                    VerticalAlignment="Stretch"
                    IsTextEditable="False"
                    DisplayMember="CodeWithName" 
                    
                                                      ItemsSource="{Binding DataContext.StagesList,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged,RelativeSource={RelativeSource AncestorType=dxlc:GroupBox,Mode=FindAncestor}}"
                                                 
                    >
                                            <!--<dxe:ComboBoxEdit.SelectedIndex>
                                            <Binding Path="DataContext.SelectedStages" RelativeSource="{RelativeSource AncestorType=dxlc:GroupBox}" ValidatesOnDataErrors="True" UpdateSourceTrigger="PropertyChanged" Mode="TwoWay">
                                                <Binding.ValidationRules>
                                                    <Validation:ActivityValidation FieldName="SelectedStages" ValidationStep="UpdatedValue" />
                                                </Binding.ValidationRules>
                                            </Binding>
                                            
                                        </dxe:ComboBoxEdit.SelectedIndex>-->
                                            <dxe:ComboBoxEdit.EditValue>
                                                <Binding Path="DataContext.SelectedStages" RelativeSource="{RelativeSource AncestorType=dxlc:GroupBox}" ValidatesOnDataErrors="True" UpdateSourceTrigger="PropertyChanged" Mode="TwoWay" >
                                                    <Binding.ValidationRules>
                                                        <Validation:AddEditWorkOperationValidation FieldName="SelectedStages" ValidationStep="UpdatedValue" />
                                                    </Binding.ValidationRules>
                                                </Binding>
                                            </dxe:ComboBoxEdit.EditValue>
                                            <dxe:ComboBoxEdit.StyleSettings>
                                                <dxe:CheckedComboBoxStyleSettings />
                                            </dxe:ComboBoxEdit.StyleSettings>
                                            <dxmvvm:Interaction.Triggers>
                                                <dxmvvm:EventToCommand EventName="PopupClosed" Command="{Binding DataContext.CommandStagesChanged, RelativeSource={RelativeSource AncestorType=dxlc:GroupBox}}" PassEventArgsToCommand="True"/>
                                            </dxmvvm:Interaction.Triggers>
                                        </dxe:ComboBoxEdit>
                                    </Grid>



                                    <!--Code-->
                                    <TextBlock Grid.Row="2" Grid.Column="1"  Text="{DynamicResource GridColCode}" Style="{StaticResource TextBlock14Style}" VerticalAlignment="Center" HorizontalAlignment="Left" Margin="0,6,0,5" Height="19"  />

                                    <dxe:TextEdit Grid.Row="2" Grid.Column="3" IsReadOnly="True"
                      VerticalContentAlignment="Center">
                                        <!--NullText="{DynamicResource GridColCode}"-->
                                        <dxe:TextEdit.EditValue>
                                            <Binding Path="DataContext.Code" RelativeSource="{RelativeSource AncestorType=dxwui:WinUIDialogWindow}" UpdateSourceTrigger="PropertyChanged" Mode="TwoWay" 
                         ValidatesOnDataErrors="True">
                                                <Binding.ValidationRules>
                                                    <Validation:AddEditWorkOperationValidation FieldName="Code" ValidationStep="UpdatedValue" ValidatesOnTargetUpdated="True"/>
                                                </Binding.ValidationRules>
                                            </Binding>
                                        </dxe:TextEdit.EditValue>
                                        <dxmvvm:Interaction.Behaviors>
                                            <dxmvvm:EventToCommand 
                    EventName="EditValueChanged" Command="{Binding DataContext.ChangeCodeCommand,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged,RelativeSource={RelativeSource AncestorType=dxwui:WinUIDialogWindow}}"/>
                                        </dxmvvm:Interaction.Behaviors>
                                    </dxe:TextEdit>

                                    <!--Name-->
                                    <TextBlock Grid.Row="4" Grid.Column="1"  Text="{DynamicResource GridColName}" Style="{StaticResource TextBlock14Style}" VerticalAlignment="Center"  HorizontalAlignment="Left" />

                                    <dxe:TextEdit Grid.Row="4" Grid.Column="3"
                      VerticalContentAlignment="Center">
                                        <!--NullText="{DynamicResource GridColName}"-->
                                        <dxe:TextEdit.EditValue>
                                            <Binding Path="DataContext.Name" RelativeSource="{RelativeSource AncestorType=dxwui:WinUIDialogWindow}" UpdateSourceTrigger="PropertyChanged" Mode="TwoWay" 
                         ValidatesOnDataErrors="True">
                                                <Binding.ValidationRules>
                                                    <Validation:AddEditWorkOperationValidation FieldName="Name" ValidationStep="UpdatedValue" ValidatesOnTargetUpdated="True"/>
                                                </Binding.ValidationRules>
                                            </Binding>
                                        </dxe:TextEdit.EditValue>
                                        <dxmvvm:Interaction.Behaviors>
                                            <dxmvvm:EventToCommand 
                    EventName="EditValueChanged" Command="{Binding DataContext.ChangeNameCommand,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged,RelativeSource={RelativeSource AncestorType=dxwui:WinUIDialogWindow}}"/>
                                        </dxmvvm:Interaction.Behaviors>
                                    </dxe:TextEdit>

                                    <!--Description-->
                                    <TextBlock Grid.Row="6" Grid.Column="1" Text="{DynamicResource GridColDescription}" Style="{StaticResource TextBlock14Style}" VerticalAlignment="Center" HorizontalAlignment="Left" />
                                    <dxe:TextEdit Grid.Row="6" Grid.Column="3"  Text="{Binding DataContext.Description,UpdateSourceTrigger=PropertyChanged,Mode=TwoWay,RelativeSource={RelativeSource AncestorType=dxwui:WinUIDialogWindow}}"  VerticalContentAlignment="Top" TextWrapping="Wrap" 
                      ScrollViewer.VerticalScrollBarVisibility="Auto" ScrollViewer.HorizontalScrollBarVisibility="Disabled" AcceptsReturn="True">
                                        <!--NullText="{DynamicResource GridColDescription}"-->
                                        <dxmvvm:Interaction.Behaviors>
                                            <dxmvvm:EventToCommand 
                    EventName="EditValueChanged" Command="{Binding DataContext.ChangeDescriptionCommand,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged,RelativeSource={RelativeSource AncestorType=dxwui:WinUIDialogWindow}}"/>
                                        </dxmvvm:Interaction.Behaviors>
                                    </dxe:TextEdit>

                                    <!--Checkbox-->
                                    <StackPanel Grid.Row="8" Grid.Column="3" HorizontalAlignment="Left">
                                        <!--<dxe:CheckEdit  IsChecked="{Binding DataContext.IsCheckedCopyNameAndDescription,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}" 
                        VerticalAlignment="Center">-->
                                        <dxe:CheckEdit  IsChecked="{Binding DataContext.IsCheckedCopyNameAndDescription,RelativeSource={RelativeSource AncestorType=dxlc:GroupBox}}" 
                                                                       IsReadOnly="False" HorizontalAlignment="Left" VerticalAlignment="Center">
                                            <dxmvvm:Interaction.Behaviors>
                                                <!--<dxmvvm:EventToCommand 
                    EventName="Checked" Command="{Binding DataContext.CheckedCopyNameDescriptionCommand,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged,RelativeSource={RelativeSource AncestorType=dxwui:WinUIDialogWindow}}"/>-->
                                                <dxmvvm:EventToCommand 	
                                                        EventName="Checked" Command="{Binding DataContext.CheckedCopyNameDescriptionCommand,RelativeSource={RelativeSource AncestorType=dxlc:GroupBox}}"/>
                                            </dxmvvm:Interaction.Behaviors>
                                            <TextBlock Grid.Row="8" Grid.Column="3" Margin="5 0 0 0" Text="{DynamicResource CopyNameDescription}" Style="{StaticResource TextBlock14Style}" VerticalAlignment="Center" />
                                        </dxe:CheckEdit>

                                    </StackPanel>

                                    <!--Parent-->
                                    <TextBlock Grid.Row="10" Grid.Column="1"  Text="{DynamicResource GridColParent}" Style="{StaticResource TextBlock14Style}" VerticalAlignment="Center"  HorizontalAlignment="Left" />

                                    <Grid Grid.Row="10" Grid.Column="3">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="270"/>
                                            <ColumnDefinition Width="8"/>
                                        </Grid.ColumnDefinitions>

                                        <dxg:LookUpEdit Grid.Column="0"  Width="270"
                            HorizontalAlignment="Left"
                            ItemsSource="{Binding DataContext.ParentList,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged,RelativeSource={RelativeSource AncestorType=dxwui:WinUIDialogWindow,Mode=FindAncestor}}"
                            DisplayMember="Name" ValueMember="IdWorkOperation" VerticalAlignment="Stretch" Name="lookUpEditParentWorkOperation" 
                            AutoPopulateColumns="False" 
                            AutoComplete="True" IsTextEditable="False" ImmediatePopup="True" IncrementalFiltering="False" 
                            IncrementalSearch="False">
                                            <dxg:LookUpEdit.SelectedItem>
                                                <Binding Path="DataContext.SelectedParent" RelativeSource="{RelativeSource AncestorType=dxwui:WinUIDialogWindow}" ValidatesOnDataErrors="True"  UpdateSourceTrigger="PropertyChanged" Mode="TwoWay">
                                                    <Binding.ValidationRules>
                                                        <Validation:AddEditCategoryValidation FieldName="SelectedParent" ValidationStep="UpdatedValue" ValidatesOnTargetUpdated="True"/>
                                                    </Binding.ValidationRules>
                                                </Binding>
                                            </dxg:LookUpEdit.SelectedItem>
                                            <dxg:LookUpEdit.StyleSettings>
                                                <dxg:SearchLookUpEditStyleSettings/>
                                            </dxg:LookUpEdit.StyleSettings>
                                            <dxg:LookUpEdit.PopupContentTemplate>
                                                <ControlTemplate>
                                                    <dxg:GridControl Name="PART_GridControl">
                                                        <dxg:GridControl.Columns>
                                                            <dxg:GridColumn FieldName="Name" FilterPopupMode="CheckedList"/>
                                                        </dxg:GridControl.Columns>
                                                        <dxg:GridControl.View>
                                                            <dxg:TreeListView Name="treeListViewParentGroup"
                                                  AutoWidth="True" 
                                                  TreeDerivationMode="Selfreference" 
                                                  KeyFieldName="KeyName" 
                                                  ParentFieldName="Parent"
                                                  ShowIndicator="False"
                                                  SearchPanelHorizontalAlignment="Stretch"
                                                  AutoExpandAllNodes="True"/>
                                                        </dxg:GridControl.View>

                                                    </dxg:GridControl>
                                                </ControlTemplate>
                                            </dxg:LookUpEdit.PopupContentTemplate>

                                            <dxmvvm:Interaction.Behaviors>
                                                <dxmvvm:EventToCommand EventName="SelectedIndexChanged"
                                           Command="{Binding DataContext.CommandEditValueChanged, RelativeSource={RelativeSource AncestorType=dxwui:WinUIDialogWindow}}" />
                                            </dxmvvm:Interaction.Behaviors>
                                        </dxg:LookUpEdit>
                                    </Grid>

                                    <!--Order-->
                                    <TextBlock Grid.Row="12" Grid.Column="1"  Text="{DynamicResource Order}" Style="{StaticResource TextBlock14Style}" VerticalAlignment="Center"  HorizontalAlignment="Left" />

                                    <Grid Grid.Row="12" Grid.Column="3"  >
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="270"/>
                                            <ColumnDefinition Width="8"/>
                                        </Grid.ColumnDefinitions>

                                        <dxe:ComboBoxEdit Grid.Column="0"  Width="270" HorizontalAlignment="Left"
                              
                                                  VerticalAlignment="Stretch" 
                                                  AutoComplete="True" ImmediatePopup="True" IncrementalFiltering="True" IsTextEditable="False"
                                                  ValidateOnTextInput="False"
                                                  ItemsSource="{Binding DataContext.OrderList,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged,RelativeSource={RelativeSource AncestorType=dxwui:WinUIDialogWindow,Mode=FindAncestor}}"
                                                  SelectedItem="{Binding DataContext.SelectedOrder,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged,RelativeSource={RelativeSource AncestorType=dxwui:WinUIDialogWindow,Mode=FindAncestor}}"
                                                  Style="{DynamicResource ComboBoxEditStyle}"
                                                  DisplayMember="Name" ValueMember="Name" IsSynchronizedWithCurrentItem="True">

                                        </dxe:ComboBoxEdit>

                                    </Grid>

                                    <!--Type -->
                                    <TextBlock Grid.Row="14" Grid.Column="1" Text="{DynamicResource Type}" Style="{StaticResource TextBlock14Style}" VerticalAlignment="Top"  HorizontalAlignment="Left" />
                                    <dxe:ComboBoxEdit Grid.Row="14" Grid.Column="3" VerticalContentAlignment="Center" 
                                              ItemsSource="{Binding DataContext.TypeList, Mode=TwoWay,UpdateSourceTrigger=PropertyChanged,RelativeSource={RelativeSource AncestorType={x:Type dxwui:WinUIDialogWindow}}}"                        
                                              AutoComplete="True" ImmediatePopup="True" IncrementalFiltering="True" 
                                              ApplyItemTemplateToSelectedItem="True"
                                              IsSynchronizedWithCurrentItem="True"
                                              AllowUpdateTwoWayBoundPropertiesOnSynchronization="True"
                                              IsTextEditable="False"
                                              AllowItemHighlighting="True"
                                              DisplayMember="Value" ValueMember="IdLookupValue" >
                                        <dxe:ComboBoxEdit.SelectedItem>
                                            <Binding Path="DataContext.SelectedType" RelativeSource="{RelativeSource AncestorType=dxwui:WinUIDialogWindow}" 
                         ValidatesOnDataErrors="True"  UpdateSourceTrigger="PropertyChanged" Mode="TwoWay">
                                                <!--<Binding.ValidationRules>
                        <Validation:EditPCMArticleValidation FieldName="SelectedStatus" ValidationStep="UpdatedValue"  ValidatesOnTargetUpdated="True"/>
                    </Binding.ValidationRules>-->
                                            </Binding>
                                        </dxe:ComboBoxEdit.SelectedItem>
                                        <dxe:ComboBoxEdit.ItemContainerStyle>
                                            <Style TargetType="dxe:ComboBoxEditItem">
                                                <Setter Property="HorizontalContentAlignment" Value="Stretch" />
                                                <Setter Property="IsEnabled" Value="{Binding InUse}" />
                                            </Style>
                                        </dxe:ComboBoxEdit.ItemContainerStyle>
                                        <!--<dxe:ComboBoxEdit.ItemTemplate>
                                            <DataTemplate>
                                                <Border Margin="2,0,0,0"
                                                        Background="{Binding HtmlColor}"
                                                        BorderBrush="{Binding ColorName}"
                                                        Padding="2">
                                                    <TextBlock Margin="2,0,0,0"
                                                               HorizontalAlignment="Center"
                                                               Foreground="Black"
                                                               Padding="2"
                                                               Text="{Binding Value}" />
                                                </Border>
                                            </DataTemplate>
                                        </dxe:ComboBoxEdit.ItemTemplate>-->
                                        <dxe:ComboBoxEdit.ItemTemplate>
                                            <DataTemplate>
                                                <DockPanel>
                                                    <!--<Rectangle Width="15" Height="15" Margin="3" Fill="{Binding HtmlColor}" /> Stretch="UniformToFill"-->
                                                    <Image Width="15" Height="15"  Source="{Binding ImageData,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged,Converter={Converters:ByteArrayToImage}}"/>
                                                    <TextBlock Text="{Binding Value}" Margin="5,0,0,0" Padding="3" />
                                                </DockPanel>
                                            </DataTemplate>
                                        </dxe:ComboBoxEdit.ItemTemplate>
                                        <!--<dxe:ComboBoxEdit.EditNonEditableTemplate>
                                            <ControlTemplate>
                                                <Border Margin="2,2,2,2"
                                                        Background="{Binding SelectedItem.HtmlColor}"
                                                        BorderBrush="{Binding SelectedItem.ColorName}">
                                                    <TextBlock HorizontalAlignment="Center"
                                                               Foreground="Black"
                                                               Text="{Binding SelectedItem.Value}" />
                                                </Border>
                                            </ControlTemplate>
                                        </dxe:ComboBoxEdit.EditNonEditableTemplate>-->
                                        <!--<Interactive:Interaction.Triggers>
                                            <Interactive:EventTrigger EventName="SelectedIndexChanged">
                                                <Interactive:InvokeCommandAction Command="{Binding DataContext.SelectedTestChangedCommand,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged,RelativeSource={RelativeSource AncestorType=dxlc:GroupBox}}" />
                                            </Interactive:EventTrigger>
                                        </Interactive:Interaction.Triggers>-->
                                    </dxe:ComboBoxEdit>


                                    <!--Status-->
                                    <TextBlock Grid.Row="16" Grid.Column="1" Text="{DynamicResource Status}" Style="{StaticResource TextBlock14Style}" VerticalAlignment="Top" Height="19"  HorizontalAlignment="Left" />
                                    <dxe:ComboBoxEdit Grid.Row="16" Grid.Column="3" VerticalContentAlignment="Center" 
                                              ItemsSource="{Binding DataContext.StatusList, Mode=TwoWay,UpdateSourceTrigger=PropertyChanged,RelativeSource={RelativeSource AncestorType={x:Type dxwui:WinUIDialogWindow}}}"                        
                                              AutoComplete="True" ImmediatePopup="True" IncrementalFiltering="True" 
                                              ApplyItemTemplateToSelectedItem="True"
                                              IsSynchronizedWithCurrentItem="True"
                                              AllowUpdateTwoWayBoundPropertiesOnSynchronization="True"
                                              IsTextEditable="False"
                                              AllowItemHighlighting="True"
                                              DisplayMember="Value" ValueMember="IdLookupValue" >
                                        <dxe:ComboBoxEdit.SelectedItem>
                                            <Binding Path="DataContext.SelectedStatus" RelativeSource="{RelativeSource AncestorType=dxwui:WinUIDialogWindow}" ValidatesOnDataErrors="True"  UpdateSourceTrigger="PropertyChanged" Mode="TwoWay">
                                                <Binding.ValidationRules>
                                                    <Validation:EditPCMArticleValidation FieldName="SelectedStatus" ValidationStep="UpdatedValue"  ValidatesOnTargetUpdated="True"/>
                                                </Binding.ValidationRules>
                                            </Binding>
                                        </dxe:ComboBoxEdit.SelectedItem>
                                        <dxe:ComboBoxEdit.ItemContainerStyle>
                                            <Style TargetType="dxe:ComboBoxEditItem">
                                                <Setter Property="HorizontalContentAlignment" Value="Stretch" />
                                                <Setter Property="IsEnabled" Value="{Binding InUse}" />
                                            </Style>
                                        </dxe:ComboBoxEdit.ItemContainerStyle>
                                        <!--<dxe:ComboBoxEdit.ItemTemplate>
                                            <DataTemplate>
