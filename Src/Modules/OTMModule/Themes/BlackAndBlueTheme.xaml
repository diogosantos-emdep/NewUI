    <ControlTemplate x:Key="EditorListBoxThemeKey_ItemTemplate" TargetType="dxe:ListBoxEditItem">
        <Grid Background="Transparent">
            <Grid.Resources>
                <SolidColorBrush x:Key="WhiteBrush" Color="White" />
            </Grid.Resources>
            <dxc:ContentContainer x:Name="contentContainer">
                <ContentPresenter Margin="4,1,4,1" HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}" VerticalAlignment="{TemplateBinding VerticalContentAlignment}" UseLayoutRounding="{TemplateBinding UseLayoutRounding}" />
            </dxc:ContentContainer>
        </Grid>
    </ControlTemplate>
    
    <Style x:Key="EditorListBoxThemeKey_DefaultItemStyle" TargetType="dxe:ListBoxEditItem">
        <Setter Property="Background" Value="Transparent" />
        <Setter Property="HorizontalContentAlignment" Value="{Binding Path=Owner.HorizontalContentAlignment, RelativeSource={RelativeSource Self}}" />
        <Setter Property="VerticalContentAlignment" Value="{Binding Path=Owner.VerticalContentAlignment, RelativeSource={RelativeSource Self}}" />
        <Setter Property="Padding" Value="2,0,0,0" />
        <Setter Property="Template" Value="{StaticResource EditorListBoxThemeKey_ItemTemplate}" />
    </Style>

