    <Style x:Key="HyperlinkTextBlock" TargetType="TextBlock">
        <Setter Property="TextBlock.Foreground" Value="Black" />
        <Style.Triggers>
            <MultiDataTrigger>
                <MultiDataTrigger.Conditions>
                    <Condition Binding="{Binding RowData.SelectionState}" Value="Focused"/>
                </MultiDataTrigger.Conditions>
                <Setter Property="TextBlock.Foreground" Value="#FF083493"/>
            </MultiDataTrigger>
            <MultiDataTrigger>
                <MultiDataTrigger.Conditions>
                    <Condition Binding="{Binding RowData.SelectionState}" Value="None"/>
                </MultiDataTrigger.Conditions>
                <Setter Property="TextBlock.Foreground" Value="#FF2AB7FF"/>
            </MultiDataTrigger>
            <Trigger Property="IsMouseOver" Value="true">
                <Setter Property="Foreground" Value="OrangeRed" />
            </Trigger>
        </Style.Triggers>
    </Style>


