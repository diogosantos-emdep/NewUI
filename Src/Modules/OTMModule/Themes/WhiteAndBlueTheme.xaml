                <Setter Property="Background" Value="#FF083493" />
                <Setter Property="Foreground" Value="White" />
                <Setter Property="TextBlock.Foreground" Value="White"/>
            </Trigger>
            <Trigger Property="IsMouseOver" Value="true">
