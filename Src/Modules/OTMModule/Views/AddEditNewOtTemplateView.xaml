<dxwui:WinUIDialogWindow x:Class="Emdep.Geos.Modules.OTM.Views.AddEditNewOtTemplateView"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Emdep.Geos.Modules.OTM.Views"
        xmlns:dxwui="http://schemas.devexpress.com/winfx/2008/xaml/windowsui" 
        xmlns:dxlc="http://schemas.devexpress.com/winfx/2008/xaml/layoutcontrol"
        xmlns:dxmvvm="http://schemas.devexpress.com/winfx/2008/xaml/mvvm"
        xmlns:UIHelper="clr-namespace:Emdep.Geos.UI.Helper;assembly=Emdep.Geos.UI.Helper"
        xmlns:dxpdf="http://schemas.devexpress.com/winfx/2008/xaml/pdf" 
        xmlns:dxsps="http://schemas.devexpress.com/winfx/2008/xaml/spreadsheet"
        xmlns:dxe="http://schemas.devexpress.com/winfx/2008/xaml/editors" 
        xmlns:Validation ="clr-namespace:Emdep.Geos.UI.Validations;assembly=Emdep.Geos.UI.Validations"                 
        xmlns:i="http://schemas.microsoft.com/xaml/behaviors"
        mc:Ignorable="d"
        Height="{Binding SystemParameters.PrimaryScreenHeight}" 
        Width="{Binding SystemParameters.PrimaryScreenWidth}"
        Template="{StaticResource Maximized_DXDialogWindow}"
        HorizontalAlignment="Stretch"
        VerticalAlignment="Stretch"
        HorizontalContentAlignment="Stretch"
        VerticalContentAlignment="Stretch"
        Title="{Binding WindowHeader}"
        TitleStyle="{DynamicResource WinUIDialogWindowTitleStyle}"
        Style="{DynamicResource WinUIDialogWindowStyle}"
        >

    <dxwui:WinUIDialogWindow.Resources>

        <Style x:Key="myGroupBoxStyle" TargetType="{x:Type dxlc:GroupBox}">
            <Setter Property="MaximizeElementVisibility" Value="Visible" />
            <Setter Property="Width" Value="900" />
            <Setter Property="Height" Value="300" />
        </Style>

        <Style x:Key="myGroupBoxStyleForMapping" TargetType="{x:Type dxlc:GroupBox}">
            <Setter Property="MaximizeElementVisibility" Value="Visible" />
            <Setter Property="Width" Value="900" />
            <Setter Property="Height" Value="570" />
            
        </Style>
        
        

    </dxwui:WinUIDialogWindow.Resources>
    
    
    

    <dxwui:WinUIDialogWindow.InputBindings>
        <KeyBinding Key="Escape"  Command="{Binding AddEditNewOtTemplateViewCancelButtonCommand}"/>
    </dxwui:WinUIDialogWindow.InputBindings>

    <Grid ShowGridLines="False" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" >

        <Grid.RowDefinitions>
            <RowDefinition Height="*"/>
            <RowDefinition Height="5"/>
            <RowDefinition Height="45"/>
        </Grid.RowDefinitions>

        <ScrollViewer HorizontalScrollBarVisibility="Auto" VerticalScrollBarVisibility="Auto">
        <dxlc:FlowLayoutControl Orientation="Vertical" Background="Transparent" ItemSpace="12" LayerSpace="12"
                         AllowMaximizedElementMoving="False" Grid.Row="0"   
                         BreakFlowToFit="True" MaximizedElementPosition="Right" AllowLayerSizing="False" Margin="5,5,5,5" Padding="0,0,0,0">

            <dxmvvm:Interaction.Behaviors>
                <UIHelper:FlowLayoutControlForMinimiseMaximise />
            </dxmvvm:Interaction.Behaviors>

           

           

            <!--file-->

            <dxlc:GroupBox x:Name="file" MaximizeElementVisibility="Collapsed" Style="{StaticResource myGroupBoxStyle}">

                <!--HeaderTemplate-->
                <dxlc:GroupBox.HeaderTemplate>
                    <DataTemplate>
                        <DockPanel Margin="-5,0,0,0">
                            <TextBlock VerticalAlignment="Center" FontWeight="Bold" Text="{DynamicResource NewOtTemplateFile}" />

                            
                        </DockPanel>
                    </DataTemplate>
                </dxlc:GroupBox.HeaderTemplate>


                <!--ContentTemplate-->
                <dxlc:GroupBox.ContentTemplate>
                    <DataTemplate>
                        <Grid ShowGridLines="True">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="190" />
                                <ColumnDefinition Width="*" />
                                <ColumnDefinition Width="30" />
                                <ColumnDefinition Width="125" />
                                <ColumnDefinition Width="*" />
                                <ColumnDefinition Width="*" />
                                <ColumnDefinition Width="*" />
                            </Grid.ColumnDefinitions>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="35" />
                                <RowDefinition Height="35" />
                                <RowDefinition Height="35" />
                            </Grid.RowDefinitions>

                            <!--PO Number-->
                            <TextBlock Grid.Row="0"
                               Grid.Column="0"
                               Margin="2" VerticalAlignment="Stretch" HorizontalAlignment="Left"
                               Text="{DynamicResource NewOtTemplatePONumber}"
                               FontWeight="Normal"
                               Padding="2" />

                            <dxe:ComboBoxEdit  Grid.Row="0" Grid.Column="1"  
                                    HorizontalAlignment="Stretch"  
                                    VerticalAlignment="Top" Height="30"
                                    ItemsSource="{Binding DataContext.Customers, RelativeSource={RelativeSource AncestorType=dxwui:WinUIDialogWindow} }" 
                                    DisplayMember="CustomerName" AutoComplete="True" ImmediatePopup="True"
                                    IncrementalFiltering="True" 
                                    IsTextEditable="False" 
                                    ValidateOnTextInput="False">


                                <dxe:ComboBoxEdit.SelectedIndex>
                                    <Binding Path="DataContext.SelectedIndexCustomerGroup" RelativeSource="{RelativeSource AncestorType=dxwui:WinUIDialogWindow}" ValidatesOnDataErrors="True" UpdateSourceTrigger="PropertyChanged" Mode="TwoWay">
                                        <Binding.ValidationRules>
                                            <!--<Validation:CustomerRequiredFieldValidation FieldName="SelectedIndexCustomerGroup" ValidationStep="UpdatedValue" />-->
                                        </Binding.ValidationRules>
                                    </Binding>
                                </dxe:ComboBoxEdit.SelectedIndex>

                                <dxmvvm:Interaction.Triggers>
                                    <dxmvvm:EventToCommand EventName="PopupClosed" Command="{Binding DataContext.ChangeCustomerNameCommand,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged,RelativeSource={RelativeSource AncestorType=dxlc:GroupBox}}" PassEventArgsToCommand="True" />
                                </dxmvvm:Interaction.Triggers>
                            </dxe:ComboBoxEdit>

                            <!--Date-->
                            <TextBlock Grid.Row="1"
                               Grid.Column="0"
                               Margin="2" VerticalAlignment="Stretch" HorizontalAlignment="Left"
                               Text="{DynamicResource NewOtTemplatePODate}"
                               FontWeight="Normal"
                               Padding="2" />

                            <dxe:ComboBoxEdit  Grid.Row="1" Grid.Column="1"  
                                    HorizontalAlignment="Stretch"  
                                    VerticalAlignment="Top" Height="30"
                                    ItemsSource="{Binding DataContext.Customers, RelativeSource={RelativeSource AncestorType=dxwui:WinUIDialogWindow} }" 
                                    DisplayMember="CustomerName" AutoComplete="True" ImmediatePopup="True"
                                    IncrementalFiltering="True" 
                                    IsTextEditable="False" 
                                    ValidateOnTextInput="False">


                                <dxe:ComboBoxEdit.SelectedIndex>
                                    <Binding Path="DataContext.SelectedIndexCustomerGroup" RelativeSource="{RelativeSource AncestorType=dxwui:WinUIDialogWindow}" ValidatesOnDataErrors="True" UpdateSourceTrigger="PropertyChanged" Mode="TwoWay">
                                        <Binding.ValidationRules>
                                            <!--<Validation:CustomerRequiredFieldValidation FieldName="SelectedIndexCustomerGroup" ValidationStep="UpdatedValue" />-->
                                        </Binding.ValidationRules>
                                    </Binding>
                                </dxe:ComboBoxEdit.SelectedIndex>

                                <dxmvvm:Interaction.Triggers>
                                    <dxmvvm:EventToCommand EventName="PopupClosed" Command="{Binding DataContext.ChangeCustomerNameCommand,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged,RelativeSource={RelativeSource AncestorType=dxlc:GroupBox}}" PassEventArgsToCommand="True" />
                                </dxmvvm:Interaction.Triggers>
                            </dxe:ComboBoxEdit>


                            <!--Key word-->
                            <TextBlock Grid.Row="0" Grid.ColumnSpan="2"
                                Grid.Column="3"
                                Margin="2" VerticalAlignment="Stretch" HorizontalAlignment="Left"
                                Text="{DynamicResource NewOtTemplateKeyWord}"
                                FontWeight="Normal"
                                Padding="2" />

                            <dxe:TextEdit Grid.Row="0" Grid.Column="4" Height="30"
                                  Text="{Binding DataContext.RegistrationNumber,RelativeSource={RelativeSource AncestorType=dxlc:GroupBox},UpdateSourceTrigger=PropertyChanged,Mode=TwoWay}"
                                  IsReadOnly="False">
                            </dxe:TextEdit>

                            <!--coordinates-->
                            <TextBlock Grid.Row="1"
                                    Grid.Column="3"
                                    Margin="2" VerticalAlignment="Stretch" HorizontalAlignment="Left"
                                    Text="{DynamicResource NewOtTemplateCoordinates}"
                                    FontWeight="Normal"
                                    Padding="2" />


                            <dxe:TextEdit Grid.Row="1" Grid.Column="4" Height="30" Grid.ColumnSpan="2"
                               Text="{Binding DataContext.RegistrationNumber,RelativeSource={RelativeSource AncestorType=dxlc:GroupBox},UpdateSourceTrigger=PropertyChanged,Mode=TwoWay}"
                               IsReadOnly="False">
                            </dxe:TextEdit>

                            <!--SelectArea-->
                            <Button Grid.Row="1" Grid.Column="7" 
                               Content="{DynamicResource NewOtTemplateSelectArea}" Width="130" Height="30" 
                               HorizontalAlignment="Right" 
                               Command="{Binding DataContext.ChooseFileActionCommand,Mode=TwoWay ,RelativeSource={RelativeSource AncestorType=dxwui:WinUIDialogWindow,Mode=FindAncestor}, UpdateSourceTrigger=PropertyChanged}"/>



                            <!--Delimiter-->
                            <TextBlock Grid.Row="0"
                                 Grid.Column="5" Grid.ColumnSpan="2"
                                 Margin="2" VerticalAlignment="Stretch" HorizontalAlignment="Left"
                                 Text="{DynamicResource NewOtTemplateDelimiter}"
                                 FontWeight="Normal"
                                 Padding="2" />

                            <dxe:TextEdit Grid.Row="0" Grid.Column="7" Height="30"
                                    Text="{Binding DataContext.RegistrationNumber,RelativeSource={RelativeSource AncestorType=dxlc:GroupBox},UpdateSourceTrigger=PropertyChanged,Mode=TwoWay}"
                                    IsReadOnly="False">
                            </dxe:TextEdit>


                        </Grid>
                    </DataTemplate>
                </dxlc:GroupBox.ContentTemplate>

                <!--MaximizedContentTemplate-->
                <dxlc:GroupBox.MaximizedContentTemplate>
                    <DataTemplate >
                        <ScrollViewer ScrollViewer.VerticalScrollBarVisibility="Auto"  Width="950">
                            <dxlc:LayoutControl VerticalAlignment="Stretch" HorizontalAlignment="Stretch" Margin="0" Padding="0" Orientation="Vertical" Style="{DynamicResource LayoutControlStyle}"
                                 DragScrolling="False">
                                <dxlc:LayoutGroup Orientation="Vertical" VerticalAlignment="Stretch" HorizontalAlignment="Stretch">

                                    <Grid ShowGridLines="True">
                                        <Grid.ColumnDefinitions>

                                                <ColumnDefinition Width="*" />
                                            </Grid.ColumnDefinitions>
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="*" />
                                        </Grid.RowDefinitions>

                                            <!-- DevExpress PdfViewer control -->

                                            <dxpdf:PdfViewerControl x:Name="PdfViewer"  DocumentLoaded="PdfViewer_DocumentLoaded" CursorMode="SelectTool" HighlightSelectionColor="Transparent"  ZoomMode="FitToWidth" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" HorizontalContentAlignment="Stretch" VerticalContentAlignment="Stretch" 
                                                                Visibility="{Binding DataContext.IsPdf ,UpdateSourceTrigger=PropertyChanged,Mode=TwoWay ,RelativeSource={RelativeSource AncestorType=dxwui:WinUIDialogWindow}}" 
                                                                DocumentSource="{Binding DataContext.PdfDoc,UpdateSourceTrigger=PropertyChanged,Mode=TwoWay,RelativeSource={RelativeSource AncestorType=dxwui:WinUIDialogWindow}}" 
                                                                CommandBarStyle="None" >

                                                

                                                <dxmvvm:Interaction.Triggers>
                                                    <!--MouseDown-->
                                                    <dxmvvm:EventToCommand EventName="MouseDown" Command="{Binding DataContext.SelectedMouseDownCommand,UpdateSourceTrigger=PropertyChanged, RelativeSource={RelativeSource AncestorType=dxwui:WinUIDialogWindow}}" 
                                                                            PassEventArgsToCommand="True"/>
                                                    <!-- MouseMove -->
                                                    <dxmvvm:EventToCommand EventName="MouseMove" Command="{Binding DataContext.SelectedMouseMovedCommand,UpdateSourceTrigger=PropertyChanged, RelativeSource={RelativeSource AncestorType=dxwui:WinUIDialogWindow}}" 
                                                        PassEventArgsToCommand="True"/>
                                                    <!-- MouseUp -->
                                                    <dxmvvm:EventToCommand EventName="MouseUp" Command="{Binding DataContext.SelectedMouseUpCommand,UpdateSourceTrigger=PropertyChanged, RelativeSource={RelativeSource AncestorType=dxwui:WinUIDialogWindow}}" 
                                                                            PassEventArgsToCommand="True"/>
                                                </dxmvvm:Interaction.Triggers>

                                            <dxpdf:PdfViewerControl.ThumbnailsViewerSettings>
                                                    <dxpdf:PdfThumbnailsViewerSettings HideThumbnailsViewer="True"/>
                                                </dxpdf:PdfViewerControl.ThumbnailsViewerSettings>
                                                
                                                
                                            
                                            
                                            <dxpdf:PdfViewerControl.AttachmentsViewerSettings>
                                                    <dxpdf:PdfAttachmentsViewerSettings HideAttachmentsViewer="True"/>
                                            </dxpdf:PdfViewerControl.AttachmentsViewerSettings>

                                            </dxpdf:PdfViewerControl>

                                            <!--IsCanvasVisible-->
                                            <Canvas Visibility="{Binding DataContext.IsCanvasVisible,UpdateSourceTrigger=PropertyChanged,Mode=TwoWay ,RelativeSource={RelativeSource AncestorType=dxwui:WinUIDialogWindow}}"  IsHitTestVisible="True">
                                                <Rectangle
                                                    
                                                 
                                                    Width="{Binding DataContext.Width,UpdateSourceTrigger=PropertyChanged,Mode=TwoWay ,RelativeSource={RelativeSource AncestorType=dxwui:WinUIDialogWindow}}"
                                                    Height="{Binding DataContext.Height ,UpdateSourceTrigger=PropertyChanged,Mode=TwoWay ,RelativeSource={RelativeSource AncestorType=dxwui:WinUIDialogWindow}}"
                                                    Canvas.Left="{Binding DataContext.X,UpdateSourceTrigger=PropertyChanged,Mode=TwoWay ,RelativeSource={RelativeSource AncestorType=dxwui:WinUIDialogWindow}}"
                                                    Canvas.Top="{Binding DataContext.Y,UpdateSourceTrigger=PropertyChanged,Mode=TwoWay ,RelativeSource={RelativeSource AncestorType=dxwui:WinUIDialogWindow}}"
                                                    Fill="Transparent"
                                                    Stroke="Blue"
                                                    StrokeThickness="3" />
                                            </Canvas>



                                            <!--<Canvas IsHitTestVisible="True">
                                                <Rectangle
                                                    Width="0"
                                                    Height="{Binding DataContext.Height ,UpdateSourceTrigger=PropertyChanged,Mode=TwoWay ,RelativeSource={RelativeSource AncestorType=dxwui:WinUIDialogWindow}}"
                                                    Canvas.Left="{Binding DataContext.X ,UpdateSourceTrigger=PropertyChanged,Mode=TwoWay ,RelativeSource={RelativeSource AncestorType=dxwui:WinUIDialogWindow}}"
                                                    Canvas.Top="{Binding DataContext.Y ,UpdateSourceTrigger=PropertyChanged,Mode=TwoWay ,RelativeSource={RelativeSource AncestorType=dxwui:WinUIDialogWindow}}"
                                                    Fill="Transparent"
                                                    Stroke="Blue"
                                                    StrokeThickness="3" />
                                            </Canvas>-->




                                            <dxsps:SpreadsheetControl  x:Name="ExcelViewer" Background="White" ActiveViewZoom="88"  DocumentLoaded="ExcelViewer_DocumentLoaded"  FontFamily="black" Foreground="black"  HorizontalAlignment="Stretch" VerticalAlignment="Stretch" HorizontalContentAlignment="Stretch" VerticalContentAlignment="Stretch" 
                                                                Visibility="{Binding DataContext.IsExcel ,UpdateSourceTrigger=PropertyChanged,Mode=TwoWay ,RelativeSource={RelativeSource AncestorType=dxwui:WinUIDialogWindow}}" 
                                                                DocumentSource="{Binding DataContext.ExcelDoc,UpdateSourceTrigger=PropertyChanged,Mode=TwoWay,RelativeSource={RelativeSource AncestorType=dxwui:WinUIDialogWindow}}" 
                                                                />







                                    </Grid>

                                </dxlc:LayoutGroup>
                            </dxlc:LayoutControl>

                        </ScrollViewer>
                    </DataTemplate>
                </dxlc:GroupBox.MaximizedContentTemplate>

            </dxlc:GroupBox>


            <!--Information-->
            <dxlc:GroupBox x:Name="Info" VerticalContentAlignment="Stretch" HorizontalContentAlignment="Stretch" MaximizeElementVisibility="Collapsed" Style="{StaticResource myGroupBoxStyle}">

                <!--HeaderTemplate-->
                <dxlc:GroupBox.HeaderTemplate>
                    <DataTemplate>
                        <DockPanel Margin="-5,0,0,0">
                            <TextBlock VerticalAlignment="Center" FontWeight="Bold" Text="{DynamicResource NewOtTemplateInformation}" />

                            <!--<dxe:TextEdit Text="{DynamicResource JobInformation}" FontWeight="Bold" HorizontalAlignment="Right" VerticalAlignment="Center"  ShowBorder="False" IsReadOnly="True">
                     <Binding Path="DataContext.InformationError" RelativeSource="{RelativeSource AncestorType=dxlc:GroupBox}" UpdateSourceTrigger="PropertyChanged" Mode="TwoWay" ValidatesOnDataErrors="True">
                         <Binding.ValidationRules>
                             <Validation:EditPCMArticleValidation FieldName="InformationError" ValidationStep="UpdatedValue" ValidatesOnTargetUpdated="True" />
                         </Binding.ValidationRules>
                     </Binding>
                 </dxe:TextEdit>-->
                        </DockPanel>
                    </DataTemplate>
                </dxlc:GroupBox.HeaderTemplate>

                <!--ContentTemplate-->
                <dxlc:GroupBox.ContentTemplate >
                    <DataTemplate>
                        <Grid  ShowGridLines="False">
                 <Grid.ColumnDefinitions>
                     <ColumnDefinition Width="190" />
                     <ColumnDefinition Width="*" />
                 </Grid.ColumnDefinitions>
                 <Grid.RowDefinitions>
                     <RowDefinition Height="*" />
                      <RowDefinition Height="*" />
                     <RowDefinition Height="*" />
                     <RowDefinition Height="*" />
                     <RowDefinition Height="*" />
                     <RowDefinition Height="*" />
                     <RowDefinition Height="*" />
                     </Grid.RowDefinitions>

                            <TextBlock Grid.Row="0"
                            Grid.Column="0"
                            Margin="2" VerticalAlignment="Stretch" HorizontalAlignment="Left"
                            Text="{DynamicResource NewOtTemplateCode}"
                            FontWeight="Normal"
                            Padding="2" />

                            <dxe:TextEdit Grid.Row="0" Grid.Column="1" Height="30"
                              Text="{Binding DataContext.RegistrationNumber,RelativeSource={RelativeSource AncestorType=dxlc:GroupBox},UpdateSourceTrigger=PropertyChanged,Mode=TwoWay}"
                              IsReadOnly="True">   
                              </dxe:TextEdit>

                            <!--Template Name-->
                            <TextBlock Grid.Row="1"
                            Grid.Column="0"
                            Margin="2" VerticalAlignment="Stretch" HorizontalAlignment="Left"
                            Text="{DynamicResource NewOtTemplateName}"
                            FontWeight="Normal"
                            Padding="2" />

                            <dxe:TextEdit Grid.Row="1" Grid.Column="1" Height="30"
                                  Text="{Binding DataContext.TemplateName,RelativeSource={RelativeSource AncestorType=dxlc:GroupBox},UpdateSourceTrigger=PropertyChanged,Mode=TwoWay}"
                                 IsReadOnly="{Binding DataContext.TempReadOnly, RelativeSource={RelativeSource AncestorType=dxlc:GroupBox}, Mode=TwoWay}">
                                
                                <dxe:TextEdit.EditValue>
                                    <Binding Path="DataContext.TemplateName" RelativeSource="{RelativeSource AncestorType=dxlc:GroupBox}" ValidatesOnDataErrors="True" UpdateSourceTrigger="PropertyChanged" Mode="TwoWay">
                                        <Binding.ValidationRules>
                                            <Validation:NewOtTemplateValidation FieldName="DataContext.TemplateName" ValidationStep="UpdatedValue" ValidatesOnTargetUpdated="True"/>
                                        </Binding.ValidationRules>
                                    </Binding>
                                </dxe:TextEdit.EditValue>
                            </dxe:TextEdit>

                            <!--<TextBlock Margin="2" Padding="1" Text="{Binding DataContext.JobDescriptionAbbreviation, RelativeSource={RelativeSource AncestorType={x:Type dxlc:GroupBox}}}" VerticalAlignment="Stretch" HorizontalAlignment="Left" Grid.Row="0" Grid.Column="1" TextWrapping="WrapWithOverflow" TextTrimming="CharacterEllipsis"/>-->

                            <!--Group-->
                            <TextBlock Grid.Row="2"
                             Grid.Column="0"
                             Margin="2" VerticalAlignment="Stretch" HorizontalAlignment="Left"
                             Text="{DynamicResource NewOtTemplateGroup}"
                             FontWeight="Normal" 
                             Padding="2" TextWrapping="WrapWithOverflow" TextTrimming="CharacterEllipsis"/>

                          
                            <dxe:ComboBoxEdit  Grid.Row="2" Grid.Column="1"  
                                               HorizontalAlignment="Stretch"  
                                               VerticalAlignment="Top" Height="30"
                                               ItemsSource="{Binding DataContext.GroupList, RelativeSource={RelativeSource AncestorType=dxwui:WinUIDialogWindow},UpdateSourceTrigger=PropertyChanged,Mode=TwoWay}" 
                                               DisplayMember="CustomerName" AutoComplete="True" ImmediatePopup="True"
                                               IncrementalFiltering="True" 
                                               IsTextEditable="False" 
                                               ValidateOnTextInput="False"
                                               IsEnabled="{Binding DataContext.GroupIsEnabled, RelativeSource={RelativeSource AncestorType=dxlc:GroupBox}, Mode=TwoWay}">


                                <dxe:ComboBoxEdit.SelectedIndex>
                                    <Binding Path="DataContext.SelectedIndexGroup" RelativeSource="{RelativeSource AncestorType=dxwui:WinUIDialogWindow}" ValidatesOnDataErrors="True" UpdateSourceTrigger="PropertyChanged" Mode="TwoWay">
                                        <Binding.ValidationRules>
                                            <Validation:NewOtTemplateValidation FieldName="SelectedIndexGroup" ValidationStep="UpdatedValue" />
                                        </Binding.ValidationRules>
                                    </Binding>
                                </dxe:ComboBoxEdit.SelectedIndex>

                                <dxmvvm:Interaction.Triggers>
                                    <dxmvvm:EventToCommand EventName="PopupClosed" Command="{Binding DataContext.ChangeCustomerNameCommand,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged,RelativeSource={RelativeSource AncestorType=dxlc:GroupBox}}" PassEventArgsToCommand="True" />
                                </dxmvvm:Interaction.Triggers>
                            </dxe:ComboBoxEdit>

                            <!--<TextBlock Margin="2" Padding="1" Text="{Binding DataContext.JobDescriptionTitle, RelativeSource={RelativeSource AncestorType={x:Type dxlc:GroupBox}}}" VerticalAlignment="Stretch" HorizontalAlignment="Left"   Grid.Row="1" Grid.Column="1" TextWrapping="WrapWithOverflow" TextTrimming="CharacterEllipsis"
             ToolTip="{Binding DataContext.JobDescriptionTitle, RelativeSource={RelativeSource AncestorType={x:Type dxlc:GroupBox}}}"/>-->

                            <!--region-->
                            <TextBlock Grid.Row="3"
                             Grid.Column="0"
                             Margin="2" VerticalAlignment="Stretch" HorizontalAlignment="Left"
                             Text="{DynamicResource NewOtTemplateRegion}"
                             FontWeight="Normal"
                             Padding="2" />

                            <dxe:ComboBoxEdit  Grid.Row="3" Grid.Column="1"  
                                        HorizontalAlignment="Stretch"  
                                        VerticalAlignment="Top" Height="30"
                                        ItemsSource="{Binding DataContext.RegionList, RelativeSource={RelativeSource AncestorType=dxwui:WinUIDialogWindow},UpdateSourceTrigger=PropertyChanged,Mode=TwoWay }" 
                                        DisplayMember="RegionName" AutoComplete="True" ImmediatePopup="True"
                                        IncrementalFiltering="True" 
                                        IsTextEditable="False" 
                                    ValidateOnTextInput="False">


                                <dxe:ComboBoxEdit.SelectedIndex>
                                    <Binding Path="DataContext.SelectedIndexRegion" RelativeSource="{RelativeSource AncestorType=dxwui:WinUIDialogWindow}" ValidatesOnDataErrors="True" UpdateSourceTrigger="PropertyChanged" Mode="TwoWay">
                                        <!--<Binding.ValidationRules>
                                            <Validation:NewOtTemplateValidation FieldName="SelectedIndexRegion" ValidationStep="UpdatedValue" />
                                        </Binding.ValidationRules>-->
                                    </Binding>
                                </dxe:ComboBoxEdit.SelectedIndex>
                                    <dxmvvm:Interaction.Triggers>
                                        <dxmvvm:EventToCommand EventName="PopupClosed" Command="{Binding ChangeRegionCommand}" PassEventArgsToCommand="True"/>
                                    </dxmvvm:Interaction.Triggers>
                                </dxe:ComboBoxEdit>

                            <!--Country-->
                            <TextBlock Grid.Row="4"
                              Grid.Column="0"
                              Margin="2" VerticalAlignment="Stretch" HorizontalAlignment="Left"
                              Text="{DynamicResource NewOtTemplateCountry}"
                              FontWeight="Normal"
                              Padding="2" />

                            <dxe:ComboBoxEdit  Grid.Row="4" Grid.Column="1"  
                                    HorizontalAlignment="Stretch"  
                                    VerticalAlignment="Top" Height="30"
                                    ItemsSource="{Binding DataContext.CountryList, RelativeSource={RelativeSource AncestorType=dxwui:WinUIDialogWindow},UpdateSourceTrigger=PropertyChanged,Mode=TwoWay }" 
                                    DisplayMember="Name" AutoComplete="True" ImmediatePopup="True"
                                    IncrementalFiltering="True" 
                                    IsTextEditable="False" 
                                    ValidateOnTextInput="False">


                                <dxe:ComboBoxEdit.SelectedIndex>
                                    <Binding Path="DataContext.SelectedIndexCountry" RelativeSource="{RelativeSource AncestorType=dxwui:WinUIDialogWindow}" ValidatesOnDataErrors="True" UpdateSourceTrigger="PropertyChanged" Mode="TwoWay">
                                        <Binding.ValidationRules>
                                            <Validation:NewOtTemplateValidation FieldName="SelectedIndexCountry" ValidationStep="UpdatedValue" />
                                        </Binding.ValidationRules>
                                    </Binding>
                                </dxe:ComboBoxEdit.SelectedIndex>
                                
                                    <!--<dxe:ComboBoxEdit.StyleSettings>
                                        <dxe:CheckedComboBoxStyleSettings />
                                    </dxe:ComboBoxEdit.StyleSettings>-->



                                    <dxmvvm:Interaction.Triggers>
                                    <dxmvvm:EventToCommand EventName="PopupClosed" Command="{Binding DataContext.ChangeCustomerNameCommand,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged,RelativeSource={RelativeSource AncestorType=dxlc:GroupBox}}" PassEventArgsToCommand="True" />
                                </dxmvvm:Interaction.Triggers>
                            </dxe:ComboBoxEdit>

                            <!--Plant-->
                            <TextBlock Grid.Row="5"
                              Grid.Column="0"
                              Margin="2" VerticalAlignment="Stretch" HorizontalAlignment="Left"
                              Text="{DynamicResource NewOtTemplatePlant}"
                              FontWeight="Normal"
                              Padding="2" />

                            <dxe:ComboBoxEdit  Grid.Row="5" Grid.Column="1"  
                                                HorizontalAlignment="Stretch"  
                                                VerticalAlignment="Top" Height="30"
                                                ItemsSource="{Binding DataContext.CustomerPlants, RelativeSource={RelativeSource AncestorType=dxwui:WinUIDialogWindow},UpdateSourceTrigger=PropertyChanged,Mode=TwoWay }" 
                                                DisplayMember="CustomerPlantName" AutoComplete="True" ImmediatePopup="True"
                                                IncrementalFiltering="True" 
                                                IsTextEditable="False" 
                                                ValidateOnTextInput="False">


                                <dxe:ComboBoxEdit.SelectedIndex>
                                    <Binding Path="DataContext.SelectedIndexPlant" RelativeSource="{RelativeSource AncestorType=dxwui:WinUIDialogWindow}" ValidatesOnDataErrors="True" UpdateSourceTrigger="PropertyChanged" Mode="TwoWay">
                                        <Binding.ValidationRules>
                                            <Validation:NewOtTemplateValidation FieldName="SelectedIndexPlant" ValidationStep="UpdatedValue" />
                                        </Binding.ValidationRules>
                                    </Binding>
                                </dxe:ComboBoxEdit.SelectedIndex>

                                    <!--<dxe:ComboBoxEdit.StyleSettings>
                                        <dxe:CheckedComboBoxStyleSettings />
                                    </dxe:ComboBoxEdit.StyleSettings>-->



                                    <dxmvvm:Interaction.Triggers>
                                        <dxmvvm:EventToCommand EventName="PopupClosed" Command="{Binding DataContext.ChangeCustomerNameCommand,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged,RelativeSource={RelativeSource AncestorType=dxwui:WinUIDialogWindow}}" PassEventArgsToCommand="True" />
                                </dxmvvm:Interaction.Triggers>
                            </dxe:ComboBoxEdit>


                            <TextBlock Grid.Row="6"
                               Grid.Column="0"
                               Margin="2" VerticalAlignment="Stretch" HorizontalAlignment="Left"
                               Text="{DynamicResource NewOtTemplateOTFile}"
                               FontWeight="Normal"
                               Padding="2" />

                            <!--<dxe:TextEdit Grid.Row="6" Grid.Column="1" Height="30" Margin="0,0,110,0"
                              Text="{Binding DataContext.RegistrationNumber,RelativeSource={RelativeSource AncestorType=dxlc:GroupBox},UpdateSourceTrigger=PropertyChanged,Mode=TwoWay}"
                              IsReadOnly="False">   
                            </dxe:TextEdit>-->

                            <dxe:ComboBoxEdit Grid.Row="6" Grid.Column="1"
                                           VerticalContentAlignment="Center"
                                           ItemsSource="{Binding DataContext.TemplateAttachementsList,RelativeSource={RelativeSource AncestorType=dxwui:WinUIDialogWindow,Mode=FindAncestor}, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
