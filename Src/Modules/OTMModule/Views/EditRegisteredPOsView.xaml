<dxwui:WinUIDialogWindow
    x:Class="Emdep.Geos.Modules.OTM.Views.EditRegisteredPOsView"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:Converters="clr-namespace:Emdep.Geos.UI.Converters;assembly=Emdep.Geos.UI.Converters"
    xmlns:GeosApplication="clr-namespace:Emdep.Geos.UI.Common;assembly=Emdep.Geos.UI.Common"
    xmlns:UIConverter="clr-namespace:Emdep.Geos.UI.Converters;assembly=Emdep.Geos.UI.Converters"
    xmlns:UIHelper="clr-namespace:Emdep.Geos.UI.Helper;assembly=Emdep.Geos.UI.Helper"
    xmlns:Validation="clr-namespace:Emdep.Geos.UI.Validations;assembly=Emdep.Geos.UI.Validations"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:dx="http://schemas.devexpress.com/winfx/2008/xaml/core"
    xmlns:dxdo="http://schemas.devexpress.com/winfx/2008/xaml/docking"
    xmlns:dxe="http://schemas.devexpress.com/winfx/2008/xaml/editors"
    xmlns:dxg="http://schemas.devexpress.com/winfx/2008/xaml/grid"
    xmlns:dxlc="http://schemas.devexpress.com/winfx/2008/xaml/layoutcontrol"
    xmlns:dxmvvm="http://schemas.devexpress.com/winfx/2008/xaml/mvvm"
    xmlns:dxwui="http://schemas.devexpress.com/winfx/2008/xaml/windowsui"
    xmlns:i="http://schemas.microsoft.com/expression/2010/interactivity"
    xmlns:local="clr-namespace:Emdep.Geos.Modules.OTM.Views"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:sysglb="clr-namespace:System.Globalization;assembly=mscorlib"
    Style="{DynamicResource WinUIDialogWindowStyle}"
    TitleStyle="{DynamicResource WinUIDialogWindowTitleStyle}"
    mc:Ignorable="d">

    <dxwui:WinUIDialogWindow.InputBindings>
        <KeyBinding Key="Escape" Command="{Binding EditRegisteredPOsViewCancelButtonCommand}" />
    </dxwui:WinUIDialogWindow.InputBindings>



    <dxwui:WinUIDialogWindow.Resources>


        <Style x:Key="myGroupBoxStyle" TargetType="{x:Type dxlc:GroupBox}">
            <Setter Property="MaximizeElementVisibility" Value="Visible" />
            <Setter Property="Width" Value="300" />
            <Setter Property="Height" Value="130" />
        </Style>

        <Style x:Key="myGroupBoxStyleForChnagelog" TargetType="{x:Type dxlc:GroupBox}">
            <Setter Property="MaximizeElementVisibility" Value="Visible" />
            <Setter Property="Width" Value="300" />
            <Setter Property="Height" Value="129" />
        </Style>
        <Converters:ForegroundColorConverter x:Key="ForegroundColorConverter" />
        <UIConverter:ImageUrlToByteArrayConverter x:Key="ImageUrlToByteArrayConverter" />
        <UIConverter:BoolToVisibilityConverter x:Key="BoolToVisibilityConverter" />
        <UIConverter:DelegatedToImageURLConverter x:Key="DelegatedToImageURLConverter" />
        <UIConverter:SelectedIndexToVisibilityConverter x:Key="SelectedIndexToVisibilityConverter" />
        <ControlTemplate x:Key="popupContentTemplate">
            <Grid Width="350">
                <Grid.RowDefinitions>
                    <RowDefinition Height="*" />
                </Grid.RowDefinitions>
                <dxg:GridControl
                    x:Name="PART_GridControl"
                    Grid.Row="0"
                    dx:DXSerializer.Enabled="True"
                    AutoExpandAllGroups="True">
                    <dxg:GridControl.Columns>
                        <dxg:GridColumn
                            Width="22"
                            MinWidth="02"
                            MaxWidth="22"
                            FieldName="IdGender"
                            Header=" ">
                            <dxg:GridColumn.CellTemplate>
                                <DataTemplate>
                                    <DockPanel>
                                        <Image
                                            Width="24"
                                            Height="24"
                                            Margin="0,0,0,0"
                                            VerticalAlignment="Center">
                                            <Image.Style>
                                                <Style TargetType="Image">
                                                    <Style.Triggers>
                                                        <DataTrigger Binding="{Binding RowData.Row.IdGender}" Value="2">
                                                            <Setter Property="Image.Source" Value="{DynamicResource OTMMaleGender}" />
                                                        </DataTrigger>
                                                        <DataTrigger Binding="{Binding RowData.Row.IdGender}" Value="1">
                                                            <Setter Property="Image.Source" Value="{DynamicResource OTMFeMaleGender}" />
                                                        </DataTrigger>
                                                        <DataTrigger Binding="{Binding RowData.Row.IdGender}" Value="0">
                                                            <Setter Property="Image.Source" Value="{DynamicResource OTMUnknowMaleGender}" />
                                                        </DataTrigger>
                                                    </Style.Triggers>
                                                </Style>
                                            </Image.Style>
                                        </Image>

                                        <!--<Image Height="24" Width="24"  >
                                            <Image.Style>
                                                <Style TargetType="Image">
                                                    <Style.Triggers>
                                                        <MultiDataTrigger>
                                                            <MultiDataTrigger.Conditions>
                                                                <Condition Binding="{Binding RowData.Row.IdGender}" Value="2" />
                                                            </MultiDataTrigger.Conditions>
                                                            <Setter Property="Source" Value="{DynamicResource OTMMaleGender}" />
                                                        </MultiDataTrigger>
                                                        <MultiDataTrigger>
                                                            <MultiDataTrigger.Conditions>
                                                                <Condition Binding="{Binding RowData.Row.IdGender}" Value="1" />
                                                            </MultiDataTrigger.Conditions>
                                                            <Setter Property="Source" Value="{DynamicResource OTMFeMaleGender}" />
                                                        </MultiDataTrigger>

                                                        <MultiDataTrigger>
                                                            <MultiDataTrigger.Conditions>
                                                                <Condition Binding="{Binding RowData.Row.IdGender}" Value="0" />
                                                            </MultiDataTrigger.Conditions>
                                                            <Setter Property="Source" Value="{DynamicResource OTMUnknowMaleGender}" />
                                                        </MultiDataTrigger>
                                                    </Style.Triggers>
                                                </Style>
                                            </Image.Style>
                                        </Image>-->

                                        <TextBlock
                                            Margin="5,0,0,0"
                                            HorizontalAlignment="Stretch"
                                            VerticalAlignment="Center"
                                            Text="{Binding Type}" />
                                    </DockPanel>
                                </DataTemplate>
                            </dxg:GridColumn.CellTemplate>

                        </dxg:GridColumn>
                        <dxg:GridColumn
                            MinWidth="50"
                            MaxWidth="90"
                            Header="{DynamicResource SenderName}"
                            FieldName="FullName" />
                        <dxg:GridColumn FieldName="SiteName" Header="{DynamicResource SenderPlant}"/>
                    </dxg:GridControl.Columns>
                    <dxg:GridControl.View>
                        <dxg:TableView
                            Name="View"
                            AllowPerPixelScrolling="True"
                            AutoWidth="True"
                            IsSynchronizedWithCurrentItem="False"
                            ScrollAnimationDuration="0"
                            SearchPanelAllowFilter="False"
                            SearchPanelHorizontalAlignment="Stretch"
                            ShowAutoFilterRow="True"
                            ShowGroupedColumns="True"
                            ShowTotalSummary="True">

                            <dxg:TableView.RowStyle>
                                <Style TargetType="{x:Type dxg:RowControl}">
                                    <Style.Triggers>
                                        <MultiDataTrigger>
                                            <MultiDataTrigger.Conditions>

                                                <Condition Binding="{Binding Row.IsInActive}" Value="True" />
                                            </MultiDataTrigger.Conditions>

                                            <MultiDataTrigger.Setters>
                                                <Setter Property="Foreground" Value="Gray" />
                                                <Setter Property="IsEnabled" Value="False" />
                                            </MultiDataTrigger.Setters>
                                        </MultiDataTrigger>
                                    </Style.Triggers>
                                </Style>
                            </dxg:TableView.RowStyle>

                        </dxg:TableView>
                    </dxg:GridControl.View>
                    <!--<dxg:GridControl.Style>
                        <Style TargetType="dxg:GridControl">
                            <Style.Triggers>
                                <DataTrigger Binding="{Binding VisibleRowCount, RelativeSource={RelativeSource Self}}" Value="0">
                                    <Setter Property="ShowLoadingPanel" Value="True" />
                                </DataTrigger>
                            </Style.Triggers>
                        </Style>
                    </dxg:GridControl.Style>-->
                </dxg:GridControl>
            </Grid>
        </ControlTemplate>

    </dxwui:WinUIDialogWindow.Resources>

    <ScrollViewer ScrollViewer.HorizontalScrollBarVisibility="Auto" ScrollViewer.VerticalScrollBarVisibility="Auto">
        <Grid
            MinHeight="800"
            MaxHeight="850"
            ShowGridLines="False">
            <Grid.RowDefinitions>
                <RowDefinition Height="4*" />
                <RowDefinition Height="40*" />
                <RowDefinition Height="50" />
            </Grid.RowDefinitions>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="500" />
                <ColumnDefinition Width="850" />
            </Grid.ColumnDefinitions>


            <!--  [pramod.misal][GEOS2-6465][16.12.2024]  -->
            <TextBlock
                Grid.ColumnSpan="2"
                Margin="0,0,0,15"
                VerticalAlignment="Center"
                FontSize="30"
                FontWeight="Bold"
                Text="{DynamicResource EditRegisteredPOsViewHeader}"
                Visibility="{Binding IsEditPO}" />


            <TextBlock
                Grid.ColumnSpan="2"
                Margin="0,0,0,15"
                VerticalAlignment="Center"
                FontSize="30"
                FontWeight="Bold"
                Text="{DynamicResource AddNewPOTitle}"
                Visibility="{Binding IsAddPO}" />
            <!--  [pramod.misal][GEOS2-6465][16.12.2024]  -->




            <Button
                Grid.Row="0"
                Grid.Column="2"
                Width="75"
                Height="40"
                Margin="0,0,0,5"
                HorizontalAlignment="Right"
                VerticalAlignment="Top"
                VerticalContentAlignment="Top"
                Command="{Binding DataContext.SendEmailToCustomerButtonCommand, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged, RelativeSource={RelativeSource AncestorType=dxwui:WinUIDialogWindow}}"
                CommandParameter="{Binding DataContext.SelectedExpense, UpdateSourceTrigger=PropertyChanged, Mode=TwoWay, RelativeSource={RelativeSource AncestorType={x:Type dxwui:WinUIDialogWindow}}}"
                Content=" "
                Visibility="{Binding DataContext.IsEmailBtnVisibile, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged, RelativeSource={RelativeSource AncestorType=dxwui:WinUIDialogWindow}}">
                <Button.ToolTip>
                    <TextBlock Text="{DynamicResource ButtonSendEmailToCustomer}" />
                </Button.ToolTip>
                <Button.ContentTemplate>
                    <DataTemplate>
                        <Image
                            Width="50"
                            Height="50"
                            Margin="-10"
                            HorizontalAlignment="Stretch"
                            Style="{DynamicResource SendEmailToCustomerButtonImageStyle}" />
                    </DataTemplate>
                </Button.ContentTemplate>
            </Button>

            <dxlc:FlowLayoutControl
                Grid.Row="1"
                Grid.Column="0"
                Grid.ColumnSpan="2"
                Margin="0"
                Padding="0,0,0,0"
                AllowLayerSizing="False"
                AllowMaximizedElementMoving="False"
                Background="Transparent"
                BreakFlowToFit="True"
                ItemSpace="12"
                LayerSpace="12"
                MaximizedElementPosition="Right"
                Orientation="Vertical">

                <dxmvvm:Interaction.Behaviors>
                    <UIHelper:FlowLayoutControlForMinimiseMaximise />
                </dxmvvm:Interaction.Behaviors>



                <!--  Information  -->

                <dxlc:GroupBox
                    x:Name="Information"
                    Selector.IsSelected="True"
                    Style="{StaticResource myGroupBoxStyle}">

                    <!--  HeaderTemplate  -->
                    <dxlc:GroupBox.HeaderTemplate>
                        <DataTemplate>
                            <DockPanel Margin="-5,0,0,0">
                                <TextBlock
                                    Margin="0,0,0,0"
                                    VerticalAlignment="Center"
                                    FontWeight="Bold"
                                    Text="{DynamicResource RegisteredPOsInformation}" />
                                <dxe:TextEdit
                                    HorizontalAlignment="Right"
                                    VerticalAlignment="Center"
                                    FontWeight="Bold"
                                    IsReadOnly="True"
                                    ShowBorder="False">
                                    <Binding
                                        Mode="TwoWay"
                                        Path="DataContext.InformationError"
                                        RelativeSource="{RelativeSource AncestorType=dxlc:GroupBox}"
                                        UpdateSourceTrigger="PropertyChanged"
                                        ValidatesOnDataErrors="True">
                                        <Binding.ValidationRules>
                                            <Validation:UserValidation FieldName="InformationError" ValidationStep="UpdatedValue" />
                                        </Binding.ValidationRules>
                                    </Binding>
                                </dxe:TextEdit>
                            </DockPanel>
                        </DataTemplate>
                    </dxlc:GroupBox.HeaderTemplate>

                    <!--  ContentTemplate  -->
                    <dxlc:GroupBox.ContentTemplate>
                        <DataTemplate>
                            <Grid ShowGridLines="False">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="*" />
                                </Grid.ColumnDefinitions>
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="30" />
                                    <RowDefinition Height="30" />
                                    <RowDefinition Height="*" />
                                </Grid.RowDefinitions>
                                <Label
                                    Grid.Row="0"
                                    Grid.Column="0"
                                    Margin="2"
                                    Padding="2"
                                    Content="{DynamicResource RegisteredPOsType}"
                                    FontWeight="Bold" />
                                <Label
                                    Grid.Row="0"
                                    Grid.Column="1"
                                    Margin="2"
                                    Content=":"
                                    FontWeight="Bold" />
                                <TextBlock
                                    Grid.Row="0"
                                    Grid.Column="2"
                                    Margin="2"
                                    Padding="2"
                                    Text="{Binding DataContext.SelectedType, RelativeSource={RelativeSource AncestorType=dxwui:WinUIDialogWindow}}" />

                                <Label
                                    Grid.Row="1"
                                    Grid.Column="0"
                                    Margin="2"
                                    Padding="2"
                                    Content="{DynamicResource RegisteredPOsCustomerGroup}"
                                    FontWeight="Bold" />
                                <Label
                                    Grid.Row="1"
                                    Grid.Column="1"
                                    Margin="2"
                                    Content=":"
                                    FontWeight="Bold" />
                                <TextBlock
                                    Grid.Row="1"
                                    Grid.Column="2"
                                    Margin="2"
                                    Padding="2"
                                    Text="{Binding DataContext.SelectedCustomerGroupName, Mode=TwoWay, RelativeSource={RelativeSource AncestorType=dxwui:WinUIDialogWindow}, UpdateSourceTrigger=PropertyChanged}"
                                    TextWrapping="Wrap"
                                    ToolTip="{Binding DataContext.SelectedCustomerGroupName, Mode=TwoWay, RelativeSource={RelativeSource AncestorType=dxwui:WinUIDialogWindow}, UpdateSourceTrigger=PropertyChanged}" />
                            </Grid>
                        </DataTemplate>
                    </dxlc:GroupBox.ContentTemplate>

                    <!--  MaximizedContentTemplate  -->
                    <dxlc:GroupBox.MaximizedContentTemplate>
                        <DataTemplate>
                            <ScrollViewer
                                HorizontalContentAlignment="Stretch"
                                ScrollViewer.HorizontalScrollBarVisibility="Auto"
                                ScrollViewer.VerticalScrollBarVisibility="Auto">
                                <Grid ShowGridLines="False">
                                    <Grid.RowDefinitions>
                                        <RowDefinition>
                                            <RowDefinition.Style>
                                                <Style TargetType="RowDefinition">
                                                    <Setter Property="RowDefinition.Height" Value="0" />
                                                    <Style.Triggers>
                                                        <DataTrigger Binding="{Binding DataContext.IsPoCanceled, Mode=TwoWay, RelativeSource={RelativeSource AncestorType=dxwui:WinUIDialogWindow, Mode=FindAncestor}, UpdateSourceTrigger=PropertyChanged}" Value="Visible">
                                                            <Setter Property="RowDefinition.Height" Value="30" />
                                                        </DataTrigger>
                                                    </Style.Triggers>
                                                </Style>
                                            </RowDefinition.Style>
                                        </RowDefinition>
                                        <RowDefinition Height="35" />
                                        <RowDefinition Height="35" />
                                        <RowDefinition Height="35" />
                                        <RowDefinition Height="35" />
                                        <RowDefinition Height="30" />
                                        <RowDefinition Height="40" />
                                        <RowDefinition Height="35" />
                                        <RowDefinition Height="80" />
                                        <RowDefinition Height="35" />
                                        <RowDefinition Height="35" />
                                        <RowDefinition Height="35" />
                                        <RowDefinition Height="35" />
                                        <RowDefinition Height="75" />
                                        <RowDefinition Height="35" />
                                        <RowDefinition Height="35" />


                                    </Grid.RowDefinitions>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="140" />
                                        <ColumnDefinition Width="390" />
                                        <ColumnDefinition Width="30" />
                                        <ColumnDefinition Width="30" />
                                        <ColumnDefinition Width="140" />
                                        <ColumnDefinition Width="250" />
                                        <ColumnDefinition Width="*" />
                                        <ColumnDefinition Width="5" />
                                    </Grid.ColumnDefinitions>

                                    <!--<dxe:TextEdit x:Name="txtStatus"
              Text="{Binding DataContext.TravelExpenseStatus.Name, RelativeSource={RelativeSource AncestorType={x:Type dxlc:GroupBox}},UpdateSourceTrigger=PropertyChanged,Mode=TwoWay}"
              Background="{Binding DataContext.TravelExpenseStatus.HtmlColor, RelativeSource={RelativeSource AncestorType={x:Type dxlc:GroupBox}},UpdateSourceTrigger=PropertyChanged,Mode=TwoWay}"
              Foreground="{Binding ElementName=txtStatus, Path=Background, Converter={Converters:ForegroundColorConverter}}" Grid.Column="0" Grid.Row="0" VerticalAlignment="Stretch" HorizontalAlignment="Stretch"
              IsReadOnly="{Binding DataContext.IsReadOnly,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged,RelativeSource={RelativeSource AncestorType=dxlc:GroupBox}}" IsEnabled="{Binding DataContext.IsEnable,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged,RelativeSource={RelativeSource AncestorType=dxlc:GroupBox}}" ShowBorder="False" TextWrapping="WrapWithOverflow" />-->

                                    <dxe:TextEdit
                                        x:Name="txtStatus"
                                        Grid.Row="0"
                                        Grid.Column="0"
                                        Grid.ColumnSpan="7"
                                        HorizontalAlignment="Stretch"
                                        VerticalAlignment="Stretch"
                                        HorizontalContentAlignment="Center"
                                        Background="red"
                                        IsReadOnly="True"
                                        Text="Canceled"
                                        Visibility="Visible" />

                                    <!--  Type  -->
                                    <Label
                                        Grid.Row="1"
                                        Grid.Column="0"
                                        HorizontalAlignment="Left"
                                        Content="{DynamicResource RegisteredPOsType}" />
                                    <dxe:ComboBoxEdit
                                        x:Name="editor"
                                        Grid.Row="1"
                                        Grid.Column="1"
                                        HorizontalAlignment="Stretch"
                                        VerticalAlignment="Top"
                                        ApplyItemTemplateToSelectedItem="True"
                                        AutoComplete="True"
                                        DisplayMember="Type"
                                        EditValue="{Binding DataContext.potypeName, RelativeSource={RelativeSource AncestorType=dxlc:GroupBox, Mode=FindAncestor}, ValidatesOnDataErrors=True, UpdateSourceTrigger=PropertyChanged, Mode=TwoWay}"
                                        ImmediatePopup="True"
                                        IncrementalFiltering="True"
                                        IsEnabled="{Binding DataContext.IsTypeVisisble, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged, RelativeSource={RelativeSource AncestorType=dxwui:WinUIDialogWindow}}"
                                        IsTextEditable="False"
                                        ItemsSource="{Binding DataContext.POTypeList, RelativeSource={RelativeSource AncestorType=dxwui:WinUIDialogWindow}}"
                                        ValidateOnTextInput="False">
                                        <!--<dxe:ComboBoxEdit.Style>
                                            <Style TargetType="dxe:ComboBoxEdit">
                                                <Setter Property="IsReadOnly" Value="True" />
                                                <Setter Property="IsEnabled" Value="False"/>
                                                <Style.Triggers>
                                                    <DataTrigger Binding="{Binding Source={x:Static GeosApplication:GeosApplication.Instance}, Path=IsOTMViewOnly}" Value="True">
                                                        <Setter Property="IsReadOnly" Value="False" />
                                                        <Setter Property="IsEnabled" Value="True"/>
                                                    </DataTrigger>
                                                    <DataTrigger Binding="{Binding Source={x:Static GeosApplication:GeosApplication.Instance}, Path=IsOTMFullUpdatePO}" Value="True">
                                                        <Setter Property="IsReadOnly" Value="False" />
                                                        <Setter Property="IsEnabled" Value="True"/>
                                                    </DataTrigger>
                                                    <DataTrigger Binding="{Binding Source={x:Static GeosApplication:GeosApplication.Instance}, Path=IsOTMCancelPO}" Value="True">
                                                        <Setter Property="IsReadOnly" Value="False" />
                                                        <Setter Property="IsEnabled" Value="True"/>
                                                    </DataTrigger>
                                                </Style.Triggers>
                                            </Style>
                                        </dxe:ComboBoxEdit.Style>-->

                                        <dxe:ComboBoxEdit.ItemTemplate>
                                            <DataTemplate>
                                                <DockPanel>
                                                    <Image
                                                        Width="24"
                                                        Height="24"
                                                        Margin="0,0,0,0"
                                                        VerticalAlignment="Center">
                                                        <Image.Style>
                                                            <Style TargetType="Image">
                                                                <Style.Triggers>
                                                                    <DataTrigger Binding="{Binding IdPoType}" Value="1">
                                                                        <Setter Property="Image.Source" Value="/Emdep.Geos.Modules.OTM;component/Assets/Images/Star.png" />
                                                                    </DataTrigger>
                                                                    <DataTrigger Binding="{Binding IdPoType}" Value="2">
                                                                        <Setter Property="Image.Source" Value="/Emdep.Geos.Modules.OTM;component/Assets/Images/Green Flag.png" />
                                                                    </DataTrigger>
                                                                    <DataTrigger Binding="{Binding IdPoType}" Value="3">
                                                                        <Setter Property="Image.Source" Value="/Emdep.Geos.Modules.OTM;component/Assets/Images/BlueFlag.png" />
                                                                    </DataTrigger>
                                                                </Style.Triggers>
                                                            </Style>
                                                        </Image.Style>
                                                    </Image>
                                                    <TextBlock
                                                        Margin="5,0,0,0"
                                                        HorizontalAlignment="Stretch"
                                                        VerticalAlignment="Center"
                                                        Text="{Binding Type}" />
                                                </DockPanel>
                                            </DataTemplate>
                                        </dxe:ComboBoxEdit.ItemTemplate>
                                        <!--  SelectedIndex="{Binding DataContext.SelectedIndexCompanyGroup,RelativeSource={RelativeSource AncestorType=dxlc:GroupBox}}"  -->
                                        <dxe:ComboBoxEdit.SelectedIndex>
                                            <Binding
                                                Mode="TwoWay"
                                                Path="DataContext.SelectedIndexPoType"
                                                RelativeSource="{RelativeSource AncestorType=dxwui:WinUIDialogWindow}"
                                                UpdateSourceTrigger="PropertyChanged"
                                                ValidatesOnDataErrors="True">
                                                <Binding.ValidationRules>
                                                    <Validation:CustomerRequiredFieldValidation FieldName="SelectedIndexPoType" ValidationStep="UpdatedValue" />
                                                </Binding.ValidationRules>
                                            </Binding>
                                        </dxe:ComboBoxEdit.SelectedIndex>
                                    </dxe:ComboBoxEdit>

                                    <!--  Customer Group  -->
                                    <Label
                                        Grid.Row="2"
                                        Grid.Column="0"
                                        HorizontalAlignment="Left"
                                        Content="{DynamicResource RegisteredPOsCustomerGroup}" />


                                    <Button
                                        Grid.Row="3"
                                        Grid.Column="0"
                                        Width="20"
                                        Height="20"
                                        Margin="0,-10,5,0"
                                        HorizontalAlignment="Right"
                                        VerticalAlignment="Center"
                                        BorderThickness="0"
                                        Command="{Binding DataContext.EditCustomerGroupButtonCommand, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged, RelativeSource={RelativeSource AncestorType=dxwui:WinUIDialogWindow}}"
                                        CommandParameter="{Binding ElementName=TripsTableView}"
                                        IsEnabled="{Binding DataContext.IsGroupVisisble, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged, RelativeSource={RelativeSource AncestorType=dxwui:WinUIDialogWindow}}"
                                        Visibility="Visible">
                                        <Button.ToolTip>
                                            <TextBlock Text="{DynamicResource CustomerAddViewAddSource}" />
                                        </Button.ToolTip>
                                        <Button.ContentTemplate>
                                            <DataTemplate>
                                                <Image
                                                    Width="20"
                                                    Height="20"
                                                    Margin="-10"
                                                    Style="{DynamicResource OTMInformationIcon}" />
                                            </DataTemplate>
                                        </Button.ContentTemplate>
                                    </Button>

                                    <dxe:ComboBoxEdit
                                        Grid.Row="2"
                                        Grid.Column="1"
                                        Height="30"
                                        HorizontalAlignment="Stretch"
                                        VerticalAlignment="Top"
                                        AutoComplete="True"
                                        DisplayMember="CustomerName"
                                        ImmediatePopup="True"
                                        IncrementalFiltering="True"
                                        IsEnabled="{Binding DataContext.IsGroupVisisble, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged, RelativeSource={RelativeSource AncestorType=dxwui:WinUIDialogWindow}}"
                                        IsTextEditable="False"
                                        ItemsSource="{Binding DataContext.Customers, RelativeSource={RelativeSource AncestorType=dxwui:WinUIDialogWindow}}"
                                        ValidateOnTextInput="False">

                                        <dxe:ComboBoxEdit.ItemContainerStyle>
                                            <Style TargetType="dxe:ComboBoxEditItem">
                                                <Setter Property="IsEnabled" Value="{Binding IsEnabled}" />
                                            </Style>
                                        </dxe:ComboBoxEdit.ItemContainerStyle>

                                        <dxe:ComboBoxEdit.SelectedIndex>
                                            <Binding
                                                Mode="TwoWay"
                                                Path="DataContext.SelectedIndexCustomerGroup"
                                                RelativeSource="{RelativeSource AncestorType=dxwui:WinUIDialogWindow}"
                                                UpdateSourceTrigger="PropertyChanged"
                                                ValidatesOnDataErrors="True">
                                                <Binding.ValidationRules>
                                                    <Validation:CustomerRequiredFieldValidation FieldName="SelectedIndexCustomerGroup" ValidationStep="UpdatedValue" />
                                                </Binding.ValidationRules>
                                            </Binding>
                                        </dxe:ComboBoxEdit.SelectedIndex>

                                        <dxmvvm:Interaction.Triggers>
                                            <dxmvvm:EventToCommand
                                                Command="{Binding DataContext.ChangeCustomerNameCommand, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged, RelativeSource={RelativeSource AncestorType=dxlc:GroupBox}}"
                                                EventName="PopupClosed"
                                                PassEventArgsToCommand="True" />
                                        </dxmvvm:Interaction.Triggers>
                                    </dxe:ComboBoxEdit>

                                    <!--  Customer Plant  -->

                                    <Label
                                        Grid.Row="3"
                                        Grid.Column="0"
                                        HorizontalAlignment="Left"
                                        Content="{DynamicResource RegisteredPOsPlant}" />



                                    <dxe:ComboBoxEdit
                                        x:Name="editor1"
                                        Grid.Row="3"
                                        Grid.Column="1"
                                        Height="30"
                                        HorizontalAlignment="Stretch"
                                        VerticalAlignment="Top"
                                        ApplyItemTemplateToSelectedItem="True"
                                        AutoComplete="True"
                                        ImmediatePopup="True"
                                        IncrementalFiltering="True"
                                        IsEnabled="{Binding DataContext.IsPlantVisisble, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged, RelativeSource={RelativeSource AncestorType=dxwui:WinUIDialogWindow}}"
                                        IsTextEditable="False"
                                        ItemsSource="{Binding DataContext.CustomerPlants, RelativeSource={RelativeSource AncestorType=dxwui:WinUIDialogWindow}}"
                                        ValidateOnTextInput="False">

                                        <dxe:ComboBoxEdit.SelectedIndex>
                                            <Binding
                                                Mode="TwoWay"
                                                Path="DataContext.SelectedIndexCompanyPlant"
                                                RelativeSource="{RelativeSource AncestorType=dxwui:WinUIDialogWindow}"
                                                UpdateSourceTrigger="PropertyChanged"
                                                ValidatesOnDataErrors="True">
                                                <Binding.ValidationRules>
                                                    <Validation:CustomerRequiredFieldValidation FieldName="SelectedIndexCompanyPlant" ValidationStep="UpdatedValue" />
