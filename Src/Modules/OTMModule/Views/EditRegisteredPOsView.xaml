                                    VerticalAlignment="Center"
                                    FontWeight="Bold"
                                    Text="{Binding DataContext.ListPOChangeLog.Count, RelativeSource={RelativeSource AncestorType=dxlc:GroupBox}}" />
                            </DockPanel>
                        </DataTemplate>
                    </dxlc:GroupBox.HeaderTemplate>

                    <!--  ContentTemplate  -->
                    <dxlc:GroupBox.ContentTemplate>
                        <DataTemplate>
                            <StackPanel Margin="0">
                                <Grid x:Name="PART_Root" Margin="-5">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="Auto" />
                                        <ColumnDefinition Width="Auto" />
                                        <ColumnDefinition Width="Auto" />
                                        <ColumnDefinition Width="*" />
                                    </Grid.ColumnDefinitions>
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="30" />
                                        <RowDefinition Height="30" />
                                        <RowDefinition Height="*" />
                                    </Grid.RowDefinitions>
                                    <Label
                                        Grid.Row="0"
                                        Grid.Column="0"
                                        Margin="3,0,0,0"
                                        Padding="2"
                                        Content="{DynamicResource EditRegisteredPOsViewChangeLogUser}"
                                        FontWeight="Bold" />
                                    <TextBlock
                                        Grid.Row="0"
                                        Grid.Column="1"
                                        Margin="3,0,0,0"
                                        Padding="2"
                                        Text="{Binding DataContext.ListPOChangeLog[0].People.FullName, RelativeSource={RelativeSource AncestorType=dxlc:GroupBox}}" />
                                    <Label
                                        Grid.Row="1"
                                        Grid.Column="0"
                                        Margin="3,0,0,0"
                                        Padding="2"
                                        Content="{DynamicResource EditRegisteredPOsViewChangeLogDate}"
                                        FontWeight="Bold" />
                                    <TextBlock
                                        Grid.Row="1"
                                        Grid.Column="1"
                                        Margin="3,0,0,0"
                                        Padding="2"
                                        Text="{Binding DataContext.ListPOChangeLog[0].DateTime, RelativeSource={RelativeSource AncestorType=dxlc:GroupBox}, StringFormat=d, ConverterCulture={x:Static sysglb:CultureInfo.CurrentCulture}}" />
                                    <Label
                                        Grid.Row="2"
                                        Grid.Column="0"
                                        Margin="3,0,0,0"
                                        Padding="2"
                                        Content="{DynamicResource EditRegisteredPOsViewChangeLogAction}"
                                        FontWeight="Bold" />
                                    <TextBlock
                                        Grid.Row="2"
                                        Grid.Column="1"
                                        Grid.ColumnSpan="3"
                                        Margin="3,0,0,0"
                                        Padding="2"
                                        Text="{Binding DataContext.ListPOChangeLog[0].Comments, RelativeSource={RelativeSource AncestorType=dxlc:GroupBox}}"
                                        TextWrapping="WrapWithOverflow"
                                        ToolTip="{Binding DataContext.ListPOChangeLog[0].Comments, RelativeSource={RelativeSource AncestorType=dxlc:GroupBox}}" />
                                    <Grid.Style>
                                        <Style TargetType="{x:Type Grid}">
                                            <Setter Property="Visibility" Value="Visible" />
                                            <Style.Triggers>
                                                <DataTrigger Binding="{Binding DataContext.ListPOChangeLog.Count, RelativeSource={RelativeSource AncestorType=dxlc:GroupBox}}" Value="0">
                                                    <Setter Property="Visibility" Value="Hidden" />
                                                </DataTrigger>
                                            </Style.Triggers>
                                        </Style>
                                    </Grid.Style>
                                </Grid>
                            </StackPanel>
                        </DataTemplate>
                    </dxlc:GroupBox.ContentTemplate>

                    <!--  MaximizedContentTemplate  -->
                    <dxlc:GroupBox.MaximizedContentTemplate>
                        <DataTemplate>
                            <DockPanel LastChildFill="True">
                                <dxg:GridControl
                                    Margin="-10,-10,-10,-10"
                                    Padding="0"
                                    HorizontalAlignment="Stretch"
                                    VerticalAlignment="Top"
                                    DockPanel.Dock="Top"
                                    FilterString="{Binding MyFilterString}"
                                    ImplyNullLikeEmptyStringWhenFiltering="True"
                                    ItemsSource="{Binding DataContext.ListPOChangeLog, RelativeSource={RelativeSource AncestorType=dxlc:GroupBox}, Mode=TwoWay}"
                                    ScrollViewer.VerticalScrollBarVisibility="Visible"
                                    SelectionMode="Row">
                                    <dxg:GridControl.Columns>
                                        <dxg:GridColumn
                                            Width="150"
                                            FieldName="People.FullName"
                                            FilterPopupMode="CheckedList"
                                            FixedWidth="True"
                                            Header="{DynamicResource EditRegisteredPOsViewCommentUser}" />
                                        <dxg:GridColumn
                                            Width="150"
                                            Binding="{Binding DateTime, StringFormat=g}"
                                            FilterPopupMode="CheckedList"
                                            FixedWidth="True"
                                            Header="{DynamicResource EditRegisteredPOsLogDate}">
                                            <dxg:GridColumn.CellTemplate>
                                                <DataTemplate>
                                                    <dxe:TextEdit Name="PART_Editor" DisplayFormatString="g" />
                                                </DataTemplate>
                                            </dxg:GridColumn.CellTemplate>
                                        </dxg:GridColumn>
                                        <dxg:GridColumn
                                            Width="*"
                                            FieldName="Comments"
                                            FilterPopupMode="CheckedList"
                                            FixedWidth="False"
                                            Header="{DynamicResource EditRegisteredPOsAction}" />
                                    </dxg:GridControl.Columns>
                                    <dxmvvm:Interaction.Behaviors>
                                        <UIHelper:GridSelectingBehavior />
                                    </dxmvvm:Interaction.Behaviors>
                                    <dxg:GridControl.View>
                                        <dxg:TableView
                                            Name="commentable"
                                            AllowFixedColumnMenu="False"
                                            AllowGrouping="False"
                                            AllowSorting="True"
                                            ColumnChooserColumnDisplayMode="ShowHiddenColumnsOnly"
                                            NavigationStyle="Row"
                                            RowDetailsVisibilityMode="Visible"
                                            SearchPanelHorizontalAlignment="Stretch"
                                            ShowGroupPanel="False"
                                            ShowHorizontalLines="True"
                                            ShowIndicator="False"
                                            VerticalScrollbarVisibility="Auto" />
                                    </dxg:GridControl.View>
                                </dxg:GridControl>
                            </DockPanel>
                        </DataTemplate>
                    </dxlc:GroupBox.MaximizedContentTemplate>


                </dxlc:GroupBox>



            </dxlc:FlowLayoutControl>
            <StackPanel
                Grid.Row="2"
                Grid.Column="1"
                Grid.ColumnSpan="2"
                Width="290"
                Margin="0,0,500,0"
                HorizontalAlignment="Center"
                Orientation="Horizontal">

                <!--  IsEnabled="{Binding DataContext.IsOTMViewOnly, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged, RelativeSource={RelativeSource AncestorType=dxwui:WinUIDialogWindow} }"  -->
                <Button
                    Width="100"
                    Height="30"
                    Margin="0,0,30,0"
                    VerticalAlignment="Bottom"
                    Command="{Binding EditRegisteredPOsViewAcceptCommand}"
                    Content="{DynamicResource EditRegisteredPOsViewAccept}" />
                <Button
                    Width="100"
                    Height="30"
                    Margin="30,0,0,0"
                    VerticalAlignment="Bottom"
                    Command="{Binding EditRegisteredPOsViewCancelButtonCommand}"
                    Content="{DynamicResource EditRegisteredPOsViewCancel}" />
            </StackPanel>



        </Grid>
    </ScrollViewer>




</dxwui:WinUIDialogWindow>
