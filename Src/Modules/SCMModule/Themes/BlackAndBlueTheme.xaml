    <Style TargetType="dxdo:LayoutPanel" x:Key="LayoutEditButtonStyle">
        <Setter Property="ControlBoxContentTemplate">
            <Setter.Value>
                <DataTemplate>
                    <Button Content="" Style="{DynamicResource SCMAddButtonStyleForMenus}">
                        <Button.ToolTip>
                            <TextBlock Text="{DynamicResource ButtonEdit}"/>
                        </Button.ToolTip>
                        <Button.ContentTemplate>
                            <DataTemplate>
                                <Image Style="{ DynamicResource SCMEditButtonImageStyle}"  Height="10" Width="10"/>
                            </DataTemplate>
                        </Button.ContentTemplate>
                    </Button>
                </DataTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <SolidColorBrush x:Key="LineColorStyle"
                     Color="WhiteSmoke" />
