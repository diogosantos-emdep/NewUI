<dxwui:WinUIDialogWindow x:Class="Emdep.Geos.Modules.SCM.Views.MyPreferencesView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:dxwui="http://schemas.devexpress.com/winfx/2008/xaml/windowsui"    
             xmlns:dx="http://schemas.devexpress.com/winfx/2008/xaml/core"
             xmlns:dxe="http://schemas.devexpress.com/winfx/2008/xaml/editors" 
             xmlns:dxg="http://schemas.devexpress.com/winfx/2008/xaml/grid"
             xmlns:ViewModels="clr-namespace:Emdep.Geos.Modules.SCM.ViewModels"
             xmlns:GeosApplication="clr-namespace:Emdep.Geos.UI.Common;assembly=Emdep.Geos.UI.Common"
             xmlns:dxlc="http://schemas.devexpress.com/winfx/2008/xaml/layoutcontrol"
             xmlns:dxmvvm="http://schemas.devexpress.com/winfx/2008/xaml/mvvm"
             xmlns:Validation ="clr-namespace:Emdep.Geos.UI.Validations;assembly=Emdep.Geos.UI.Validations"
                         xmlns:i="clr-namespace:System.Windows.Interactivity;assembly=System.Windows.Interactivity"
                         xmlns:behaviors="clr-namespace:Emdep.Geos.UI.Behaviors;assembly=Emdep.Geos.UI.Common"
             mc:Ignorable="d" 
             d:DesignHeight="400" d:DesignWidth="900"
             Title="{DynamicResource SCMMainViewModelMyPreferences}"
             TitleStyle="{DynamicResource WinUIDialogWindowTitleStyle}">
    <!--[Shweta.Thube][GEOS2-6629][27.03.2025]-->
    <dxwui:WinUIDialogWindow.Resources>
        <Style x:Key="TextBlock14Style" BasedOn="{ StaticResource TextBlockStyle}" TargetType="{x:Type TextBlock}">
            <Setter Property="FontSize" Value="14" />
        </Style>
    </dxwui:WinUIDialogWindow.Resources>
    <dxwui:WinUIDialogWindow.InputBindings>
        <KeyBinding Key="Escape" Command="{Binding MyPreferencesViewCancelButtonCommand}"/>
    </dxwui:WinUIDialogWindow.InputBindings>
    
    <Grid HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Background="Transparent">
        <Grid.RowDefinitions>
            <RowDefinition Height="10"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="10"/>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="10"/>
            <RowDefinition Height="Auto"/>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="Auto"/>
            <ColumnDefinition Width="20"/>
            <ColumnDefinition Width="*"/>
        </Grid.ColumnDefinitions>
        <dx:DXTabControl Grid.Row="1" Width="750">

            <dx:DXTabItem Header="Shortcuts" Width="150" Height="60">
                <dx:DXTabItem.HeaderTemplate>
                    <DataTemplate>
                        <StackPanel Orientation="Horizontal">
                            <Image Height="25" Width="25" Style="{ DynamicResource ShortcutImageStyle}" />
                            <TextBlock Text="{DynamicResource Shortcuts}" Margin="5,0,0,0"/>
                        </StackPanel>
                    </DataTemplate>
                </dx:DXTabItem.HeaderTemplate>

                <dx:DXTabItem.ContentTemplate>
                    <DataTemplate>
                        <ScrollViewer ScrollViewer.VerticalScrollBarVisibility="Auto" ScrollViewer.HorizontalScrollBarVisibility="Disabled" >
                        <Grid Grid.Column="0" Grid.Row="0" >
                            <Grid.RowDefinitions>
                                <RowDefinition Height="10"/>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="10"/>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="10"/>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="10"/>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="10"/>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="10"/>
                                <RowDefinition Height="Auto"/>
                            </Grid.RowDefinitions>

                            <!--Samples-->
                            <Grid Grid.Row="1">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="5"/>
                                    <ColumnDefinition Width="30"/>
                                    <ColumnDefinition Width="10"/>
                                    <ColumnDefinition Width="Auto"/>
                                </Grid.ColumnDefinitions>
                                <Image Grid.Row="1" Grid.Column="1" Height="25" Width="25" Style="{ DynamicResource SamplesShortcutImageStyle}" />
                                <TextBlock Grid.Row="1" Grid.Column="3"  Text="{DynamicResource PreferenceSamples}" Style="{StaticResource TextBlock14Style}" VerticalAlignment="Center" FontWeight="Bold"/>
                            </Grid>
                            <Grid Grid.Row="3">
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="1"/>
                                    <RowDefinition Height="Auto"/>
                                    <RowDefinition Height="10"/>
                                    <RowDefinition Height="Auto"/>
                                    <RowDefinition Height="10"/>
                                </Grid.RowDefinitions>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="20"/>
                                    <ColumnDefinition Width="30"/>
                                    <ColumnDefinition Width="10"/>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="75"/>
                                    <ColumnDefinition Width="*"/>
                                </Grid.ColumnDefinitions>

                                <!--Create-->

                                <TextBlock Grid.Row="1" Grid.Column="3" Margin="-16,0,0,0"  Text="{DynamicResource PreferenceCreate}" Style="{StaticResource TextBlock14Style}" VerticalAlignment="Center" />
                                <dxe:TextEdit Margin="28,0,0,0" Text="{Binding DataContext.Create,RelativeSource={RelativeSource AncestorType=dxwui:WinUIDialogWindow}}" 
                              Height="30"   IsEnabled="{Binding DataContext.IsEnabledSampleRegistration,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged,RelativeSource={RelativeSource AncestorType={x:Type dxwui:WinUIDialogWindow}}}"
										  
                              MaxLength="100" TextWrapping="Wrap"
                              Grid.Row="1" Grid.Column="5" Grid.ColumnSpan="2"
                              HorizontalAlignment="Stretch" IsReadOnly="True"
                              >
                                    <dxmvvm:Interaction.Behaviors>
                                        <dxmvvm:EventToCommand 
                                            EventName="KeyDown" Command="{Binding DataContext.Create_KeyDownCommand, RelativeSource={RelativeSource AncestorType=dxwui:WinUIDialogWindow}}" PassEventArgsToCommand="True">
                                        </dxmvvm:EventToCommand>
                                        <dxmvvm:KeyToCommand Event="PreviewKeyDown" KeyGesture="Ctrl+A"  Command="{Binding DataContext.Create_PreviewKeyDownAllCommand, RelativeSource={RelativeSource AncestorType={x:Type dxwui:WinUIDialogWindow}}}" MarkRoutedEventsAsHandled="True"/>
                                        <dxmvvm:KeyToCommand Event="PreviewKeyDown" KeyGesture="Ctrl+C"  Command="{Binding DataContext.Create_PreviewKeyDownCopyCommand, RelativeSource={RelativeSource AncestorType={x:Type dxwui:WinUIDialogWindow}}}" MarkRoutedEventsAsHandled="True"/>
                                        <dxmvvm:KeyToCommand Event="PreviewKeyDown" KeyGesture="Ctrl+V" Command="{Binding DataContext.Create_PreviewKeyDownPasteCommand, RelativeSource={RelativeSource AncestorType={x:Type dxwui:WinUIDialogWindow}}}" CommandParameter="{Binding Path=InvokeParameter, RelativeSource={RelativeSource Self}}" MarkRoutedEventsAsHandled="True"/>
                                    </dxmvvm:Interaction.Behaviors>
                                    <!--<dxe:TextEdit.EditValue>
                                        <Binding Path="DataContext.Create" RelativeSource="{RelativeSource AncestorType=dxwui:WinUIDialogWindow}" ValidatesOnDataErrors="True" UpdateSourceTrigger="PropertyChanged" Mode="TwoWay">
                                            -->
                                    <!--<Binding.ValidationRules>
                                                <Validation:MyPreferencesValidations FieldName="Create" />
                                            </Binding.ValidationRules>-->
                                    <!--
                                        </Binding>
                                    </dxe:TextEdit.EditValue>-->
                                </dxe:TextEdit>

                                <!--Search-->

                                <TextBlock Grid.Row="3" Grid.Column="3" Margin="-16,0,0,0" Text="{DynamicResource PreferenceSearch}" Style="{StaticResource TextBlock14Style}" VerticalAlignment="Center" />
                                <dxe:TextEdit Text="{Binding DataContext.Search,RelativeSource={RelativeSource AncestorType=dxwui:WinUIDialogWindow}}" 
                              Height="30" Margin="28,0,0,0" 
                                              IsEnabled="{Binding DataContext.IsEnabledReportAndSampleSearch,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged,RelativeSource={RelativeSource AncestorType={x:Type dxwui:WinUIDialogWindow}}}"
                              MaxLength="100" TextWrapping="Wrap"
                              Grid.Row="3" Grid.Column="5" Grid.ColumnSpan="4"
                              HorizontalAlignment="Stretch" IsReadOnly="True"
                              >
                                    <!--<dxe:TextEdit.EditValue>
                                        <Binding Path="DataContext.Search" RelativeSource="{RelativeSource AncestorType=dxwui:WinUIDialogWindow}" ValidatesOnDataErrors="True" UpdateSourceTrigger="PropertyChanged" Mode="TwoWay">
                                            <Binding.ValidationRules>
                                                <Validation:MyPreferencesValidations FieldName="Search" />
                                            </Binding.ValidationRules>
                                        </Binding>
                                    </dxe:TextEdit.EditValue>-->
                                    <dxmvvm:Interaction.Behaviors>
                                        <dxmvvm:EventToCommand 
                                            EventName="KeyDown" Command="{Binding DataContext.Search_KeyDownCommand, RelativeSource={RelativeSource AncestorType=dxwui:WinUIDialogWindow}}" PassEventArgsToCommand="True">
                                        </dxmvvm:EventToCommand>
                                        <dxmvvm:KeyToCommand Event="PreviewKeyDown" KeyGesture="Ctrl+A"  Command="{Binding DataContext.Search_PreviewKeyDownAllCommand, RelativeSource={RelativeSource AncestorType={x:Type dxwui:WinUIDialogWindow}}}" MarkRoutedEventsAsHandled="True"/>
                                        <dxmvvm:KeyToCommand Event="PreviewKeyDown" KeyGesture="Ctrl+C"  Command="{Binding DataContext.Search_PreviewKeyDownCopyCommand, RelativeSource={RelativeSource AncestorType={x:Type dxwui:WinUIDialogWindow}}}" MarkRoutedEventsAsHandled="True"/>
                                        <dxmvvm:KeyToCommand Event="PreviewKeyDown" KeyGesture="Ctrl+V" Command="{Binding DataContext.Search_PreviewKeyDownPasteCommand, RelativeSource={RelativeSource AncestorType={x:Type dxwui:WinUIDialogWindow}}}" CommandParameter="{Binding Path=InvokeParameter, RelativeSource={RelativeSource Self}}" MarkRoutedEventsAsHandled="True"/>
                                    </dxmvvm:Interaction.Behaviors>
                                </dxe:TextEdit>

                            </Grid>
                            <Grid Grid.Row="5">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="5"/>
                                    <ColumnDefinition Width="30"/>
                                    <ColumnDefinition Width="10"/>
                                    <ColumnDefinition Width="Auto"/>
                                </Grid.ColumnDefinitions>
                                <!--Managers-->
                                <Image Grid.Row="3" Grid.Column="1" Height="25" Width="25" Style="{ DynamicResource ManagerShortcutImageStyle}" />
                                <TextBlock Grid.Row="3" Grid.Column="3"  Text="{DynamicResource PreferenceManager}" Style="{StaticResource TextBlock14Style}" VerticalAlignment="Center" FontWeight="Bold" />
                            </Grid>
                            <Grid Grid.Row="7">
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="1"/>
                                    <RowDefinition Height="Auto"/>
                                    <RowDefinition Height="10"/>
                                    <RowDefinition Height="Auto"/>
                                    <RowDefinition Height="10"/>
                                    <RowDefinition Height="Auto"/>
                                    <RowDefinition Height="10"/>
                                    <RowDefinition Height="Auto"/>
                                </Grid.RowDefinitions>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="20"/>
                                    <ColumnDefinition Width="30"/>
                                    <ColumnDefinition Width="10"/>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="30"/>
                                    <ColumnDefinition Width="*"/>
                                </Grid.ColumnDefinitions>
                                <!--Locations-->

                                <TextBlock Grid.Row="1" Grid.Column="3"  Margin="-16,0,0,0" Text="{DynamicResource PreferenceManagerLocations}" Style="{StaticResource TextBlock14Style}" VerticalAlignment="Center" />
                                <dxe:TextEdit Text="{Binding DataContext.Locations,RelativeSource={RelativeSource AncestorType=dxwui:WinUIDialogWindow}}" 
                              Height="30" Margin="48,0,0,0"  
                                                IsEnabled="{Binding DataContext.IsEnabledLocationManager,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged,RelativeSource={RelativeSource AncestorType={x:Type dxwui:WinUIDialogWindow}}}"
                              MaxLength="100" TextWrapping="Wrap"
                              Grid.Row="1" Grid.Column="5" Grid.ColumnSpan="4"
                              HorizontalAlignment="Stretch" IsReadOnly="True"
                              >
                                    <!--<dxe:TextEdit.EditValue>
                                        <Binding Path="DataContext.Locations" RelativeSource="{RelativeSource AncestorType=dxwui:WinUIDialogWindow}" ValidatesOnDataErrors="True" UpdateSourceTrigger="PropertyChanged" Mode="TwoWay">
                                            <Binding.ValidationRules>
                                                <Validation:MyPreferencesValidations FieldName="Locations" />
                                            </Binding.ValidationRules>
                                        </Binding>
                                    </dxe:TextEdit.EditValue>-->
                                    <dxmvvm:Interaction.Behaviors>
                                        <dxmvvm:EventToCommand 
                                            EventName="KeyDown" Command="{Binding DataContext.Locations_KeyDownCommand, RelativeSource={RelativeSource AncestorType=dxwui:WinUIDialogWindow}}" PassEventArgsToCommand="True">
                                        </dxmvvm:EventToCommand>
                                        <dxmvvm:KeyToCommand Event="PreviewKeyDown" KeyGesture="Ctrl+A"  Command="{Binding DataContext.Locations_PreviewKeyDownAllCommand, RelativeSource={RelativeSource AncestorType={x:Type dxwui:WinUIDialogWindow}}}" MarkRoutedEventsAsHandled="True"/>
                                        <dxmvvm:KeyToCommand Event="PreviewKeyDown" KeyGesture="Ctrl+C"  Command="{Binding DataContext.Locations_PreviewKeyDownCopyCommand, RelativeSource={RelativeSource AncestorType={x:Type dxwui:WinUIDialogWindow}}}" MarkRoutedEventsAsHandled="True"/>
                                        <dxmvvm:KeyToCommand Event="PreviewKeyDown" KeyGesture="Ctrl+V" Command="{Binding DataContext.Locations_PreviewKeyDownPasteCommand, RelativeSource={RelativeSource AncestorType={x:Type dxwui:WinUIDialogWindow}}}" CommandParameter="{Binding Path=InvokeParameter, RelativeSource={RelativeSource Self}}" MarkRoutedEventsAsHandled="True"/>

                                    </dxmvvm:Interaction.Behaviors>
                                </dxe:TextEdit>
                                <!--Properties-->

                                <TextBlock Grid.Row="3" Grid.Column="3"  Margin="-16,0,0,0" Text="{DynamicResource PreferenceManagerProperties}" Style="{StaticResource TextBlock14Style}" VerticalAlignment="Center" />
                                <dxe:TextEdit  Margin="48,0,0,0" Text="{Binding DataContext.Properties,RelativeSource={RelativeSource AncestorType=dxwui:WinUIDialogWindow}}" 
                              Height="30" 
                              MaxLength="100" TextWrapping="Wrap"  IsEnabled="{Binding DataContext.IsEnabledConfigurationSection,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged,RelativeSource={RelativeSource AncestorType={x:Type dxwui:WinUIDialogWindow}}}"
                            
                              Grid.Row="3" Grid.Column="5" Grid.ColumnSpan="4"
                              HorizontalAlignment="Stretch" IsReadOnly="True"
                              >
                                    <!--<dxe:TextEdit.EditValue>
                                        <Binding Path="DataContext.Properties" RelativeSource="{RelativeSource AncestorType=dxwui:WinUIDialogWindow}" ValidatesOnDataErrors="True" UpdateSourceTrigger="PropertyChanged" Mode="TwoWay">
                                            <Binding.ValidationRules>
                                                <Validation:MyPreferencesValidations FieldName="Properties" />
                                            </Binding.ValidationRules>
                                        </Binding>
                                    </dxe:TextEdit.EditValue>-->
                                    <dxmvvm:Interaction.Behaviors>
                                        <dxmvvm:EventToCommand 
                                            EventName="KeyDown" Command="{Binding DataContext.Properties_KeyDownCommand, RelativeSource={RelativeSource AncestorType=dxwui:WinUIDialogWindow}}" PassEventArgsToCommand="True">
                                        </dxmvvm:EventToCommand>
                                        <dxmvvm:KeyToCommand Event="PreviewKeyDown" KeyGesture="Ctrl+A"  Command="{Binding DataContext.Properties_PreviewKeyDownAllCommand, RelativeSource={RelativeSource AncestorType={x:Type dxwui:WinUIDialogWindow}}}" MarkRoutedEventsAsHandled="True"/>
                                        <dxmvvm:KeyToCommand Event="PreviewKeyDown" KeyGesture="Ctrl+C"  Command="{Binding DataContext.Properties_PreviewKeyDownCopyCommand, RelativeSource={RelativeSource AncestorType={x:Type dxwui:WinUIDialogWindow}}}" MarkRoutedEventsAsHandled="True"/>
                                        <dxmvvm:KeyToCommand Event="PreviewKeyDown" KeyGesture="Ctrl+V" Command="{Binding DataContext.Properties_PreviewKeyDownPasteCommand, RelativeSource={RelativeSource AncestorType={x:Type dxwui:WinUIDialogWindow}}}" CommandParameter="{Binding Path=InvokeParameter, RelativeSource={RelativeSource Self}}" MarkRoutedEventsAsHandled="True"/>

                                    </dxmvvm:Interaction.Behaviors>
                                </dxe:TextEdit>

                                <!--Families-->

                                <TextBlock Grid.Row="5" Grid.Column="3" Margin="-16,0,0,0" Text="{DynamicResource PreferenceManagerFamilies}" Style="{StaticResource TextBlock14Style}" VerticalAlignment="Center" />
                                <dxe:TextEdit  Margin="48,0,0,0" Text="{Binding DataContext.Families,RelativeSource={RelativeSource AncestorType=dxwui:WinUIDialogWindow}}" 
Height="30" IsEnabled="{Binding DataContext.IsEnabledConfigurationSection,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged,RelativeSource={RelativeSource AncestorType={x:Type dxwui:WinUIDialogWindow}}}"
MaxLength="100" TextWrapping="Wrap"  
Grid.Row="5" Grid.Column="5" Grid.ColumnSpan="4"
HorizontalAlignment="Stretch" IsReadOnly="True"
>
                                    <!--<dxe:TextEdit.EditValue>
                                        <Binding Path="DataContext.Families" RelativeSource="{RelativeSource AncestorType=dxwui:WinUIDialogWindow}" ValidatesOnDataErrors="True" UpdateSourceTrigger="PropertyChanged" Mode="TwoWay">
                                            <Binding.ValidationRules>
                                                <Validation:MyPreferencesValidations FieldName="Families" />
                                            </Binding.ValidationRules>
                                        </Binding>
                                    </dxe:TextEdit.EditValue>-->
                                    <dxmvvm:Interaction.Behaviors>
                                        <dxmvvm:EventToCommand 
              EventName="KeyDown" Command="{Binding DataContext.Families_KeyDownCommand, RelativeSource={RelativeSource AncestorType=dxwui:WinUIDialogWindow}}" PassEventArgsToCommand="True">
                                        </dxmvvm:EventToCommand>
                                        <dxmvvm:KeyToCommand Event="PreviewKeyDown" KeyGesture="Ctrl+A"  Command="{Binding DataContext.Families_PreviewKeyDownAllCommand, RelativeSource={RelativeSource AncestorType={x:Type dxwui:WinUIDialogWindow}}}" MarkRoutedEventsAsHandled="True"/>
                                        <dxmvvm:KeyToCommand Event="PreviewKeyDown" KeyGesture="Ctrl+C"  Command="{Binding DataContext.Families_PreviewKeyDownCopyCommand, RelativeSource={RelativeSource AncestorType={x:Type dxwui:WinUIDialogWindow}}}" MarkRoutedEventsAsHandled="True"/>
                                        <dxmvvm:KeyToCommand Event="PreviewKeyDown" KeyGesture="Ctrl+V" Command="{Binding DataContext.Families_PreviewKeyDownPasteCommand, RelativeSource={RelativeSource AncestorType={x:Type dxwui:WinUIDialogWindow}}}" CommandParameter="{Binding Path=InvokeParameter, RelativeSource={RelativeSource Self}}" MarkRoutedEventsAsHandled="True"/>

                                    </dxmvvm:Interaction.Behaviors>
                                </dxe:TextEdit>

                                <TextBlock Grid.Row="7" Margin="-16,0,0,0" Grid.Column="3"  Text="{DynamicResource PreferenceManagerSearch}" Style="{StaticResource TextBlock14Style}" VerticalAlignment="Center" />
                                <dxe:TextEdit  Margin="48,0,0,0" Text="{Binding DataContext.SearchManager,RelativeSource={RelativeSource AncestorType=dxwui:WinUIDialogWindow}}" 
Height="30"  
                                               IsEnabled="{Binding DataContext.IsEnabledConfigurationSection,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged,RelativeSource={RelativeSource AncestorType={x:Type dxwui:WinUIDialogWindow}}}"
MaxLength="100" TextWrapping="Wrap" 
Grid.Row="7" Grid.Column="5" Grid.ColumnSpan="4"
HorizontalAlignment="Stretch" IsReadOnly="True"
>
                                    <!--<dxe:TextEdit.EditValue>
                                        <Binding Path="DataContext.SearchManager" RelativeSource="{RelativeSource AncestorType=dxwui:WinUIDialogWindow}" ValidatesOnDataErrors="True" UpdateSourceTrigger="PropertyChanged" Mode="TwoWay">
                                            <Binding.ValidationRules>
                                                <Validation:MyPreferencesValidations FieldName="SearchManager" />
                                            </Binding.ValidationRules>
                                        </Binding>
                                    </dxe:TextEdit.EditValue>-->
                                    <dxmvvm:Interaction.Behaviors>
                                        <dxmvvm:EventToCommand 
              EventName="KeyDown" Command="{Binding DataContext.SearchManager_KeyDownCommand, RelativeSource={RelativeSource AncestorType=dxwui:WinUIDialogWindow}}" PassEventArgsToCommand="True">
                                        </dxmvvm:EventToCommand>
                                        <dxmvvm:KeyToCommand Event="PreviewKeyDown" KeyGesture="Ctrl+A"  Command="{Binding DataContext.SearchManager_PreviewKeyDownAllCommand, RelativeSource={RelativeSource AncestorType={x:Type dxwui:WinUIDialogWindow}}}" MarkRoutedEventsAsHandled="True"/>
                                        <dxmvvm:KeyToCommand Event="PreviewKeyDown" KeyGesture="Ctrl+C"  Command="{Binding DataContext.SearchManager_PreviewKeyDownCopyCommand, RelativeSource={RelativeSource AncestorType={x:Type dxwui:WinUIDialogWindow}}}" MarkRoutedEventsAsHandled="True"/>
                                        <dxmvvm:KeyToCommand Event="PreviewKeyDown" KeyGesture="Ctrl+V" Command="{Binding DataContext.SearchManager_PreviewKeyDownPasteCommand, RelativeSource={RelativeSource AncestorType={x:Type dxwui:WinUIDialogWindow}}}" CommandParameter="{Binding Path=InvokeParameter, RelativeSource={RelativeSource Self}}" MarkRoutedEventsAsHandled="True"/>

                                    </dxmvvm:Interaction.Behaviors>
                                </dxe:TextEdit>

                            </Grid>
                            <Grid Grid.Row="9">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="5"/>
                                    <ColumnDefinition Width="30"/>
                                    <ColumnDefinition Width="10"/>
                                    <ColumnDefinition Width="Auto"/>
                                </Grid.ColumnDefinitions>
                                <!--Reports-->
                                <Image Grid.Row="3" Grid.Column="1" Height="25" Width="25" Style="{ DynamicResource ReportShortcutImageStyle}" />
                                <TextBlock Grid.Row="3" Grid.Column="3"  Text="{DynamicResource PreferenceReports}" Style="{StaticResource TextBlock14Style}" VerticalAlignment="Center" FontWeight="Bold" />
                            </Grid>
                            <Grid Grid.Row="11">
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="1"/>
                                    <RowDefinition Height="Auto"/>
                                    <RowDefinition Height="10"/>
                                    <RowDefinition Height="Auto"/>
                                    <RowDefinition Height="10"/>
                                    <RowDefinition Height="Auto"/>
                                    <RowDefinition Height="10"/>
                                    <RowDefinition Height="Auto"/>
                                </Grid.RowDefinitions>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="20"/>
                                    <ColumnDefinition Width="30"/>
                                    <ColumnDefinition Width="10"/>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="30"/>
                                    <ColumnDefinition Width="*"/>
                                </Grid.ColumnDefinitions>
                                <!--New samples-->

                                <TextBlock Grid.Row="1" Margin="-16,0,0,0" Grid.Column="3"  Text="{DynamicResource PreferenceReportsNewSamples}" Style="{StaticResource TextBlock14Style}" VerticalAlignment="Center" />
                                <dxe:TextEdit Text="{Binding DataContext.NewSamples,RelativeSource={RelativeSource AncestorType=dxwui:WinUIDialogWindow}}" 
                              Height="30" 
                                              IsEnabled="{Binding DataContext.IsEnabledReportAndSampleSearch,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged,RelativeSource={RelativeSource AncestorType={x:Type dxwui:WinUIDialogWindow}}}"
                              MaxLength="100" TextWrapping="Wrap"
                              Grid.Row="1" Grid.Column="5" Grid.ColumnSpan="4"
                              HorizontalAlignment="Stretch" IsReadOnly="True"
                              >
                                    <!--<dxe:TextEdit.EditValue>
                                        <Binding Path="DataContext.NewSamples" RelativeSource="{RelativeSource AncestorType=dxwui:WinUIDialogWindow}" ValidatesOnDataErrors="True" UpdateSourceTrigger="PropertyChanged" Mode="TwoWay">
                                            <Binding.ValidationRules>
                                                <Validation:MyPreferencesValidations FieldName="NewSamples" />
                                            </Binding.ValidationRules>
                                        </Binding>
                                    </dxe:TextEdit.EditValue>-->
                                    <dxmvvm:Interaction.Behaviors>
                                        <dxmvvm:EventToCommand 
                                            EventName="KeyDown" Command="{Binding DataContext.NewSamples_KeyDownCommand, RelativeSource={RelativeSource AncestorType=dxwui:WinUIDialogWindow}}" PassEventArgsToCommand="True">
                                        </dxmvvm:EventToCommand>
                                        <dxmvvm:KeyToCommand Event="PreviewKeyDown" KeyGesture="Ctrl+A"  Command="{Binding DataContext.NewSamples_PreviewKeyDownAllCommand, RelativeSource={RelativeSource AncestorType={x:Type dxwui:WinUIDialogWindow}}}" MarkRoutedEventsAsHandled="True"/>
                                        <dxmvvm:KeyToCommand Event="PreviewKeyDown" KeyGesture="Ctrl+C"  Command="{Binding DataContext.NewSamples_PreviewKeyDownCopyCommand, RelativeSource={RelativeSource AncestorType={x:Type dxwui:WinUIDialogWindow}}}" MarkRoutedEventsAsHandled="True"/>
                                        <dxmvvm:KeyToCommand Event="PreviewKeyDown" KeyGesture="Ctrl+V" Command="{Binding DataContext.NewSamples_PreviewKeyDownPasteCommand, RelativeSource={RelativeSource AncestorType={x:Type dxwui:WinUIDialogWindow}}}" CommandParameter="{Binding Path=InvokeParameter, RelativeSource={RelativeSource Self}}" MarkRoutedEventsAsHandled="True"/>

                                    </dxmvvm:Interaction.Behaviors>
                                </dxe:TextEdit>
                                <!--Modified samples-->

                                <TextBlock Grid.Row="3" Grid.Column="3" Margin="-16,0,0,0" Text="{DynamicResource PreferenceReportsModifiedSamples}" Style="{StaticResource TextBlock14Style}" VerticalAlignment="Center" />
                                <dxe:TextEdit Text="{Binding DataContext.ModifiedSamples,RelativeSource={RelativeSource AncestorType=dxwui:WinUIDialogWindow}}" 
                              Height="30"
                              MaxLength="100" TextWrapping="Wrap" IsEnabled="{Binding DataContext.IsEnabledReportAndSampleSearch,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged,RelativeSource={RelativeSource AncestorType={x:Type dxwui:WinUIDialogWindow}}}"
                              Grid.Row="3" Grid.Column="5" Grid.ColumnSpan="4"
                              HorizontalAlignment="Stretch" IsReadOnly="True"
                              >
                                    <!--<dxe:TextEdit.EditValue>
                                        <Binding Path="DataContext.ModifiedSamples" RelativeSource="{RelativeSource AncestorType=dxwui:WinUIDialogWindow}" ValidatesOnDataErrors="True" UpdateSourceTrigger="PropertyChanged" Mode="TwoWay">
                                            <Binding.ValidationRules>
                                                <Validation:MyPreferencesValidations FieldName="ModifiedSamples" />
                                            </Binding.ValidationRules>
                                        </Binding>
                                    </dxe:TextEdit.EditValue>-->
                                    <dxmvvm:Interaction.Behaviors>
                                        <dxmvvm:EventToCommand 
                                            EventName="KeyDown" Command="{Binding DataContext.ModifiedSamples_KeyDownCommand, RelativeSource={RelativeSource AncestorType=dxwui:WinUIDialogWindow}}" PassEventArgsToCommand="True">
                                        </dxmvvm:EventToCommand>
                                        <dxmvvm:KeyToCommand Event="PreviewKeyDown" KeyGesture="Ctrl+A"  Command="{Binding DataContext.ModifiedSamples_PreviewKeyDownAllCommand, RelativeSource={RelativeSource AncestorType={x:Type dxwui:WinUIDialogWindow}}}" MarkRoutedEventsAsHandled="True"/>
                                        <dxmvvm:KeyToCommand Event="PreviewKeyDown" KeyGesture="Ctrl+C"  Command="{Binding DataContext.ModifiedSamples_PreviewKeyDownCopyCommand, RelativeSource={RelativeSource AncestorType={x:Type dxwui:WinUIDialogWindow}}}" MarkRoutedEventsAsHandled="True"/>
                                        <dxmvvm:KeyToCommand Event="PreviewKeyDown" KeyGesture="Ctrl+V" Command="{Binding DataContext.ModifiedSamples_PreviewKeyDownPasteCommand, RelativeSource={RelativeSource AncestorType={x:Type dxwui:WinUIDialogWindow}}}" CommandParameter="{Binding Path=InvokeParameter, RelativeSource={RelativeSource Self}}" MarkRoutedEventsAsHandled="True"/>

                                    </dxmvvm:Interaction.Behaviors>
                                </dxe:TextEdit>

                                <!--3D connectors-->

                                <TextBlock Grid.Row="5" Grid.Column="3" Margin="-16,0,0,0" Text="{DynamicResource PreferenceReports3DConnectors}" Style="{StaticResource TextBlock14Style}" VerticalAlignment="Center" />
