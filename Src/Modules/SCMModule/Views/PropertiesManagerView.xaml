<dxwui:WinUIDialogWindow x:Class="Emdep.Geos.Modules.SCM.Views.PropertiesManagerView"
           xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:dxwui="http://schemas.devexpress.com/winfx/2008/xaml/windowsui"    
             xmlns:dxmvvm="http://schemas.devexpress.com/winfx/2008/xaml/mvvm" 
             xmlns:ViewModels="clr-namespace:Emdep.Geos.Modules.SCM.ViewModels"
             xmlns:dx="http://schemas.devexpress.com/winfx/2008/xaml/core"
             xmlns:dxe="http://schemas.devexpress.com/winfx/2008/xaml/editors" 
             xmlns:dxg="http://schemas.devexpress.com/winfx/2008/xaml/grid"
             xmlns:GeosApplication="clr-namespace:Emdep.Geos.UI.Common;assembly=Emdep.Geos.UI.Common"
             xmlns:dxlc="http://schemas.devexpress.com/winfx/2008/xaml/layoutcontrol"
             xmlns:Validation ="clr-namespace:Emdep.Geos.UI.Validations;assembly=Emdep.Geos.UI.Validations"
             xmlns:local="clr-namespace:Emdep.Geos.Modules.SCM.Views"
             xmlns:sys="clr-namespace:System;assembly=mscorlib"
             xmlns:io="clr-namespace:System.IO.Ports;assembly=System"
             xmlns:dxdo="http://schemas.devexpress.com/winfx/2008/xaml/docking"
             xmlns:UIHelper="clr-namespace:Emdep.Geos.UI.Helper;assembly=Emdep.Geos.UI.Helper"
             xmlns:Converters="clr-namespace:Emdep.Geos.UI.Converters;assembly=Emdep.Geos.UI.Converters"
             xmlns:i="clr-namespace:System.Windows.Interactivity;assembly=System.Windows.Interactivity"
             xmlns:behaviors="clr-namespace:Emdep.Geos.UI.Behaviors;assembly=Emdep.Geos.UI.Common"
             mc:Ignorable="d"
             d:DesignHeight="300" d:DesignWidth="300"
                      >
    <dxwui:WinUIDialogWindow.Resources>
        <Style x:Key="ImageStyle" TargetType="Image">
            <Style.Triggers>
                <MultiDataTrigger>
                    <MultiDataTrigger.Conditions>
                        <Condition Binding="{Binding Id}" Value="2" />
                        <Condition Binding="{Binding Path=(dx:ThemeManager.TreeWalker).ThemeName, RelativeSource={RelativeSource Self}}" Value="BlackAndBlue" />
                    </MultiDataTrigger.Conditions>
                    <Setter Property="Source" Value="/Emdep.Geos.Modules.SCM;component/Assets/Images/FemaleUser_White_32x32.png" />
                </MultiDataTrigger>
                <MultiDataTrigger>
                    <MultiDataTrigger.Conditions>
                        <Condition Binding="{Binding Id}" Value="2" />
                        <Condition Binding="{Binding Path=(dx:ThemeManager.TreeWalker).ThemeName, RelativeSource={RelativeSource Self}}" Value="WhiteAndBlue" />
                    </MultiDataTrigger.Conditions>
                    <Setter Property="Source" Value="/Emdep.Geos.Modules.SCM;component/Assets/Images/FemaleUser_Blue_32x32.png" />
                </MultiDataTrigger>
                <MultiDataTrigger>
                    <MultiDataTrigger.Conditions>
                        <Condition Binding="{Binding Id}" Value="1" />
                        <Condition Binding="{Binding Path=(dx:ThemeManager.TreeWalker).ThemeName, RelativeSource={RelativeSource Self}}" Value="BlackAndBlue" />
                    </MultiDataTrigger.Conditions>
                    <Setter Property="Source" Value="/Emdep.Geos.Modules.SCM;component/Assets/Images/MaleUser_White_32x32.png" />
                </MultiDataTrigger>
                <MultiDataTrigger>
                    <MultiDataTrigger.Conditions>
                        <Condition Binding="{Binding Id}" Value="1" />
                        <Condition Binding="{Binding Path=(dx:ThemeManager.TreeWalker).ThemeName, RelativeSource={RelativeSource Self}}" Value="WhiteAndBlue" />
                    </MultiDataTrigger.Conditions>
                    <Setter Property="Source" Value="/Emdep.Geos.Modules.SCM;component/Assets/Images/MaleUser_Blue_32x32.png" />
                </MultiDataTrigger>
            </Style.Triggers>
        </Style>
        <Style x:Key="ColorImageStyle" TargetType="Image">
            <Style.Triggers>
                <MultiDataTrigger>
                    <MultiDataTrigger.Conditions>
                        <Condition Binding="{Binding Id}" Value="12" />
                        <Condition Binding="{Binding Path=(dx:ThemeManager.TreeWalker).ThemeName, RelativeSource={RelativeSource Self}}" Value="BlackAndBlue" />
                    </MultiDataTrigger.Conditions>
                    <Setter Property="Source" Value="/Emdep.Geos.Modules.SCM;component/Assets/Images/wt_color_transparent.png" />
                </MultiDataTrigger>
                <MultiDataTrigger>
                    <MultiDataTrigger.Conditions>
                        <Condition Binding="{Binding Id}" Value="12" />
                        <Condition Binding="{Binding Path=(dx:ThemeManager.TreeWalker).ThemeName, RelativeSource={RelativeSource Self}}" Value="WhiteAndBlue" />
                    </MultiDataTrigger.Conditions>
                    <Setter Property="Source" Value="/Emdep.Geos.Modules.SCM;component/Assets/Images/bt_color_transparent.png" />
                </MultiDataTrigger>
                <DataTrigger Binding="{Binding Id}" Value="17">
                    <Setter Property="Source" Value="/Emdep.Geos.Modules.SCM;component/Assets/Images/ic_color_multicolor.png" />
                </DataTrigger>
            </Style.Triggers>
        </Style>
        <Style x:Key="HotTrackedRowStyle" TargetType="{x:Type dxg:RowControl}">
            <Style.Triggers>
                <DataTrigger Binding="{DXBinding DataContext.IdConnectorCategory gt 0}" Value="false">
                    <Setter Property="FontWeight" Value="Bold"/>
                </DataTrigger>
            </Style.Triggers>
        </Style>

    </dxwui:WinUIDialogWindow.Resources>
    <dxwui:WinUIDialogWindow.InputBindings>
        <KeyBinding Key="Escape"  Command="{Binding PropertiesManagerViewCancelButtonCommand}"/>
    </dxwui:WinUIDialogWindow.InputBindings>
    <ScrollViewer HorizontalScrollBarVisibility="Auto" VerticalScrollBarVisibility="Auto" Focusable="False">
        <Grid >
            <Grid.RowDefinitions>
                <RowDefinition Height="50"/>
                <RowDefinition Height="555"/>
                <RowDefinition Height="45"/>
            </Grid.RowDefinitions>

            <Label Content="{DynamicResource SCMPropertiesManagerTitle}" VerticalAlignment="Top" HorizontalAlignment="Left" Margin="10,0,0,0" Style="{StaticResource LabelTitleStyle}" Grid.Row="0"  Padding="0,-10,0,0"/>
            <Grid Grid.Row="1" VerticalAlignment="Stretch" HorizontalAlignment="Stretch">

                <Border BorderThickness="0.0" Padding="3,10,17,10">

                    <Grid>
                        <Grid.ColumnDefinitions>

                            <ColumnDefinition Width="428"/>

                            <ColumnDefinition Width="428"/>

                            <ColumnDefinition Width="428"/>
                        </Grid.ColumnDefinitions>


                        <dxlc:LayoutGroup VerticalAlignment="Stretch"  Grid.Column="0" Margin="7,0,0,0"  
                                                                          View="GroupBox"    
                                      Padding="-10" 
                                                                          IsCollapsible="False" IsCollapsed="False" >
                            <dxlc:LayoutGroup.HeaderTemplate>
                                <DataTemplate>
                                    <TextBlock Text="{DynamicResource CaptionFamilies}" Style="{StaticResource TextBlock14Style}" FontWeight="Bold"/>
                                </DataTemplate>
                            </dxlc:LayoutGroup.HeaderTemplate>
                           
                            <Grid HorizontalAlignment="Stretch" >
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="*"/>
                                </Grid.RowDefinitions>
                                <dxg:GridControl x:Name="gridControl" Grid.Row="0"    
                                                 SelectionMode="Row"   
                                               ItemsSource="{Binding DataContext.ListFamily, Mode=TwoWay,UpdateSourceTrigger=PropertyChanged,RelativeSource={RelativeSource AncestorType=dxwui:WinUIDialogWindow}}"
                                               SelectedItems="{Binding DataContext.SelectedFamiles,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged,RelativeSource={RelativeSource AncestorType=dxwui:WinUIDialogWindow}}"  
                                               SelectedItem="{Binding DataContext.SelectedFamily,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged,RelativeSource={RelativeSource AncestorType=dxwui:WinUIDialogWindow}}"
                >
                                    <dxg:GridColumn Width="*" FieldName="Name"   ReadOnly="True"></dxg:GridColumn>
                                    <dxg:GridControl.View>
                                        <dxg:TableView x:Name="FamilyTableView" 
                                                                       
                                                                     HorizontalScrollbarVisibility="Hidden"
                                                                       VerticalScrollbarVisibility="Auto"
                                                                       ShowIndicator="False"
                                                                       ShowColumnHeaders="False"
                                                                       
                                                                       AllowEditing="False"
                                                                       NavigationStyle="Row"
                                                                       ShowHorizontalLines="False" 
                                                                       ShowVerticalLines="False"
                                                                       ShowSearchPanelMode="Always"                                                      
                                                                       SearchPanelHorizontalAlignment="Stretch"
                                                                       AllowDragDrop="False"    
                                                       FadeSelectionOnLostFocus="False"
                                                       
                                                       
                                                     
                                   RowDetailsVisibilityMode="Visible"
                                 
                                   AutoWidth="False"  
                                        AllowFixedColumnMenu="False"
                                   ShowFixedTotalSummary="False"
                                  
                                   ColumnChooserColumnDisplayMode="ShowHiddenColumnsOnly"
                                   AllowGrouping="True"                                                             
                                   ShowGroupPanel="False"                                                           
                               
                                                  >
                                            <dxmvvm:Interaction.Behaviors>
                                                <dxmvvm:EventToCommand EventName="NodeCheckStateChanged"
                                                   Command="{Binding FamilyCheckBoxCheckUnCheckCommand}" 
                                                     PassEventArgsToCommand="True">
                                                </dxmvvm:EventToCommand>
                                            </dxmvvm:Interaction.Behaviors>
                                        </dxg:TableView>
                                    </dxg:GridControl.View>
                                </dxg:GridControl>
                            </Grid>
                        </dxlc:LayoutGroup>
                        <!--Properties start-->
                        <dxlc:LayoutGroup x:Name="Properties" 
                                                                        
                                                                          View="GroupBox"    
                                      Padding="-10" 
                                                                          IsCollapsible="False" IsCollapsed="False" 
                                                                       Grid.Row="0"   Grid.Column="1" Margin="5,0,0,0" >
                            <dxlc:LayoutGroup.HeaderTemplate>
                                <DataTemplate>
                                    <Grid>
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="Auto" />
                                            <ColumnDefinition Width="*" />
                                        </Grid.ColumnDefinitions>
                                        <TextBlock Text="{DynamicResource SCMPMProperties}" Style="{StaticResource TextBlock14Style}" FontWeight="Bold"
                                    Grid.Column="0"
                                    HorizontalAlignment="Left"
                                    VerticalAlignment="Stretch" />
                                        <Grid  ShowGridLines="False" Grid.Column="1" VerticalAlignment="Center" HorizontalAlignment="Right">
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="*"/>
                                                <ColumnDefinition Width="*"/>
                                                <ColumnDefinition Width="40"/>
                                                <ColumnDefinition Width="5"/>
                                                <ColumnDefinition Width="40"/>
                                                <ColumnDefinition Width="5"/>
                                                <ColumnDefinition Width="40"/>
                                            </Grid.ColumnDefinitions>
                                            <Button Content=" " Grid.Row="1" Grid.Column="2" 
                                        Background="Gray" 
                                        HorizontalContentAlignment="Center"
                                        Command="{Binding DataContext.ExpandAndCollapsePropertyCommand, UpdateSourceTrigger=PropertyChanged, Mode=TwoWay, RelativeSource={RelativeSource AncestorType=dxwui:WinUIDialogWindow}}"
                                        CommandParameter="{Binding ElementName=GridPropertyMenuTreeList}">
                                                <Button.ContentTemplate>
                                                    <DataTemplate>
                                                        <Image 
                                                   Height="13" Width="14"
                                                   HorizontalAlignment="Center" VerticalAlignment="Center">
                                                            <Image.Style>
                                                                    <Style TargetType="{x:Type Image}">
                                                                        <Style.Triggers>
                                                                        <DataTrigger Binding="{Binding DataContext.IsPropertyExpand, Mode=TwoWay,UpdateSourceTrigger=PropertyChanged,RelativeSource={RelativeSource AncestorType=dxwui:WinUIDialogWindow}}" Value="true">
                                                                                <Setter Property="Source" Value="{DynamicResource ExpandButtonImage}" />
                                                                                <Setter Property="ToolTip" Value="Expand Property" />
                                                                            </DataTrigger>
                                                                        <DataTrigger Binding="{Binding DataContext.IsPropertyExpand, Mode=TwoWay,UpdateSourceTrigger=PropertyChanged,RelativeSource={RelativeSource AncestorType=dxwui:WinUIDialogWindow}}" Value="false">
                                                                                <Setter Property="Source" Value="{DynamicResource CollapsButtonImage}" />
                                                                                <Setter Property="ToolTip" Value="Collapse Property" />
                                                                            </DataTrigger>
                                                                        </Style.Triggers>
                                                                    </Style>
                                                                </Image.Style>
                                                        </Image>
                                                    </DataTemplate>
                                                </Button.ContentTemplate>
                                            </Button>
                                            <!--Edit Button-->
                                            <Button Content=" " 
                                        Background="Gray" Grid.Row="1" Grid.Column="4" 
                                        HorizontalContentAlignment="Center"
                                        Command="{Binding DataContext.EditCustomerCommand, RelativeSource={RelativeSource AncestorType=dxwui:WinUIDialogWindow, Mode=FindAncestor}}"
                                  CommandParameter="{Binding ElementName=GridPropertyMenuTreeList}"
                                          IsEnabled="{Binding DataContext.IsEditButtonEnabled,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged, RelativeSource={RelativeSource AncestorType=dxwui:WinUIDialogWindow, Mode=FindAncestor}}">
                                                <Button.ToolTip>
                                                    <TextBlock Text="{DynamicResource ButtonEdit}"/>
                                                </Button.ToolTip>
                                                <Button.ContentTemplate>
                                                    <DataTemplate>
                                                        <Image Style="{ DynamicResource SCMEditButtonImageStyle}"   Height="13" Width="14"
                                                   HorizontalAlignment="Center" VerticalAlignment="Center"  Margin="-10"/>
                                                    </DataTemplate>
                                                </Button.ContentTemplate>
                                            </Button>
                                            <!--Add Button-->
                                            <Button Content=" " 
                                        Background="Gray" Grid.Row="1" Grid.Column="6" 
                                        HorizontalContentAlignment="Center"
                                        Command="{Binding DataContext.AddButtonCommand, RelativeSource={RelativeSource AncestorType=dxwui:WinUIDialogWindow, Mode=FindAncestor}}"
                                        CommandParameter="{Binding RelativeSource={RelativeSource AncestorType=Grid}}"
                                             IsEnabled="{Binding DataContext.IsAddButtonEnabled,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged, RelativeSource={RelativeSource AncestorType=dxwui:WinUIDialogWindow, Mode=FindAncestor}}">
                                                <Button.ToolTip>
                                                    <TextBlock Text="{DynamicResource ButtonAdd}"/>
                                                </Button.ToolTip>
                                                <Button.ContentTemplate>
                                                    <DataTemplate>
                                                        <Image Source="{DynamicResource AddButtonImage}"
                                                   Height="13" Width="14"
                                                   HorizontalAlignment="Center" VerticalAlignment="Center"
                                                  
                                                   />
                                                    </DataTemplate>
                                                </Button.ContentTemplate>
                                            </Button>
                                        </Grid>
                                    </Grid>
                                </DataTemplate>
                            </dxlc:LayoutGroup.HeaderTemplate>
                            <dxg:TreeListControl x:Name="treeListControl"
                                                 HorizontalAlignment="Stretch" VerticalAlignment="Stretch" AutoGenerateColumns="None" 
                                                 ItemsSource="{Binding DataContext.ListConnectorProperties, Mode=TwoWay,UpdateSourceTrigger=PropertyChanged,RelativeSource={RelativeSource AncestorType=dxwui:WinUIDialogWindow}}" 
                                                 SelectedItem="{Binding DataContext.SelectedConnectorProperty,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged,RelativeSource={RelativeSource AncestorType=dxwui:WinUIDialogWindow}}" 
                                             SelectionMode="None"   >
                                <dxg:TreeListControl.Columns>
                                    <dxg:TreeListColumn Header=" "  FieldName ="IsChecked" MinWidth="30" MaxWidth="40" Width="30" FixedWidth="True" FilterPopupMode="Default"   ReadOnly="False" AllowEditing="true" >
                                        <dxg:TreeListColumn.CellTemplate>
                                            <DataTemplate>
                                                <!--IsSCMEditPropertiesManagerBtn-->
                                                <!--IsReadOnly="{Binding Source={x:Static GeosApplication:GeosApplication.Instance}, Path=IsSCMViewConfigurationPermission, Mode=TwoWay ,UpdateSourceTrigger=PropertyChanged}"-->
                                                <dxe:CheckEdit x:Name="IncludedCheckALL" Width="30" Content=" " 
                                                               IsChecked="{Binding RowData.Row.IsChecked,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}" Padding="2.5" Margin="3" 
                                                               IsReadOnly="{Binding DataContext.IsSCMEditPropertiesManager,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged,RelativeSource={RelativeSource AncestorType=dxwui:WinUIDialogWindow}}"
                                                                     Visibility="{Binding RowData.Row.IsParentCheckBoxVisible, Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}"              >
                                                    <dxmvvm:Interaction.Behaviors>
                                                        <dxmvvm:EventToCommand  EventName="Checked" 
                                                                    Command="{Binding DataContext.PropertyCheckBoxSelectedCommand,Mode=TwoWay,RelativeSource={RelativeSource AncestorType=dxwui:WinUIDialogWindow}}"                                                                   
                                                                    CommandParameter="{Binding ElementName=treeListControl}" >
                                                            <dxmvvm:EventToCommand.EventArgsConverter>
                                                                <dx:EventArgsToDataRowConverter />
                                                            </dxmvvm:EventToCommand.EventArgsConverter>
                                                        </dxmvvm:EventToCommand>
                                                        <dxmvvm:EventToCommand  EventName="Unchecked" 
                                                                    Command="{Binding DataContext.PropertyUnCheckBoxSelectedCommand,Mode=TwoWay,RelativeSource={RelativeSource AncestorType=dxwui:WinUIDialogWindow}}"
                                                                    CommandParameter="{Binding ElementName=treeListControl}"/>
                                                    </dxmvvm:Interaction.Behaviors>
                                                </dxe:CheckEdit>
                                            </DataTemplate>
                                        </dxg:TreeListColumn.CellTemplate>
                                        
                                    </dxg:TreeListColumn>
                                 
                                    <dxg:TreeListColumn Width="*"  FieldName="PropertyName" ReadOnly="True" UnboundType="Object" FilterPopupMode="CheckedList"/>
                                </dxg:TreeListControl.Columns>
                                <dxg:TreeListControl.View>
                                    <dxg:TreeListView Name="GridPropertyMenuTreeList" 
                                                      ShowTotalSummary="False"
                                                      AllowRecursiveNodeChecking="False"
                                                      ShowSearchPanelMode="Always"
                                                      ShowColumnHeaders="False"
                                                      ShowIndicator="False"
                                                      VerticalScrollbarVisibility="Visible"
                                                      ParentFieldName="IdConnectorCategory" 
                                                      KeyFieldName="IdConnectorProperty"
                                                      FilterMode="Extended"
                                                      SearchPanelHorizontalAlignment="Stretch"
                                                      AllowDragDrop="False"                                                     
                                                      AutoExpandAllNodes="False"
                                                      NavigationStyle="Row" 
                                                      ShowVerticalLines="True"
                                                     RowStyle="{StaticResource HotTrackedRowStyle}"
                                               ExpandCollapseNodesOnNavigation="True"
                              CustomUnboundColumnData="View_CustomUnboundColumnData"
                                                        SearchString="{Binding ConnectorPropertyFilterString,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}"
                                                      >
                                        <!---->
                                    </dxg:TreeListView>
                                   
                                </dxg:TreeListControl.View>
                                <dxmvvm:Interaction.Behaviors>
                                    <dxmvvm:EventToCommand EventName="MouseLeftButtonDown" Command="{Binding DataContext.PropertyTreeListChangedCommand,Mode=TwoWay,RelativeSource={RelativeSource AncestorType=dxwui:WinUIDialogWindow}}" >

                                    </dxmvvm:EventToCommand>
                                     <dxmvvm:EventToCommand EventName="Loaded" Command="{Binding DataContext.LoadedPropertyTreeListChangedCommand,Mode=TwoWay,RelativeSource={RelativeSource AncestorType=dxwui:WinUIDialogWindow}}" 
                                                            CommandParameter="{Binding ElementName=GridPropertyMenuTreeList}">

                                    </dxmvvm:EventToCommand>
                                </dxmvvm:Interaction.Behaviors>

                            </dxg:TreeListControl>
                    </dxlc:LayoutGroup>
                       

                        <!--Actions start-->
                        <dxlc:LayoutGroup x:Name="Actions" 
                                                                           dxlc:LayoutControl.AllowHorizontalSizing="False"
                                                                           View="GroupBox" 
                                       Padding="-10" 
                                                                           IsCollapsible="False" IsCollapsed="False"
                                                                           Grid.Row="0" Grid.Column="2" Margin="5,0,-7,0" >
                            <dxlc:LayoutGroup.HeaderTemplate>
                                <DataTemplate>
                                    <Grid>
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="Auto" />
                                            <ColumnDefinition Width="*" />
                                        </Grid.ColumnDefinitions>
                                        <TextBlock Text="{DynamicResource SCMActions}" Style="{StaticResource TextBlock14Style}" FontWeight="Bold"
                                    Grid.Column="0"
                                    HorizontalAlignment="Left"
                                    VerticalAlignment="Stretch" />
                                    </Grid>
                                </DataTemplate>
                            </dxlc:LayoutGroup.HeaderTemplate>
                            <Border BorderBrush="{DynamicResource LineColorStyle}" BorderThickness="0.3">
                                <Grid>


                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="100" />
                                        <ColumnDefinition Width="5" />
                                        <ColumnDefinition Width="320" />
                                    </Grid.ColumnDefinitions>
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="5"/>
                                        <RowDefinition Height="30"/>
                                        <RowDefinition Height="5"/>
                                        <RowDefinition Height="30"/>
                                        <RowDefinition Height="5"/>
                                        <RowDefinition Height="30"/>
                                        <RowDefinition Height="5"/>
                                        <RowDefinition Height="30"/>
                                        <RowDefinition Height="*"/>
                                    </Grid.RowDefinitions>
                                    <Label Grid.Row="1" Grid.Column="0"
                                                                   Margin="10,0,0,0"
                                                                   
                                                                   Content="{DynamicResource SCMIsEnabled}" 
                                           VerticalAlignment="Center"
                                           Visibility="{Binding DataContext.VisibleIsEnabled, UpdateSourceTrigger=PropertyChanged,Mode=TwoWay,RelativeSource={RelativeSource AncestorType=dxwui:WinUIDialogWindow}}"
                                           />
                                                    
                                    <dxe:CheckEdit Grid.Row="1" Grid.Column="2" Margin="0,0,10,0" HorizontalAlignment="Left" VerticalAlignment=" Center"                                                                                                   
                                                   IsChecked= "{Binding DataContext.IsPropertyEnabled,RelativeSource={RelativeSource AncestorType=dxwui:WinUIDialogWindow}}" 
                                                   IsReadOnly="{Binding DataContext.IsSCMEditPropertiesManager,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged,RelativeSource={RelativeSource AncestorType=dxwui:WinUIDialogWindow}}"
                                                   Visibility="{Binding DataContext.VisibleIsEnabled, UpdateSourceTrigger=PropertyChanged,Mode=TwoWay,RelativeSource={RelativeSource AncestorType=dxwui:WinUIDialogWindow}}"/>
                                    
                                    <Label Grid.Row="3" Grid.Column="0" Margin="10,0,0,0"                                                                                                                                    
                                           Content="{DynamicResource SCMDefaultValue}"
                                           Visibility="{Binding DataContext.VisibleLabelDefaultValue, UpdateSourceTrigger=PropertyChanged,Mode=TwoWay,RelativeSource={RelativeSource AncestorType=dxwui:WinUIDialogWindow}}"/>
                                    
                                    <dxe:TextEdit Grid.Row="3" Grid.Column="2"                                              
                                                  Margin="0,0,10,0" HorizontalAlignment="Stretch"                                                                          
                                                  Text="{Binding DataContext.DefaultValue,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged,RelativeSource={RelativeSource AncestorType=dxwui:WinUIDialogWindow}}"
                                                  IsEnabled="{Binding DataContext.IsSCMEditPropertiesManagerBtn,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged,RelativeSource={RelativeSource AncestorType=dxwui:WinUIDialogWindow}}"
                                                  VerticalContentAlignment="Center"                                                                          
                                                  Mask="###,###,###,###,###,###,###,##0;" AllowNullInput="True" MaskUseAsDisplayFormat="True" MaskType="Numeric"  NullText="{x:Null}"
                                                  Visibility="{Binding DataContext.VisibleTextDefaultValue, UpdateSourceTrigger=PropertyChanged,Mode=TwoWay,RelativeSource={RelativeSource AncestorType=dxwui:WinUIDialogWindow}}"/>
                                                  
                                                    
                                    <Label Grid.Row="5" Grid.Column="0" Margin="10,0,0,0" Content="{DynamicResource SCMMinValue}" 
                                           Visibility="{Binding DataContext.VisibleMinValue, UpdateSourceTrigger=PropertyChanged,Mode=TwoWay,RelativeSource={RelativeSource AncestorType=dxwui:WinUIDialogWindow}}"/>
                                    
                                    <dxe:TextEdit Grid.Row="5" Grid.Column="2" HorizontalAlignment="Stretch"
                                                  Margin="0,0,10,0" Text="{Binding DataContext.MinValue,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged,RelativeSource={RelativeSource AncestorType=dxwui:WinUIDialogWindow}}"
                                                  IsEnabled="{Binding DataContext.IsSCMEditPropertiesManagerBtn,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged,RelativeSource={RelativeSource AncestorType=dxwui:WinUIDialogWindow}}"
                                                  VerticalContentAlignment="Center" 
                                                  Mask="###,###,###,###,###,###,###,##0;" AllowNullInput="True" MaskUseAsDisplayFormat="True" MaskType="Numeric"   NullText="{x:Null}"
                                                  Visibility="{Binding DataContext.VisibleMinValue, UpdateSourceTrigger=PropertyChanged,Mode=TwoWay,RelativeSource={RelativeSource AncestorType=dxwui:WinUIDialogWindow}}"/>
                                    
                                    <Label Grid.Row="7" Grid.Column="0" Margin="10,0,0,0" Content="{DynamicResource SCMMaxValue}"
                                           Visibility="{Binding DataContext.VisibleMaxValue, UpdateSourceTrigger=PropertyChanged,Mode=TwoWay,RelativeSource={RelativeSource AncestorType=dxwui:WinUIDialogWindow}}"/>

                                                    
                                    <dxe:TextEdit Grid.Row="7" Grid.Column="2" Margin="0,0,10,0" HorizontalAlignment="Stretch" VerticalContentAlignment="Center"                                                                  
                                                  Text="{Binding DataContext.MaxValue,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged,RelativeSource={RelativeSource AncestorType=dxwui:WinUIDialogWindow}}" 
                                                  IsEnabled="{Binding DataContext.IsSCMEditPropertiesManagerBtn,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged,RelativeSource={RelativeSource AncestorType=dxwui:WinUIDialogWindow}}"
                                                  Mask="###,###,###,###,###,###,###,##0;" AllowNullInput="True" MaskUseAsDisplayFormat="True" MaskType="Numeric"  NullText="{x:Null}"
                                                  Visibility="{Binding DataContext.VisibleMaxValue, UpdateSourceTrigger=PropertyChanged,Mode=TwoWay,RelativeSource={RelativeSource AncestorType=dxwui:WinUIDialogWindow}}"/>
                                    <!--Color drop download-->
                                    <dxe:ComboBoxEdit Height="30"
                                                                              Grid.Row="3"
                                                                              Grid.Column="2"
                                                                              Margin="0,0,10,0"
                                                                              Name="cmbColor"
                                                                              HorizontalAlignment="Stretch"
                                                                              VerticalAlignment="Stretch"
                                                                              ScrollViewer.VerticalScrollBarVisibility="Auto"
                                                                              TextTrimming="CharacterEllipsis"
                                                                              TextWrapping="Wrap"
                                                                              Style="{DynamicResource ComboBoxEditStyle}"
                                                                              IsTextEditable="False"
                                                                              DisplayMember="Name"
                                                                              AllowDefaultButton="False"
                                                                              ItemsSource="{Binding DataContext.ListColor, RelativeSource={RelativeSource AncestorType=dxwui:WinUIDialogWindow, Mode=FindAncestor},
                                                UpdateSourceTrigger=PropertyChanged , Mode=TwoWay}" 
                                                      Visibility="{Binding DataContext.VisibleColorDefaultValue, UpdateSourceTrigger=PropertyChanged,Mode=TwoWay,RelativeSource={RelativeSource AncestorType=dxwui:WinUIDialogWindow}}"
                                                      IsEnabled="{Binding DataContext.IsSCMEditPropertiesManagerBtn,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged,RelativeSource={RelativeSource AncestorType=dxwui:WinUIDialogWindow}}"
                                                                              SelectedItem="{Binding DataContext.SelectedColor, UpdateSourceTrigger=PropertyChanged, Mode=TwoWay, RelativeSource={RelativeSource AncestorType=dxwui:WinUIDialogWindow}}">
                                        <dxe:ComboBoxEdit.Buttons>
                                            <dxe:ButtonInfo GlyphKind="Custom"
                                                                                    x:Name="buttonClearColor">
                                                <dxe:ButtonInfo.Template>
                                                    <DataTemplate>
                                                        <dxe:ButtonClose HorizontalAlignment="Center"
                                                                                                 VerticalAlignment="Center"
                                                                                                 Margin="2,0"
                                                                                                 Visibility="{Binding DataContext.ClearColor, UpdateSourceTrigger=PropertyChanged,Mode=TwoWay,RelativeSource={RelativeSource AncestorType=dxwui:WinUIDialogWindow}}"
                                                                                                  IsEnabled="{Binding DataContext.IsSCMEditPropertiesManagerBtn,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged,RelativeSource={RelativeSource AncestorType=dxwui:WinUIDialogWindow}}"
