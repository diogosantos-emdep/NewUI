<dxwui:WinUIDialogWindow x:Class="Emdep.Geos.Modules.Warehouse.Views.ArticleDetailsView"
                         xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                         xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                         xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
                         xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
                         xmlns:local="clr-namespace:Emdep.Geos.Modules.Warehouse.Views"
                         xmlns:dxwui="http://schemas.devexpress.com/winfx/2008/xaml/windowsui" 
                         xmlns:dxlc="http://schemas.devexpress.com/winfx/2008/xaml/layoutcontrol"
                         xmlns:dxmvvm="http://schemas.devexpress.com/winfx/2008/xaml/mvvm"
                         xmlns:dxg="http://schemas.devexpress.com/winfx/2008/xaml/grid"
                         xmlns:dxe="http://schemas.devexpress.com/winfx/2008/xaml/editors"
                         xmlns:dxc="http://schemas.devexpress.com/winfx/2008/xaml/charts"
                         xmlns:Converters="clr-namespace:Emdep.Geos.UI.Converters;assembly=Emdep.Geos.UI.Converters"
                         xmlns:GeosApplication="clr-namespace:Emdep.Geos.UI.Common;assembly=Emdep.Geos.UI.Common"
                         xmlns:WarehouseCommon="clr-namespace:Emdep.Geos.Modules.Warehouse;assembly=Emdep.Geos.Modules.Warehouse"
                         xmlns:sysglb="clr-namespace:System.Globalization;assembly=mscorlib"
                         xmlns:Validation ="clr-namespace:Emdep.Geos.UI.Validations;assembly=Emdep.Geos.UI.Validations"
                         xmlns:UIHelper="clr-namespace:Emdep.Geos.UI.Helper;assembly=Emdep.Geos.UI.Helper"
                         xmlns:dx="http://schemas.devexpress.com/winfx/2008/xaml/core"
                         xmlns:dxdo="http://schemas.devexpress.com/winfx/2008/xaml/docking"
                         xmlns:WmsApplication="clr-namespace:Emdep.Geos.Modules.Warehouse"
                         xmlns:ve="http://schemas.devexpress.com/winfx/2008/xaml/docking/visualelements"
                         xmlns:dxgt="http://schemas.devexpress.com/winfx/2008/xaml/grid/themekeys"
                         xmlns:dxb="http://schemas.devexpress.com/winfx/2008/xaml/bars"
                         mc:Ignorable="d" Height="{Binding DialogHeight}"
                         Style="{DynamicResource WinUIDialogWindowStyle}"
                         UIHelper:FocusExtension.IsFocused="{Binding FocusUserControl}">

    <dxwui:WinUIDialogWindow.InputBindings>
        <KeyBinding Key="Escape" Command="{Binding CancelButtonCommand}"/>
    </dxwui:WinUIDialogWindow.InputBindings>

    <dxwui:WinUIDialogWindow.TitleStyle>
        <Style TargetType="ContentControl">
            <Setter Property="ContentTemplate">
                <Setter.Value>
                    <DataTemplate>
                        <Grid>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="Auto"/>
                                <ColumnDefinition Width="Auto"/>
                            </Grid.ColumnDefinitions>

                            <TextBlock Grid.Column="0"
                                       Text="{DynamicResource ArticleDetailsViewTitle}"
                                       HorizontalAlignment="Left"
                                       Style="{DynamicResource PickingWinUIDialogWindowTitleStyle}" >
                            </TextBlock>

                            <Button Content=" " Grid.Column="1" Height="40" Width="40" HorizontalAlignment="Right" Margin="5 0 0 5"
                                    Command="{Binding DataContext.PrintArticleLabelCommand,RelativeSource={RelativeSource AncestorType=dxwui:WinUIDialogWindow}}"
                                    CommandParameter="{Binding RelativeSource={RelativeSource AncestorType=Grid}}">
                                <Button.ToolTip>
                                    <TextBlock Text="{DynamicResource PrintArticleLabel}"/>
                                </Button.ToolTip>
                                <Button.ContentTemplate>
                                    <DataTemplate>
                                        <Image Style="{ DynamicResource PrintArticleLabelButtonImageStyle}"  Height="24" Width="24" Margin="-10"/>
                                    </DataTemplate>
                                </Button.ContentTemplate>
                            </Button>

                        </Grid>

                    </DataTemplate>
                </Setter.Value>
            </Setter>
        </Style>
    </dxwui:WinUIDialogWindow.TitleStyle>


    <dxwui:WinUIDialogWindow.Resources>

        <Style x:Key="myGroupBoxStyle" TargetType="{x:Type dxlc:GroupBox}">
            <Setter Property="MaximizeElementVisibility" Value="Visible" />
            <Setter Property="Width" Value="300" />
            <Setter Property="Height" Value="138" />
        </Style>

        <Style x:Key="myGroupBoxStyle1" TargetType="{x:Type dxlc:GroupBox}">
            <Setter Property="MaximizeElementVisibility" Value="Visible" />
            <Setter Property="Padding" Value="0" />
            <Setter Property="Width" Value="300" />
        </Style>

        <Style TargetType="{x:Type ve:AutoHideTray}">
            <Setter Property="ItemsPanel">
                <Setter.Value>
                    <ItemsPanelTemplate>
                        <ve:psvStackPanel HorizontalAlignment="Right" />
                    </ItemsPanelTemplate>
                </Setter.Value>
            </Setter>
        </Style>


        <Style x:Key="LockImageStyle" TargetType="Image">
            <Style.Triggers>
                <MultiDataTrigger>
                    <MultiDataTrigger.Conditions>
                        <Condition Binding="{Binding  RowData.Row.WarehouseDeliveryNoteItems[0].IsLocked}" Value="True" />
                        <Condition Binding="{Binding Path=(dx:ThemeManager.TreeWalker).ThemeName, RelativeSource={RelativeSource Self}}" Value="BlackAndBlue" />
                    </MultiDataTrigger.Conditions>
                    <Setter Property="Source" Value="/Emdep.Geos.Modules.Warehouse;component/Assets/Images/wClosedBox.png" />
                    <Setter Property="ToolTip" Value="{Binding  RowData.Row.WarehouseDeliveryNoteItems[0].ReasonComment}"/>
                </MultiDataTrigger>
                <MultiDataTrigger>
                    <MultiDataTrigger.Conditions>
                        <Condition Binding="{Binding  RowData.Row.WarehouseDeliveryNoteItems[0].IsLocked}" Value="True" />
                        <Condition Binding="{Binding Path=(dx:ThemeManager.TreeWalker).ThemeName, RelativeSource={RelativeSource Self}}" Value="WhiteAndBlue" />
                    </MultiDataTrigger.Conditions>
                    <Setter Property="Source" Value="/Emdep.Geos.Modules.Warehouse;component/Assets/Images/Lock.png" />
                    <Setter Property="ToolTip" Value="{Binding  RowData.Row.WarehouseDeliveryNoteItems[0].ReasonComment}"/>
                </MultiDataTrigger>
                
               
            </Style.Triggers>
        </Style>

    </dxwui:WinUIDialogWindow.Resources>

    <ScrollViewer ScrollViewer.HorizontalScrollBarVisibility="Auto" ScrollViewer.VerticalScrollBarVisibility="Auto">

        <Grid Height="{Binding DialogHeight}" Width="{Binding DialogWidth}">

            <Grid.RowDefinitions>
                <RowDefinition Height="*"/>
                <RowDefinition Height="40"/>
            </Grid.RowDefinitions>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="Auto" />
                <ColumnDefinition Width="*" />
            </Grid.ColumnDefinitions>

            <dxlc:FlowLayoutControl Grid.Row="0"
                                    Grid.Column="0" Grid.ColumnSpan="2"
                                    AllowLayerSizing="False"
                                    AllowMaximizedElementMoving="False"
                                    Background="Transparent"
                                    BreakFlowToFit="True"
                                    DragScrolling="True"
                                    MaximizedElementPosition="Right"
                                    Orientation="Horizontal"
                                    Padding="0,0,0,0"
                                    StretchContent="False">
                <dxmvvm:Interaction.Behaviors>
                    <UIHelper:FlowLayoutControlForMinimiseMaximise />
                </dxmvvm:Interaction.Behaviors>

                <!--Information Details-->
                <dxlc:GroupBox x:Name="InformationDetails" Selector.IsSelected="True" Style="{StaticResource myGroupBoxStyle}">
                    <dxlc:GroupBox.HeaderTemplate>
                        <DataTemplate>
                            <DockPanel Margin="-5,0,0,0">
                                <TextBlock Margin="5,0,0,0"
                                           VerticalAlignment="Center"
                                           FontWeight="Bold"
                                           Text="{DynamicResource ArticleDetailsViewInformationHeader}" />
                            </DockPanel>
                        </DataTemplate>
                    </dxlc:GroupBox.HeaderTemplate>
                    <dxlc:GroupBox.ContentTemplate>
                        <DataTemplate>
                            <Grid>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="*" />
                                </Grid.ColumnDefinitions>
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="*" />
                                    <RowDefinition Height="*" />
                                </Grid.RowDefinitions>
                                <Label Grid.Row="0"
                                       Grid.Column="0"
                                       Margin="2" VerticalAlignment="Stretch" HorizontalAlignment="Left"
                                       Content="{DynamicResource ArticleDetailsViewInformationReference}"
                                       FontWeight="Bold"
                                       Padding="2" />
                                <TextBlock Margin="2" Padding="2" Text="{Binding DataContext.ArticleData.Reference, RelativeSource={RelativeSource AncestorType={x:Type dxlc:GroupBox}}}" VerticalAlignment="Stretch" HorizontalAlignment="Left" Grid.Row="0" Grid.Column="1" TextWrapping="WrapWithOverflow" TextTrimming="CharacterEllipsis"/>
                                <Label Grid.Row="1"
                                       Grid.Column="0"
                                       Margin="2" VerticalAlignment="Stretch" HorizontalAlignment="Left"
                                       Content="{DynamicResource ArticleDetailsViewInformationDesc}"
                                       FontWeight="Bold"
                                       Padding="2" />
                                <TextBlock Margin="2" Padding="2" Text="{Binding DataContext.ArticleData.Description, RelativeSource={RelativeSource AncestorType={x:Type dxlc:GroupBox}}}" VerticalAlignment="Stretch" HorizontalAlignment="Left"   Grid.Row="1" Grid.Column="1" TextWrapping="WrapWithOverflow" TextTrimming="CharacterEllipsis"/>
                            </Grid>
                        </DataTemplate>
                    </dxlc:GroupBox.ContentTemplate>
                    <dxlc:GroupBox.MaximizedContentTemplate>
                        <DataTemplate>
                            <ScrollViewer ScrollViewer.VerticalScrollBarVisibility="Auto" ScrollViewer.HorizontalScrollBarVisibility="Auto">
                                <Grid>
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="30"/>
                                        <RowDefinition Height="5"/>
                                        <RowDefinition Height="170"/>
                                        <RowDefinition Height="5"/>
                                        <RowDefinition Height="47"/>
                                        <RowDefinition Height="5"/>
                                        <RowDefinition Height="30"/>
                                        <RowDefinition Height="5"/>
                                        <RowDefinition Height="30"/>
                                        <RowDefinition Height="5"/>
                                        <RowDefinition Height="30"/>
                                        <RowDefinition Height="5"/>
                                        <RowDefinition Height="30"/>
                                        <RowDefinition Height="5"/>
                                        <RowDefinition Height="30"/>
                                        <RowDefinition Height="5"/>
                                        <RowDefinition Height="Auto"/>
                                        <RowDefinition Height="5"/>
                                        <RowDefinition Height="*"/>
                                        <RowDefinition Height="5"/>
                                        <RowDefinition Height="Auto"/>
                                    </Grid.RowDefinitions>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="Auto"/>
                                        <ColumnDefinition Width="5" MaxWidth="4"/>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="5" MaxWidth="4"/>
                                        <ColumnDefinition Width="Auto"/>
                                        <ColumnDefinition Width="5" MaxWidth="4"/>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="5" MaxWidth="4"/>
                                        <ColumnDefinition Width="Auto"/>
                                        <ColumnDefinition Width="5" MaxWidth="4"/>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="5" MaxWidth="4"/>
                                        <ColumnDefinition Width="Auto"/>
                                        <ColumnDefinition Width="5" MaxWidth="4"/>
                                        <ColumnDefinition Width="*"/>
                                    </Grid.ColumnDefinitions>

                                    <!--Reference-->
                                    <Label Grid.Row="0" Grid.Column="0" Content="{DynamicResource ArticleDetailsViewInformationReference}" HorizontalAlignment="Left" VerticalAlignment="Center" />

                                    <dxe:TextEdit Grid.Row="0" Grid.Column="2" Grid.ColumnSpan="7"
                                                    IsReadOnly="True"
                                                    HorizontalAlignment="Stretch"
                                                    VerticalAlignment="Stretch" 
                                                    Text="{Binding DataContext.ArticleData.Reference,RelativeSource={RelativeSource AncestorType=dxlc:GroupBox, Mode=FindAncestor},ValidatesOnDataErrors=True, UpdateSourceTrigger=PropertyChanged, Mode=TwoWay}">
                                    </dxe:TextEdit>

                                    <!--Description-->
                                    <Label Grid.Row="2" Grid.Column="0"  Content="{DynamicResource ArticleDetailsViewInformationDesc}" HorizontalAlignment="Left" VerticalAlignment="Center" />
                                    <dxe:TextEdit Grid.Row="2" Grid.Column="2"  Grid.ColumnSpan="7" VerticalScrollBarVisibility="Visible" IsReadOnly="True"
                                                    HorizontalAlignment="Stretch"  HorizontalContentAlignment="Left" TextWrapping="Wrap" MaxWidth="1000"
                                                    VerticalAlignment="Stretch"  VerticalContentAlignment="Top" AcceptsReturn="True"
                                                    Text="{Binding DataContext.ArticleData.Description,RelativeSource={RelativeSource AncestorType=dxlc:GroupBox, Mode=FindAncestor},ValidatesOnDataErrors=True, UpdateSourceTrigger=PropertyChanged, Mode=TwoWay}">
                                    </dxe:TextEdit>

                                    <!--Observations-->
                                    <Label Grid.Row="4" Grid.Column="0"  Content="{DynamicResource Observationss}" HorizontalAlignment="Left" VerticalAlignment="Center" />
                                    <dxe:TextEdit Grid.Row="4" Grid.Column="2" Grid.ColumnSpan="7" VerticalScrollBarVisibility="Auto"
               IsReadOnly="{Binding DataContext.IsObeservationReadOnly, Mode=TwoWay, RelativeSource={RelativeSource AncestorType=dxlc:GroupBox, Mode=FindAncestor}, UpdateSourceTrigger=PropertyChanged}"
               HorizontalAlignment="Stretch" HorizontalContentAlignment="Left" TextWrapping="Wrap" MaxWidth="725"
               VerticalAlignment="Stretch" VerticalContentAlignment="Top" AcceptsReturn="True" 
               Text="{Binding DataContext.ArticleData.MyWarehouse.InternalNotes, RelativeSource={RelativeSource AncestorType=dxlc:GroupBox, Mode=FindAncestor}, ValidatesOnDataErrors=True, UpdateSourceTrigger=PropertyChanged, Mode=TwoWay}">
                                    </dxe:TextEdit>


                                    <!--Category-->
                                    <Label  Grid.Row="6" Grid.Column="0"  Content="{DynamicResource ArticleDetailsViewInformationCategory}" HorizontalAlignment="Left" VerticalAlignment="Center" />

                                    <dxg:LookUpEdit Grid.Row="6" Grid.Column="2" Grid.ColumnSpan="7" ItemsSource="{Binding DataContext.ListArticleCategory,RelativeSource={RelativeSource AncestorType=dxlc:GroupBox},Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}" DisplayMember="Name"
                                                    ValueMember="IdArticleCategory" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Name="lookUpEditCategory" 
                                                    EditValue="{Binding DataContext.ArticleData.IdArticleCategory,RelativeSource={RelativeSource AncestorType=dxlc:GroupBox, Mode=FindAncestor},UpdateSourceTrigger=PropertyChanged, Mode=TwoWay}"
                                                    IsReadOnly="True"   AutoPopulateColumns="False">
                                        <dxg:LookUpEdit.PopupContentTemplate>
                                            <ControlTemplate>
                                                <dxg:GridControl Name="PART_GridControl">
                                                    <dxg:GridControl.Columns>
                                                        <dxg:GridColumn FieldName="Name" FilterPopupMode="CheckedList"/>
                                                    </dxg:GridControl.Columns>
                                                    <dxg:GridControl.View>
                                                        <dxg:TreeListView Name="treeListViewCategory" AutoWidth="True" 
                                                                            TreeDerivationMode="Selfreference" 
                                                                            KeyFieldName="IdArticleCategory" 
                                                                            ParentFieldName="Parent"
                                                                            ShowIndicator="False"/>
                                                    </dxg:GridControl.View>
                                                </dxg:GridControl>
                                            </ControlTemplate>
                                        </dxg:LookUpEdit.PopupContentTemplate>
                                    </dxg:LookUpEdit>

                                    <!--Type-->
                                    <Label  Grid.Row="8" Grid.Column="0" Content="{DynamicResource ArticleType}" HorizontalAlignment="Left" VerticalAlignment="Center" />

                                    <dxe:ComboBoxEdit Grid.Row="8" Grid.Column="2" Grid.ColumnSpan="7" 
                                                      IsReadOnly="{Binding DataContext.IsArticleFamilyListen,Mode=TwoWay,RelativeSource={RelativeSource AncestorType=dxlc:GroupBox, Mode=FindAncestor},UpdateSourceTrigger=PropertyChanged}"
                                                      HorizontalAlignment="Stretch" 
                                                      VerticalAlignment="Center"  
                                                      ItemsSource="{Binding DataContext.ArticleTypesList,Mode=TwoWay,RelativeSource={RelativeSource AncestorType=dxlc:GroupBox, Mode=FindAncestor},UpdateSourceTrigger=PropertyChanged}" 
                                                      DisplayMember="ArticleTypeName" ValueMember="IdArticleType" 
                                                      AutoComplete="True" ImmediatePopup="True" >



                                        <dxe:ComboBoxEdit.SelectedItem>
                                            <Binding Path="DataContext.SelectedArticleType" RelativeSource="{RelativeSource AncestorType=dxlc:GroupBox}" ValidatesOnDataErrors="True" UpdateSourceTrigger="PropertyChanged" Mode="TwoWay">
                                                <Binding.ValidationRules>
                                                    <Validation:EditWarehouseFamily FieldName="SelectedArticleType" ValidationStep="UpdatedValue"/>
                                                </Binding.ValidationRules>
                                            </Binding>
                                        </dxe:ComboBoxEdit.SelectedItem>

                                    </dxe:ComboBoxEdit>

                                    <!--Article Reference Image-->
                                    <Border Grid.Row="0" Grid.Column="10" Grid.ColumnSpan="5" Grid.RowSpan="5" BorderThickness=".5" BorderBrush="Gray" VerticalAlignment="Stretch" HorizontalAlignment="Stretch">
                                        <dxe:ImageEdit Source="{Binding DataContext.ReferenceImage,RelativeSource={RelativeSource AncestorType=dxlc:GroupBox},Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}"
                                                       IsEnabled="{Binding Source={x:Static WarehouseCommon:WarehouseCommon.Instance}, Path=IsPermissionEnabled}">
                                            <dxe:ImageEdit.Style >
                                                <Style>
                                                    <Style.Triggers >
                                                        <DataTrigger Binding="{Binding DataContext.IsReferenceImageExist,RelativeSource={RelativeSource AncestorType=dxlc:GroupBox},UpdateSourceTrigger=PropertyChanged,Mode=TwoWay}" Value="false">
                                                            <Setter Property="dxe:ImageEdit.Stretch" Value="None" ></Setter>
                                                        </DataTrigger>
                                                    </Style.Triggers>
                                                </Style>
                                            </dxe:ImageEdit.Style>
                                        </dxe:ImageEdit>
                                    </Border>

                                    <!--Orientation-->
                                    <StackPanel Orientation="Horizontal" Grid.Row="6" Grid.Column="10" Grid.ColumnSpan="5" HorizontalAlignment="Left">
                                        <Label Content="{DynamicResource ArticleDetailsViewInformationOrientation}" HorizontalAlignment="Left" VerticalAlignment="Center"/>
                                        <dxe:ToggleSwitch IsReadOnly="True" ContentPlacement="Inside" HorizontalAlignment="Right" VerticalAlignment="Stretch"
                                                          CheckedStateContent="Yes" UncheckedStateContent="No" 
                                                          ToggleSwitchWidth="50" 
                                                          ToggleSwitchHeight="30"  IsThreeState="False"
                                                          IsChecked="{Binding DataContext.ArticleData.IsOrientativeVisualAid,RelativeSource={RelativeSource AncestorType=dxlc:GroupBox},Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}"/>
                                    </StackPanel>

                                    <!--Family-->
                                    <Label  Grid.Row="10" Grid.Column="0" Content="{DynamicResource ArticleFamily}" HorizontalAlignment="Left" VerticalAlignment="Center" />

                                    <dxe:ComboBoxEdit Grid.Row="10" Grid.Column="2" Grid.ColumnSpan="7" 
                                              IsReadOnly="{Binding DataContext.IsArticleFamilyListen, Mode=TwoWay, RelativeSource={RelativeSource AncestorType=dxlc:GroupBox, Mode=FindAncestor}, UpdateSourceTrigger=PropertyChanged}" 
                                              HorizontalAlignment="Stretch" 
                                              VerticalAlignment="Center"  
                                              ItemsSource="{Binding DataContext.ArticleFamilyList, Mode=TwoWay, RelativeSource={RelativeSource AncestorType=dxlc:GroupBox, Mode=FindAncestor}, UpdateSourceTrigger=PropertyChanged}" 
                                              DisplayMember="Value" ValueMember="IdLookupValue" 
                                              AutoComplete="True" ImmediatePopup="True" >           
                                        <dxe:ComboBoxEdit.SelectedIndex>
                                            <Binding Path="DataContext.SelectedFamilyIndex" RelativeSource="{RelativeSource AncestorType=dxlc:GroupBox}" ValidatesOnDataErrors="True" UpdateSourceTrigger="PropertyChanged" Mode="TwoWay">
                                                <Binding.ValidationRules>
                                                    <Validation:EditWarehouseFamily FieldName="SelectedFamilyIndex" ValidationStep="UpdatedValue"/>
                                                </Binding.ValidationRules>
                                            </Binding>
                                        </dxe:ComboBoxEdit.SelectedIndex>
                                    </dxe:ComboBoxEdit>


                                    <!--Weight-->
                                    <StackPanel Orientation="Horizontal" Grid.Row="12" Grid.Column="0" HorizontalAlignment="Left" VerticalAlignment="Stretch">
                                        <Label  Content="{DynamicResource ArticleDetailsViewInformationWeight}" VerticalAlignment="Center"/>
                                        <TextBlock Text="{Binding DataContext.ArticleWeightSymbol,RelativeSource={RelativeSource AncestorType=dxlc:GroupBox},Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}" VerticalAlignment="Center" />
                                    </StackPanel>

                                    <dxe:TextEdit Grid.Row="12" Grid.Column="2" IsReadOnly="True" HorizontalContentAlignment="Right" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" 
                                                  Text="{Binding DataContext.ArticleWeight,RelativeSource={RelativeSource AncestorType=dxlc:GroupBox},Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}">
                                    </dxe:TextEdit>

                                    <!--Length-->
                                    <Label Grid.Row="12" Grid.Column="4"   Content="{DynamicResource ArticleDetailsViewInformationLength}" HorizontalAlignment="Left" VerticalAlignment="Center"/>

                                    <dxe:TextEdit Grid.Row="12" Grid.Column="6" IsReadOnly="True" HorizontalContentAlignment="Right" HorizontalAlignment="Stretch"  VerticalAlignment="Stretch"
                                                  Text="{Binding DataContext.ArticleData.Length,RelativeSource={RelativeSource AncestorType=dxlc:GroupBox},Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}">
                                    </dxe:TextEdit>

                                    <!--Width-->
                                    <Label Grid.Row="12" Grid.Column="8"  Content="{DynamicResource ArticleDetailsViewInformationWidth}" HorizontalAlignment="Left" VerticalAlignment="Center"/>

                                    <dxe:TextEdit Grid.Row="12" Grid.Column="10" HorizontalContentAlignment="Right" HorizontalAlignment="Stretch"  VerticalAlignment="Stretch"
                                                    Text="{Binding DataContext.ArticleData.Width,RelativeSource={RelativeSource AncestorType=dxlc:GroupBox},Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}"
                                                    IsReadOnly="True">
                                    </dxe:TextEdit>

                                    <!--Height-->
                                    <Label Grid.Row="12" Grid.Column="12"  Content="{DynamicResource ArticleDetailsViewInformationHeight}" HorizontalAlignment="Left" VerticalAlignment="Center"/>

                                    <dxe:TextEdit Grid.Row="12" Grid.Column="14" HorizontalContentAlignment="Right"    HorizontalAlignment="Stretch"  VerticalAlignment="Stretch"
                                                          Text="{Binding DataContext.ArticleData.Height,RelativeSource={RelativeSource AncestorType=dxlc:GroupBox},Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}"
                                                          IsReadOnly="True">
                                    </dxe:TextEdit>
                                    <Grid Grid.Row="14" Grid.Column="0" Grid.ColumnSpan="15">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="Auto"/>
                                            <ColumnDefinition Width="5"/>
                                            <ColumnDefinition Width="*"/>
                                            <ColumnDefinition Width="10"/>
                                            <ColumnDefinition Width="Auto"/>
                                            <ColumnDefinition Width="5"/>
                                            <ColumnDefinition Width="*"/>
                                            <ColumnDefinition Width="10"/>
                                            <ColumnDefinition Width="Auto"/>
                                            <ColumnDefinition Width="5"/>
                                            <ColumnDefinition Width="*"/>
                                            <ColumnDefinition Width="10"/>
                                            <ColumnDefinition Width="Auto"/>
                                            <ColumnDefinition Width="5"/>
                                            <ColumnDefinition Width="*"/>
                                            <ColumnDefinition Width="10"/>
                                            <ColumnDefinition Width="Auto"/>
                                            <ColumnDefinition Width="5"/>
                                            <ColumnDefinition Width="*"/>

                                        </Grid.ColumnDefinitions>
                                        <!--RegisterSerialNumber-->
                                        <Label  Grid.Column="0" Content="{DynamicResource ArticleDetailsViewInformationSerialNo}" HorizontalAlignment="Left" VerticalAlignment="Center"/>
                                        <dxe:ToggleSwitch ContentPlacement="Inside" Grid.Column="2" 
                                                      CheckedStateContent="Yes" UncheckedStateContent="No" 
                                                      ToggleSwitchWidth="50" 
                                                      ToggleSwitchHeight="30" 
                                                      IsThreeState="False" IsReadOnly="True"
                                                      IsChecked="{Binding DataContext.ArticleData.RegisterSerialNumber,RelativeSource={RelativeSource AncestorType=dxlc:GroupBox},Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}"   />
                                        <!--Shubham[skadam] GEOS2-3532 [QUALITY_INSPECTION] Ignore the Items in TRANSIT without “Product Inspection” OK 29 12 2022-->
                                        <!--Requires Inspection :-->
                                        <Label  Grid.Column="4" Margin="5,0,0,0"  Content="{DynamicResource IsInspectionRequired}" HorizontalAlignment="Right" VerticalAlignment="Center"/>
                                        <dxe:ToggleSwitch  IsReadOnly="False" ContentPlacement="Inside"  Grid.Column="6" HorizontalAlignment="Left"
                                                       CheckedStateContent="Yes" UncheckedStateContent="No"  Margin="0 0 0 0"
                                                       ToggleSwitchWidth="50" 
                                                       ToggleSwitchHeight="30"
                                                       IsThreeState="False"
                                                       IsChecked="{Binding DataContext.IsInspectionRequired,RelativeSource={RelativeSource AncestorType=dxlc:GroupBox},Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}"/>
                                        <!--Requires Count after picking :-->
                                        <Label  Grid.Column="8"  Content="{DynamicResource IsCountRequiredAfterPicking}" HorizontalAlignment="Right" VerticalAlignment="Center"/>
                                        <dxe:ToggleSwitch  IsReadOnly="{Binding DataContext.IsCountRequiredReadOnly,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged,RelativeSource={RelativeSource AncestorType=dxlc:GroupBox}}" ContentPlacement="Inside"  Grid.Column="10" HorizontalAlignment="Left"
                                                       CheckedStateContent="Yes" UncheckedStateContent="No"  Margin="0 0 0 0"
                                                       ToggleSwitchWidth="50" 
                                                       ToggleSwitchHeight="30"
                                                       IsThreeState="False"
                                                       IsChecked="{Binding DataContext.IsCountRequiredAfterPicking,RelativeSource={RelativeSource AncestorType=dxlc:GroupBox},Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}"/>


                                        <!--//Shubham[skadam] GEOS2-4533 Referencia solapada 05 06 2023-->
                                        <!--ArticleActive-->
                                        <Label  Grid.Column="18"  Content="{DynamicResource ArticleDetailsViewInformationActive}" HorizontalAlignment="Left" VerticalAlignment="Center"/>
                                        <dxe:ToggleSwitch  IsReadOnly="True" ContentPlacement="Inside"  Grid.Column="18" HorizontalAlignment="Center"
                                                       CheckedStateContent="Yes" UncheckedStateContent="No" Margin="70,0,0,0"
                                                       ToggleSwitchWidth="50" 
                                                       ToggleSwitchHeight="30"
                                                       IsThreeState="False"
                                                       IsChecked="{Binding DataContext.ArticleActive,RelativeSource={RelativeSource AncestorType=dxlc:GroupBox},Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}"/>



                                        <Label  Grid.Column="12" Content="{DynamicResource ArticleDetailsViewInformationReplacement}" HorizontalAlignment="Right" VerticalAlignment="Center">
                                            <Label.Style>
                                                <Style TargetType="{x:Type Label}">
                                                    <Setter Property="Visibility" Value="Hidden" />
                                                    <Style.Triggers>
                                                        <DataTrigger Binding="{Binding DataContext.IsReplacementAvailable, RelativeSource={RelativeSource AncestorType=dxlc:GroupBox}}" Value="true">
                                                            <Setter Property="Visibility" Value="Visible" />
                                                        </DataTrigger>
                                                    </Style.Triggers>
                                                </Style>
                                            </Label.Style>
                                        </Label>

                                        <dxe:HyperlinkEdit Height="30" 
                                                       Grid.Column="14" Grid.ColumnSpan="3" 
                                                       HorizontalAlignment="Left" VerticalAlignment="Center"
                                                       EditValue="{Binding DataContext.ArticleData.ReplacementArticle,RelativeSource={RelativeSource AncestorType=dxlc:GroupBox},Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}"
                                                       Command="{Binding DataContext.ReferenceHyperlinkClickCommand,RelativeSource={RelativeSource AncestorType=dxlc:GroupBox}}"
                                                       CommandParameter="{Binding DataContext.ArticleData,RelativeSource={RelativeSource AncestorType=dxlc:GroupBox}}">
                                            <dxe:HyperlinkEdit.Style>
                                                <Style TargetType="{x:Type dxe:HyperlinkEdit}">
                                                    <Setter Property="Visibility" Value="Hidden" />
                                                    <Setter Property="FontWeight" Value="Regular" />
                                                    <Setter Property="Foreground" Value="{DynamicResource HyperlinkColor}" />
                                                    <Style.Triggers>
                                                        <DataTrigger Binding="{Binding DataContext.IsReplacementAvailable, RelativeSource={RelativeSource AncestorType=dxlc:GroupBox}}" Value="true">
                                                            <Setter Property="Visibility" Value="Visible" />
                                                        </DataTrigger>
                                                        <Trigger Property="IsMouseOver" Value="true">
                                                            <Setter Property="Foreground" Value="OrangeRed" />
                                                        </Trigger>
                                                    </Style.Triggers>
                                                </Style>
