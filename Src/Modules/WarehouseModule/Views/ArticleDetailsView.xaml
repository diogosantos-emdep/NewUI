                                <!--Download-->
                                <TextBlock Name="txtWO" Grid.Row="0" Grid.Column="1" Text="{Binding DataContext.ArticleForeCastWOCount,UpdateSourceTrigger=PropertyChanged, Mode=TwoWay,RelativeSource={RelativeSource AncestorType=dxlc:GroupBox}}" 
                                           TextAlignment="Center" TextWrapping="Wrap" Style="{StaticResource TextBlock40Style}" FontWeight="Bold" VerticalAlignment="Center" HorizontalAlignment="Center" />
                                <Image Grid.Row="1" Grid.Column="1" Style="{DynamicResource ArticleDetailsForeCastDownImage}" Width="40" Height="40"  VerticalAlignment="Center" />
                            </Grid>

                            <!--<DataTemplate.Triggers>
                                <DataTrigger Binding="{Binding DataContext.ArticleData.LstArticleForecast.Count,RelativeSource={RelativeSource AncestorType=dxlc:GroupBox}, FallbackValue=0}" Value="0">
                                    <Setter TargetName="ForeCast" Property="Visibility" Value="Collapsed" />
                                </DataTrigger>
                            </DataTemplate.Triggers>-->
                        </DataTemplate>
                    </dxlc:GroupBox.ContentTemplate>

                    <dxlc:GroupBox.MaximizedContentTemplate>
                        <DataTemplate>
                            <Grid Margin="5" >
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="*"/>
                                </Grid.RowDefinitions>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="*"/>
                                </Grid.ColumnDefinitions>

                                <dxg:GridControl HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Margin="7" Padding="0"  Grid.Row="0" Grid.Column="0" MaxHeight="1000"
                                                 ImplyNullLikeEmptyStringWhenFiltering="True"  ItemsSource="{Binding DataContext.ArticleData.LstArticleForecast,RelativeSource={RelativeSource AncestorType=dxlc:GroupBox}, Mode=TwoWay}">
                                    <dxg:GridControl.Columns>
                                        <dxg:GridColumn Header="{DynamicResource GridColType }"  FieldName="Type" FilterPopupMode="CheckedList" Width="40" />
                                        <dxg:GridColumn Header="{DynamicResource GridColCode }"   FieldName="Code"  FilterPopupMode="CheckedList" Width="80"  >
                                            <dxg:GridColumn.CellTemplate>
                                                <DataTemplate>
                                                    <TextBlock Margin="10,0,0,0">
                                                        <Hyperlink x:Name="hyperlink" 
                                                                   TargetName="_blank"
                                                                   Command="{Binding DataContext.ForeCastCodeHyperlinkClickCommand,RelativeSource={RelativeSource AncestorType=dxlc:GroupBox}}"
                                                                   CommandParameter="{Binding ElementName=tblForecast}" Style="{DynamicResource hyperlinkGridStyle}" >
                                                         <InlineUIContainer>
                                                         <TextBlock Text="{Binding Data.Code}"  TextDecorations="Underline" />
                                                        </InlineUIContainer>
                                                        </Hyperlink>
                                                    </TextBlock>
                                                </DataTemplate>
                                            </dxg:GridColumn.CellTemplate>
                                        </dxg:GridColumn>
                                        <dxg:GridColumn Header="{DynamicResource GridColCustomerORSupplier}" FieldName="CustomerORSupplier" FilterPopupMode="CheckedList" Width="130" />
                                        <dxg:GridColumn Header="{DynamicResource GridColQuantity}"  FilterPopupMode="CheckedList" FieldName="Quantity" Width="50" >
                                            <dxg:GridColumn.DisplayTemplate>
                                                <ControlTemplate>
                                                    <StackPanel Orientation="Horizontal">
                                                        <Image Source="{Binding Path=RowData.Row.Type, Converter={Converters:CellImageSourceConverter}}" Height="19" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="2,0,0,0"/>
                                                        <TextBlock Text="{Binding Path=RowData.Row.Quantity}" TextAlignment="Center" Margin="20,0,0,0" />
                                                    </StackPanel>
                                                </ControlTemplate>
                                            </dxg:GridColumn.DisplayTemplate>
                                        </dxg:GridColumn>
                                    </dxg:GridControl.Columns>

                                    <dxmvvm:Interaction.Behaviors>
                                        <UIHelper:GridSelectingBehavior />
                                    </dxmvvm:Interaction.Behaviors>

                                    <dxg:GridControl.View>
                                        <dxg:TableView x:Name="tblForecast" 
                                                       NavigationStyle="Row" 
                                                       AutoWidth="True" 
                                                       ShowGroupPanel="False" 
                                                       AllowEditing="True" 
                                                       HorizontalScrollbarVisibility="Hidden" 
                                                       VerticalScrollbarVisibility="Auto" 
                                                       ShowIndicator="False"
                                                       SearchPanelHorizontalAlignment="Stretch"
                                                       ColumnChooserColumnDisplayMode="ShowHiddenColumnsOnly"/>
                                    </dxg:GridControl.View>
                                </dxg:GridControl>
                            </Grid>
                        </DataTemplate>
                    </dxlc:GroupBox.MaximizedContentTemplate>
                </dxlc:GroupBox>

                <!--End-->

                <!--Stock History-->
                <dxlc:GroupBox x:Name="HistoryDetails" Style="{StaticResource myGroupBoxStyle}">
                    <dxlc:GroupBox.HeaderTemplate>
                        <DataTemplate>
                            <DockPanel Margin="-5,0,0,0">
                                <TextBlock Margin="5,0,0,0"
                                            VerticalAlignment="Center"
                                            FontWeight="Bold"
                                            Text="{DynamicResource ArticleDetailsViewHistoryHeader}" />
                                <TextBlock Width="50"
                                            Margin="5,1,0,0"
                                            HorizontalAlignment="Left"
                                            VerticalAlignment="Center"
                                            FontWeight="Bold"
                                            x:Name="ContactCount"
                                            Text="{Binding DataContext.ArticleData.LstArticlesStock.Count, RelativeSource={RelativeSource AncestorType={x:Type dxlc:GroupBox}}}" />
                            </DockPanel>
                        </DataTemplate>
                    </dxlc:GroupBox.HeaderTemplate>
                    <dxlc:GroupBox.ContentTemplate>
                        <DataTemplate>
                            <Grid>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="10" />
                                    <ColumnDefinition Width="Auto" />
                                </Grid.ColumnDefinitions>
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="*" />
                                    <RowDefinition Height="*" />
                                    <RowDefinition Height="*" />
                                    <RowDefinition Height="*" />
                                </Grid.RowDefinitions>
                                <Label Grid.Row="0" Grid.Column="0" Content="{DynamicResource ArticleDetailsViewHistoryName}" FontWeight="Bold"/>
                                <TextBlock VerticalAlignment="Bottom" Grid.Row="0" Grid.Column="2"  Text="{Binding DataContext.ArticleData.LstArticlesStock[0].PeopleModifiedBy.FullName, RelativeSource={RelativeSource AncestorType={x:Type dxlc:GroupBox}}}"  TextWrapping="WrapWithOverflow" TextTrimming="CharacterEllipsis"/>
                                <Label Grid.Row="1"
                                        Grid.Column="0"
                                        Content="{DynamicResource ArticleDetailsViewHistoryDate}"
                                        FontWeight="Bold" />
                                <dxe:TextEdit DisplayFormatString="g" ShowBorder="False"  Grid.Row="1" Grid.Column="2" VerticalAlignment="Bottom" EditValue="{Binding DataContext.ArticleData.LstArticlesStock[0].UploadedIn , RelativeSource={RelativeSource AncestorType={x:Type dxlc:GroupBox}}}"    TextWrapping="WrapWithOverflow" TextTrimming="CharacterEllipsis"/>

                                <Label Grid.Row="2"
                                        Grid.Column="0"
                                        Content="{DynamicResource ArticleDetailsViewHistoryComments}"
                                        FontWeight="Bold"/>
                                <TextBlock  Grid.Row="2" Grid.Column="2" VerticalAlignment="Bottom" Text="{Binding DataContext.ArticleData.LstArticlesStock[0].Comments, RelativeSource={RelativeSource AncestorType={x:Type dxlc:GroupBox}}}"  ToolTip="{Binding DataContext.ArticleData.LstArticlesStock[0].Comments, RelativeSource={RelativeSource AncestorType={x:Type dxlc:GroupBox}}}"  TextWrapping="WrapWithOverflow" Width="185" TextTrimming="CharacterEllipsis"/>

                                <Label Grid.Row="3"
                                        Grid.Column="0"
                                        Content="{DynamicResource ArticleDetailsViewHistoryQuantity}"
                                        FontWeight="Bold"/>

                                <TextBlock  Grid.Row="3" Grid.Column="2" VerticalAlignment="Bottom" Text="{Binding DataContext.ArticleData.LstArticlesStock[0].Quantity, RelativeSource={RelativeSource AncestorType={x:Type dxlc:GroupBox}}}"   TextWrapping="WrapWithOverflow" TextTrimming="CharacterEllipsis">
                                    <TextBlock.Style>
                                        <Style TargetType="{x:Type TextBlock}">
                                            <Style.Triggers>
                                                <DataTrigger Binding="{Binding DataContext.ArticleData.LstArticlesStock[0].IsDamaged, RelativeSource={RelativeSource AncestorType={x:Type dxlc:GroupBox}}}" Value="1">
                                                    <Setter Property="TextBlock.Foreground" Value="Gray"></Setter>
                                                </DataTrigger>
                                                <DataTrigger Binding="{Binding DataContext.ArticleData.LstArticlesStock[0].IsDamaged, RelativeSource={RelativeSource AncestorType={x:Type dxlc:GroupBox}}}" Value="0">
                                                    <Setter Property="TextBlock.Foreground" Value="{Binding DataContext.ArticleData.LstArticlesStock[0].Quantity, RelativeSource={RelativeSource AncestorType={x:Type dxlc:GroupBox}},Converter={Converters:ForeGroundColourBasedOnValue}}"></Setter>
                                                </DataTrigger>
                                            </Style.Triggers>
                                        </Style>
                                    </TextBlock.Style>
                                </TextBlock>
                            </Grid>
                        </DataTemplate>
                    </dxlc:GroupBox.ContentTemplate>
                    <dxlc:GroupBox.MaximizedContentTemplate>
                        <DataTemplate>
                            <Grid>
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="*"/>
                                    <RowDefinition Height="Auto"/>
                                </Grid.RowDefinitions>
                                <dxg:GridControl
                                   FilterString="{Binding DataContext.FilterString,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged, RelativeSource={RelativeSource AncestorType=dxlc:GroupBox}}"
                                  Grid.Row="0" HorizontalAlignment="Stretch" VerticalAlignment="Top" Margin="0,0,0,0" Padding="0"
                                                 ItemsSource="{Binding DataContext.ArticleData.LstArticlesStock, RelativeSource={RelativeSource AncestorType=dxlc:GroupBox}, Mode=TwoWay}"
                                      Visibility="{Binding DataContext.VisibilityOfGridStockHistory, Mode=TwoWay,RelativeSource={RelativeSource AncestorType=dxwui:WinUIDialogWindow}}">
                                    <dxg:GridControl.Columns>
                                        <dxg:GridColumn Header="{DynamicResource GridColUser}" FieldName="PeopleModifiedBy.FullName"  Width="40*" FilterPopupMode="CheckedList" />
                                        <dxg:GridColumn Header="{DynamicResource GridColDate }" FieldName="UploadedIn" Width="30" MinWidth="150"  FilterPopupMode="CheckedList">
                                            <dxg:GridColumn.EditSettings>
                                                <dxe:TextEditSettings DisplayFormat="g" />
                                            </dxg:GridColumn.EditSettings>
                                        </dxg:GridColumn>
                                        <dxg:GridColumn Header="{DynamicResource GridColStockType}" FieldName="Type"  Width="22*" FilterPopupMode="CheckedList" />
                                        <dxg:GridColumn Header="{DynamicResource GridColAction}" FieldName="Comments" Width="Auto"  FilterPopupMode="CheckedList" />

                                        <dxg:GridColumn Header="{DynamicResource Location}" FieldName="WarehouseLocation.FullName"  Width="25" MinWidth="130" FilterPopupMode="CheckedList"  />

                                        <dxg:GridColumn Header="{DynamicResource DeliveryNote}" FieldName="WarehouseDeliveryNoteItem.WarehouseDeliveryNote.Code"  Width="30" MinWidth="130" FilterPopupMode="CheckedList" >
                                            <dxg:GridColumn.CellTemplate>
                                                <DataTemplate>
                                                    <TextBlock Margin="4 0 0 0">
                                                        <Hyperlink x:Name="hyperlink"
                                                                    TargetName="_blank"
                                                                    Command="{Binding DataContext.ArticleDetailsViewDNHyperlinkClickCommand, RelativeSource={RelativeSource AncestorType=dxlc:GroupBox}}"
                                                                    CommandParameter="{Binding ElementName=tblStockHistory}">
                                                                <InlineUIContainer>
                                                                    <TextBlock Text="{Binding Data.WarehouseDeliveryNoteItem.WarehouseDeliveryNote.Code}" VerticalAlignment="Center" />
                                                                </InlineUIContainer>
                                                        </Hyperlink>
                                                    </TextBlock>
                                                    <DataTemplate.Triggers>
                                                        <Trigger Property="dxg:DataViewBase.IsFocusedRow" Value="True">
                                                            <Setter TargetName="hyperlink" Property="Foreground" Value="{DynamicResource ColorMain}"/>
                                                        </Trigger>
                                                        <DataTrigger Binding="{Binding RowData.SelectionState}" Value="Selected">
                                                            <Setter TargetName="hyperlink" Property="Foreground" Value="{DynamicResource ColorMain}"/>
                                                        </DataTrigger>
                                                        <Trigger Property="IsMouseOver" Value="True">
                                                            <Setter TargetName="hyperlink" Property="Foreground" Value="OrangeRed" />
                                                        </Trigger>
                                                    </DataTemplate.Triggers>
                                                </DataTemplate>
                                            </dxg:GridColumn.CellTemplate>
                                        </dxg:GridColumn>

                                        <dxg:GridColumn Header="{DynamicResource GridColQuantity }" FieldName="Quantity" Width="80" MinWidth="80"  FilterPopupMode="CheckedList" FixedWidth="True" Fixed="Right" AllowMoving="False" >
                                            <dxg:GridColumn.CellTemplate>
                                                <DataTemplate>
                                                    <Grid >
                                                        <Grid.ColumnDefinitions>
                                                            <ColumnDefinition Width="*"/>
                                                            <ColumnDefinition Width="*"/>
                                                        </Grid.ColumnDefinitions>
                                                        <dxe:TextEdit x:Name="PART_Editor" HorizontalContentAlignment="Center" Grid.Column="0" Grid.ColumnSpan="2"  HorizontalAlignment="Center" Text="{Binding Row.RowData.Quantity}"  VerticalAlignment="Stretch" IsReadOnly="True" />
                                                    </Grid>
                                                    <DataTemplate.Triggers >
                                                        <DataTrigger Binding="{Binding RowData.Row.IsDamaged}" Value="1">
                                                            <Setter Property="dxe:TextEdit.Foreground" Value="Gray"/>
                                                        </DataTrigger>
                                                        <DataTrigger Binding="{Binding RowData.Row.IsDamaged}" Value="0">
                                                            <Setter Property="dxe:TextEdit.Foreground" Value="{Binding RowData.Row.Quantity, Converter={Converters:ForeGroundColourBasedOnValue}}"/>
                                                        </DataTrigger>
                                                    </DataTemplate.Triggers>
                                                </DataTemplate>
                                            </dxg:GridColumn.CellTemplate>
                                        </dxg:GridColumn>
                                    </dxg:GridControl.Columns>
                                    <dxmvvm:Interaction.Behaviors>
                                        <UIHelper:GridSelectingBehavior />
                                    </dxmvvm:Interaction.Behaviors>
                                    <dxg:GridControl.View>
                                        <dxg:TableView x:Name="tblStockHistory" 
                                                       NavigationStyle="Row" 
                                                       AutoWidth="True" 
                                                       ShowGroupPanel="False" 
                                                       AllowEditing="True" 
                                                       HorizontalScrollbarVisibility="Hidden" 
                                                       VerticalScrollbarVisibility="Auto" 
                                                       HorizontalAlignment="Center"
                                                       ShowSearchPanelMode="Always"
                                                       VerticalAlignment="Center"
                                                       ShowIndicator="False"
                                                       SearchPanelHorizontalAlignment="Stretch"
                                                       ColumnChooserColumnDisplayMode="ShowHiddenColumnsOnly"/>
                                    </dxg:GridControl.View>
                                </dxg:GridControl>

                                <Grid x:Name="GridChartControl"
                                      Grid.Row="0" HorizontalAlignment="Stretch" VerticalAlignment="Top" Margin="0,-10,-10, 0"
                                      Visibility="{Binding DataContext.VisibilityOfGridChartControl, Mode=TwoWay,RelativeSource={RelativeSource AncestorType=dxwui:WinUIDialogWindow}}">
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="Auto"/>
                                        <RowDefinition Height="Auto"/>
                                        <RowDefinition Height="*"/>
                                        <RowDefinition Height="2"/>
                                    </Grid.RowDefinitions>

                                    <StackPanel Orientation="Horizontal" Margin="5 10 0 0" Grid.Row="0">
                                        <TextBlock Text="Group by" />
                                        <RadioButton GroupName="FilterType" Content="Year" IsChecked="{Binding DataContext.IsYear, Mode=TwoWay,RelativeSource={RelativeSource AncestorType=dxwui:WinUIDialogWindow}}" Margin="5,0,0,0"/>
                                        <RadioButton GroupName="FilterType" Content="Month" IsChecked="{Binding DataContext.IsMonth, Mode=TwoWay,RelativeSource={RelativeSource AncestorType=dxwui:WinUIDialogWindow}}" Margin="5,0,0,0"/>
                                        <RadioButton GroupName="FilterType" Content="Week" IsChecked="{Binding DataContext.IsWeek, Mode=TwoWay,RelativeSource={RelativeSource AncestorType=dxwui:WinUIDialogWindow}}" Margin="5,0,0,0"/>
                                    </StackPanel>

                                    <dxlc:LayoutControl Name="layoutControl1" Orientation="Vertical" Grid.Row="2" Margin="0"
                                                        Visibility="{Binding DataContext.VisibilityOfGridChartControl, Mode=TwoWay,RelativeSource={RelativeSource AncestorType=dxwui:WinUIDialogWindow}}">
                                        <dxlc:LayoutGroup Orientation="Vertical" Margin="0" Padding="0">
                                            <dxlc:LayoutGroup dxlc:LayoutControl.AllowHorizontalSizing="True" Margin="0"
                                                              >
                                                <dxc:ChartControl x:Name="ChartItemsForecast"
                                                    DataSource="{Binding DataContext.ArticleData.LstArticlesStock,RelativeSource={RelativeSource AncestorType=dxwui:WinUIDialogWindow}}">
                                                    <!--<dxc:ChartControl.CrosshairOptions>
                                                        <dxc:CrosshairOptions ShowArgumentLabels="True" ShowValueLabels="True" ShowValueLine="True"/>
                                                    </dxc:ChartControl.CrosshairOptions>
                                                    <dxc:ChartControl.Titles>
                                                        <dxc:Title Content="Stock History"  HorizontalAlignment="Center"/>
                                                    </dxc:ChartControl.Titles>
                                                    <dxc:ChartControl.Legends>
                                                        <dxc:Legend/>
                                                    </dxc:ChartControl.Legends>
                                                    <dxc:XYDiagram2D>
                                                        <dxc:XYDiagram2D.AxisX>
                                                            <dxc:AxisX2D >
                                                                <dxc:AxisX2D.Title>
                                                                    <dxc:AxisTitle Content="Time Period"/>
                                                                </dxc:AxisX2D.Title>
                                                            </dxc:AxisX2D>
                                                        </dxc:XYDiagram2D.AxisX>
                                                        <dxc:XYDiagram2D.AxisY>
                                                            <dxc:AxisY2D>
                                                                <dxc:AxisY2D.Title>
                                                                    <dxc:AxisTitle Content="Quantity"/>
                                                                </dxc:AxisY2D.Title>
                                                            </dxc:AxisY2D>
                                                        </dxc:XYDiagram2D.AxisY>
                                                        <dxc:BarSideBySideSeries2D DisplayName="" ArgumentDataMember="UploadedIn" ValueDataMember="Quantity" 
                                                                 CrosshairLabelPattern="${V:f2}M"/>
                                                        -->
                                                    <!--<dxc:BarSideBySideSeries2D DisplayName="Outcome" ValueDataMember="QuantityValue" 
                                                                 CrosshairLabelPattern="${V:f2}M"/>-->
                                                    <!--
                                                    </dxc:XYDiagram2D>-->
                                                    <!--Command="{Binding ChartItemsForecastLoadCommand}"-->
                                                    <dxmvvm:Interaction.Triggers>
                                                        <dxmvvm:EventToCommand EventName="Loaded"
                                                                               Command="{Binding DataContext.ChartItemsForecastLoadCommand, RelativeSource={RelativeSource AncestorType=dxlc:GroupBox, Mode=FindAncestor}}"
                                                                               CommandParameter="{Binding ElementName=ChartItemsForecast}"/>
                                                        <!--<dxmvvm:EventToCommand EventName="CustomDrawCrosshair" Command="{Binding ChartDashboardSaleCustomDrawCrosshairCommand}" PassEventArgsToCommand="True"/>-->
                                                    </dxmvvm:Interaction.Triggers>
                                                </dxc:ChartControl>
                                            </dxlc:LayoutGroup>

                                        </dxlc:LayoutGroup>
                                    </dxlc:LayoutControl>
                                </Grid>

                                <!--Switch Button-->
                                <dx:DropDownButton x:Name="dropdownButton" Grid.Row="1" Height="40" Width="40" Margin="5 5 50 0"
                                        HorizontalAlignment="Right" VerticalAlignment="Bottom">
                                    <dx:DropDownButton.Style>
                                        <Style>
                                            <Setter Property="dx:DropDownButtonBase.ArrowAlignment" Value="Right" />
                                            <Setter Property="dx:DropDownButtonBase.ArrowPadding" Value="0" />
                                        </Style>
                                    </dx:DropDownButton.Style>

                                    <Button.ToolTip>
                                        <TextBlock Text="{DynamicResource SwitchViewButtonTooltip}"></TextBlock>
                                    </Button.ToolTip>

                                    <Button.ContentTemplate>
                                        <DataTemplate>
                                            <Grid>
                                                <Grid.ColumnDefinitions>
                                                    <ColumnDefinition Width="Auto" />
                                                </Grid.ColumnDefinitions>
                                                <Image Style="{DynamicResource SwitchOrganizationImageStyle}" Grid.Column="0" Height="24" Width="24" Margin="-20" />
                                            </Grid>
                                        </DataTemplate>
                                    </Button.ContentTemplate>

                                    <dxb:PopupMenu HorizontalAlignment="Stretch">
                                        <dxb:BarButtonItem Name="GridView"  Content="Grid View" Glyph="{DynamicResource SwitchViewButtonForGridView}" 
                                                           Command="{Binding DataContext.StockHistoryGraphCommand, RelativeSource={RelativeSource AncestorType={x:Type dxlc:GroupBox}}}"
                                                           CommandParameter="GridView">
                                        </dxb:BarButtonItem>

                                        <dxb:BarButtonItem Name="GraphView" Content="Graph View" Glyph="{DynamicResource SwitchViewButtonForGraphView}" 
                                                           Command="{Binding DataContext.StockHistoryGraphCommand, RelativeSource={RelativeSource AncestorType={x:Type dxlc:GroupBox}}}"
                                                           CommandParameter="GraphView">
                                        </dxb:BarButtonItem>
                                    </dxb:PopupMenu>
                                </dx:DropDownButton>

                                <!--<Button Content="" Grid.Row="1" Height="40" Width="40" 
                                        Margin="5 5 50 0"
                                        HorizontalAlignment="Right" VerticalAlignment="Bottom"
                                        Command="{Binding DataContext.StockHistoryGraphCommand, RelativeSource={RelativeSource AncestorType={x:Type dxlc:GroupBox}}}">
                                    <Button.ToolTip>
                                        <TextBlock Text="{DynamicResource SwitchViewButtonTooltip}"/>
                                    </Button.ToolTip>
                                    <Button.ContentTemplate>
                                        <DataTemplate>
                                            <Image Style="{DynamicResource SwitchOrganizationImageStyle}" Height="24" Width="24" Margin="-10"/>
                                        </DataTemplate>
                                    </Button.ContentTemplate>
                                </Button>-->

                                <!--Export to Excel Button-->
                                <Button Content="" Grid.Row="1" Height="40" Width="40" 
                                        Margin="5 5 0 0"
                                        HorizontalAlignment="Right" VerticalAlignment="Bottom"
                                        Command="{Binding DataContext.ExportButtonCommand, RelativeSource={RelativeSource AncestorType={x:Type dxlc:GroupBox}}}"
                                        CommandParameter="{Binding ElementName=tblStockHistory}">
                                    <Button.ToolTip>
                                        <TextBlock Text="{DynamicResource ButtonExportToExcel}"/>
                                    </Button.ToolTip>
                                    <Button.ContentTemplate>
                                        <DataTemplate>
                                            <Image Style="{DynamicResource ExcelExportButtonImageStyle}" Height="24" Width="24" Margin="-10"/>
                                        </DataTemplate>
                                    </Button.ContentTemplate>
                                </Button>
                            </Grid>
                        </DataTemplate>
                    </dxlc:GroupBox.MaximizedContentTemplate>
                </dxlc:GroupBox>
                <!--End-->

            </dxlc:FlowLayoutControl>

            <StackPanel Grid.Row="1"
                        Grid.Column="1" 
                        Margin="20,0,0,0"
                        Width="290"
                        HorizontalAlignment="Center"
                        Orientation="Horizontal">
                <Button Height="30"
                        Width="100"
                        Margin="0,0,20,0"
                        IsEnabled="{Binding Source={x:Static WarehouseCommon:WarehouseCommon.Instance}, Path=IsPermissionEnabled}"
                        VerticalAlignment="Bottom"
                        Command="{Binding AcceptButtonCommand}"
                        Content="Accept"/>
                <Button Grid.Row="1" Grid.Column="1" Content="{DynamicResource Cancel}" 
                        Margin="20,0,0,0"
                        Command="{Binding CancelButtonCommand}" 
                        Height="30"
                        Width="100"
                        VerticalAlignment="Bottom" 
                        HorizontalAlignment="Right"/>
            </StackPanel>
        </Grid>

    </ScrollViewer>
</dxwui:WinUIDialogWindow>
