                                           Grid.Column="2" Grid.ColumnSpan="2"
                                           TextWrapping="WrapWithOverflow" TextTrimming="CharacterEllipsis"/>

                                <TextBlock Grid.Row="2"
                                           Grid.Column="0"
                                           VerticalAlignment="Top" HorizontalAlignment="Left"
                                           Text="{DynamicResource ArticleDetailsViewSupplierCountry}"
                                           FontWeight="Bold"/>

                                <DataGridCell  VerticalAlignment="Top" HorizontalAlignment="Left"
                                           Grid.Row="2" Grid.RowSpan="2"
                                           Grid.Column="2" >
                                    <StackPanel Orientation="Horizontal" Height="24">
                                        <Image Source="{Binding DataContext.FavouriteSupplier.ArticleSupplier.Country.CountryIconImage, RelativeSource={RelativeSource AncestorType={x:Type dxlc:GroupBox}}}"  
                                                   VerticalAlignment="Center" Height="24" Width="24" Margin="2,0,0,0" HorizontalAlignment="Left"     Grid.Row="2" Grid.RowSpan="2"
                                           Grid.Column="2"  />


                                        <TextBlock x:Name="ArticleSupplierCountryName"
                                           ToolTip="{Binding DataContext.FavouriteSupplier.ArticleSupplier.Country.Name, RelativeSource={RelativeSource AncestorType={x:Type dxlc:GroupBox}}}"
                                           Text="{Binding DataContext.FavouriteSupplier.ArticleSupplier.Country.Name, RelativeSource={RelativeSource AncestorType={x:Type dxlc:GroupBox}}}"
                                           VerticalAlignment="Top" HorizontalAlignment="Left"
                                           Grid.Row="2" Grid.RowSpan="2"
                                           Grid.Column="2" 
                                           TextWrapping="WrapWithOverflow"
                                           TextTrimming="CharacterEllipsis" Margin="5,0,0,0">
                                            <TextBlock.Style>
                                                <Style TargetType="{x:Type TextBlock}">
                                                    <Style.Triggers>
                                                        <DataTrigger Binding="{Binding DataContext.FavouriteSupplier.ArticleSupplier.ArticleSupplierType.Name, RelativeSource={RelativeSource AncestorType=dxlc:GroupBox}}" Value="{x:Null}">
                                                            <Setter Property="Grid.ColumnSpan" Value="2" />
                                                        </DataTrigger>
                                                    </Style.Triggers>
                                                </Style>
                                            </TextBlock.Style>
                                        </TextBlock>
                                    </StackPanel>
                                </DataGridCell>


                                <Border Grid.Row="2" Grid.Column="3" Grid.RowSpan="2"
                                        Height="40" Width="40"
                                        VerticalAlignment="Top" HorizontalAlignment="Right"
                                        Background="{Binding DataContext.FavouriteSupplier.ArticleSupplier.ArticleSupplierType.HtmlColor,UpdateSourceTrigger=PropertyChanged, RelativeSource={RelativeSource AncestorType= dxwui:WinUIDialogWindow}}"  
                                        BorderThickness="1" BorderBrush="Gray" >
                                    <TextBlock Text="{Binding DataContext.FavouriteSupplier.ArticleSupplier.ArticleSupplierType.Name,UpdateSourceTrigger=PropertyChanged,
                                               RelativeSource={RelativeSource AncestorType= dxwui:WinUIDialogWindow}}" 
                                               TextBlock.Foreground="{DynamicResource ColorMain}" 
                                               FontWeight="Bold"  TextAlignment="Center" 
                                               Style="{DynamicResource TextBlock40Style}" 
                                               HorizontalAlignment="Center" VerticalAlignment="Center"/>
                                    <Border.Style>
                                        <Style TargetType="{x:Type Border}">
                                            <Setter Property="Visibility" Value="Visible" />
                                            <Style.Triggers>
                                                <DataTrigger Binding="{Binding DataContext.FavouriteSupplier.ArticleSupplier.ArticleSupplierType.Name, RelativeSource={RelativeSource AncestorType=dxlc:GroupBox}}" Value="{x:Null}">
                                                    <Setter Property="Visibility" Value="Hidden" />
                                                </DataTrigger>
                                            </Style.Triggers>
                                        </Style>
                                    </Border.Style>
                                </Border>

                                <Grid.Style>
                                    <Style TargetType="{x:Type Grid}">
                                        <Setter Property="Visibility" Value="Visible" />
                                        <Style.Triggers>
                                            <DataTrigger Binding="{Binding DataContext.FavouriteSupplier, RelativeSource={RelativeSource AncestorType=dxlc:GroupBox}}" Value="{x:Null}">
                                                <Setter Property="Visibility" Value="Hidden" />
                                            </DataTrigger>
                                        </Style.Triggers>
                                    </Style>
                                </Grid.Style>
                            </Grid>
                        </DataTemplate>
                    </dxlc:GroupBox.ContentTemplate>
                    <dxlc:GroupBox.MaximizedContentTemplate>
                        <DataTemplate>
                            <dxg:GridControl HorizontalAlignment="Stretch" VerticalAlignment="Top" Margin="-10,-10,-10,-10" Padding="0"
                                            ItemsSource="{Binding DataContext.ArticleData.LstArticleBySupplier,RelativeSource={RelativeSource AncestorType=dxlc:GroupBox}, Mode=TwoWay}">
                                <dxg:GridControl.Columns>

                                    <dxg:GridColumn FixedWidth="True" Width="30" AllowColumnFiltering="False" ShowInColumnChooser="False">
                                        <dxg:GridColumn.CellTemplate>
                                            <DataTemplate>
                                                <Image Height="24" Width="24" Name="image" Source="/Emdep.Geos.Modules.Warehouse;component/Assets/Images/FavouriteSupplier.png" ToolTip="{DynamicResource ArticleDetailsViewFavouriteSupplierTooltip}"/>
                                                <DataTemplate.Triggers>
                                                    <DataTrigger Binding="{Binding RowData.Row.IsTheFavorite}" Value="0">
                                                        <Setter TargetName="image" Property="Source" Value="{x:Null}" />
                                                    </DataTrigger>
                                                    <DataTrigger Binding="{Binding RowData.Row.IsTheFavorite}" Value="{x:Null}">
                                                        <Setter TargetName="image" Property="Source" Value="{x:Null}" />
                                                    </DataTrigger>
                                                </DataTemplate.Triggers>
                                            </DataTemplate>
                                        </dxg:GridColumn.CellTemplate>
                                    </dxg:GridColumn>

                                    <dxg:GridColumn Header="{DynamicResource GridColReference }" Width="100" FieldName="Reference" FilterPopupMode="CheckedList">
                                        <!--<dxg:GridColumn.CellTemplate>
                                            <DataTemplate>
                                            <TextBlock HorizontalAlignment="Stretch" VerticalAlignment="Center">
                                            <Hyperlink x:Name="hyperlink" 
                                               TargetName="_blank"
                                               Command="{Binding DataContext.ReferenceHyperlinkClickCommand,RelativeSource={RelativeSource AncestorType=dxlc:GroupBox}}"
                                               CommandParameter="{Binding ElementName=tblSupplierDetails}" Style="{DynamicResource hyperlinkGridStyle}">
                                             <InlineUIContainer>
                                             <TextBlock Text="{Binding Data.Reference}"  Style="{DynamicResource HyperlinkTextBlock}" TextDecorations="Underline" />
                                            </InlineUIContainer>
                                            </Hyperlink>
                                          </TextBlock>
                                            </DataTemplate>
                                        </dxg:GridColumn.CellTemplate>-->
                                    </dxg:GridColumn>

                                    <dxg:GridColumn Header="{DynamicResource GridColName}" FieldName="ArticleSupplier.Name" FilterPopupMode="CheckedList" />

                                    <dxg:GridColumn Header="{DynamicResource ArticleDetailsViewPurchaseQty}" FixedWidth="True" MinWidth="80" MaxWidth="120" Width="100" FieldName="PurchaseQty" FilterPopupMode="CheckedList" />

                                    <dxg:GridColumn Header="{DynamicResource GridColType}" FixedWidth="True"  Width="50" MinWidth="50" MaxWidth="50" FilterPopupMode="CheckedList" ReadOnly="True">
                                        <dxg:GridColumn.CellTemplate>
                                            <DataTemplate>
                                                <TextBlock Margin="0,2,0,2"  VerticalAlignment="Center" 
                                                           HorizontalAlignment="Center"
                                                           Text="{Binding RowData.Row.ArticleSupplier.ArticleSupplierType.Name}" 
                                                           Width="24" Height="24" 
                                                           Background="{Binding RowData.Row.ArticleSupplier.ArticleSupplierType.HtmlColor}"
                                                           Foreground="{Binding RowData.Row.ArticleSupplier.ArticleSupplierType.HtmlColor,Converter={Converters:ForegroundColorConverter}}"
                                                           FontWeight="Bold" TextAlignment="Center" Style="{DynamicResource TextBlock18Style}" />
                                            </DataTemplate>
                                        </dxg:GridColumn.CellTemplate>
                                    </dxg:GridColumn>
                                </dxg:GridControl.Columns>
                                <dxmvvm:Interaction.Behaviors>
                                    <UIHelper:GridSelectingBehavior />
                                </dxmvvm:Interaction.Behaviors>
                                <dxg:GridControl.View>
                                    <dxg:TableView x:Name="tblSupplierDetails" 
                                                   NavigationStyle="Row" 
                                                   AutoWidth="True" 
                                                   ShowGroupPanel="False" 
                                                   AllowEditing="True" 
                                                   HorizontalScrollbarVisibility="Hidden" 
                                                   VerticalScrollbarVisibility="Auto" 
                                                   ShowIndicator="False"
                                                   SearchPanelHorizontalAlignment="Stretch"
                                                   ColumnChooserColumnDisplayMode="ShowHiddenColumnsOnly"/>
                                </dxg:GridControl.View>
                            </dxg:GridControl>
                        </DataTemplate>
                    </dxlc:GroupBox.MaximizedContentTemplate>
                </dxlc:GroupBox>
                <!--End-->

                <!--ManuFacturers-->
                <dxlc:GroupBox x:Name="ManufactureDetails" Style="{StaticResource myGroupBoxStyle1}">
