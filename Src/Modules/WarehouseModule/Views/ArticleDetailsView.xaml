                                            <dxdo:LayoutPanel x:Name="AddLocation" ItemHeight="360" 
                                                              Caption="{DynamicResource WarehouseAddLocation}" 
                                                              AllowClose="False" AllowDrag="False" AllowContextMenu="False" AllowFloat="False" AllowSizing="False">
                                                <Grid>
                                                    <dxg:GridControl x:Name="WarehouseLocationGrid"
