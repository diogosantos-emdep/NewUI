                                                                        <dxg:TableView.IsDetailButtonVisibleBinding>
                                                                            <Binding Path="Row.SerialCodes">
                                                                                <Binding.Converter>
                                                                                    <dxmvvm:ObjectToObjectConverter DefaultTarget="True">
                                                                                        <dxmvvm:MapItem Source="{x:Null}" Target="False"/>
                                                                                    </dxmvvm:ObjectToObjectConverter>
