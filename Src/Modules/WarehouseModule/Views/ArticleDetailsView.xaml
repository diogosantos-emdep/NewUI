                                                                        <dxg:GridColumn.CellTemplate>
                                                                            <DataTemplate>
                                                                                <Button ToolTip="Print" Command="{Binding DataContext.PrintDeliveryNoteCommand,RelativeSource={RelativeSource AncestorType=dxwui:WinUIDialogWindow}}" 
