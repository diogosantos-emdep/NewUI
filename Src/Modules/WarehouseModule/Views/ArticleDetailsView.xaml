                                                                            <dxg:GridControl.Columns>
                                                                                <dxg:GridColumn Header="Serial Numbers">
                                                                                    <dxg:GridColumn.DisplayMemberBinding>
                                                                                        <Binding Path="RowData.Row" RelativeSource="{RelativeSource Self}"/>
                                                                                    </dxg:GridColumn.DisplayMemberBinding>
                                                                                </dxg:GridColumn>
