                                    </TextBlock>

                                    <dxe:TextEdit  Grid.Row="2" Grid.Column="2" FontSize="14" ShowErrorToolTip="False" ShowError="False"
                                               Mask="HH:mm" MaskType="DateTimeAdvancingCaret" MaskUseAsDisplayFormat="True"
                                               EditValue="{Binding StartTimeTuesdayForFilling}">
                                    </dxe:TextEdit>

                                    <TextBlock Grid.Row="2" Grid.Column="3" HorizontalAlignment="Right" VerticalAlignment="Center"
                                           Text="{DynamicResource CommonTo}" Margin="0 0 5 0" FontSize="14">
                                    </TextBlock>

                                    <dxe:TextEdit  Grid.Row="2" Grid.Column="4" FontSize="14" ShowErrorToolTip="False" ShowError="False"
                                               Mask="HH:mm" MaskType="DateTimeAdvancingCaret" MaskUseAsDisplayFormat="True"
                                               EditValue="{Binding EndTimeTuesdayForFilling}">
                                    </dxe:TextEdit>
                                    <!--Tuesday-->

                                    <!--Wednesday-->
