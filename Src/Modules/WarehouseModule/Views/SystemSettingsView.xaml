                                    </TextBlock>

                                    <dxe:TextEdit  Grid.Row="6" Grid.Column="4" FontSize="14" ShowErrorToolTip="False" ShowError="False"
                                               Mask="HH:mm" MaskType="DateTimeAdvancingCaret" MaskUseAsDisplayFormat="True"
                                               EditValue="{Binding EndTimeThursdayForFilling}">
                                    </dxe:TextEdit>
                                    <!--Thursday-->

                                    <!--Friday-->
                                    <TextBlock Grid.Row="8" Grid.Column="0" HorizontalAlignment="Left" VerticalAlignment="Center"
                                           Text="{DynamicResource CommonFriday}" FontWeight="Bold" FontSize="14">
                                    </TextBlock>

                                    <TextBlock Grid.Row="8" Grid.Column="1" HorizontalAlignment="Right" VerticalAlignment="Center"
                                           Text="{DynamicResource CommonFrom}" Margin="0 0 5 0" FontSize="14">
                                    </TextBlock>

                                    <dxe:TextEdit  Grid.Row="8" Grid.Column="2" FontSize="14" ShowErrorToolTip="False" ShowError="False"
                                               Mask="HH:mm" MaskType="DateTimeAdvancingCaret" MaskUseAsDisplayFormat="True"
                                               EditValue="{Binding StartTimeFridayForFilling}">
                                    </dxe:TextEdit>

                                    <TextBlock Grid.Row="8" Grid.Column="3" HorizontalAlignment="Right" VerticalAlignment="Center"
                                           Text="{DynamicResource CommonTo}" Margin="0 0 5 0" FontSize="14">
                                    </TextBlock>

                                    <dxe:TextEdit  Grid.Row="8" Grid.Column="4" FontSize="14" ShowErrorToolTip="False" ShowError="False"
                                               Mask="HH:mm" MaskType="DateTimeAdvancingCaret" MaskUseAsDisplayFormat="True"
                                               EditValue="{Binding EndTimeFridayForFilling}">
                                    </dxe:TextEdit>
                                    <!--Friday-->

                                    <!--Saturday-->
                                    <TextBlock Grid.Row="10" Grid.Column="0" HorizontalAlignment="Left" VerticalAlignment="Center"
                                           Text="{DynamicResource CommonSaturday}" FontWeight="Bold" FontSize="14">
                                    </TextBlock>

                                    <TextBlock Grid.Row="10" Grid.Column="1" HorizontalAlignment="Right" VerticalAlignment="Center"
                                           Text="{DynamicResource CommonFrom}" Margin="0 0 5 0" FontSize="14">
                                    </TextBlock>

                                    <dxe:TextEdit  Grid.Row="10" Grid.Column="2" FontSize="14" ShowErrorToolTip="False" ShowError="False"
                                               Mask="HH:mm" MaskType="DateTimeAdvancingCaret" MaskUseAsDisplayFormat="True"
                                               EditValue="{Binding StartTimeSaturdayForFilling}">
                                    </dxe:TextEdit>

                                    <TextBlock Grid.Row="10" Grid.Column="3" HorizontalAlignment="Right" VerticalAlignment="Center"
                                           Text="{DynamicResource CommonTo}" Margin="0 0 5 0" FontSize="14">
                                    </TextBlock>

                                    <dxe:TextEdit  Grid.Row="10" Grid.Column="4" FontSize="14" ShowErrorToolTip="False" ShowError="False"
                                               Mask="HH:mm" MaskType="DateTimeAdvancingCaret" MaskUseAsDisplayFormat="True"
                                               EditValue="{Binding EndTimeSaturdayForFilling}">
                                    </dxe:TextEdit>
                                    <!--Saturday-->

                                    <!--Sunday-->
                                    <TextBlock Grid.Row="12" Grid.Column="0" HorizontalAlignment="Left" VerticalAlignment="Center"
                                           Text="{DynamicResource CommonSunday}" FontWeight="Bold" FontSize="14">
                                    </TextBlock>

                                    <TextBlock Grid.Row="12" Grid.Column="1" HorizontalAlignment="Right" VerticalAlignment="Center"
