                                    </TextBlock>

                                    <dxe:TextEdit  Grid.Row="12" Grid.Column="4" FontSize="14" ShowErrorToolTip="False" ShowError="False"
                                               Mask="HH:mm" MaskType="DateTimeAdvancingCaret" MaskUseAsDisplayFormat="True"
                                               EditValue="{Binding EndTimeSundayForFilling}">
                                    </dxe:TextEdit>
                                    <!--Sunday-->
                                </Grid>
                            </dx:DXTabItem>

                            <dx:DXTabItem Header="{DynamicResource CommonPicking}" FontSize="18" 
                                      Visibility="{Binding IsPickingVisible,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}" Width="Auto" >
