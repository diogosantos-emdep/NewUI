                                <DataTemplate>
                                    <Border x:Name="stackBorder"  BorderThickness="0,0,2,0" Margin="0,5,12,5" >
                                        <StackPanel Name="stkImage_Text" Orientation="Horizontal" Height="21" >

                                            <dxe:ImageEdit Name="Img_Editor" Height="17" Width="17" ShowBorder="False" EditMode="InplaceInactive" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" Margin="1,0,0,0" />
                                            <dxe:TextEdit Name="PART_Editor"  Height="17"   VerticalContentAlignment="Center" HorizontalContentAlignment="Center"/>
                                        </StackPanel>
                                    </Border>
                                    <DataTemplate.Triggers>
                                        <DataTrigger Binding="{Binding RowData.Row.IsShow}" Value="True">
                                            <!--<Setter Property="Background" TargetName="stkImage_Text" Value="#F0FFE1"></Setter>-->
                                            <Setter Property="Source" TargetName="Img_Editor" Value="{Binding Path=Source, Source={StaticResource certification}}"></Setter>
                                            
                                        </DataTrigger>
                                        <DataTrigger Binding="{Binding RowData.Row.IsShow}" Value="False">
                                            <!--<Setter Property="Background" TargetName="stkImage_Text" Value="#FFEDCF"></Setter>-->
                                            <Setter Property="Source" TargetName="Img_Editor" Value="{Binding Path=Source, Source={StaticResource Transparent}}"></Setter>
                                            
                                        </DataTrigger>
                                    </DataTemplate.Triggers>
                                </DataTemplate>
                            </dxg:GridColumn.CellTemplate>

                        </dxg:GridColumn>
                        
                        
                        <dxg:GridColumn FieldName="Type" Header="Type"/>
                        <!--<dxg:TreeListColumn FieldName="RevisionNumber" Header="Rev no."/>-->
                        <dxg:GridColumn FieldName="Site" Header="Site"/>
                        <dxg:GridColumn FieldName="Problem" Header="Problem"/>
                        <dxg:GridColumn FieldName="RootCause" Header="RootCause"/>
                        <dxg:GridColumn FieldName="ChangeLog" Header="Change Log"/>
                        <dxg:GridColumn FieldName="CreatedIn" Header="Created In"/>
                        <dxg:GridColumn FieldName="CreatedBy" Header="Created By"/>
                        <dxg:GridColumn FieldName="AproovedIn" Header="Approved In"/>
                        <dxg:GridColumn FieldName="AproovedBy" Header="Approved By"/>
                    </dxg:GridControl.Columns>
                    <dxg:GridControl.View>
                        <dxg:TableView Name="tableView1"
                                       AutoWidth="True" 
                                       ShowAutoFilterRow="False"
                                       ShowGroupPanel="False"
                                       NavigationStyle="Row" 
                                       RowMinHeight="2"
                                       BestFitMode="AllRows" 
                                       AllowSorting="True" 
                                       AllowScrollHeaders="False" 
                                       AllowGrouping="False"
                                       AllowEditing="False"
                                       AllowColumnMoving="False" 
                                       AutoMoveRowFocus="False"
                                       ShowIndicator="False"
                                       AllowScrollAnimation="True"
                                       AllowBestFit="True" AllowResizing="False" AllowConditionalFormattingMenu="True">
                        </dxg:TableView>
                    </dxg:GridControl.View>
                </dxg:GridControl>
            </dx:DXTabItem>

        </dx:DXTabControl>
