                    <dxg:GridControl   MinHeight="100"  ItemsSource="clsBOM" Name="gridBOM"  Margin="-13" FontSize="12" Grid.Column="0" Grid.Row="0"  HorizontalAlignment="Stretch" VerticalAlignment="Stretch" >
