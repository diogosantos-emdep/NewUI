                    <StackPanel Grid.Column="1" Height="auto" Orientation="Vertical" VerticalAlignment="Stretch" HorizontalAlignment="Stretch">
                        <Label Content="Pages" FontFamily="calibri" FontWeight="Bold" HorizontalContentAlignment="Left" Background="#515151" FontSize="20" Margin="2,-3,0,0"></Label>
                        <dxe:ListBoxEdit ShowBorder="False" Name="lstSheets" SelectedIndexChanged="ListBoxEdit_SelectedIndexChanged" VerticalAlignment="Bottom" HorizontalAlignment="Left" Margin="10,0,0,0" Height="593" Width="250"   />
                    </StackPanel>
