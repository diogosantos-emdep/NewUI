        <Grid.RowDefinitions>
            <RowDefinition Height="50"/>
            <RowDefinition Height="50*"/>
        </Grid.RowDefinitions>
        <dxnav:TileNavPane VerticalAlignment="Top"  Grid.Row="0" Panel.ZIndex="1"   FlyoutShowMode="Popup" Margin="0,0,10,0" >
       
        <dxnav:TileNavPane.NavButtons>
                <dxnav:NavButton Content="VIEW" FontFamily="Segoe UI" FontWeight="Bold"   >
                    <dxnav:TileNavSubItem Content="Assembly" FontFamily="Segoe UI" Click="TileNavSubItem_Click"    >                       
                    
                    </dxnav:TileNavSubItem>
                    <dxnav:TileNavSubItem Content="Connector" FontFamily="Segoe UI"  Click="TileNavSubItem_Click_1" />
                    <dxnav:TileNavSubItem Content="Drawing"  FontFamily="Segoe UI" Click="TileNavSubItem_Click_2" />
                </dxnav:NavButton>
                <dxnav:NavButton Content="B.O.M" FontFamily="Segoe UI" FontWeight="Bold" Click="NavButton_Click"  ></dxnav:NavButton>
                <dxnav:NavButton Content="REVISIONS" FontFamily="Segoe UI" FontWeight="Bold" Click="NavButton_Click_1" ></dxnav:NavButton>
            </dxnav:TileNavPane.NavButtons>
        </dxnav:TileNavPane>

