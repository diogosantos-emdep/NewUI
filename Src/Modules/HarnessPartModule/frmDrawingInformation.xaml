    <Window.Resources>
        <DataTemplate x:Key="FieldValueTemplate">
            <ContentPresenter Content="{Binding}" >
                <ContentPresenter.LayoutTransform>
                    <RotateTransform CenterX="0.5" CenterY="0.5" Angle="90"/>
                </ContentPresenter.LayoutTransform>
            </ContentPresenter>
        </DataTemplate>

        <Image x:Key="certification" Source="/Emdep.Geos.Modules.HarnessPart;component/Image/certification.png"></Image>
        <Image x:Key="Transparent" Source="/Emdep.Geos.Modules.HarnessPart;component/Image/transparent.gif"></Image>

      

    </Window.Resources>


