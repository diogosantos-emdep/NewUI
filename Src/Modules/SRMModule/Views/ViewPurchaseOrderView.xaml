<dxwui:WinUIDialogWindow  x:Class="Emdep.Geos.Modules.SRM.Views.ViewPurchaseOrderView"
                            xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                            xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                            xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
                            xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
                            xmlns:local="clr-namespace:Emdep.Geos.Modules.SRM.Views"
                            xmlns:ViewModels="clr-namespace:Emdep.Geos.Modules.SRM.ViewModels"
                            xmlns:dxnav="http://schemas.devexpress.com/winfx/2008/xaml/navigation"
                            xmlns:dxn="http://schemas.devexpress.com/winfx/2008/xaml/navbar"
                            xmlns:dxwui="http://schemas.devexpress.com/winfx/2008/xaml/windowsui" 
                            xmlns:dxwuin="http://schemas.devexpress.com/winfx/2008/xaml/windowsui/navigation"
                            xmlns:dxmvvm="http://schemas.devexpress.com/winfx/2008/xaml/mvvm"  
                            xmlns:dxg="http://schemas.devexpress.com/winfx/2008/xaml/grid"
                            xmlns:dxlc="http://schemas.devexpress.com/winfx/2008/xaml/layoutcontrol" 
                            xmlns:UIHelper="clr-namespace:Emdep.Geos.UI.Helper;assembly=Emdep.Geos.UI.Helper"
                            xmlns:dxe="http://schemas.devexpress.com/winfx/2008/xaml/editors"
                            xmlns:Converters="clr-namespace:Emdep.Geos.UI.Converters;assembly=Emdep.Geos.UI.Converters"
                            xmlns:GeosApplication="clr-namespace:Emdep.Geos.UI.Common;assembly=Emdep.Geos.UI.Common"
                            xmlns:behaviors="clr-namespace:Emdep.Geos.UI.Behaviors;assembly=Emdep.Geos.UI.Common"     
                            xmlns:dxgt="http://schemas.devexpress.com/winfx/2008/xaml/grid/themekeys"
                            xmlns:sysglb="clr-namespace:System.Globalization;assembly=mscorlib"
                            xmlns:dx="http://schemas.devexpress.com/winfx/2008/xaml/core"
                            xmlns:core="http://schemas.devexpress.com/winfx/2008/xaml/core"
                            xmlns:UIConverter="clr-namespace:Emdep.Geos.UI.Converters;assembly=Emdep.Geos.UI.Converters"
                                                
                          
                            d:DesignHeight="300" d:DesignWidth="300"
                            Height="{Binding SystemParameters.PrimaryScreenHeight}" 
                            Width="{Binding SystemParameters.PrimaryScreenWidth}"
                            HorizontalAlignment="Stretch"
                            VerticalAlignment="Stretch"
                            HorizontalContentAlignment="Stretch"
                            VerticalContentAlignment="Stretch"
                            Template="{StaticResource Maximized_DXDialogWindow}"
                            Style="{DynamicResource WinUIDialogWindowStyle}"
                            mc:Ignorable="d">


    <dxwui:WinUIDialogWindow.InputBindings>
        <KeyBinding Key="Escape" Command="{Binding CancelButtonCommand}" />
    </dxwui:WinUIDialogWindow.InputBindings>

    <dxwui:WinUIDialogWindow.Resources>
        <UIConverter:ImageUrlToByteArrayConverter x:Key="ImageUrlToByteArrayConverter" />
        <Converters:ForegroundColorConverter x:Key="ForegroundColorConverter"/>

        <Style x:Key="myGroupBoxStyle" TargetType="{x:Type dxlc:GroupBox}">
            <Setter Property="MaximizeElementVisibility" Value="Visible" />
            <Setter Property="Width" Value="300" />
            <Setter Property="Height" Value="140" />
        </Style>

        <SolidColorBrush x:Key="ColorMain" Color="White" />
        <Converters:GridMasterDetailButtonVisibilityConverter x:Key="GridMasterDetailButtonVisibilityConverter"/>

        <DataTemplate x:Key="ColumnHeaderTemplate">
            <ContentControl>
                <dxg:GridColumnHeader>
                    <dxg:GridColumnHeader.Template>
                        <ControlTemplate>
                            <Border BorderBrush="Black" BorderThickness="1" Height="45" Margin="0">
                                <Label Content="{Binding}" 
                                     FontWeight="Bold" />
                            </Border>
                        </ControlTemplate>
                    </dxg:GridColumnHeader.Template>
                </dxg:GridColumnHeader>
            </ContentControl>
        </DataTemplate>

    </dxwui:WinUIDialogWindow.Resources>

    <dxwui:WinUIDialogWindow.TitleStyle>
        <Style TargetType="ContentControl">
            <Setter Property="ContentTemplate">
                <Setter.Value>
                    <DataTemplate>
                        <Grid ShowGridLines="False" Grid.Row="0" HorizontalAlignment="Stretch" VerticalAlignment="Stretch">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="Auto"/>
                                <ColumnDefinition Width="40"/>
                                <ColumnDefinition Width="Auto"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="50"/>
                                <ColumnDefinition Width="5"/>
                                <ColumnDefinition Width="50"/>
                            </Grid.ColumnDefinitions>

                            <TextBlock Text="{DynamicResource ViewPurchaseOrder}" Style="{DynamicResource CustomTitleStyle}"/>
                            <dxe:ListBoxEdit x:Name="WorkflowStatusBtns" Grid.Column="2"
                                 ItemsSource="{Binding DataContext.WorkflowStatusButtons,UpdateSourceTrigger=PropertyChanged,Mode=TwoWay,RelativeSource={RelativeSource AncestorType={x:Type dxwui:WinUIDialogWindow}}}"
                                 SelectedItem="{Binding DataContext.SelectedWorkflowStatusButton, Mode=TwoWay,UpdateSourceTrigger=PropertyChanged,RelativeSource={RelativeSource AncestorType={x:Type dxwui:WinUIDialogWindow}}}"
                                 AllowItemHighlighting="False" SelectionMode="Single" Focusable="False" Margin="0,0,0,0" ShowBorder="False" >
                                <dxe:ListBoxEdit.ItemsPanel>
                                    <ItemsPanelTemplate>
                                        <StackPanel Orientation="Horizontal" HorizontalAlignment="Stretch" >
                                        </StackPanel>
                                    </ItemsPanelTemplate>
                                </dxe:ListBoxEdit.ItemsPanel>
                                <dxe:ListBoxEdit.ItemTemplate>
                                    <DataTemplate>
                                        <Button x:Name="WorkflowStatusBtn" Style="{DynamicResource Button16Style}"
                                                     Content="{Binding Name}"
                                                     IsEnabled="True"
                                                     Height="30"
                                                     Width="200"
                                                Command="{Binding DataContext.WorkflowButtonClickCommand, RelativeSource={RelativeSource AncestorType={x:Type dxwui:WinUIDialogWindow}}}"
                                                CommandParameter="{Binding IdWorkflowStatus}"
                                                Background="{Binding HtmlColor}" core:ThemeManager.ThemeName="None"/>
                                    </DataTemplate>
                                </dxe:ListBoxEdit.ItemTemplate>
                            </dxe:ListBoxEdit>

                            <!--Send Mail Button-->

                            <Button Content=" " Grid.Column="6" Height="40" Width="40" HorizontalAlignment="Right"
                                                Command="{Binding DataContext.SendEmailCommand, RelativeSource={RelativeSource AncestorType={x:Type dxwui:WinUIDialogWindow}}}">
                                <Button.ToolTip>
                                    <TextBlock Text="{DynamicResource SRMSendPOrequestMail}"/>
                                </Button.ToolTip>
                                <Button.ContentTemplate>
                                    <DataTemplate>
                                        <Image Style="{DynamicResource SendEmailButtonImageStyle}"  Height="35" Width="30" Margin="-10"/>
                                    </DataTemplate>
                                </Button.ContentTemplate>
                            </Button>

                            <!--Edit Button-->
                            <Button Content=" " Grid.Column="4" Height="40" Width="40" HorizontalAlignment="Right"
                            Margin="0,0,5,5" Visibility="Hidden">
                                <Button.ToolTip>
                                    <TextBlock Text="Signature"/>
                                </Button.ToolTip>
                                <Button.ContentTemplate>
                                    <DataTemplate>
                                        <Image Style="{ DynamicResource EditButtonImageStyle}"  Height="24" Width="24" Margin="-10"/>
                                    </DataTemplate>
                                </Button.ContentTemplate>
                            </Button>
                        </Grid>
                    </DataTemplate>
                </Setter.Value>
            </Setter>
        </Style>
    </dxwui:WinUIDialogWindow.TitleStyle>
    <!--[GEOS2-4653][rdixit][12.07.2023] Made design compatible with diff resolution-->
    <!--<ScrollViewer HorizontalScrollBarVisibility="Auto" VerticalScrollBarVisibility="Auto" >-->
    <!--Commented to avoid unnecessary vertical scroll bar-->

    <!--[pramod.misal][GEOS2-5082][28-12-2023]-->
    <Grid HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Margin="10,10,10,10" Width="{Binding DialogWidth}" >
        <Grid.RowDefinitions>
            <RowDefinition Height="80*" />
            <RowDefinition Height="10*" />
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="100*"/>
            <ColumnDefinition Width="Auto"/>
            <ColumnDefinition Width="Auto"/>
        </Grid.ColumnDefinitions>

        <dxlc:FlowLayoutControl Grid.Row="0"
			Grid.Column="0"
			Grid.ColumnSpan="2"
			AllowLayerSizing="False"
            Orientation="Horizontal"
            ItemSpace="5" 
            ScrollBars="Auto"
            LayerSpace="5" 
            Background="Transparent" 
            AllowMaximizedElementMoving="False" 
            VerticalAlignment="Stretch"
            HorizontalAlignment="Stretch"					
			BreakFlowToFit="False"                
			DragScrolling="True"
			MaximizedElementPosition="Right"
			Padding="5"                   
			StretchContent="False">
            <dxmvvm:Interaction.Behaviors>
                <UIHelper:FlowLayoutControlForMinimiseMaximise />
            </dxmvvm:Interaction.Behaviors>

            <!--PO Information-->
            <dxlc:GroupBox x:Name="Information" MaximizeElementVisibility="Visible"
				    Selector.IsSelected="True"
				    Style="{StaticResource myGroupBoxStyle}">

                <dxlc:GroupBox.HeaderTemplate>
                    <DataTemplate>
                        <DockPanel Margin="-5,0,0,0">
                            <TextBlock Margin="5,0,0,0"
								VerticalAlignment="Center"
								FontWeight="Bold"
								Text="{DynamicResource POInformation}"/>
                        </DockPanel>
                    </DataTemplate>
                </dxlc:GroupBox.HeaderTemplate>

                <dxlc:GroupBox.ContentTemplate>

                    <DataTemplate>
                        <Grid>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="Auto" />
                                <ColumnDefinition Width="*" />
                            </Grid.ColumnDefinitions>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="30" />
                                <RowDefinition Height="30" />
                                <RowDefinition Height="*" />
                            </Grid.RowDefinitions>

                            <Label Grid.Row="0"
								Grid.Column="0"
								Margin="2"
								Content="{DynamicResource WMSCode}"
								FontWeight="Bold"
								Padding="2" />
                            <TextBlock Text="{Binding DataContext.WarehousePurchaseOrder.Code, RelativeSource={RelativeSource AncestorType={x:Type dxlc:GroupBox}}}" VerticalAlignment="Center"  Grid.Row="0" Grid.Column="1" Margin="3,0,0,0" Padding="2"/>
                            <Label Grid.Row="1"
								Grid.Column="0"
								Margin="2"
								Content="{DynamicResource Supplier}"
								FontWeight="Bold"
								Padding="2" />
                            <TextBlock Text="{Binding DataContext.WarehousePurchaseOrder.ArticleSupplier.Name, RelativeSource={RelativeSource AncestorType={x:Type dxlc:GroupBox}}}" VerticalAlignment="Center"  Grid.Row="1" Grid.Column="1" Margin="3,0,0,0" Padding="2"
                                ToolTip="{Binding DataContext.WarehousePurchaseOrder.ArticleSupplier.Name, RelativeSource={RelativeSource AncestorType={x:Type dxlc:GroupBox}}}"  TextWrapping="WrapWithOverflow" Width="Auto" TextTrimming="CharacterEllipsis"/>
                            <Label Grid.Row="2"
								Grid.Column="0"
								Margin="2"
								Content="{DynamicResource ExpectedDate}"
								FontWeight="Bold"
								Padding="2" />

                            <TextBlock Text="{Binding DataContext.WarehousePurchaseOrder.DeliveryDate, RelativeSource={RelativeSource AncestorType={x:Type dxlc:GroupBox}},StringFormat=d,ConverterCulture={x:Static sysglb:CultureInfo.CurrentCulture}}" 
                             VerticalAlignment="Center"  Grid.Row="2" Grid.Column="1" Margin="3,0,0,0" Padding="2">
                                <TextBlock.Style>
                                    <Style>
                                        <Style.Triggers>
                                            <DataTrigger Value="True" Binding="{Binding DataContext.WarehousePurchaseOrder.DeliveryDate,Converter={Converters:DateCellColorConvertor},RelativeSource={RelativeSource AncestorType={x:Type dxlc:GroupBox}}}">
                                                <Setter Property="TextBlock.Foreground" Value="{DynamicResource SleepDaysRedColor}"/>
                                            </DataTrigger>
                                        </Style.Triggers>
                                    </Style>
                                </TextBlock.Style>
                            </TextBlock>
                        </Grid>
                    </DataTemplate>
                </dxlc:GroupBox.ContentTemplate>

                <dxlc:GroupBox.MaximizedContentTemplate>
                    <DataTemplate>
                        <!--Informatiom-->

                        <Grid HorizontalAlignment="Stretch" VerticalAlignment="Stretch" ScrollViewer.VerticalScrollBarVisibility="Visible" ScrollViewer.HorizontalScrollBarVisibility="Auto">
                            <Grid.RowDefinitions >
                                <RowDefinition Height="auto"/>
                                <RowDefinition Height="20"/>
                                <RowDefinition Height="auto"/>
                                <RowDefinition Height="*"/>
                                <RowDefinition Height="10"/>
                                <RowDefinition Height="auto" MaxHeight="90"/>
                            </Grid.RowDefinitions>

                            <dxlc:LayoutGroup View="GroupBox" Header="{DynamicResource Information}" IsCollapsible="True" Grid.Row="0" ScrollViewer.VerticalScrollBarVisibility="Auto">

                                <ScrollViewer HorizontalScrollBarVisibility="Auto" VerticalScrollBarVisibility="Auto" >
                                    <Grid HorizontalAlignment="Stretch" VerticalAlignment="Stretch" ShowGridLines="False" >
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="30"/>
                                            <RowDefinition Height="5"/>
                                            <RowDefinition Height="30"/>
                                            <RowDefinition Height="5"/>
                                            <RowDefinition Height="30"/>
                                            <RowDefinition Height="5"/>
                                            <RowDefinition Height="30"/>
                                        </Grid.RowDefinitions>

                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="Auto"/>
                                            <ColumnDefinition Width="5"/>
                                            <ColumnDefinition Width="Auto"/>
                                            <ColumnDefinition Width="10"/>
                                            <ColumnDefinition Width="Auto"/>
                                            <ColumnDefinition Width="5"/>
                                            <ColumnDefinition Width="Auto"/>
                                            <ColumnDefinition Width="10"/>
                                            <ColumnDefinition Width="50*"/>
                                            <ColumnDefinition Width="5"/>
                                            <ColumnDefinition Width="Auto"/>
                                            <ColumnDefinition Width="10"/>
                                            <ColumnDefinition Width="30*"/>
                                            <ColumnDefinition Width="5"/>
                                            <ColumnDefinition Width="Auto"/>
                                            <ColumnDefinition Width="10"/>
                                            <ColumnDefinition Width="Auto"/>
                                            <ColumnDefinition Width="5"/>
                                            <ColumnDefinition Width="Auto"/>
                                            <ColumnDefinition Width="10"/>
                                        </Grid.ColumnDefinitions>

                                        <!--Code-->
                                        <TextBlock Text="{DynamicResource WMSCode}" Grid.Row="0" Grid.Column="0" VerticalAlignment="Center"  HorizontalAlignment="Left"/>
                                        <TextBlock Text="{Binding DataContext.WarehousePurchaseOrder.Code, RelativeSource={RelativeSource AncestorType={x:Type dxlc:GroupBox}}}" VerticalAlignment="Center"  Grid.Row="0" Grid.Column="2"/>

                                        <!--Supplier-->
                                        <TextBlock Text="{DynamicResource Supplier}" VerticalAlignment="Center"  HorizontalAlignment="Left" Grid.Row="2" Grid.Column="0"/>
                                        <DockPanel Grid.Row="2" Grid.Column="2" LastChildFill="True" Grid.ColumnSpan="6">
                                            <TextBlock Text="{Binding DataContext.WarehousePurchaseOrder.ArticleSupplier.Name, RelativeSource={RelativeSource AncestorType={x:Type dxlc:GroupBox}}}" VerticalAlignment="Center" HorizontalAlignment="Stretch" TextWrapping="WrapWithOverflow"  />
                                        </DockPanel>
                                        <!--View Supplier-->
                                        <!--[pramod.misal][GEOS2-4408][20.06.2023]-->
                                        <dxe:HyperlinkEdit AllowAutoNavigate="True" Text="[View Supplier]"
                                                           ToolTip="[View Supplier]"
                                                           ShowNavigationUrlToolTip="False"
                                                           VerticalAlignment="Center"  Grid.Row="2" Grid.Column="8" Command="{Binding DataContext.OpenViewSupplierCommand,RelativeSource={RelativeSource AncestorType={x:Type dxlc:GroupBox}}}"/>

                                        <!--Country-->
                                        <TextBlock Text="{DynamicResource SRM_POCountry}" VerticalAlignment="Center"  HorizontalAlignment="Left" Grid.Row="4" Grid.Column="0" />

                                        <StackPanel Orientation="Horizontal" Height="24" Grid.Row="4" Grid.Column="2" Grid.ColumnSpan="6">
                                            <Image Source="{Binding DataContext.WarehousePurchaseOrder.ArticleSupplier.Country.CountryIconImage,RelativeSource={RelativeSource AncestorType={x:Type dxlc:GroupBox}}}"  
                                                    VerticalAlignment="Center" Height="24" Width="24" Margin="0,0,5,0" />
                                            <TextBlock Text="{Binding DataContext.WarehousePurchaseOrder.ArticleSupplier.Country.Name, RelativeSource={RelativeSource AncestorType={x:Type dxlc:GroupBox}}}"  VerticalAlignment="Center"/>
                                        </StackPanel>

                                        <!--Expected date-->
                                        <TextBlock Text="{DynamicResource ExpectedDate}" VerticalAlignment="Center"  HorizontalAlignment="Left" Grid.Row="6" Grid.Column="0"/>
                                        <TextBlock Text="{Binding DataContext.WarehousePurchaseOrder.DeliveryDate, RelativeSource={RelativeSource AncestorType={x:Type dxlc:GroupBox}},StringFormat=d,ConverterCulture={x:Static sysglb:CultureInfo.CurrentCulture}}"                                                   
                                         VerticalAlignment="Center" HorizontalAlignment="Stretch" Grid.Row="6" Grid.Column="2">
                                            <TextBlock.Style>
                                                <Style>
                                                    <Style.Triggers>
                                                        <DataTrigger Value="True" Binding="{Binding DataContext.WarehousePurchaseOrder.DeliveryDate,Converter={Converters:DateCellColorConvertor},RelativeSource={RelativeSource AncestorType={x:Type dxlc:GroupBox}}}">
                                                            <Setter Property="TextBlock.Foreground" Value="{DynamicResource SleepDaysRedColor}"/>
                                                        </DataTrigger>
                                                    </Style.Triggers>
                                                </Style>
                                            </TextBlock.Style>
                                        </TextBlock>

                                        <!--Status-->
                                        <TextBlock Text="{DynamicResource Status}" Grid.Row="6" Grid.Column="4" VerticalAlignment="Center" HorizontalAlignment="Left"/>
                                        <dxe:TextEdit x:Name="txtStatus" 
                                                      Text="{Binding DataContext.WarehousePurchaseOrder.WorkflowStatus.Name, RelativeSource={RelativeSource AncestorType={x:Type dxlc:GroupBox}},UpdateSourceTrigger=PropertyChanged,Mode=TwoWay}" 
                                                      Background="{Binding DataContext.WarehousePurchaseOrder.WorkflowStatus.HtmlColor, RelativeSource={RelativeSource AncestorType={x:Type dxlc:GroupBox}},UpdateSourceTrigger=PropertyChanged,Mode=TwoWay}"
                                                      Foreground="{Binding ElementName=txtStatus, Path=Background, Converter={Converters:ForegroundColorConverter}}"
                                                      Grid.Column="6" Grid.Row="6" VerticalAlignment="Center" 
                                                      IsReadOnly="True" ShowBorder="False" TextWrapping="WrapWithOverflow"/>

                                        <!--<TextBlock Text="[View  Workflow]" VerticalAlignment="Center"  Grid.Row="6" Grid.Column="8"/>-->
                                        <dxe:HyperlinkEdit AllowAutoNavigate="True" Text="[View Workflow]"
                                                           ToolTip="[View Workflow]"
                                                           ShowNavigationUrlToolTip="False"
                                                           VerticalAlignment="Center"  Grid.Row="6" Grid.Column="8" Command="{Binding DataContext.OpenWorkflowDiagramCommand,RelativeSource={RelativeSource AncestorType={x:Type dxlc:GroupBox}}}"/>




                                        <!--Assignee-->
                                        <TextBlock Grid.Row="0" Grid.Column="10"  Text="{DynamicResource SRM_POAssignee}" Style="{StaticResource TextBlock14Style}" VerticalAlignment="Center"  />
                                        <dxe:ComboBoxEdit Grid.Row="0" Grid.Column="12" 
                                            VerticalAlignment="Stretch"
                                            HorizontalAlignment="Stretch"
                                            ItemsSource="{Binding DataContext.AssigneeList,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged, RelativeSource={RelativeSource AncestorType={x:Type dxlc:GroupBox}}}"
                                                          SelectedItem="{Binding DataContext.SelectedAssignee,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged, RelativeSource={RelativeSource AncestorType={x:Type dxlc:GroupBox}}}"
                                            DisplayMember="FullName" ValueMember="IdUser"
                                            AutoComplete="True" ImmediatePopup="True" IncrementalFiltering="True" 
                                            >
                                        </dxe:ComboBoxEdit>


                                        <!--Approver-->
                                        <TextBlock Grid.Row="2" Grid.Column="10"  Text="{DynamicResource SRM_POApprover}" Style="{StaticResource TextBlock14Style}" VerticalAlignment="Center"  />
                                        <dxe:ComboBoxEdit Grid.Row="2" Grid.Column="12" 
                                            VerticalAlignment="Stretch"
                                            HorizontalAlignment="Stretch"
                                            ItemsSource="{Binding DataContext.ApproverList,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged, RelativeSource={RelativeSource AncestorType={x:Type dxlc:GroupBox}}}"
                                                          SelectedItem="{Binding DataContext.SelectedApprover,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged, RelativeSource={RelativeSource AncestorType={x:Type dxlc:GroupBox}}}"
                                            DisplayMember="FullName" ValueMember="IdUser"
                                            AutoComplete="True" ImmediatePopup="True" IncrementalFiltering="True" 
                                            >
                                        </dxe:ComboBoxEdit>
                                        
                                        <!--Currency-->
                                        <!--[pallavi.kale][04-03-2025][GEOS2-7013]-->
                                        <TextBlock Text="{DynamicResource Currency}" VerticalAlignment="Center"  HorizontalAlignment="Right" Grid.Row="4" Grid.Column="10" />

                                        <StackPanel Orientation="Horizontal" Height="24" Grid.Row="4" Grid.Column="12" Grid.ColumnSpan="6">
                                            <Image Source="{Binding DataContext.WarehousePurchaseOrder.Currency.CurrencyIconImage,RelativeSource={RelativeSource AncestorType={x:Type dxlc:GroupBox}}}"  
                                              VerticalAlignment="Center" Height="24" Width="24" Margin="0,0,5,0" />

                                            <TextBlock Text="{Binding DataContext.WarehousePurchaseOrder.Currency.Name, RelativeSource={RelativeSource AncestorType={x:Type dxlc:GroupBox}}}"  VerticalAlignment="Center"/>
                                        </StackPanel>


                                        <!--CreatedBy-->
                                        <TextBlock Text="{DynamicResource CreatedBy}" VerticalAlignment="Center"  HorizontalAlignment="Left" Grid.Row="0" Grid.Column="14"/>
                                        <TextBlock Text="{Binding DataContext.WarehousePurchaseOrder.Creator.FullName, RelativeSource={RelativeSource AncestorType={x:Type dxlc:GroupBox}}}" Grid.Row="0" Grid.Column="16" VerticalAlignment="Center"/>

                                        <!--CreatedIn-->
                                        <TextBlock Text="{DynamicResource CreatedIn}" VerticalAlignment="Center"  HorizontalAlignment="Left" Grid.Row="2" Grid.Column="14"/>
                                        <TextBlock Text="{Binding DataContext.WarehousePurchaseOrder.CreatedIn, RelativeSource={RelativeSource AncestorType={x:Type dxlc:GroupBox}},StringFormat=d,ConverterCulture={x:Static sysglb:CultureInfo.CurrentCulture}}"  Grid.Row="2" Grid.Column="16" VerticalAlignment="Center"/>

                                        <!--ModifiedBy-->
                                        <TextBlock Text="{DynamicResource ModifiedBy}" VerticalAlignment="Center"  HorizontalAlignment="Left" Grid.Row="4" Grid.Column="14"/>
                                        <TextBlock Text="{Binding DataContext.WarehousePurchaseOrder.Modifier.FullName, RelativeSource={RelativeSource AncestorType={x:Type dxlc:GroupBox}}}" Grid.Row="4" Grid.Column="16" VerticalAlignment="Center"/>

                                        <!--ModifiedIn-->
                                        <TextBlock Text="{DynamicResource ModifiedIn}" VerticalAlignment="Center"  HorizontalAlignment="Left" Grid.Row="6" Grid.Column="14"/>
                                        <TextBlock Text="{Binding DataContext.WarehousePurchaseOrder.ModifiedIn, RelativeSource={RelativeSource AncestorType={x:Type dxlc:GroupBox}},StringFormat=d,ConverterCulture={x:Static sysglb:CultureInfo.CurrentCulture}}"  Grid.Row="6" Grid.Column="16" VerticalAlignment="Center" />

                                        <!--ArticleSupplierType-->
                                        <!--<TextBlock Grid.Row="1" Grid.RowSpan="4" Grid.Column="16" Margin="5,0,0,0" VerticalAlignment="Stretch" HorizontalAlignment="Right"  Text="{Binding DataContext.WarehousePurchaseOrder.ArticleSupplier.ArticleSupplierType.Name,RelativeSource={RelativeSource AncestorType=dxlc:GroupBox}}" 
                                               Background="{Binding DataContext.WarehousePurchaseOrder.ArticleSupplier.ArticleSupplierType.HtmlColor,RelativeSource={RelativeSource AncestorType=dxlc:GroupBox}}" 
                                               Width="64" Height="64" TextAlignment="Center" FontWeight="Bold" FontSize="40"/>-->

                                        <TextBlock Grid.Row="1" Grid.Column="18" Grid.RowSpan="4" VerticalAlignment="Stretch" HorizontalAlignment="Right"  Text="{Binding DataContext.WarehousePurchaseOrder.ArticleSupplier.ArticleSupplierType.Name,RelativeSource={RelativeSource AncestorType=dxlc:GroupBox}}" 
                                               Background="{Binding DataContext.WarehousePurchaseOrder.ArticleSupplier.ArticleSupplierType.HtmlColor,RelativeSource={RelativeSource AncestorType=dxlc:GroupBox}}" 
                                               Width="64" Height="64" TextAlignment="Center" FontWeight="Bold" FontSize="40"/>

                                    </Grid>
                                </ScrollViewer>
                            </dxlc:LayoutGroup>




                            <!--Items Grid-->
                            <dxlc:LayoutGroup Orientation="Vertical" Width="auto" Height="auto"
									            Grid.Row="2" Grid.RowSpan="2"
                                                ScrollBars="Auto"
									            Header="{DynamicResource Items}" 
									            MaxHeight="1000"
									            View="GroupBox" 
									            IsCollapsible="False"
									            IsCollapsed="False" ScrollViewer.VerticalScrollBarVisibility="Auto">

                                <Grid VerticalAlignment="Stretch" HorizontalAlignment="Stretch" ScrollViewer.HorizontalScrollBarVisibility="Visible"
                                      ScrollViewer.VerticalScrollBarVisibility="Visible">
                                    <dxg:GridControl x:Name="gridControl"
										            Margin="-10"
                                                    HorizontalContentAlignment="Stretch"
                                                    VerticalContentAlignment="Stretch"
                                                    ScrollViewer.VerticalScrollBarVisibility="Auto"
                                                    ScrollViewer.HorizontalScrollBarVisibility="Hidden"
										            HorizontalAlignment="Stretch" VerticalAlignment="Stretch"
										            ShowBorder="True" SelectionMode="Cell" MinHeight="100"
                                                    
										            ItemsSource="{Binding DataContext.WarehousePurchaseOrder.WarehousePurchaseOrderItemWithComments, Mode=TwoWay, RelativeSource={RelativeSource AncestorType={x:Type dxlc:GroupBox}}, UpdateSourceTrigger=PropertyChanged}" ImplyNullLikeEmptyStringWhenFiltering="True" >
                                        <dxg:GridControl.Resources>

                                            <Style TargetType="{x:Type dxg:DetailColumnHeadersControl}">
                                                <Setter Property="FontWeight" Value="Bold"/>
                                            </Style>
                                        </dxg:GridControl.Resources>

                                        <dxmvvm:Interaction.Behaviors>
                                            <UIHelper:BestFitBehavior/>
                                            <!--<dxmvvm:EventToCommand EventName="CustomSummary"
                                                     Command="{Binding DataContext.CustomSummaryCommand,RelativeSource={RelativeSource AncestorType={x:Type dxwui:WinUIDialogWindow}}}"
                                                     PassEventArgsToCommand="True">
                                            </dxmvvm:EventToCommand>-->
                                            <!--<dxmvvm:EventToCommand EventName="CustomSummary"
                                                     Command="{Binding DataContext.CustomSummaryCommand,RelativeSource={RelativeSource AncestorType={x:Type dxwui:WinUIDialogWindow}}}"
                                                     PassEventArgsToCommand="True" CommandParameter="{Binding Path=RowData.Row}">
                                            </dxmvvm:EventToCommand>-->
                                        </dxmvvm:Interaction.Behaviors>

                                        <dxg:GridControl.TotalSummary>
                                            <!--<dxg:GridSummaryItem x:Name="summary" FieldName="Article.Reference" SummaryType="Count" DisplayFormat="Total Count: {0}"/>-->
                                            <!--<dxg:GridSummaryItem x:Name="summary1" FieldName="Article.Reference" SummaryType="Count" DisplayFormat="Total Count: {0}"/>-->
                                            <!--<dxg:GridSummaryItem x:Name="summary" FieldName="Summary" SummaryType="Average" DisplayFormat="Total Count: {0}"/>-->
                                            <!--<dxg:GridSummaryItem x:Name="IdArticle" FieldName="IdArticle" SummaryType="Custom" Visible="True"/>-->
                                            <!--<dxg:GridSummaryItem x:Name="Summary" FieldName="Summary" SummaryType="Sum" DisplayFormat="Total Count: {0}"/>-->

