                <Setter Property="FontWeight" Value="Bold" />
                <Setter Property="Background" Value="Transparent" />
                <Setter Property="Foreground" Value="Transparent" />
                <Setter Property="TextBlock.Foreground" Value="#FF3366CC" />
            </Trigger>
        </Style.Triggers>
    </Style>

