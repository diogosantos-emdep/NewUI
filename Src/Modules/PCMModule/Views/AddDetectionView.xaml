                                                <Interactive:InvokeCommandAction Command="{Binding DataContext.SelectedTestChangedCommand,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged,RelativeSource={RelativeSource AncestorType=dxlc:GroupBox}}" />
                                            </Interactive:EventTrigger>
                                        </Interactive:Interaction.Triggers>

                                    </dxe:ComboBoxEdit>

                                    <!--Description-->
                                    <TextBlock Grid.Row="3" Grid.Column="1" Text="{DynamicResource Description}" Style="{StaticResource TextBlock14Style}" VerticalAlignment="Center" />
