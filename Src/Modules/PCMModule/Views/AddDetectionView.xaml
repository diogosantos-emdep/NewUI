                                        </dxmvvm:Interaction.Behaviors>
                                    </dxe:TextEdit>

                                    <!--<CopyNameAndDescription Checkbox>-->
                                    <dxe:CheckEdit Grid.Row="5" Grid.Column="3" IsChecked="{Binding DataContext.IsCheckedCopyNameDescription, Mode=TwoWay,UpdateSourceTrigger=PropertyChanged,RelativeSource={RelativeSource AncestorType={x:Type dxlc:GroupBox}}}" IsReadOnly="False" HorizontalAlignment="Left" VerticalAlignment="Top">
                                    </dxe:CheckEdit>
                                    <TextBlock Grid.Row="5" Grid.Column="3" Grid.ColumnSpan="1" Margin="20 0 0 0"  Text="{DynamicResource CopyNameDescription}" Style="{StaticResource TextBlock14Style}" VerticalAlignment="Top" />

                                    <!--<Status>-->
